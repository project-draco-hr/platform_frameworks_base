{
  if (!PATH_CHANNEL.equals(channelUri.getPathSegments().get(0))) {
    throw new IllegalArgumentException("Not a channel: " + channelUri);
  }
  String channelId=String.valueOf(ContentUris.parseId(channelUri));
  return new Uri.Builder().scheme(ContentResolver.SCHEME_CONTENT).authority(AUTHORITY).appendPath(PATH_CHANNEL).appendPath(channelId).appendPath(PATH_PROGRAM).build();
}
