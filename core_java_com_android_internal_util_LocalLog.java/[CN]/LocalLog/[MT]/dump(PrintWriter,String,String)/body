{
synchronized (mLines) {
    if (mLines.size() <= 0) {
      return false;
    }
    if (header != null) {
      pw.println(header);
    }
    for (int i=0; i < mLines.size(); i++) {
      if (prefix != null) {
        pw.print(prefix);
      }
      pw.println(mLines.get(i));
    }
    return true;
  }
}
