{
  int code=STATUS_ERROR;
  try {
    code=mModelManagementService.stopRecognition(mVoiceInteractionService,mKeyphraseMetadata.id,mInternalCallback);
  }
 catch (  RemoteException e) {
    Slog.w(TAG,"RemoteException in stopRecognition!",e);
  }
  if (code != STATUS_OK) {
    Slog.w(TAG,"stopRecognition() failed with error code " + code);
  }
  return code;
}
