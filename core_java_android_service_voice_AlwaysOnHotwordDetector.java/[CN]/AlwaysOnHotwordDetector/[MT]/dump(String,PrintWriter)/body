{
synchronized (mLock) {
    pw.print(prefix);
    pw.print("Text=");
    pw.println(mText);
    pw.print(prefix);
    pw.print("Locale=");
    pw.println(mLocale);
    pw.print(prefix);
    pw.print("Availability=");
    pw.println(mAvailability);
    pw.print(prefix);
    pw.print("KeyphraseMetadata=");
    pw.println(mKeyphraseMetadata);
    pw.print(prefix);
    pw.print("EnrollmentInfo=");
    pw.println(mKeyphraseEnrollmentInfo);
  }
}
