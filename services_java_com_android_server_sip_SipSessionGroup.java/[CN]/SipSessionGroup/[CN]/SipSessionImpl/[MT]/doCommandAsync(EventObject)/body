{
  new Thread(new Runnable(){
    public void run(){
      try {
        processCommand(command);
      }
 catch (      SipException e) {
        Log.w(TAG,"command error: " + command,e);
        if ((command instanceof RegisterCommand) || (command == DEREGISTER)) {
          onRegistrationFailed(e);
        }
 else {
          onError(e);
        }
      }
    }
  }
).start();
}
