{
  CompareResult<RouteInfo> result=new CompareResult<RouteInfo>();
  result.removed=getAllRoutes();
  result.added.clear();
  if (target != null) {
    for (    RouteInfo r : target.getAllRoutes()) {
      if (!result.removed.remove(r)) {
        result.added.add(r);
      }
    }
  }
  return result;
}
