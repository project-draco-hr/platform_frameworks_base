{
  if (whichUser == UserHandle.USER_SYSTEM) {
    if (mGlobalDisable) {
      Slog.i(TAG,"Backup/restore not supported");
      return;
    }
synchronized (this) {
      if (!mSuppressFile.exists()) {
        mService=new BackupManagerService(mContext,this);
      }
 else {
        Slog.i(TAG,"Backup inactive in user " + whichUser);
      }
    }
  }
}
