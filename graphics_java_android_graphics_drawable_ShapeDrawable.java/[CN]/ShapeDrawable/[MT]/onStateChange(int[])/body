{
  final ColorStateList tint=mShapeState.mTint;
  if (tint != null) {
    final int newColor=tint.getColorForState(stateSet,0);
    final int oldColor=mTintFilter.getColor();
    if (oldColor != newColor) {
      mTintFilter.setColor(newColor);
      invalidateSelf();
      return true;
    }
  }
  return false;
}
