{
switch (securityMode) {
case None:
    return SECURITY_SELECTOR_ID;
case Pattern:
  return SECURITY_PATTERN_ID;
case Password:
return SECURITY_PASSWORD_ID;
case Biometric:
return SECURITY_BIOMETRIC_ID;
case Account:
return SECURITY_ACCOUNT_ID;
case SimPin:
return SECURITY_SIM_PIN_ID;
case SimPuk:
return SECURITY_SIM_PUK_ID;
}
return 0;
}
