{
synchronized (mLock) {
    if (DEBUG) {
      Slog.d(TAG,"onContentAllowed()");
    }
    if (sessionState.session == null || sessionState.client == null) {
      return;
    }
    try {
      sessionState.client.onContentAllowed(sessionState.seq);
    }
 catch (    RemoteException e) {
      Slog.e(TAG,"error in onContentAllowed",e);
    }
  }
}
