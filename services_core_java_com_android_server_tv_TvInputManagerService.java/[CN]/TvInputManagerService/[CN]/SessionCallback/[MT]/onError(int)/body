{
synchronized (mLock) {
    if (DEBUG) {
      Slog.d(TAG,"onError()");
    }
    if (mSessionState.session == null || mSessionState.client == null) {
      return;
    }
    try {
      mSessionState.client.onError(error,mSessionState.seq);
    }
 catch (    RemoteException e) {
      Slog.e(TAG,"error in onError",e);
    }
  }
}
