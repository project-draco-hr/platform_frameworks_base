{
  final int callingUid=Binder.getCallingUid();
  final int resolvedUserId=resolveCallingUserId(Binder.getCallingPid(),callingUid,userId,"sendAppPrivateCommand");
  final long identity=Binder.clearCallingIdentity();
  try {
synchronized (mLock) {
      try {
        getSessionLocked(sessionToken,callingUid,resolvedUserId).appPrivateCommand(command,data);
      }
 catch (      RemoteException e) {
        Slog.e(TAG,"error in appPrivateCommand",e);
      }
    }
  }
  finally {
    Binder.restoreCallingIdentity(identity);
  }
}
