{
  final int callingUid=Binder.getCallingUid();
  final int resolvedUserId=resolveCallingUserId(Binder.getCallingPid(),callingUid,userId,"setSurface");
  final long identity=Binder.clearCallingIdentity();
  try {
synchronized (mLock) {
      try {
        getSessionLocked(sessionToken,callingUid,resolvedUserId).setSurface(surface);
      }
 catch (      RemoteException e) {
        Log.e(TAG,"error in setSurface",e);
      }
    }
  }
  finally {
    Binder.restoreCallingIdentity(identity);
  }
}
