    }
    @Override public void onLayoutSurface(    int left,    int top,    int right,    int bottom){
synchronized (mLock) {
        if (DEBUG) {
          Slog.d(TAG,"onLayoutSurface (left=" + left + ", top="+ top+ ", right="+ right+ ", bottom="+ bottom+ ",)");
        }
        if (sessionState.mSession == null || sessionState.mClient == null) {
          return;
