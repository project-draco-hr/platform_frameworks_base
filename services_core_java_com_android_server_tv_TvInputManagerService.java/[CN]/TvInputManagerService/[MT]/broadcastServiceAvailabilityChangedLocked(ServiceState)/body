{
  for (  IBinder clientToken : serviceState.mClientTokens) {
    try {
      ITvInputClient.Stub.asInterface(clientToken).onAvailabilityChanged(serviceState.mTvInputInfo.getId(),serviceState.mAvailable);
    }
 catch (    RemoteException e) {
      Slog.e(TAG,"error in onAvailabilityChanged",e);
    }
  }
}
