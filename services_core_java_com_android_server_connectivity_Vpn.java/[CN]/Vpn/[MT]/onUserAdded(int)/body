{
synchronized (Vpn.this) {
    UserManager mgr=UserManager.get(mContext);
    UserInfo user=mgr.getUserInfo(userId);
    if (user.isRestricted()) {
      try {
        addVpnUserLocked(userId);
      }
 catch (      Exception e) {
        Log.wtf(TAG,"Failed to add restricted user to owner",e);
      }
    }
  }
}
