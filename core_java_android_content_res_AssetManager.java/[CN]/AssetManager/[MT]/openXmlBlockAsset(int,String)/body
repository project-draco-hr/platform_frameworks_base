{
synchronized (mSync) {
    if (!mOpen) {
      throw new RuntimeException("Assetmanager has been closed");
    }
    int xmlBlock=openXmlAssetNative(cookie,fileName);
    if (xmlBlock != 0) {
      mNumRefs++;
      return new XmlBlock(this,xmlBlock);
    }
  }
  throw new FileNotFoundException("Asset XML file: " + fileName);
}
