{
  long start=System.currentTimeMillis();
  robotTex=Allocation.createFromBitmapResource(mRS,mRes,R.drawable.robot,MipmapControl.MIPMAP_ON_SYNC_TO_TEXTURE,Allocation.USAGE_GRAPHICS_TEXTURE);
  FileA3D model=FileA3D.createFromResource(mRS,mRes,R.raw.robot);
  FileA3D.IndexEntry entry=model.getIndexEntry(0);
  if (entry != null && entry.getEntryType() == FileA3D.EntryType.MESH) {
    robotMesh=entry.getMesh();
  }
  initPFS();
  initPF();
  initPV();
  long end=System.currentTimeMillis();
  Log.v("TIMER","Loading load time: " + (end - start));
  return new Boolean(true);
}
