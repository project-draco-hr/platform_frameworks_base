{
switch (state) {
case IDLE:
case INITING:
    break;
case CONNECTING:
case SCANNING:
  phone.notifyDataConnection(reason,apnIdToType(apnId),Phone.DataState.CONNECTING);
break;
case CONNECTED:
case DISCONNECTING:
phone.notifyDataConnection(reason,apnIdToType(apnId),Phone.DataState.CONNECTING);
phone.notifyDataConnection(reason,apnIdToType(apnId),Phone.DataState.CONNECTED);
break;
}
}
