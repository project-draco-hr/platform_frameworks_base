{
synchronized (mPackages) {
    if (!isExternalMediaAvailable()) {
      return null;
    }
    final ArrayList<PackageCleanItem> pkgs=mSettings.mPackagesToBeCleaned;
    if (lastPackage != null) {
      pkgs.remove(lastPackage);
    }
    if (pkgs.size() > 0) {
      return pkgs.get(0);
    }
  }
  return null;
}
