{
  int count=1;
  int i=0;
  while ((i=str.indexOf(sep,i)) >= 0) {
    count++;
    i++;
  }
  String[] res=new String[count];
  i=0;
  count=0;
  int lastI=0;
  while ((i=str.indexOf(sep,i)) >= 0) {
    res[count]=str.substring(lastI,i);
    count++;
    i++;
    lastI=i;
  }
  res[count]=str.substring(lastI,str.length());
  return res;
}
