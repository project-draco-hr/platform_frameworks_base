{
synchronized (mPackages) {
    PackageParser.Package p=mPackages.get(packageName);
    if (DEBUG_PACKAGE_INFO)     Log.v(TAG,"getPackageGids" + packageName + ": "+ p);
    if (p != null) {
      final PackageSetting ps=(PackageSetting)p.mExtras;
      return ps.getGids();
    }
  }
  return new int[0];
}
