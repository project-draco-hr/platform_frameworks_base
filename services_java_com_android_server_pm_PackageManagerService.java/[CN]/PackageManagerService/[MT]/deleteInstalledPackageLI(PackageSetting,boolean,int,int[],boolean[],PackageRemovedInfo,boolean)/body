{
  if (outInfo != null) {
    outInfo.uid=ps.appId;
  }
  removePackageDataLI(ps,allUserHandles,perUserInstalled,outInfo,flags,writeSettings);
  if (deleteCodeAndResources && (outInfo != null)) {
    outInfo.args=createInstallArgs(packageFlagsToInstallFlags(ps),ps.codePathString,ps.resourcePathString,ps.nativeLibraryPathString,getAppInstructionSetFromSettings(ps));
  }
  return true;
}
