{
  int[] users=sUserManager.getUserIds();
  int res=mInstaller.install(packageName,uid,uid);
  if (res < 0) {
    return res;
  }
  for (  int user : users) {
    if (user != 0) {
      res=mInstaller.createUserData(packageName,UserHandle.getUid(user,uid),user);
      if (res < 0) {
        return res;
      }
    }
  }
  return res;
}
