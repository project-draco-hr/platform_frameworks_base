{
  pw.println("HdmiCecService (dumpsys hdmi_cec)");
  pw.println("");
synchronized (mLock) {
    for (int i=0; i < mLogicalDevices.size(); ++i) {
      HdmiCecDevice device=mLogicalDevices.valueAt(i);
      pw.println("Device: type=" + device.getType() + ", active="+ device.isActiveSource());
    }
  }
}
