{
  FileOutputStream fos=null;
  try {
    fos=new FileOutputStream(path);
    fos.write(data.getBytes());
  }
 catch (  IOException e) {
    Slog.w(ActivityManagerService.TAG,"Unable to write " + path);
  }
 finally {
    if (fos != null) {
      try {
        fos.close();
      }
 catch (      IOException e) {
      }
    }
  }
}
