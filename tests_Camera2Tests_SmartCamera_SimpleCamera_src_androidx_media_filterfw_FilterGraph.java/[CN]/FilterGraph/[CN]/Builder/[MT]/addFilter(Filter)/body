{
  if (mFilterMap.values().contains(filter)) {
    throw new IllegalArgumentException("Attempting to add filter " + filter + " that "+ "is in the graph already!");
  }
 else   if (mFilterMap.containsKey(filter.getName())) {
    throw new IllegalArgumentException("Graph contains filter with name '" + filter.getName() + "' already!");
  }
 else {
    mFilterMap.put(filter.getName(),filter);
  }
}
