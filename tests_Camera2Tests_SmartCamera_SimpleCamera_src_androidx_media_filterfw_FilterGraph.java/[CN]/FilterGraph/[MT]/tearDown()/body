{
  assertNotRunning();
  if (mParentGraph != null) {
    throw new RuntimeException("Attempting to tear down sub-graph!");
  }
  if (mRunner != null) {
    mRunner.tearDownGraph(this);
  }
  for (  FilterGraph subGraph : mSubGraphs) {
    subGraph.mParentGraph=null;
    subGraph.tearDown();
  }
  mSubGraphs.clear();
}
