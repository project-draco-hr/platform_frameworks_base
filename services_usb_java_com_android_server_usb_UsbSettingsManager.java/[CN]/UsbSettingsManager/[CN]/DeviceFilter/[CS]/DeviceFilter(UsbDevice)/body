{
  mVendorId=device.getVendorId();
  mProductId=device.getProductId();
  mClass=device.getDeviceClass();
  mSubclass=device.getDeviceSubclass();
  mProtocol=device.getDeviceProtocol();
  mManufacturerName=device.getManufacturerName();
