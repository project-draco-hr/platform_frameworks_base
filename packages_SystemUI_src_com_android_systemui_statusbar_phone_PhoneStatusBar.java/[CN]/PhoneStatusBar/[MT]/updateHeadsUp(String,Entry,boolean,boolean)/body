{
  final boolean wasHeadsUp=isHeadsUp(key);
  if (wasHeadsUp) {
    if (!shouldInterrupt) {
      mHeadsUpManager.removeNotification(key);
    }
 else {
      mHeadsUpManager.updateNotification(entry,alertAgain);
    }
  }
 else   if (shouldInterrupt && alertAgain) {
    mHeadsUpManager.showNotification(entry);
  }
}
