{
  boolean altBack=(backDisposition == InputMethodService.BACK_DISPOSITION_WILL_DISMISS) || ((vis & InputMethodService.IME_VISIBLE) != 0);
  mCommandQueue.setNavigationIconHints(altBack ? (mNavigationIconHints | NAVIGATION_HINT_BACK_ALT) : (mNavigationIconHints & ~NAVIGATION_HINT_BACK_ALT));
  if (mQS != null)   mQS.setImeWindowStatus(vis > 0);
}
