{
  if (mScreenOn != null && mScreenOn && (mState == StatusBarState.KEYGUARD || mState == StatusBarState.SHADE_LOCKED)) {
    if (PhoneStatusBar.DEBUG_EMPTY_KEYGUARD) {
      Log.i(TAG,"Collapsing panel from onSpacePressed");
    }
    animateCollapsePanels(0,true);
    return true;
  }
  return false;
}
