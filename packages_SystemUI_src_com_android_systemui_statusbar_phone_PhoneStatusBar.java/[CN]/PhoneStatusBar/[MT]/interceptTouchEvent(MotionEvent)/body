{
  if (SPEW) {
    Slog.d(TAG,"Touch: rawY=" + event.getRawY() + " event="+ event+ " mDisabled="+ mDisabled+ " mTracking="+ mTracking);
  }
 else   if (CHATTY) {
    if (event.getAction() != MotionEvent.ACTION_MOVE) {
      Slog.d(TAG,String.format("panel: %s at (%f, %f) mDisabled=0x%08x",MotionEvent.actionToString(event.getAction()),event.getRawX(),event.getRawY(),mDisabled));
    }
  }
  if (DEBUG_GESTURES) {
    mGestureRec.add(event);
  }
  final int act=event.getActionMasked();
  if (mSuppressStatusBarDrags) {
    return true;
  }
 else   if (act == MotionEvent.ACTION_UP && !mClingShown) {
    showCling();
  }
 else {
    hideCling();
  }
  return false;
}
