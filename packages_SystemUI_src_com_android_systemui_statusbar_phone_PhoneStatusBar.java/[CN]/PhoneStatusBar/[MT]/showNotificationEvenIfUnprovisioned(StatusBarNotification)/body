{
  if ("android".equals(sbn.pkg)) {
    if (sbn.notification.kind != null) {
      for (      String aKind : sbn.notification.kind) {
        if ("android.system.imeswitcher".equals(aKind))         return true;
        if ("android.system.update".equals(aKind))         return true;
      }
    }
  }
  return false;
}
