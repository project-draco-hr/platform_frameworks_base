{
  if (mRecents == null) {
    return false;
  }
  int dockSide=WindowManagerProxy.getInstance().getDockSide();
  if (dockSide == WindowManager.DOCKED_INVALID) {
    return mRecents.dockTopTask(NavigationBarGestureHelper.DRAG_MODE_NONE,ActivityManager.DOCKED_STACK_CREATE_MODE_TOP_OR_LEFT,null);
  }
 else {
    EventBus.getDefault().send(new UndockingTaskEvent());
    return false;
  }
}
