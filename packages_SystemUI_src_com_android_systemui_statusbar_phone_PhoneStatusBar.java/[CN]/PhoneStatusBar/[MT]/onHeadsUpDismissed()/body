{
  if (mInterruptingNotificationEntry == null)   return;
  mHandler.sendEmptyMessage(MSG_HIDE_HEADS_UP);
  if (mHeadsUpNotificationView.isClearable()) {
    try {
      mBarService.onNotificationClear(mInterruptingNotificationEntry.notification.getPackageName(),mInterruptingNotificationEntry.notification.getTag(),mInterruptingNotificationEntry.notification.getId(),mInterruptingNotificationEntry.notification.getUserId());
    }
 catch (    android.os.RemoteException ex) {
    }
  }
}
