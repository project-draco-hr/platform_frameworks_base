{
  Log.v(TAG,"setLightsOn(" + on + ")");
  if (on) {
    setSystemUiVisibility(0,0,0,View.SYSTEM_UI_FLAG_LOW_PROFILE,mLastFullscreenStackBounds,mLastDockedStackBounds);
  }
 else {
    setSystemUiVisibility(View.SYSTEM_UI_FLAG_LOW_PROFILE,0,0,View.SYSTEM_UI_FLAG_LOW_PROFILE,mLastFullscreenStackBounds,mLastDockedStackBounds);
  }
}
