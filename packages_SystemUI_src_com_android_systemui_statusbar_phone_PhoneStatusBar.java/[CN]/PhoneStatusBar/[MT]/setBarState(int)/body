{
  if (state != mState) {
    EventLogTags.writeSysuiStatusBarState(state);
    if (mVisible && state == StatusBarState.SHADE_LOCKED) {
      try {
        mBarService.clearNotificationEffects();
      }
 catch (      RemoteException e) {
      }
    }
  }
  mState=state;
  mStatusBarWindowManager.setStatusBarState(state);
}
