{
  if ((mDisabled & StatusBarManager.DISABLE_EXPAND) != 0) {
    return false;
  }
  if (mExpanded) {
    return false;
  }
  if (mUniverseBackground.consumeEvent(event)) {
    if (mTracking) {
      mFlingY=mTrackingPosition;
      mViewDelta=0;
      mFlingVelocity=-1;
      mHandler.post(mPerformFling);
    }
    return true;
  }
  return false;
}
