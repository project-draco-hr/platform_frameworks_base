{
  if (SPEW)   Log.d(TAG,"animateExpand: mExpandedVisible=" + mExpandedVisible);
  if (!panelsEnabled()) {
    return;
  }
  mNotificationPanel.expand();
  if (mHasFlipSettings && mNotificationScroller.getVisibility() != View.VISIBLE) {
    flipToNotifications();
  }
  if (false)   postStartTracing();
}
