{
  super.onInitializeAccessibilityNodeInfoInternal(info);
  if (isEnabled()) {
    final int progress=getProgress();
    if (progress > 0) {
      info.addAction(AccessibilityNodeInfo.ACTION_SCROLL_BACKWARD);
    }
    if (progress < getMax()) {
      info.addAction(AccessibilityNodeInfo.ACTION_SCROLL_FORWARD);
    }
  }
}
