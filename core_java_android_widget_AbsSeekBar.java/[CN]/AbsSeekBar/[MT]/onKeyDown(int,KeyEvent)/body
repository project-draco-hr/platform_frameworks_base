{
  if (isEnabled()) {
    int progress=getProgress();
switch (keyCode) {
case KeyEvent.KEYCODE_DPAD_LEFT:
      if (progress <= 0)       break;
    animateSetProgress(progress - mKeyProgressIncrement);
  onKeyChange();
return true;
case KeyEvent.KEYCODE_DPAD_RIGHT:
if (progress >= getMax()) break;
animateSetProgress(progress + mKeyProgressIncrement);
onKeyChange();
return true;
}
}
return super.onKeyDown(keyCode,event);
}
