{
  if (locale == null) {
    return;
  }
  final String country=locale.getCountry();
  if (country.isEmpty()) {
    return;
  }
  for (  LocaleInfo li : sLocaleCache.values()) {
    if (country.equals(li.getLocale().getCountry())) {
      li.mSuggestionFlags|=LocaleInfo.SUGGESTION_TYPE_SIM;
    }
  }
}
