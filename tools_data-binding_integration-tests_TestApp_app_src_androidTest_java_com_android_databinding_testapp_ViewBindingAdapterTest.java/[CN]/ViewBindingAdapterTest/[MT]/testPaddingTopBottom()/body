{
  View view=mBinder.getPaddingTopBottom();
  assertEquals(mBindingObject.getPaddingTop(),view.getPaddingTop());
  assertEquals(mBindingObject.getPaddingBottom(),view.getPaddingBottom());
  changeValues();
  assertEquals(mBindingObject.getPaddingTop(),view.getPaddingTop());
  assertEquals(mBindingObject.getPaddingBottom(),view.getPaddingBottom());
}
