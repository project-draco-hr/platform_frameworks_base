{
  View view=mBinder.getPaddingLeftRight();
  assertEquals(mBindingObject.getPaddingLeft(),view.getPaddingLeft());
  assertEquals(mBindingObject.getPaddingRight(),view.getPaddingRight());
  changeValues();
  assertEquals(mBindingObject.getPaddingLeft(),view.getPaddingLeft());
  assertEquals(mBindingObject.getPaddingRight(),view.getPaddingRight());
}
