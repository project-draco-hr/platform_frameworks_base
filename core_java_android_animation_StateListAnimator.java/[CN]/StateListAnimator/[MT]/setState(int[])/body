{
  Tuple match=null;
  final int count=mTuples.size();
  for (int i=0; i < count; i++) {
    final Tuple tuple=mTuples.get(i);
    if (StateSet.stateSetMatches(tuple.mSpecs,state)) {
      match=tuple;
      break;
    }
  }
  if (match == mLastMatch) {
    return;
  }
  if (mLastMatch != null) {
    cancel();
  }
  mLastMatch=match;
  if (match != null) {
    start(match);
  }
}
