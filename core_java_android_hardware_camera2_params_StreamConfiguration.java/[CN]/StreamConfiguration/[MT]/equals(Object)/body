{
  if (obj == null) {
    return false;
  }
  if (this == obj) {
    return true;
  }
  if (obj instanceof StreamConfiguration) {
    final StreamConfiguration other=(StreamConfiguration)obj;
    return mFormat == other.mFormat && mWidth == other.mWidth && mHeight == other.mHeight && mInput == other.mInput;
  }
  return false;
}
