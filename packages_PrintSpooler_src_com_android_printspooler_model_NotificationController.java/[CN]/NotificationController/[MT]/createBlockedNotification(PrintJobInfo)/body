{
  Notification.Builder builder=new Notification.Builder(mContext).setContentIntent(createContentIntent(printJob.getId())).setSmallIcon(computeNotificationIcon(printJob)).setContentTitle(computeNotificationTitle(printJob)).addAction(R.drawable.stat_notify_cancelling,mContext.getString(R.string.cancel),createCancelIntent(printJob)).setContentText(printJob.getPrinterName()).setWhen(System.currentTimeMillis()).setOngoing(true).setShowWhen(true).setColor(mContext.getResources().getColor(com.android.internal.R.color.system_notification_accent_color));
  mNotificationManager.notify(0,builder.build());
}
