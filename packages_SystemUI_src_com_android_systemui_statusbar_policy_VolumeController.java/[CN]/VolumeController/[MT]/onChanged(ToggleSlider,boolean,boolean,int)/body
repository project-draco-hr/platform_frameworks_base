{
  if (!tracking) {
    if (mute) {
      boolean vibeInSilent=(1 == Settings.System.getInt(mContext.getContentResolver(),Settings.System.VIBRATE_IN_SILENT,1));
      mAudioManager.setRingerMode(vibeInSilent ? AudioManager.RINGER_MODE_VIBRATE : AudioManager.RINGER_MODE_SILENT);
    }
 else {
      mAudioManager.setRingerMode(AudioManager.RINGER_MODE_NORMAL);
      mAudioManager.setStreamVolume(STREAM,level,AudioManager.FLAG_PLAY_SOUND);
    }
  }
}
