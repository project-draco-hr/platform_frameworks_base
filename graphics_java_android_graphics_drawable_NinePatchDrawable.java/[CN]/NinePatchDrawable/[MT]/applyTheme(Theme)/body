{
  super.applyTheme(t);
  final NinePatchState state=mNinePatchState;
  if (state == null) {
    throw new RuntimeException("Can't apply theme to <nine-patch> with no constant state");
  }
  final int[] themeAttrs=state.mThemeAttrs;
  if (themeAttrs != null) {
    final TypedArray a=t.resolveAttributes(themeAttrs,R.styleable.NinePatchDrawable,0,0);
    updateStateFromTypedArray(a);
    a.recycle();
  }
}
