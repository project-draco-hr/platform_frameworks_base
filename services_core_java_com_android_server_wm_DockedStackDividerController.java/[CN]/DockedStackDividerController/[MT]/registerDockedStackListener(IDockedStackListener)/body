{
  mDockedStackListeners.register(listener);
  notifyDockedDividerVisibilityChanged(wasVisible());
  notifyDockedStackExistsChanged(mDisplayContent.mService.mStackIdToStack.get(DOCKED_STACK_ID) != null);
  notifyDockedStackMinimizedChanged(mMinimizedDock,0);
  notifyAdjustedForImeChanged(mAdjustedForIme,0);
}
