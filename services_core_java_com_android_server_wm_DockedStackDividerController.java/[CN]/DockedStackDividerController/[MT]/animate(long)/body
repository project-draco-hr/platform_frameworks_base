{
  if (mAnimatingForMinimizedDockedStack) {
    return animateForMinimizedDockedStack(now);
  }
 else   if (mAnimatingForIme) {
    return animateForIme(now);
  }
 else {
    if (mDimLayer != null) {
      mDimLayer.setLayer(mService.mLayersController.getResizeDimLayer());
    }
    return false;
  }
}
