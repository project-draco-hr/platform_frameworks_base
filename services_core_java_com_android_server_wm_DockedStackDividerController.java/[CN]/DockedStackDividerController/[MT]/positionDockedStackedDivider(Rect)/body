{
  TaskStack stack=mDisplayContent.getDockedStackLocked();
  if (stack == null) {
    return;
  }
  int side=stack.getDockSide();
  stack.getBounds(mTmpRect);
switch (side) {
case DOCKED_LEFT:
    frame.set(mTmpRect.right - mDividerInsets,frame.top,mTmpRect.right + frame.width() - mDividerInsets,frame.bottom);
  break;
case DOCKED_TOP:
frame.set(frame.left,mTmpRect.bottom - mDividerInsets,mTmpRect.right,mTmpRect.bottom + frame.height() - mDividerInsets);
break;
case DOCKED_RIGHT:
frame.set(mTmpRect.left - frame.width() + mDividerInsets,frame.top,mTmpRect.left + mDividerInsets,frame.bottom);
break;
case DOCKED_BOTTOM:
frame.set(frame.left,mTmpRect.top - frame.height() + mDividerInsets,frame.right,mTmpRect.top + mDividerInsets);
break;
}
}
