{
  mTmpRect.set(0,0,mDisplayWidth,mDisplayHeight);
switch (mDockSide) {
case DOCKED_LEFT:
    mTmpRect.left=x;
  break;
case DOCKED_TOP:
mTmpRect.top=y;
break;
case DOCKED_RIGHT:
mTmpRect.right=x;
break;
case DOCKED_BOTTOM:
mTmpRect.top=y;
break;
default :
Slog.wtf(TAG,"setMaximizeDimLayerVisible: Unknown dock side when setting dim " + "layer=" + mDockSide);
}
mDimLayer.setBounds(mTmpRect);
setDimLayerVisible(true);
}
