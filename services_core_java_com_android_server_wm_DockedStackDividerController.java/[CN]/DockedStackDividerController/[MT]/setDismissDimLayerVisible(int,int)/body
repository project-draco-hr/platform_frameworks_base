{
  mTmpRect.set(mOriginalRect);
switch (mDockSide) {
case DOCKED_LEFT:
    mTmpRect.right=x;
  break;
case DOCKED_TOP:
mTmpRect.bottom=y;
break;
case DOCKED_RIGHT:
mTmpRect.left=x;
break;
case DOCKED_BOTTOM:
mTmpRect.top=y;
break;
default :
Slog.wtf(TAG,"setDismissDimLayerVisible: Unknown dock side when setting dim " + "layer=" + mDockSide);
return;
}
mDimLayer.setBounds(mTmpRect);
setDimLayerVisible(true);
}
