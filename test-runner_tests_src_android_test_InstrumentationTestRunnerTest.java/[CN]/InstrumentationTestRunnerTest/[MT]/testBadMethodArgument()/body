{
  String testClassName=PlaceHolderTest.class.getName();
  String invalidMethodName="testNoExist";
  String classAndMethod=testClassName + "#" + invalidMethodName;
  mInstrumentationTestRunner.onCreate(createBundle(InstrumentationTestRunner.ARGUMENT_TEST_CLASS,classAndMethod));
  assertTestRunnerCalledWithExpectedParameters(testClassName,invalidMethodName);
}
