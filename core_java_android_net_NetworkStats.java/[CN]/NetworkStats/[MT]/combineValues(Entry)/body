{
  final int i=findIndex(entry.iface,entry.uid,entry.set,entry.tag);
  if (i == -1) {
    addValues(entry);
  }
 else {
    rxBytes[i]+=entry.rxBytes;
    rxPackets[i]+=entry.rxPackets;
    txBytes[i]+=entry.txBytes;
    txPackets[i]+=entry.txPackets;
    operations[i]+=entry.operations;
  }
  return this;
}
