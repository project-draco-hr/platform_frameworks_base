{
  final Entry entry=recycle != null ? recycle : new Entry();
  entry.iface=iface[i];
  entry.uid=uid[i];
  entry.set=set[i];
  entry.tag=tag[i];
  entry.rxBytes=rxBytes[i];
  entry.rxPackets=rxPackets[i];
  entry.txBytes=txBytes[i];
  entry.txPackets=txPackets[i];
  entry.operations=operations[i];
  return entry;
}
