{
  pw.print(prefix);
  pw.print("NetworkStats: elapsedRealtime=");
  pw.println(elapsedRealtime);
  for (int i=0; i < iface.length; i++) {
    pw.print(prefix);
    pw.print("  iface=");
    pw.print(iface[i]);
    pw.print(" uid=");
    pw.print(uid[i]);
    pw.print(" tag=");
    pw.print(tag[i]);
    pw.print(" rx=");
    pw.print(rx[i]);
    pw.print(" tx=");
    pw.println(tx[i]);
  }
}
