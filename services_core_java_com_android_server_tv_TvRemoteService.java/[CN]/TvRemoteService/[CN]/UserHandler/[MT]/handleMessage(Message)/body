{
switch (msg.what) {
case MSG_START:
{
      start();
      break;
    }
case MSG_INPUT_BRIDGE_CONNECTED:
{
    IBinder token=(IBinder)msg.obj;
    TvRemoteProviderProxy provider=mProviderMap.get(token);
    if (provider != null) {
      provider.inputBridgeConnected(token);
    }
    break;
  }
}
}
