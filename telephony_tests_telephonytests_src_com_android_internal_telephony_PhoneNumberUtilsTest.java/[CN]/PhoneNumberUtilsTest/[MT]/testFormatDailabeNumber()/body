{
  assertEquals("(650) 291-0000",PhoneNumberUtils.formatNumber("6502910000","+16502910000","CN"));
  assertEquals("011 86 10 8888 0000",PhoneNumberUtils.formatNumber("011861088880000","+861088880000","US"));
  assertEquals("00 86 10 8888 0000",PhoneNumberUtils.formatNumber("00861088880000","+861088880000","GB"));
  assertEquals("+86 10 8888 0000",PhoneNumberUtils.formatNumber("+861088880000","+861088880000","GB"));
  assertEquals("011861088880000",PhoneNumberUtils.formatNumber("011861088880000","+861088880000","GB"));
  assertEquals("(650) 291-0000",PhoneNumberUtils.formatNumber("6502910000",null,"US"));
  assertEquals("+1 650-291-0000",PhoneNumberUtils.formatNumber("+16502910000",null,"CN"));
  assertEquals("650-291-0000",PhoneNumberUtils.formatNumber("650-291-0000",null,"US"));
  assertEquals("506 128 687",PhoneNumberUtils.formatNumber("506128687",null,"PL"));
  assertEquals("0506128687",PhoneNumberUtils.formatNumber("0506128687",null,"PL"));
  assertEquals("011861088880000",PhoneNumberUtils.formatNumber("011861088880000","","GB"));
}
