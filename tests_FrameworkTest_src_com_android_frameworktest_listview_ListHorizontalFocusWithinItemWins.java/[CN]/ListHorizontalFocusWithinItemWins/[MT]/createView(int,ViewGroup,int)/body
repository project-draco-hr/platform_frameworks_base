{
  final Context context=parent.getContext();
  if (position == 0) {
    return ListItemFactory.horizontalButtonSlots(context,desiredHeight,Slot.Left,Slot.Right);
  }
 else   if (position == 1) {
    return ListItemFactory.horizontalButtonSlots(context,desiredHeight,Slot.Middle);
  }
 else {
    throw new IllegalArgumentException("expecting position 0 or 1");
  }
}
