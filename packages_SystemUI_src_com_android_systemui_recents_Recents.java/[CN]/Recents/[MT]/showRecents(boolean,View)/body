{
  if (!isDeviceProvisioned()) {
    return;
  }
  if (proxyToOverridePackage(ACTION_SHOW_RECENTS)) {
    return;
  }
  int currentUser=sSystemServicesProxy.getCurrentUser();
  if (sSystemServicesProxy.isSystemUser(currentUser)) {
    mImpl.showRecents(triggeredFromAltTab,false);
  }
 else {
    if (mSystemUserCallbacks != null) {
      IRecentsNonSystemUserCallbacks callbacks=mSystemUserCallbacks.getNonSystemUserRecentsForUser(currentUser);
      if (callbacks != null) {
        try {
          callbacks.showRecents(triggeredFromAltTab,false);
        }
 catch (        RemoteException e) {
          Log.e(TAG,"Callback failed",e);
        }
      }
 else {
        Log.e(TAG,"No SystemUI callbacks found for user: " + currentUser);
      }
    }
  }
}
