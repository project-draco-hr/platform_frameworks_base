{
  if (mDevicePluggedIn != pluggedIn) {
    return true;
  }
  if (pluggedIn && mBatteryLevel != batteryLevel) {
    return true;
  }
  if (!pluggedIn) {
    if (batteryLevel < LOW_BATTERY_THRESHOLD && mBatteryLevel >= LOW_BATTERY_THRESHOLD) {
      return true;
    }
    if (mBatteryLevel < LOW_BATTERY_THRESHOLD && batteryLevel >= LOW_BATTERY_THRESHOLD) {
      return true;
    }
  }
  return false;
}
