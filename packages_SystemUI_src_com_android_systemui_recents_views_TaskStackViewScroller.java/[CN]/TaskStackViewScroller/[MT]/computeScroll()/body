{
  if (mScroller.computeScrollOffset()) {
    float deltaP=mLayoutAlgorithm.getDeltaPForY(mFlingDownY,mScroller.getCurrY());
    float scroll=mFlingDownScrollP + deltaP;
    setStackScroll(scroll);
    if (DEBUG) {
      Log.d(TAG,"computeScroll: " + scroll);
    }
    return true;
  }
  return false;
}
