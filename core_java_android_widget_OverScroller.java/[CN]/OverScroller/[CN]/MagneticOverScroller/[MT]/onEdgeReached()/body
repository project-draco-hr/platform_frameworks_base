{
  final float distance=mVelocity / TIME_COEF;
  if (Math.abs(distance) < mOver) {
    mState=TO_BOUNCE;
    mFinal=mStart;
    mDuration=OVERSCROLL_SPRINGBACK_DURATION;
  }
 else {
    mState=TO_BOUNDARY;
    int over=mVelocity > 0 ? mOver : -mOver;
    mFinal=mStart + over;
    mDuration=(int)(1000.0f * Math.asin(over / distance) / TIME_COEF);
  }
}
