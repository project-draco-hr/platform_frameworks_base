{
  if (mOriginalValues != null) {
synchronized (mLock) {
      for (      T item : items) {
        mOriginalValues.add(item);
      }
      if (mNotifyOnChange)       notifyDataSetChanged();
    }
  }
 else {
    for (    T item : items) {
      mObjects.add(item);
    }
    if (mNotifyOnChange)     notifyDataSetChanged();
  }
}
