{
  checkPermission(android.Manifest.permission.SET_WALLPAPER_HINTS);
  if (width <= 0 || height <= 0) {
    throw new IllegalArgumentException("width and height must be > 0");
  }
synchronized (mLock) {
    if (width != mWidth || height != mHeight) {
      mWidth=width;
      mHeight=height;
      saveSettingsLocked();
    }
  }
}
