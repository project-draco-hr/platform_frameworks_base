{
  if (TextUtils.isEmpty(activity)) {
    return INVALID_ACTIVITY_TYPE;
  }
  int supporteActivitiesLength=mSupportedActivities.length;
  for (int i=0; i < supporteActivitiesLength; ++i) {
    if (activity.equals(mSupportedActivities[i])) {
      return i;
    }
  }
  return INVALID_ACTIVITY_TYPE;
}
