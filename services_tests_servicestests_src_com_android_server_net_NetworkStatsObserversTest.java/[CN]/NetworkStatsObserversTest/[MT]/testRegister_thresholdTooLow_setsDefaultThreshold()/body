{
  long thresholdTooLowBytes=1L;
  NetworkTemplate[] templates=new NetworkTemplate[]{sTemplateWifi};
  DataUsageRequest inputRequest=new DataUsageRequest(DataUsageRequest.REQUEST_ID_UNSET,templates,NO_UIDS,thresholdTooLowBytes);
  DataUsageRequest request=mStatsObservers.register(inputRequest,mMessenger,mockBinder,Process.SYSTEM_UID,NetworkStatsAccess.Level.DEVICE);
  assertTrue(request.requestId > 0);
  assertTrue(Arrays.deepEquals(templates,request.templates));
  assertNull(request.uids);
  assertEquals(THRESHOLD_BYTES,request.thresholdInBytes);
}
