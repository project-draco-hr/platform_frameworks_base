{
  type=in.readInt();
  packageName=in.readString();
  installerPackageName=in.readString();
  processName=in.readString();
  time=in.readLong();
  systemApp=in.readInt() == 1;
switch (type) {
case TYPE_CRASH:
    crashInfo=new CrashInfo(in);
  anrInfo=null;
batteryText=null;
break;
case TYPE_ANR:
anrInfo=new AnrInfo(in);
crashInfo=null;
batteryText=null;
break;
case TYPE_BATTERY:
batteryText=in.readString();
anrInfo=null;
crashInfo=null;
break;
}
}
