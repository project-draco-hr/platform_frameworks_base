{
  dest.writeInt(type);
  dest.writeString(packageName);
  dest.writeString(installerPackageName);
  dest.writeString(processName);
  dest.writeLong(time);
  dest.writeInt(systemApp ? 1 : 0);
switch (type) {
case TYPE_CRASH:
    crashInfo.writeToParcel(dest,flags);
  break;
case TYPE_ANR:
anrInfo.writeToParcel(dest,flags);
break;
}
}
