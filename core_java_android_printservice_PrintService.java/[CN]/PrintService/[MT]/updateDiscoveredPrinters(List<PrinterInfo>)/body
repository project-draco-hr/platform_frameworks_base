{
  final IPrinterDiscoveryObserver observer;
synchronized (mLock) {
    observer=mDiscoveryObserver;
  }
  if (observer != null) {
    try {
      observer.onPrintersUpdated(printers);
    }
 catch (    RemoteException re) {
      Log.e(LOG_TAG,"Error updating discovered printers",re);
    }
  }
}
