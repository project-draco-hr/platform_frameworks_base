{
synchronized (mLock) {
    if (mClient == null) {
      throw new IllegalStateException("Print serivice not connected!");
    }
    if (mDiscoveringPrinters) {
      try {
        mClient.addDiscoveredPrinters(printers);
      }
 catch (      RemoteException re) {
        Log.e(LOG_TAG,"Error adding discovered printers!",re);
      }
    }
  }
}
