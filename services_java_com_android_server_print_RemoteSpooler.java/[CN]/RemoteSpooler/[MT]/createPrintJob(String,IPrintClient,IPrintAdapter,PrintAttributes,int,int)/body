{
  try {
    return mCreatePrintJobCaller.createPrintJob(getRemoteInstance(userId),printJobName,client,printAdapter,attributes,appId);
  }
 catch (  RemoteException re) {
    Slog.e(LOG_TAG,"Error creating print job!",re);
  }
catch (  TimeoutException te) {
    Slog.e(LOG_TAG,"Error creating print job!",te);
  }
  return null;
}
