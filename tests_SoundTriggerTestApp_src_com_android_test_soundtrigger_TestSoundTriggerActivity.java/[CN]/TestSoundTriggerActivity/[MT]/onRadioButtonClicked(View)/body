{
  boolean checked=((RadioButton)view).isChecked();
switch (view.getId()) {
case R.id.model_one:
    if (checked) {
      mSelectedModelId=1;
      postMessage("Selected model one.");
    }
  break;
case R.id.model_two:
if (checked) {
  mSelectedModelId=2;
  postMessage("Selected model two.");
}
break;
case R.id.model_three:
if (checked) {
mSelectedModelId=3;
postMessage("Selected model three.");
}
break;
}
}
