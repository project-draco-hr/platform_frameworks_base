{
switch (attrs.type) {
case TYPE_STATUS_BAR:
    if (mStatusBar != null) {
      return WindowManagerImpl.ADD_MULTIPLE_SINGLETON;
    }
  mStatusBar=win;
break;
case TYPE_KEYGUARD:
if (mKeyguard != null) {
return WindowManagerImpl.ADD_MULTIPLE_SINGLETON;
}
mKeyguard=win;
break;
}
return WindowManagerImpl.ADD_OKAY;
}
