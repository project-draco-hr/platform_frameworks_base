{
  try {
    if (mSafeMode) {
      ActivityManagerNative.getDefault().enterSafeMode();
    }
    mKeyguardMediator.onSystemReady();
    android.os.SystemProperties.set("dev.bootcomplete","1");
synchronized (mLock) {
      updateOrientationListenerLp();
      mVibrator=new Vibrator();
    }
  }
 catch (  RemoteException e) {
  }
}
