{
  if (code == KeyEvent.KEYCODE_HEADSETHOOK || code == KeyEvent.KEYCODE_PLAYPAUSE || code == KeyEvent.KEYCODE_STOP || code == KeyEvent.KEYCODE_NEXTSONG || code == KeyEvent.KEYCODE_PREVIOUSSONG || code == KeyEvent.KEYCODE_PREVIOUSSONG || code == KeyEvent.KEYCODE_FORWARD) {
    return true;
  }
  return false;
}
