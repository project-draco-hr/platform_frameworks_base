{
  if (mTopFullscreenOpaqueWindowState == null && attrs.type >= FIRST_APPLICATION_WINDOW && attrs.type <= LAST_APPLICATION_WINDOW && win.fillsScreenLw(mW,mH,true) && win.isVisibleLw()) {
    mTopFullscreenOpaqueWindowState=win;
  }
 else   if ((attrs.flags & FLAG_FORCE_NOT_FULLSCREEN) != 0) {
    mForceStatusBar=true;
  }
}
