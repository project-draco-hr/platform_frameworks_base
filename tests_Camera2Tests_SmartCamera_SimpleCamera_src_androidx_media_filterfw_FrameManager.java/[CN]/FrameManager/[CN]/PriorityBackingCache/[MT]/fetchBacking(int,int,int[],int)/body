{
  for (  Backing backing : mQueue) {
    int backingAccess=(mode == Frame.MODE_WRITE) ? backing.writeAccess() : backing.readAccess();
    if ((backingAccess & access) == access && dimensionsCompatible(backing.getDimensions(),dimensions) && (elemSize == backing.getElementSize())) {
      mQueue.remove(backing);
      mSize-=backing.getSize();
      onFetchBacking(backing);
      return backing;
    }
  }
  return null;
}
