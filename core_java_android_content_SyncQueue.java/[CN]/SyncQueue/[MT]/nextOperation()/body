{
  SyncOperation best=null;
  long bestRunTime=0;
  for (  SyncOperation op : mOperationsMap.values()) {
    long opRunTime=SyncManager.runTimeWithBackoffs(mSyncStorageEngine,op.account,op.authority,op.extras.getBoolean(ContentResolver.SYNC_EXTRAS_MANUAL,false),op.earliestRunTime);
    if (best == null || (best.expedited == op.expedited ? opRunTime < bestRunTime : op.expedited)) {
      best=op;
      bestRunTime=opRunTime;
    }
  }
  if (best == null) {
    return null;
  }
  return Pair.create(best,bestRunTime);
}
