{
  try {
synchronized (mManagerCallback) {
      if (mService != null) {
        int state=mService.getState();
        if (VDBG)         Log.d(TAG,"getLeState() returning " + state);
        return state;
      }
    }
  }
 catch (  RemoteException e) {
    Log.e(TAG,"",e);
  }
  return BluetoothAdapter.STATE_OFF;
}
