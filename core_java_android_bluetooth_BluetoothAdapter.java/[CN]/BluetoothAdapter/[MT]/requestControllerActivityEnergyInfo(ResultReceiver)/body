{
  try {
synchronized (mManagerCallback) {
      if (mService != null) {
        mService.requestActivityInfo(result);
        result=null;
      }
    }
  }
 catch (  RemoteException e) {
    Log.e(TAG,"getControllerActivityEnergyInfoCallback: " + e);
  }
 finally {
    if (result != null) {
      result.send(0,null);
    }
  }
}
