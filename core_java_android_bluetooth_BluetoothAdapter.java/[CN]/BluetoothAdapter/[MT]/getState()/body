{
  try {
synchronized (mManagerCallback) {
      if (mService != null) {
        int state=mService.getState();
        if (VDBG)         Log.d(TAG,"" + hashCode() + ": getState(). Returning "+ state);
        return state;
      }
    }
  }
 catch (  RemoteException e) {
    Log.e(TAG,"",e);
  }
  if (DBG)   Log.d(TAG,"" + hashCode() + ": getState() :  mService = null. Returning STATE_OFF");
  return STATE_OFF;
}
