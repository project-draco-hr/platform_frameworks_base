{
  if (getState() != STATE_ON)   return false;
  try {
    IBluetoothGatt iGatt=mManagerService.getBluetoothGatt();
    if (iGatt == null) {
      return false;
    }
    return (iGatt.numHwTrackFiltersAvailable() != 0);
  }
 catch (  RemoteException e) {
    Log.e(TAG,"",e);
  }
  return false;
}
