{
  if (getState() != STATE_ON) {
    return toDeviceSet(new BluetoothDevice[0]);
  }
  try {
synchronized (mManagerCallback) {
      if (mService != null)       return toDeviceSet(mService.getBondedDevices());
    }
    return toDeviceSet(new BluetoothDevice[0]);
  }
 catch (  RemoteException e) {
    Log.e(TAG,"",e);
  }
  return null;
}
