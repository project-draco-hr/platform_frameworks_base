{
  try {
    IBluetoothGatt iGatt=mManagerService.getBluetoothGatt();
    if (iGatt == null) {
      return false;
    }
    if (mAdvertisingCallback == null) {
      return false;
    }
    mAdvertisingCallback.stopAdvertising();
    mAdvertisingCallback=null;
    return true;
  }
 catch (  RemoteException e) {
    Log.e(TAG,"",e);
    return false;
  }
}
