{
  boolean started=false;
synchronized (this) {
    if (mLeHandle == -1)     return false;
    int count=0;
    while (mLeHandle == 0 && count < maxWaitCount) {
      try {
        wait(LE_CALLBACK_REG_TIMEOUT);
      }
 catch (      InterruptedException e) {
        Log.e(TAG,"Callback reg wait interrupted: " + e);
      }
      count++;
    }
    started=(mLeHandle > 0);
  }
  return started;
}
