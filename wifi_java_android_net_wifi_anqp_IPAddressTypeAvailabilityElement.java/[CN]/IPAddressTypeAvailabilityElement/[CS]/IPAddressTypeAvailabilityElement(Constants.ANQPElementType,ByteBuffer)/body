{
  super(infoID);
  if (payload.remaining() != 1)   throw new ProtocolException("Bad IP Address Type Availability length: " + payload.remaining());
  int ipField=payload.get();
  mV6Availability=IPv6Availability.values()[ipField & 0x3];
  ipField=(ipField >> 2) & 0x3f;
  mV4Availability=ipField <= IPv4Availability.values().length ? IPv4Availability.values()[ipField] : IPv4Availability.Unknown;
}
