{
  if (pluginLib == null || pluginLib.length() == 0) {
    return null;
  }
synchronized (mPackageInfoCache) {
    for (    PackageInfo pkgInfo : mPackageInfoCache) {
      if (pluginLib.startsWith(pkgInfo.applicationInfo.dataDir)) {
        return pkgInfo.packageName;
      }
    }
  }
  return null;
}
