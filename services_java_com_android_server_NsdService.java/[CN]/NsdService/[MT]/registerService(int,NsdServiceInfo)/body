{
  if (DBG)   Slog.d(TAG,"registerService: " + regId + " "+ service);
  try {
    mNativeConnector.execute("mdnssd","register",regId,service.getServiceName(),service.getServiceType(),service.getPort());
  }
 catch (  NativeDaemonConnectorException e) {
    Slog.e(TAG,"Failed to execute registerService " + e);
    return false;
  }
  return true;
}
