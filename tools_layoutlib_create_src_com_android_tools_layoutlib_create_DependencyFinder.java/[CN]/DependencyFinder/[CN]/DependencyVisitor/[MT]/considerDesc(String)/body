{
  if (desc != null) {
    try {
      if (desc.length() > 0 && desc.charAt(0) == '(') {
        Type t=Type.getReturnType(desc);
        considerType(t);
        for (        Type arg : Type.getArgumentTypes(desc)) {
          considerType(arg);
        }
      }
 else {
        Type t=Type.getType(desc);
        considerType(t);
      }
      return true;
    }
 catch (    ArrayIndexOutOfBoundsException e) {
    }
  }
  return false;
}
