{
  if (!mHasFeature) {
    return null;
  }
  if (!callingUserOnly) {
    enforceManageUsers();
  }
synchronized (this) {
    if (!mOwners.hasDeviceOwner()) {
      return null;
    }
    if (callingUserOnly && mInjector.userHandleGetCallingUserId() != mOwners.getDeviceOwnerUserId()) {
      return null;
    }
    return mOwners.getDeviceOwnerComponent();
  }
}
