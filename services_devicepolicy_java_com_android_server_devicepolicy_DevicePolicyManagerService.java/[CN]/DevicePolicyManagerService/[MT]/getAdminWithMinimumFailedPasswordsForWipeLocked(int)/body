{
  int count=0;
  ActiveAdmin strictestAdmin=null;
  for (  UserInfo userInfo : mUserManager.getProfiles(userHandle)) {
    DevicePolicyData policy=getUserData(userInfo.getUserHandle().getIdentifier());
    for (    ActiveAdmin admin : policy.mAdminList) {
      if (admin.maximumFailedPasswordsForWipe == ActiveAdmin.DEF_MAXIMUM_FAILED_PASSWORDS_FOR_WIPE) {
        continue;
      }
      if (count == 0 || count > admin.maximumFailedPasswordsForWipe || (userInfo.isPrimary() && count >= admin.maximumFailedPasswordsForWipe)) {
        count=admin.maximumFailedPasswordsForWipe;
        strictestAdmin=admin;
      }
    }
  }
  return strictestAdmin;
}
