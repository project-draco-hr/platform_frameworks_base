{
synchronized (this) {
    final long now=System.currentTimeMillis();
    List<ActiveAdmin> admins=getActiveAdminsForLockscreenPoliciesLocked(userHandle,false);
    final int N=admins.size();
    for (int i=0; i < N; i++) {
      ActiveAdmin admin=admins.get(i);
      if (admin.info.usesPolicy(DeviceAdminInfo.USES_POLICY_EXPIRE_PASSWORD) && admin.passwordExpirationTimeout > 0L && now >= admin.passwordExpirationDate - EXPIRATION_GRACE_PERIOD_MS && admin.passwordExpirationDate > 0L) {
        sendAdminCommandLocked(admin,DeviceAdminReceiver.ACTION_PASSWORD_EXPIRING);
      }
    }
    setExpirationAlarmCheckLocked(mContext,userHandle);
  }
}
