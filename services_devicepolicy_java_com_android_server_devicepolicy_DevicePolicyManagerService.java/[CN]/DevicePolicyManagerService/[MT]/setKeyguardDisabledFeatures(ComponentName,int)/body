{
  if (!mHasFeature) {
    return;
  }
  Preconditions.checkNotNull(who,"ComponentName is null");
  final int userHandle=UserHandle.getCallingUserId();
  enforceNotManagedProfile(userHandle,"disable keyguard features");
synchronized (this) {
    ActiveAdmin ap=getActiveAdminForCallerLocked(who,DeviceAdminInfo.USES_POLICY_DISABLE_KEYGUARD_FEATURES);
    if (ap.disabledKeyguardFeatures != which) {
      ap.disabledKeyguardFeatures=which;
      saveSettingsLocked(userHandle);
    }
    syncDeviceCapabilitiesLocked(getUserData(userHandle));
  }
}
