{
synchronized (this) {
    if (mOwners.hasDeviceOwner()) {
      return false;
    }
  }
  if (getProfileOwner(callingUserId) != null) {
    return false;
  }
  if (mInjector.settingsGlobalGetInt(Settings.Global.DEVICE_PROVISIONED,0) != 0) {
    return false;
  }
  if (callingUserId != UserHandle.USER_SYSTEM) {
    return false;
  }
  return true;
}
