{
  if (!mHasFeature) {
    return;
  }
  enforceCrossUserPermission(userHandle);
synchronized (this) {
    if (who == null) {
      throw new NullPointerException("ComponentName is null");
    }
    getActiveAdminForCallerLocked(who,DeviceAdminInfo.USES_POLICY_WIPE_DATA);
    ActiveAdmin ap=getActiveAdminForCallerLocked(who,DeviceAdminInfo.USES_POLICY_WATCH_LOGIN);
    if (ap.maximumFailedPasswordsForWipe != num) {
      ap.maximumFailedPasswordsForWipe=num;
      saveSettingsLocked(userHandle);
    }
  }
}
