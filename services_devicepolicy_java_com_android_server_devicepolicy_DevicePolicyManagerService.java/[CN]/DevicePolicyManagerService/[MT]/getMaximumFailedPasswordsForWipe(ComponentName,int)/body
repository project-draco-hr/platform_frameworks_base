{
  if (!mHasFeature) {
    return 0;
  }
  enforceCrossUserPermission(userHandle);
synchronized (this) {
    int count=0;
    if (who != null) {
      ActiveAdmin admin=getActiveAdminUncheckedLocked(who,userHandle);
      return admin != null ? admin.maximumFailedPasswordsForWipe : count;
    }
    List<UserInfo> profiles=mUserManager.getProfiles(userHandle);
    for (    UserInfo userInfo : profiles) {
      DevicePolicyData policy=getUserData(userInfo.getUserHandle().getIdentifier());
      final int N=policy.mAdminList.size();
      for (int i=0; i < N; i++) {
        ActiveAdmin admin=policy.mAdminList.get(i);
        if (count == 0) {
          count=admin.maximumFailedPasswordsForWipe;
        }
 else         if (admin.maximumFailedPasswordsForWipe != 0 && count > admin.maximumFailedPasswordsForWipe) {
          count=admin.maximumFailedPasswordsForWipe;
        }
      }
    }
    return count;
  }
}
