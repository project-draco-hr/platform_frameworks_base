{
  Preconditions.checkNotNull(who,"ComponentName is null");
synchronized (this) {
    getActiveAdminForCallerLocked(who,DeviceAdminInfo.USES_POLICY_PROFILE_OWNER);
    IAudioService iAudioService=IAudioService.Stub.asInterface(ServiceManager.getService(Context.AUDIO_SERVICE));
    try {
      iAudioService.adjustMasterVolume(on ? AudioManager.ADJUST_MUTE : AudioManager.ADJUST_UNMUTE,0,who.getPackageName());
    }
 catch (    RemoteException re) {
      Slog.e(LOG_TAG,"Failed to setMasterMute",re);
    }
  }
}
