{
  long time=0;
  final int N=admins.size();
  for (int i=0; i < N; i++) {
    ActiveAdmin admin=admins.get(i);
    if (time == 0) {
      time=admin.maximumTimeToUnlock;
    }
 else     if (admin.maximumTimeToUnlock != 0 && time > admin.maximumTimeToUnlock) {
      time=admin.maximumTimeToUnlock;
    }
  }
  return time;
}
