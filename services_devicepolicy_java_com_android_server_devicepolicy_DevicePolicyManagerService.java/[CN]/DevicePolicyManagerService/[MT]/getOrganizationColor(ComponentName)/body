{
  if (!mHasFeature) {
    return ActiveAdmin.DEF_ORGANIZATION_COLOR;
  }
  Preconditions.checkNotNull(who,"ComponentName is null");
  enforceManagedProfile(mInjector.userHandleGetCallingUserId(),"get organization color");
synchronized (this) {
    ActiveAdmin admin=getActiveAdminForCallerLocked(who,DeviceAdminInfo.USES_POLICY_PROFILE_OWNER);
    return admin.organizationColor;
  }
}
