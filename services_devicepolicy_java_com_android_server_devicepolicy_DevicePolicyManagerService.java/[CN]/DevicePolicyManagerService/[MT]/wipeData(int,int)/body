{
  if (!mHasFeature) {
    return;
  }
  enforceCrossUserPermission(userHandle);
  if ((flags & DevicePolicyManager.WIPE_EXTERNAL_STORAGE) != 0) {
    enforceNotManagedProfile(userHandle,"wipe external storage");
  }
synchronized (this) {
    final ActiveAdmin admin=getActiveAdminForCallerLocked(null,DeviceAdminInfo.USES_POLICY_WIPE_DATA);
    final String source;
    if (admin != null && admin.info != null) {
      final ComponentName cname=admin.info.getComponent();
      if (cname != null) {
        source=cname.flattenToShortString();
      }
 else {
        source=admin.info.getPackageName();
      }
    }
 else {
      source="?";
    }
    long ident=Binder.clearCallingIdentity();
    try {
      wipeDeviceOrUserLocked(flags,userHandle,"DevicePolicyManager.wipeData() from " + source);
    }
  finally {
      Binder.restoreCallingIdentity(ident);
    }
  }
}
