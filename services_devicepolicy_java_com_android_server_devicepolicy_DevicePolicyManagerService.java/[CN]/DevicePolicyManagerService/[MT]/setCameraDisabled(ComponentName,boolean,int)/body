{
  if (!mHasFeature) {
    return;
  }
  enforceCrossUserPermission(userHandle);
  enforceNotManagedProfile(userHandle,"enable/disable cameras");
synchronized (this) {
    if (who == null) {
      throw new NullPointerException("ComponentName is null");
    }
    ActiveAdmin ap=getActiveAdminForCallerLocked(who,DeviceAdminInfo.USES_POLICY_DISABLE_CAMERA);
    if (ap.disableCamera != disabled) {
      ap.disableCamera=disabled;
      saveSettingsLocked(userHandle);
    }
    syncDeviceCapabilitiesLocked(getUserData(userHandle));
  }
}
