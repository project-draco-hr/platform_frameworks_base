{
  Object viewKey;
  if (attrs instanceof BridgeXmlBlockParser) {
    viewKey=((BridgeXmlBlockParser)attrs).getViewCookie();
  }
 else {
    viewKey=null;
  }
  Preference preference=super.onCreateItem(name,attrs);
  if (viewKey != null) {
    mViewCookieMap.put(preference,viewKey);
  }
  return preference;
}
