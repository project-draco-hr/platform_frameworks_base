{
  return new InputMethodSubtypeBuilder().setSubtypeNameResId(0).setSubtypeIconResId(0).setSubtypeLocale(locale).setSubtypeMode(mode).setSubtypeExtraValue("").setIsAuxiliary(isAuxiliary).setOverridesImplicitlyEnabledSubtype(overridesImplicitlyEnabledSubtype).setIsAsciiCapable(isAsciiCapable).build();
}
