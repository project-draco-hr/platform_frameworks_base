{
  mService.enforceNotIsolatedCaller("ActivityContainer.startActivity");
  int userId=mService.handleIncomingUser(Binder.getCallingPid(),Binder.getCallingUid(),mCurrentUser,false,true,"ActivityContainer",null);
  intent.addFlags(FORCE_NEW_TASK_FLAGS);
  String mimeType=intent.getType();
  if (mimeType == null && intent.getData() != null && "content".equals(intent.getData().getScheme())) {
    mimeType=mService.getProviderMimeType(intent.getData(),userId);
  }
  return startActivityMayWait(null,-1,null,intent,mimeType,null,null,null,null,0,0,null,null,null,null,null,userId,this);
}
