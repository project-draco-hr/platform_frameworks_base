{
  if (display.mDisplayId != Display.DEFAULT_DISPLAY) {
    return;
  }
  final float fraction=mService.mContext.getResources().getFraction(com.android.internal.R.fraction.config_displayFractionForDefaultMinimalSizeOfResizeableTask,1,1);
  mDefaultMinimalSizeOfResizeableTask=(int)(fraction * Math.min(display.mDisplayInfo.logicalWidth,display.mDisplayInfo.logicalHeight));
}
