{
  boolean putIntoExistingTask=((launchFlags & FLAG_ACTIVITY_NEW_TASK) != 0 && (launchFlags & FLAG_ACTIVITY_MULTIPLE_TASK) == 0) || launchSingleInstance || launchSingleTask;
  putIntoExistingTask&=inTask == null && r.resultTo == null;
  ActivityRecord intentActivity=null;
  if (putIntoExistingTask) {
    intentActivity=launchSingleInstance ? findActivityLocked(intent,r.info) : findTaskLocked(r);
  }
  return intentActivity;
}
