{
  int returnValue=0;
  if (mActions == null) {
    return returnValue;
  }
  final int actionSize=mActions.size();
  for (int i=0; i < actionSize; i++) {
    int actionId=mActions.get(i).getId();
    if (actionId <= LAST_LEGACY_STANDARD_ACTION) {
      returnValue|=actionId;
    }
  }
  return returnValue;
}
