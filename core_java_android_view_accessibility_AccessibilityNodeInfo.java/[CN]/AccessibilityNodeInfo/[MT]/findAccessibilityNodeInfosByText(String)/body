{
  enforceSealed();
  if (!canPerformRequestOverConnection(mAccessibilityViewId)) {
    return null;
  }
  try {
    return mConnection.findAccessibilityNodeInfosByViewText(text,mAccessibilityWindowId,mAccessibilityViewId);
  }
 catch (  RemoteException e) {
  }
  return Collections.emptyList();
}
