{
synchronized (this) {
    if (!mIsRunning) {
      return;
    }
  }
  try {
    mServerSocket.close();
  }
 catch (  IOException e) {
    Log.e(LOG_TAG,"mPort=" + mPort,e);
  }
synchronized (this) {
    mIsRunning=false;
  }
  for (  ConnectionHandler connectionHandler : mConnectionHandlers) {
    connectionHandler.stop();
  }
  mConnectionHandlers.clear();
}
