{
  Region_Delegate dstRegion=sManager.getDelegate(native_dst);
  if (dstRegion == null) {
    return true;
  }
  Region_Delegate srcRegion=sManager.getDelegate(native_src);
  if (srcRegion == null) {
    return true;
  }
  dstRegion.mArea.reset();
  dstRegion.mArea.add(srcRegion.mArea);
  return true;
}
