{
  WebBackForwardList l=new WebBackForwardList();
  if (mClearPending) {
    l.addHistoryItem(getCurrentItem());
    return l;
  }
  l.mCurrentIndex=mCurrentIndex;
  int size=getSize();
  l.mArray=new ArrayList<WebHistoryItem>(size);
  for (int i=0; i < size; i++) {
    l.mArray.add(mArray.get(i).clone());
  }
  return l;
}
