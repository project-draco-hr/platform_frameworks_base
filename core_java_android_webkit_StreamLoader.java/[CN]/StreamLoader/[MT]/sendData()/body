{
  if (mDataStream != null) {
    try {
      int amount=mDataStream.read(mData);
      if (amount > 0) {
        mHandler.data(mData,amount);
        return false;
      }
    }
 catch (    IOException ex) {
      mHandler.error(EventHandler.FILE_ERROR,ex.getMessage());
    }
  }
  return true;
}
