{
synchronized (mLock) {
    if (isFinishedLocked()) {
      return;
    }
    SomeArgs args=SomeArgs.obtain();
    args.arg1=mPrintAdapter;
    args.arg2=pages;
    args.arg3=fd.getFileDescriptor();
    args.arg4=progressListener;
    mHandler.obtainMessage(MyHandler.MESSAGE_PRINT,args).sendToTarget();
  }
}
