{
  final ILayoutResultCallback callback;
synchronized (mLock) {
    callback=mCallback;
    clearLocked();
  }
  if (callback != null) {
    try {
      callback.onLayoutFailed(error,mSequence);
    }
 catch (    RemoteException re) {
      Log.e(LOG_TAG,"Error calling onLayoutFailed",re);
    }
  }
}
