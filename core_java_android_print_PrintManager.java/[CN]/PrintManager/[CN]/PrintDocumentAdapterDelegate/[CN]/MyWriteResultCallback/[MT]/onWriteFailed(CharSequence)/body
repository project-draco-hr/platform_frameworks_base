{
  final IWriteResultCallback callback;
synchronized (mLock) {
    callback=mCallback;
    clearLocked();
  }
  if (callback != null) {
    try {
      callback.onWriteFailed(error,mSequence);
    }
 catch (    RemoteException re) {
      Log.e(LOG_TAG,"Error calling onWriteFailed",re);
    }
  }
}
