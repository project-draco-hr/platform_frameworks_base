{
  if (mService == null) {
    Log.w(LOG_TAG,"Feature android.software.print not available");
    return null;
  }
  try {
    return mService.getCustomPrinterIcon(printerId,mUserId);
  }
 catch (  RemoteException re) {
    Log.e(LOG_TAG,"Error getting custom printer icon",re);
  }
  return null;
}
