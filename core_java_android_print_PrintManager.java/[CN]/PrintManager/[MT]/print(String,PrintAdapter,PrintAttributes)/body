{
  PrintAdapterDelegate delegate=new PrintAdapterDelegate(printAdapter,mContext.getMainLooper());
  try {
    PrintJobInfo printJob=mService.print(printJobName,mPrintClient,delegate,attributes,mAppId,mUserId);
    if (printJob != null) {
      return new PrintJob(printJob,this);
    }
  }
 catch (  RemoteException re) {
    Log.e(LOG_TAG,"Error creating a print job",re);
  }
  return null;
}
