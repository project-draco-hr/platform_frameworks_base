{
  pw.print("WatchdogStatus: ");
  pw.print("State " + mStatus.state);
  pw.println(", network [" + mStatus.ssid + ", "+ mStatus.bssid+ "]");
  pw.print("checkCount " + mStatus.numFullDNSchecks);
  pw.print(", bssids: " + mStatus.allBssids.size());
  pw.print(", hasCheckMessages? " + mHandler.hasMessages(WifiWatchdogHandler.CHECK_SEQUENCE_STEP));
  pw.println(" hasSingleCheckMessages? " + mHandler.hasMessages(WifiWatchdogHandler.SINGLE_DNS_CHECK));
}
