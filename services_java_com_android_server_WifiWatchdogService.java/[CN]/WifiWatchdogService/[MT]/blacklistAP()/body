{
  Slog.i(WWS_TAG,String.format("Blacklisting current BSSID %s [ssid %s].  " + "numChecks %d, numAPs %d",mStatus.bssid,mStatus.ssid,mStatus.numFullDNSchecks,mStatus.allBssids.size()));
  mWifiManager.addToBlacklist(mStatus.bssid);
  mWifiManager.reassociate();
  mStatus.state=WatchdogState.BLACKLISTED_AP;
}
