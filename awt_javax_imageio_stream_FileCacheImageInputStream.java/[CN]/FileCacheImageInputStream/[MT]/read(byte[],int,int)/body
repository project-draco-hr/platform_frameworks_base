{
  bitOffset=0;
  if (streamPos >= raf.length()) {
    int nBytes=is.read(b,off,len);
    if (nBytes < 0) {
      return -1;
    }
    raf.seek(streamPos);
    raf.write(b,off,nBytes);
    streamPos+=nBytes;
    return nBytes;
  }
  raf.seek(streamPos);
  int nBytes=raf.read(b,off,len);
  streamPos+=nBytes;
  return nBytes;
}
