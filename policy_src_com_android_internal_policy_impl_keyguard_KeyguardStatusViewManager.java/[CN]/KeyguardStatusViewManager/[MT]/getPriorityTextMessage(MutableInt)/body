{
  CharSequence string=null;
  if (mShowingBatteryInfo) {
    if (mPluggedIn) {
      string=getContext().getString(mBatteryCharged ? com.android.internal.R.string.lockscreen_charged : com.android.internal.R.string.lockscreen_plugged_in,mBatteryLevel);
      icon.value=CHARGING_ICON;
    }
 else     if (mBatteryIsLow) {
      string=getContext().getString(com.android.internal.R.string.lockscreen_low_battery);
      icon.value=BATTERY_LOW_ICON;
    }
  }
  return string;
}
