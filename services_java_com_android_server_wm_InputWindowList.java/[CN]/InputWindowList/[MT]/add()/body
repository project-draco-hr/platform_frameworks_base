{
  if (mCount + 1 == mArray.length) {
    InputWindow[] oldArray=mArray;
    mArray=new InputWindow[oldArray.length * 2];
    System.arraycopy(oldArray,0,mArray,0,mCount);
  }
  InputWindow item=mArray[mCount + 1];
  if (item == null) {
    item=new InputWindow();
  }
  mArray[mCount]=item;
  mCount+=1;
  mArray[mCount]=null;
  return item;
}
