{
  mInstalledServices.clear();
  List<ResolveInfo> installedServices=mPackageManager.queryIntentServices(new Intent(AccessibilityService.SERVICE_INTERFACE),PackageManager.GET_SERVICES);
  for (int i=0, count=installedServices.size(); i < count; i++) {
    mInstalledServices.add(installedServices.get(i).serviceInfo);
  }
}
