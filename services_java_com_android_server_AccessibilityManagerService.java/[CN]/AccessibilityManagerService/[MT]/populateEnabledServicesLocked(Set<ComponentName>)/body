{
  enabledServices.clear();
  String servicesValue=Settings.Secure.getString(mContext.getContentResolver(),Settings.Secure.ENABLED_ACCESSIBILITY_SERVICES);
  if (servicesValue != null) {
    TextUtils.SimpleStringSplitter splitter=mStringColonSplitter;
    splitter.setString(servicesValue);
    while (splitter.hasNext()) {
      ComponentName enabledService=ComponentName.unflattenFromString(splitter.next());
      enabledServices.add(enabledService);
    }
  }
}
