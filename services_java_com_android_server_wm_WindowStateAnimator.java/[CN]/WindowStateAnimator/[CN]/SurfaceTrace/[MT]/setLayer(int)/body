{
  super.setLayer(zorder);
  mLayer=zorder;
  Slog.v(SURFACE_TAG,"setLayer: " + this + ". Called by "+ Debug.getCallers(3));
  sSurfaces.remove(this);
  int i;
  for (i=sSurfaces.size() - 1; i >= 0; i--) {
    SurfaceTrace s=sSurfaces.get(i);
    if (s.mLayer < zorder) {
      break;
    }
  }
  sSurfaces.add(i + 1,this);
}
