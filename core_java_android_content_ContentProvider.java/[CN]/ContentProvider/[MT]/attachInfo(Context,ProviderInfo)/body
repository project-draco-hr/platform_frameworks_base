{
  AsyncTask.init();
  if (mContext == null) {
    mContext=context;
    mMyUid=Process.myUid();
    if (info != null) {
      setReadPermission(info.readPermission);
      setWritePermission(info.writePermission);
      setPathPermissions(info.pathPermissions);
      mExported=info.exported;
      mNoPerms=false;
    }
 else {
      mNoPerms=true;
    }
    ContentProvider.this.onCreate();
  }
}
