{
  final Locale voiceLocale=voiceInfo.getLocale();
  if (mLocale.equals(voiceLocale)) {
    return 3;
  }
 else {
    if (mLocale.getLanguage().equals(voiceLocale.getLanguage())) {
      if (mLocale.getCountry().equals(voiceLocale.getCountry())) {
        return 2;
      }
      return 1;
    }
    return 0;
  }
}
