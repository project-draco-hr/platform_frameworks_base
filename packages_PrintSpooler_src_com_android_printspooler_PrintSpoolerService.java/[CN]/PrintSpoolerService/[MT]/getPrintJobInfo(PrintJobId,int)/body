{
synchronized (mLock) {
    final int printJobCount=mPrintJobs.size();
    for (int i=0; i < printJobCount; i++) {
      PrintJobInfo printJob=mPrintJobs.get(i);
      if (printJob.getId().equals(printJobId) && (appId == PrintManager.APP_ID_ANY || appId == printJob.getAppId())) {
        return printJob;
      }
    }
    return null;
  }
}
