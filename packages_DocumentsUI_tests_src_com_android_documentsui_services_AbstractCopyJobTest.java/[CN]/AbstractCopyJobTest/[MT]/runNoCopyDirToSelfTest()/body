{
  Uri testDir=mDocs.createFolder(mSrcRoot,"someDir");
  createJob(newArrayList(testDir),DocumentsContract.buildDocumentUri(AUTHORITY,mSrcRoot.documentId),testDir).run();
  mJobListener.waitForFinished();
  mJobListener.assertFailed();
  mJobListener.assertFilesFailed(newArrayList("someDir"));
  mDocs.assertChildCount(mDestRoot,0);
}
