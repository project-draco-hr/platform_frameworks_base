{
  if (level >= mLowBatteryAlertCloseLevel) {
    return 1;
  }
  if (level >= mLowBatteryReminderLevels[0]) {
    return 0;
  }
  final int N=mLowBatteryReminderLevels.length;
  for (int i=N - 1; i >= 0; i--) {
    if (level <= mLowBatteryReminderLevels[i]) {
      return -1 - i;
    }
  }
  throw new RuntimeException("not possible!");
}
