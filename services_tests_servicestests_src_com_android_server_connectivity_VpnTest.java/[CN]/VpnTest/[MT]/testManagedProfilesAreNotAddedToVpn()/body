{
  setMockedUsers(primaryUser,managedProfileA);
  final Vpn vpn=new MockVpn(primaryUser.id);
  final Set<UidRange> ranges=vpn.createUserAndRestrictedProfilesRanges(primaryUser.id,null,null);
  assertEquals(new ArraySet<>(Arrays.asList(new UidRange[]{UidRange.createForUser(primaryUser.id)})),ranges);
}
