{
  setMockedUsers(primaryUser,secondaryUser,restrictedProfileA,restrictedProfileB);
  final Vpn vpn=createVpn(primaryUser.id);
  final Set<UidRange> ranges=vpn.createUserAndRestrictedProfilesRanges(primaryUser.id,null,null);
  assertEquals(new ArraySet<>(Arrays.asList(new UidRange[]{UidRange.createForUser(primaryUser.id),UidRange.createForUser(restrictedProfileA.id)})),ranges);
}
