{
  if (mIntent != null) {
    dest.writeInt(1);
    mIntent.writeToParcel(dest,flags);
  }
 else {
    dest.writeInt(0);
  }
  if (mClipData != null) {
    dest.writeInt(1);
    mClipData.writeToParcel(dest,flags);
  }
 else {
    dest.writeInt(0);
  }
  if (mUri != null) {
    dest.writeInt(1);
    mUri.writeToParcel(dest,flags);
  }
 else {
    dest.writeInt(0);
  }
}
