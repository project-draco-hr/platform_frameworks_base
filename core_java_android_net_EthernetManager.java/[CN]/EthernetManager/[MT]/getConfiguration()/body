{
  if (mService == null) {
    return new IpConfiguration(IpAssignment.UNASSIGNED,ProxySettings.UNASSIGNED,new LinkProperties());
  }
  try {
    return mService.getConfiguration();
  }
 catch (  RemoteException e) {
    return new IpConfiguration(IpAssignment.UNASSIGNED,ProxySettings.UNASSIGNED,new LinkProperties());
  }
}
