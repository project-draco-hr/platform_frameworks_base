{
synchronized (mSync) {
    if (mCacheDir == null) {
      mCacheDir=new File(mFileContext.getCacheDir(),renamedFileName("cache"));
    }
    if (!mCacheDir.exists()) {
      if (!mCacheDir.mkdirs()) {
        Log.w("RenamingDelegatingContext","Unable to create cache directory");
        return null;
      }
      FileUtils.setPermissions(mCacheDir.getPath(),FileUtils.S_IRWXU | FileUtils.S_IRWXG | FileUtils.S_IXOTH,-1,-1);
    }
  }
  return mCacheDir;
}
