{
  if (mPopupLayout == 0) {
    return false;
  }
  if (mCurrentKey < 0 || mCurrentKey >= mKeyboard.getKeys().size()) {
    return false;
  }
  Key popupKey=mKeyboard.getKeys().get(mCurrentKey);
  boolean result=onLongPress(popupKey);
  if (result) {
    mAbortKey=true;
    showPreview(NOT_A_KEY);
  }
  return result;
}
