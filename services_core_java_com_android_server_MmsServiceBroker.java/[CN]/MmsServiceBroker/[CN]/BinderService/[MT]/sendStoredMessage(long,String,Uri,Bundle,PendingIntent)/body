{
  mContext.enforceCallingPermission(Manifest.permission.SEND_SMS,"Send stored MMS message");
  if (getAppOpsManager().noteOp(AppOpsManager.OP_SEND_SMS,Binder.getCallingUid(),callingPkg) != AppOpsManager.MODE_ALLOWED) {
    return;
  }
  getServiceGuarded().sendStoredMessage(subId,callingPkg,messageUri,configOverrides,sentIntent);
}
