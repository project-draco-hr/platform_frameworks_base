{
  mContext.enforceCallingPermission(Manifest.permission.SEND_SMS,"Send MMS message");
  if (getAppOpsManager().noteOp(AppOpsManager.OP_SEND_SMS,Binder.getCallingUid(),callingPkg) != AppOpsManager.MODE_ALLOWED) {
    return;
  }
  getServiceGuarded().sendMessage(subId,callingPkg,contentUri,locationUrl,configOverrides,sentIntent);
}
