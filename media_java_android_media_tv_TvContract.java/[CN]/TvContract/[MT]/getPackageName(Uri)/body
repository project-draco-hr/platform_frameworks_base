{
  final List<String> paths=channelsUri.getPathSegments();
  if (paths.size() < 4) {
    throw new IllegalArgumentException("Not channels: " + channelsUri);
  }
  if (!PATH_INPUT.equals(paths.get(0)) || !PATH_CHANNEL.equals(paths.get(3))) {
    throw new IllegalArgumentException("Not channels: " + channelsUri);
  }
  return paths.get(1);
}
