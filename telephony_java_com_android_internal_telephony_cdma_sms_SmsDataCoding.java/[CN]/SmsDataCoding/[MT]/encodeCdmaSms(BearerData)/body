{
  byte[] encodedSms;
  if (nativeCdmaSmsConstructClientBD() == JNI_CDMA_SMS_FAILURE) {
    return null;
  }
  generateBearerDataBitMask(bearerData);
  encodedSms=startEncoding(bearerData);
  if (nativeCdmaSmsDestructClientBD() == JNI_CDMA_SMS_FAILURE) {
    return null;
  }
  return encodedSms;
}
