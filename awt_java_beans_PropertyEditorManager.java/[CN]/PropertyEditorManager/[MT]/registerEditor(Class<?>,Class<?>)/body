{
  if (targetType == null) {
    throw new NullPointerException();
  }
  SecurityManager sm=System.getSecurityManager();
  if (sm != null) {
    sm.checkPropertiesAccess();
  }
  if (editorClass != null) {
    registeredEditors.put(targetType,editorClass);
  }
 else {
    registeredEditors.remove(targetType);
  }
}
