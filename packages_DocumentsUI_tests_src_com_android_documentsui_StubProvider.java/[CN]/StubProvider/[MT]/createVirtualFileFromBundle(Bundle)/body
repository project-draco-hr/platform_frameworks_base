{
  try {
    Uri uri=createVirtualFile(extras.getString(EXTRA_ROOT),extras.getString(EXTRA_PATH),extras.getString(Document.COLUMN_MIME_TYPE),extras.getStringArrayList(EXTRA_STREAM_TYPES),extras.getByteArray(EXTRA_CONTENT));
    String documentId=DocumentsContract.getDocumentId(uri);
    Bundle result=new Bundle();
    result.putString(Document.COLUMN_DOCUMENT_ID,documentId);
    return result;
  }
 catch (  IOException e) {
    Log.e(TAG,"Couldn't create virtual file.");
  }
  return null;
}
