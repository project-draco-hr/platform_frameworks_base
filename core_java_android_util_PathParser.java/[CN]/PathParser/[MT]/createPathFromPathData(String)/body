{
  Path path=new Path();
  PathDataNode[] nodes=createNodesFromPathData(pathData);
  if (nodes != null) {
    try {
      PathDataNode.nodesToPath(nodes,path);
    }
 catch (    RuntimeException e) {
      throw new RuntimeException("Error in parsing " + pathData,e);
    }
    return path;
  }
  return null;
}
