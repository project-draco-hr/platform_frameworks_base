{
  if (pathString == null) {
    throw new IllegalArgumentException("Path string can not be null.");
  }
  Path path=new Path();
  boolean hasValidPathData=nParseStringForPath(path.mNativePath,pathString,pathString.length());
  if (!hasValidPathData) {
    throw new IllegalArgumentException("Path string: " + pathString + " does not contain valid path data");
  }
  return path;
}
