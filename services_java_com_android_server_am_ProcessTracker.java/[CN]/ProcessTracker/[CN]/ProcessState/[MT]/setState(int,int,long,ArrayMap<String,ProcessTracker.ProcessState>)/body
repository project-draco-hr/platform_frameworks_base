{
  if (state < 0) {
    state=STATE_NOTHING;
  }
 else {
    state=PROCESS_STATE_TO_STATE[state] + (memFactor * STATE_COUNT);
  }
  mCommonProcess.setState(state,now);
  if (!mCommonProcess.mMultiPackage) {
    return;
  }
  for (int ip=pkgList.size() - 1; ip >= 0; ip--) {
    pullFixedProc(pkgList,ip).setState(state,now);
  }
}
