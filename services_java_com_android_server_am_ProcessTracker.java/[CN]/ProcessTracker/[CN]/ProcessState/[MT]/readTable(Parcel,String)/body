{
  int size=in.readInt();
  if (size < 0) {
    Slog.w(TAG,"Ignoring existing stats; bad " + what + " table size: "+ size);
    return null;
  }
  int[] table=new int[size];
  for (int i=0; i < size; i++) {
    table[i]=in.readInt();
    if (!mState.validateLongOffset(table[i])) {
      Slog.w(TAG,"Ignoring existing stats; bad " + what + " table entry: 0x"+ Integer.toHexString(table[i]));
      return null;
    }
  }
  return table;
}
