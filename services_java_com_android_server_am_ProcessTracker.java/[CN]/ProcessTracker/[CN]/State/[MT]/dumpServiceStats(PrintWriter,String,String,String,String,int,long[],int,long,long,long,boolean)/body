{
  if (count != 0) {
    if (dumpAll) {
      pw.print(prefix);
      pw.print(header);
      pw.print(" op count ");
      pw.print(count);
      pw.println(":");
      dumpSingleTime(pw,prefixInner,durations,state,startTime,now);
    }
 else {
      long myTime=dumpSingleTime(null,null,durations,state,startTime,now);
      pw.print(prefix);
      pw.print(headerPrefix);
      pw.print(header);
      pw.print(" count ");
      pw.print(count);
      pw.print(" / time ");
      printPercent(pw,(double)myTime / (double)totalTime);
      pw.println();
    }
  }
}
