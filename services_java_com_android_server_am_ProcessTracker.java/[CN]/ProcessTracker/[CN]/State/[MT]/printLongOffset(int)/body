{
  StringBuilder sb=new StringBuilder(16);
  sb.append("a");
  sb.append((off >> OFFSET_ARRAY_SHIFT) & OFFSET_ARRAY_MASK);
  sb.append("i");
  sb.append((off >> OFFSET_INDEX_SHIFT) & OFFSET_INDEX_MASK);
  sb.append("t");
  sb.append((off >> OFFSET_TYPE_SHIFT) & OFFSET_TYPE_MASK);
  return sb.toString();
}
