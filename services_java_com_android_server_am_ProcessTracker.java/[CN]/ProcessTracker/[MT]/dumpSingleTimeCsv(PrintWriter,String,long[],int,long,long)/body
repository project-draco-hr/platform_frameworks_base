{
  for (int iscreen=0; iscreen < ADJ_COUNT; iscreen+=ADJ_SCREEN_MOD) {
    for (int imem=0; imem < ADJ_MEM_FACTOR_COUNT; imem++) {
      int state=imem + iscreen;
      long time=durations[state];
      if (curState == state) {
        time+=now - curStartTime;
      }
      pw.print(sep);
      if (time != 0) {
        pw.print(time);
      }
 else {
        pw.print("0");
      }
    }
  }
}
