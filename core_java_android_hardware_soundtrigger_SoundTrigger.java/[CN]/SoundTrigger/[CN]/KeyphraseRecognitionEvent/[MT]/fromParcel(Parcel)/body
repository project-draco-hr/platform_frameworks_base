{
  int status=in.readInt();
  int soundModelHandle=in.readInt();
  boolean captureAvailable=in.readByte() == 1;
  int captureSession=in.readInt();
  int captureDelayMs=in.readInt();
  int capturePreambleMs=in.readInt();
  byte[] data=in.readBlob();
  boolean keyphraseInCapture=in.readByte() == 1;
  KeyphraseRecognitionExtra[] keyphraseExtras=in.createTypedArray(KeyphraseRecognitionExtra.CREATOR);
  return new KeyphraseRecognitionEvent(status,soundModelHandle,captureAvailable,captureSession,captureDelayMs,capturePreambleMs,data,keyphraseInCapture,keyphraseExtras);
}
