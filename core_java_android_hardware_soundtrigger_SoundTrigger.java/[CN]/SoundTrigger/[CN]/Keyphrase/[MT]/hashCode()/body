{
  final int prime=31;
  int result=1;
  result=prime * result + ((text == null) ? 0 : text.hashCode());
  result=prime * result + id;
  result=prime * result + ((locale == null) ? 0 : locale.hashCode());
  result=prime * result + recognitionModes;
  result=prime * result + Arrays.hashCode(users);
  return result;
}
