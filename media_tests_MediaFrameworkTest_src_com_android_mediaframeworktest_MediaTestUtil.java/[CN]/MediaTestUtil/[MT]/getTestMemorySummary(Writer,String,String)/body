{
  int endMemory=0;
  int memDiff=0;
  if (type == "mediaMem") {
    endMemory=getMediaServerVsize();
    memDiff=endMemory - mMediaStartMemory;
  }
 else   if (type == "drmMem") {
    endMemory=getDrmServerVsize();
    memDiff=endMemory - mDrmStartMemory;
  }
  output.write("End Memory :" + endMemory + "\n");
  if (memDiff < 0) {
    memDiff=0;
  }
  output.write(tag + " total diff = " + memDiff);
  output.write("\n\n");
}
