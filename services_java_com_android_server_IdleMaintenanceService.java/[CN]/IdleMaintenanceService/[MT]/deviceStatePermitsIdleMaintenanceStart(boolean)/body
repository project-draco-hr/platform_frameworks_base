{
  final int minBatteryLevel=isBatteryCharging() ? MIN_BATTERY_LEVEL_IDLE_MAINTENANCE_START_CHARGING : MIN_BATTERY_LEVEL_IDLE_MAINTENANCE_START_NOT_CHARGING;
  boolean allowed=(mLastUserActivityElapsedTimeMillis != LAST_USER_ACTIVITY_TIME_INVALID && mBatteryService.getBatteryLevel() > minBatteryLevel);
  if (!allowed && noisy) {
    Slog.i("IdleMaintenance","Idle maintenance not allowed due to power");
  }
  return allowed;
}
