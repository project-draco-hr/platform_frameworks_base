{
  if (PERF_PROBE) {
    mWebCoreThreadTime=SystemClock.currentThreadTimeMillis();
    mWebCoreIdleTime=0;
    if (!JniUtil.useChromiumHttpStack()) {
      Network.getInstance(mContext).startTiming();
    }
  }
  Message msg=obtainMessage(PAGE_STARTED);
  msg.obj=favicon;
  msg.getData().putString("url",url);
  sendMessage(msg);
}
