{
  mPMUtils.installAppAndVerifyExistsOnDevice(String.format("%s%s%s",getTestAppPath(),File.separator,FILE_DOWNLOAD_APK),FILE_DOWNLOAD_PKG,true);
  boolean testPassed=mPMUtils.runDeviceTestsDidAllTestsPass(FILE_DOWNLOAD_PKG,FILE_DOWNLOAD_CLASS,"runDownloadMultipleSwitching",DOWNLOAD_TEST_RUNNER_NAME,mExtraParams);
  assertTrue(testPassed);
}
