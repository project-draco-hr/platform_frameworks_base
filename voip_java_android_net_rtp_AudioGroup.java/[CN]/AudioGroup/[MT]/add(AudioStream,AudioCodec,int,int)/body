{
  if (!mStreams.containsKey(stream)) {
    try {
      int id=add(stream.getMode(),stream.dup(),stream.getRemoteAddress().getHostAddress(),stream.getRemotePort(),codec.name,codec.sampleRate,codec.sampleCount,codecType,dtmfType);
      mStreams.put(stream,id);
    }
 catch (    NullPointerException e) {
      throw new IllegalStateException(e);
    }
  }
}
