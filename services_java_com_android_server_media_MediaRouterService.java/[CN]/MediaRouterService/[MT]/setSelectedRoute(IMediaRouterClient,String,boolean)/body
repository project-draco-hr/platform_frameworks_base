{
  if (client == null) {
    throw new IllegalArgumentException("client must not be null");
  }
  final long token=Binder.clearCallingIdentity();
  try {
synchronized (mLock) {
      setSelectedRouteLocked(client,routeId,explicit);
    }
  }
  finally {
    Binder.restoreCallingIdentity(token);
  }
}
