{
  StringBuilder sb=new StringBuilder(getScString());
  sb.append("\n");
  if (ar.exception != null) {
    state=State.FAILED;
    sb.append(context.getText(com.android.internal.R.string.mmiError));
  }
 else {
    state=State.FAILED;
    sb.append(context.getText(com.android.internal.R.string.mmiError));
  }
  message=sb;
  phone.onFeatureCodeDone(this);
}
