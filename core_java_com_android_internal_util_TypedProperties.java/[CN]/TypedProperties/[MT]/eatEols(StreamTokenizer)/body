{
  int token;
  boolean eolSeen=false;
  do {
    token=st.nextToken();
    if (token == StreamTokenizer.TT_EOF) {
      return -1;
    }
 else     if (token == StreamTokenizer.TT_EOL) {
      eolSeen=true;
    }
  }
 while (token == StreamTokenizer.TT_EOL);
  st.pushBack();
  return eolSeen ? 1 : 0;
}
