{
  if (proxy != null) {
    if (mProxies.unregister(proxy)) {
      try {
        proxy.onServiceDisconnected(mClassName);
      }
 catch (      RemoteException e) {
        Slog.e(TAG,"Unable to disconnect proxy",e);
      }
    }
  }
 else {
    Slog.w(TAG,"Trying to remove a null proxy");
  }
}
