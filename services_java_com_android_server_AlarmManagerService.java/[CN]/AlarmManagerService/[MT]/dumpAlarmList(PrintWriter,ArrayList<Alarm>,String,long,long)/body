{
  for (int i=list.size() - 1; i >= 0; i--) {
    Alarm a=list.get(i);
    final String label=labelForType(a.type);
    long now=(a.type <= RTC) ? nowRTC : nowELAPSED;
    pw.print(prefix);
    pw.print(label);
    pw.print(" #");
    pw.print(i);
    pw.print(": ");
    pw.println(a);
    a.dump(pw,prefix + "  ",now);
  }
}
