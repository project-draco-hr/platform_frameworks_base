{
  try {
    if (ws != null) {
      mWakeLock.setWorkSource(ws);
      return;
    }
    final int uid=ActivityManagerNative.getDefault().getUidForIntentSender(pi.getTarget());
    if (uid >= 0) {
      mWakeLock.setWorkSource(new WorkSource(uid));
      return;
    }
  }
 catch (  Exception e) {
  }
  mWakeLock.setWorkSource(null);
}
