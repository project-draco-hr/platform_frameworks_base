{
  StringBuilder b=new StringBuilder(40);
  b.append("Batch{");
  b.append(Integer.toHexString(this.hashCode()));
  b.append(" num=");
  b.append(size());
  b.append(" start=");
  b.append(start);
  b.append(" end=");
  b.append(end);
  if (standalone) {
    b.append(" STANDALONE");
  }
  b.append('}');
  return b.toString();
}
