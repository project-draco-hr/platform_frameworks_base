{
  final int version=in.readInt();
switch (version) {
case VERSION_INIT:
    final int size=in.readInt();
  for (int i=0; i < size; i++) {
    final DocumentInfo doc=new DocumentInfo();
    doc.read(in);
    add(doc);
  }
break;
default :
throw new ProtocolException("Unknown version " + version);
}
}
