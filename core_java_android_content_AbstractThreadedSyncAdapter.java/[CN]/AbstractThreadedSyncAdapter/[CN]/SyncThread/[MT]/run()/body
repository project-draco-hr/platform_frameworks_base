{
  if (isCanceled()) {
    return;
  }
  SyncResult syncResult=new SyncResult();
  ContentProviderClient provider=null;
  try {
    provider=mContext.getContentResolver().acquireContentProviderClient(mAuthority);
    if (provider != null) {
      AbstractThreadedSyncAdapter.this.performSync(mAccount,mExtras,mAuthority,provider,syncResult);
    }
 else {
    }
  }
  finally {
    if (provider != null) {
      provider.release();
    }
    if (!isCanceled()) {
      mSyncContext.onFinished(syncResult);
    }
synchronized (this) {
      mSyncThread=null;
    }
  }
}
