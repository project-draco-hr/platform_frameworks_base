{
  int start=out.dataPosition();
  ComponentName.writeToParcel(mActivityComponent,out);
  final int N=mRootViews.size();
  out.writeInt(N);
  for (int i=0; i < N; i++) {
    mRootViews.get(i).writeToParcel(out);
  }
  Log.i(TAG,"Flattened assist data: " + (out.dataPosition() - start) + " bytes");
}
