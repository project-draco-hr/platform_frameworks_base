{
  cancel(DISAPPEARING);
  if (changesLayout) {
    cancel(CHANGE_APPEARING);
  }
  if (mListeners != null) {
    for (    TransitionListener listener : mListeners) {
      listener.startTransition(this,parent,child,APPEARING);
    }
  }
  if (changesLayout) {
    runChangeTransition(parent,child,APPEARING);
  }
  runAppearingTransition(parent,child);
}
