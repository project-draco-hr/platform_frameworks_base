{
switch (transitionType) {
case CHANGE_APPEARING:
    mChangingAppearingDelay=delay;
  break;
case CHANGE_DISAPPEARING:
mChangingDisappearingDelay=delay;
break;
case CHANGING:
mChangingDelay=delay;
break;
case APPEARING:
mAppearingDelay=delay;
break;
case DISAPPEARING:
mDisappearingDelay=delay;
break;
}
}
