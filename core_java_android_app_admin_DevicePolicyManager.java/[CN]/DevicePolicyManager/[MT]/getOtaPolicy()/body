{
  if (mService != null) {
    try {
      PersistableBundle bundle=mService.getOtaPolicy();
      if (bundle != null) {
        return new OtaPolicy(bundle);
      }
 else {
        return null;
      }
    }
 catch (    RemoteException re) {
      Log.w(TAG,"Error calling getOtaPolicy",re);
    }
  }
  return null;
}
