{
  if (mParentInstance) {
    throw new SecurityException("Reset password does not work across profiles.");
  }
  if (mService != null) {
    try {
      return mService.resetPassword(password,flags);
    }
 catch (    RemoteException e) {
      Log.w(TAG,REMOTE_EXCEPTION_MESSAGE,e);
    }
  }
  return false;
}
