{
  throwIfParentInstance("setAlwaysOnVpnPackage");
  if (mService != null) {
    try {
      if (!mService.setAlwaysOnVpnPackage(admin,vpnPackage)) {
        throw new NameNotFoundException(vpnPackage);
      }
    }
 catch (    RemoteException e) {
      throw e.rethrowFromSystemServer();
    }
  }
}
