{
  if (mService != null) {
    try {
      String profileOwnerPackage=mService.getProfileOwner(Process.myUserHandle().getIdentifier()).getPackageName();
      return profileOwnerPackage != null && profileOwnerPackage.equals(packageName);
    }
 catch (    RemoteException re) {
      Log.w(TAG,"Failed to check profile owner");
    }
  }
  return false;
}
