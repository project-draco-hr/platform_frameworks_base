{
  if (mService != null) {
    try {
      if (policy != null) {
        mService.setSystemUpdatePolicy(who,policy.getPolicyBundle());
      }
 else {
        mService.setSystemUpdatePolicy(who,null);
      }
    }
 catch (    RemoteException re) {
      Log.w(TAG,"Error calling setSystemUpdatePolicy",re);
    }
  }
}
