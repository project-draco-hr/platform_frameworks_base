{
  if (mService != null) {
    try {
      List<String> providers=mService.getCrossProfileWidgetProviders(admin);
      if (providers != null) {
        return providers;
      }
    }
 catch (    RemoteException re) {
      Log.w(TAG,"Error calling getCrossProfileWidgetProviders",re);
    }
  }
  return Collections.emptyList();
}
