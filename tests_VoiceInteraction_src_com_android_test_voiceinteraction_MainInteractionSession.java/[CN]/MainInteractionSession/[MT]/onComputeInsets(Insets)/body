{
  super.onComputeInsets(outInsets);
  if (mState != STATE_IDLE) {
    outInsets.contentInsets.top=mBottomContent.getTop();
    outInsets.touchableInsets=Insets.TOUCHABLE_INSETS_CONTENT;
  }
 else   if ((mShowFlags & SHOW_SOURCE_ACTIVITY) != 0) {
    outInsets.touchableInsets=Insets.TOUCHABLE_INSETS_CONTENT;
  }
}
