{
  if ((packageInfo.applicationInfo.flags & ApplicationInfo.FLAG_SYSTEM) == 0 && packageInfo.versionCode < getMinimumVersionCode() && !mSystemInterface.systemIsDebuggable()) {
    return false;
  }
  if (providerHasValidSignature(configInfo,packageInfo,mSystemInterface) && WebViewFactory.getWebViewLibrary(packageInfo.applicationInfo) != null) {
    return true;
  }
  return false;
}
