{
  if (mTargetIds.size() == 0 && mTargets.size() == 0) {
    return true;
  }
  if (mTargetIds.size() > 0) {
    for (int i=0; i < mTargetIds.size(); ++i) {
      if (mTargetIds.get(i) == targetId) {
        return true;
      }
    }
  }
  if (target != null && mTargets.size() > 0) {
    for (int i=0; i < mTargets.size(); ++i) {
      if (mTargets.get(i) == target) {
        return true;
      }
    }
  }
  return false;
}
