{
  if (netId != INVALID_NETWORK_ID) {
    WifiConfiguration config=mConfiguredNetworks.get(netId);
    if (config == null)     return;
switch (state) {
case CONNECTED:
      config.status=Status.CURRENT;
    break;
case DISCONNECTED:
  if (config.status == Status.CURRENT) {
    config.status=Status.ENABLED;
  }
break;
default :
break;
}
}
}
