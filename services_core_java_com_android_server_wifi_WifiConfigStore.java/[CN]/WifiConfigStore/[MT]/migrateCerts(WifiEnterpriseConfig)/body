{
  String client=config.getClientCertificateAlias();
  if (!TextUtils.isEmpty(client)) {
    if (!mKeyStore.contains(Credentials.USER_PRIVATE_KEY + client,Process.WIFI_UID)) {
      mKeyStore.duplicate(Credentials.USER_PRIVATE_KEY + client,-1,Credentials.USER_PRIVATE_KEY + client,Process.WIFI_UID);
      mKeyStore.duplicate(Credentials.USER_CERTIFICATE + client,-1,Credentials.USER_CERTIFICATE + client,Process.WIFI_UID);
    }
  }
  String ca=config.getCaCertificateAlias();
  if (!TextUtils.isEmpty(ca)) {
    if (!mKeyStore.contains(Credentials.CA_CERTIFICATE + ca,Process.WIFI_UID)) {
      mKeyStore.duplicate(Credentials.CA_CERTIFICATE + ca,-1,Credentials.CA_CERTIFICATE + ca,Process.WIFI_UID);
    }
  }
}
