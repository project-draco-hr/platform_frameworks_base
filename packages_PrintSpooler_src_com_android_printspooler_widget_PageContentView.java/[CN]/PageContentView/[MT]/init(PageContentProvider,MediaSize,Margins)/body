{
  if (mProvider == provider && ((mMediaSize == null) ? mediaSize == null : mMediaSize.equals(mediaSize)) && ((mMinMargins == null) ? minMargins == null : mMinMargins.equals(minMargins))) {
    return;
  }
  mProvider=provider;
  mMediaSize=mediaSize;
  mMinMargins=minMargins;
  mContentRequested=false;
  if (mProvider == null && getBackground() != mEmptyState) {
    setBackground(mEmptyState);
  }
  requestPageContentIfNeeded();
}
