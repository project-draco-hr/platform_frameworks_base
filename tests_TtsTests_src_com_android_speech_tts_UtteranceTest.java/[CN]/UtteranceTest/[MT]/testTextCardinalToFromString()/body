{
  Utterance utt=new Utterance().append(new TtsCardinal(55)).append(new TtsText("this is a text."));
  String str=utt.toString();
  assertEquals("type: \"utterance\" " + "markup { " + "type: \"cardinal\" "+ "integer: \"55\" "+ "} "+ "markup { "+ "type: \"text\" "+ "text: \"this is a text.\" "+ "}",str);
  Utterance utt_new=Utterance.utteranceFromString(str);
  assertEquals(str,utt_new.toString());
}
