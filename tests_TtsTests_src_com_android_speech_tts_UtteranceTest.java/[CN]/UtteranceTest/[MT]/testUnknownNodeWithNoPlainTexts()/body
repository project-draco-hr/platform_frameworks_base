{
  String str="type: \"utterance\" " + "markup { " + "type: \"some_future_feature\" "+ "bombom: \"lorum ipsum\" "+ "markup { type: \"cardinal\" integer: \"10\" } "+ "markup { type: \"text\" text: \"pears\" } "+ "}";
  Utterance utt=Utterance.utteranceFromString(str);
  assertEquals("type: \"utterance\" " + "markup { type: \"cardinal\" integer: \"10\" } " + "markup { type: \"text\" text: \"pears\" }",utt.toString());
}
