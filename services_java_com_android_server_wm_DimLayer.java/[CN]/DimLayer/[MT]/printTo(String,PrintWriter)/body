  pw.print(" curTime=");
  pw.println(SystemClock.uptimeMillis());
  pw.print(prefix);
  pw.print(" mStartAlpha=");
  pw.print(mStartAlpha);
  pw.print(" mTargetAlpha=");
  pw.println(mTargetAlpha);
