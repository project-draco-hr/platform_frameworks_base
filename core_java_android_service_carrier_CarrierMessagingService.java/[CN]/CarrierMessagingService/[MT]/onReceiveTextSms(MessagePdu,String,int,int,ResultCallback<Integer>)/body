{
  onFilterSms(pdu,format,destPort,subId,new ResultCallback<Boolean>(){
    @Override public void onReceiveResult(    Boolean result) throws RemoteException {
      callback.onReceiveResult(result ? RECEIVE_OPTIONS_DEFAULT : RECEIVE_OPTIONS_DROP | RECEIVE_OPTIONS_SKIP_NOTIFY_WHEN_CREDENTIAL_ENCRYPTED_STORAGE_UNAVAILABLE);
    }
  }
);
}
