{
  for (  String instructionSet : instructionSets) {
    assertValidInstructionSet(instructionSet);
  }
  final String rawRes=mInstaller.executeForResult("get_app_size",uuid,pkgname,userid,flags,apkPath,libDirPath,fwdLockApkPath,asecPath,instructionSets[0]);
  final String res[]=rawRes.split(" ");
  if ((res == null) || (res.length != 5)) {
    throw new InstallerException("Invalid size result: " + rawRes);
  }
  try {
    pStats.codeSize=Long.parseLong(res[1]);
    pStats.dataSize=Long.parseLong(res[2]);
    pStats.cacheSize=Long.parseLong(res[3]);
    pStats.externalCodeSize=Long.parseLong(res[4]);
  }
 catch (  NumberFormatException e) {
    throw new InstallerException("Invalid size result: " + rawRes);
  }
}
