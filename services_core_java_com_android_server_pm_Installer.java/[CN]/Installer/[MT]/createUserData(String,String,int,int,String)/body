{
  StringBuilder builder=new StringBuilder("mkuserdata");
  builder.append(' ');
  builder.append(escapeNull(uuid));
  builder.append(' ');
  builder.append(name);
  builder.append(' ');
  builder.append(uid);
  builder.append(' ');
  builder.append(userId);
  builder.append(' ');
  builder.append(seinfo != null ? seinfo : "!");
  return mInstaller.execute(builder.toString());
}
