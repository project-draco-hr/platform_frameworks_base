{
  final ProcFileReader reader=buildReader("1\n\n2\n\n3\n");
  assertEquals(1,reader.nextInt());
  reader.finishLine();
  assertTrue(reader.hasMoreData());
  reader.finishLine();
  assertTrue(reader.hasMoreData());
  assertEquals(2,reader.nextInt());
  reader.finishLine();
  assertTrue(reader.hasMoreData());
  reader.finishLine();
  assertTrue(reader.hasMoreData());
  assertEquals(3,reader.nextInt());
  reader.finishLine();
  assertFalse(reader.hasMoreData());
}
