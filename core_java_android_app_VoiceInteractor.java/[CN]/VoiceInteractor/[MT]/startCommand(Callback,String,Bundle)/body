{
  try {
    callback.mInteractor=this;
    Request req=storeRequest(mInteractor.startCommand(mContext.getOpPackageName(),callback.mWrapper,command,extras));
    if (DEBUG)     Log.d(TAG,"startCommand: req=" + req.mRequestInterface.asBinder() + " command="+ command+ " extras="+ extras);
    return req;
  }
 catch (  RemoteException e) {
    throw new RuntimeException("Voice interactor has died",e);
  }
}
