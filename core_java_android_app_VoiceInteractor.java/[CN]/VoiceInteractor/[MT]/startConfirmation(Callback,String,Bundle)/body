{
  try {
    callback.mInteractor=this;
    Request req=storeRequest(mInteractor.startConfirmation(mContext.getOpPackageName(),callback.mWrapper,prompt,extras));
    if (DEBUG)     Log.d(TAG,"startConfirmation: req=" + req.mRequestInterface.asBinder() + " prompt="+ prompt+ " extras="+ extras);
    return req;
  }
 catch (  RemoteException e) {
    throw new RuntimeException("Voice interactor has died",e);
  }
}
