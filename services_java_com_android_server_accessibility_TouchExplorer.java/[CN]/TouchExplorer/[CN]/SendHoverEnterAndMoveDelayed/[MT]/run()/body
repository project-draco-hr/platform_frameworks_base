{
  sendAccessibilityEvent(AccessibilityEvent.TYPE_TOUCH_EXPLORATION_GESTURE_START);
  if (!mEvents.isEmpty()) {
    sendMotionEvent(mEvents.get(0),MotionEvent.ACTION_HOVER_ENTER,mPointerIdBits,mPolicyFlags);
    if (DEBUG) {
      Slog.d(LOG_TAG_SEND_HOVER_DELAYED,"Injecting motion event: ACTION_HOVER_ENTER");
    }
    final int eventCount=mEvents.size();
    for (int i=1; i < eventCount; i++) {
      sendMotionEvent(mEvents.get(i),MotionEvent.ACTION_HOVER_MOVE,mPointerIdBits,mPolicyFlags);
      if (DEBUG) {
        Slog.d(LOG_TAG_SEND_HOVER_DELAYED,"Injecting motion event: ACTION_HOVER_MOVE");
      }
    }
  }
  clear();
}
