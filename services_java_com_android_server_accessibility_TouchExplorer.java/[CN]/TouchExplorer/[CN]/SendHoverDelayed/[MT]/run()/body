{
  if (DEBUG) {
    Slog.d(LOG_TAG_SEND_HOVER_DELAYED,"Injecting motion event: " + MotionEvent.actionToString(mHoverAction));
    Slog.d(LOG_TAG_SEND_HOVER_DELAYED,mGestureStarted ? "touchExplorationGestureStarted" : "touchExplorationGestureEnded");
  }
  if (mGestureStarted) {
    mAms.touchExplorationGestureStarted();
  }
 else {
    mAms.touchExplorationGestureEnded();
  }
  sendMotionEvent(mPrototype,mHoverAction,mPointerIdBits,mPolicyFlags);
  clear();
}
