{
  if (mTargetIds == null && mTargets == null) {
    return true;
  }
  if (mTargetIds != null) {
    for (int i=0; i < mTargetIds.length; ++i) {
      if (mTargetIds[i] == targetId) {
        return true;
      }
    }
  }
  if (target != null && mTargets != null) {
    for (int i=0; i < mTargets.length; ++i) {
      if (mTargets[i] == target) {
        return true;
      }
    }
  }
  return false;
}
