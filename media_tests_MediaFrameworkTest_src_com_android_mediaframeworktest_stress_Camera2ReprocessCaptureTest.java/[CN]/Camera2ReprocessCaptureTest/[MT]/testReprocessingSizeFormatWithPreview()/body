{
  for (  String id : mCameraIds) {
    if (!isYuvReprocessSupported(id) && !isOpaqueReprocessSupported(id)) {
      continue;
    }
    try {
      openDevice(id);
      for (int iteration=0; iteration < getIterationCount(); ++iteration) {
        Log.v(TAG,String.format("Reprocessing size format with preview: %d/%d",iteration + 1,getIterationCount()));
        testReprocessingAllCombinations(id,mOrderedPreviewSizes.get(0),CaptureTestCase.SINGLE_SHOT);
        getResultPrinter().printStatus(getIterationCount(),iteration + 1,id);
        Thread.sleep(getTestWaitIntervalMs());
      }
    }
  finally {
      closeDevice();
    }
  }
}
