{
synchronized (mIconMap) {
    StatusBarIcon icon=null;
    final int N=mIconList.size();
    for (int i=0; i < N; i++) {
      StatusBarIcon ic=mIconList.get(i);
      if (ic.view == v) {
        icon=ic;
        break;
      }
    }
    if (icon != null) {
      return icon.getNumber();
    }
 else {
      return -1;
    }
  }
}
