{
  title=in.readString();
  items=new ArrayList<Item>();
  int size=in.readInt();
  for (int i=0; i < size; i++) {
    Item item=in.readParcelable(null);
    items.add(item);
  }
  defaultItem=in.readInt();
  softKeyPreferred=in.readInt() == 1 ? true : false;
  helpAvailable=in.readInt() == 1 ? true : false;
}
