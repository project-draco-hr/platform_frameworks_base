{
  for (int winNdx=windows.size() - 1; winNdx >= 0; --winNdx) {
    WindowState win=windows.get(winNdx);
    if (WindowManagerService.DEBUG_WINDOW_MOVEMENT)     Slog.w(WindowManagerService.TAG,"removeAllWindows: removing win=" + win);
    win.mService.removeWindowLocked(win);
  }
  windows.clear();
}
