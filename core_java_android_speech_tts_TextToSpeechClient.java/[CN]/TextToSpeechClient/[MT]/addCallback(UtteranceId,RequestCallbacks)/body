{
synchronized (mLock) {
    if (utteranceId == null || callback == null) {
      return Status.ERROR_INVALID_REQUEST;
    }
    if (mCallbacks.put(utteranceId.toUniqueString(),new Pair<UtteranceId,RequestCallbacks>(utteranceId,callback)) != null) {
      return Status.ERROR_NON_UNIQUE_UTTERANCE_ID;
    }
    return Status.SUCCESS;
  }
}
