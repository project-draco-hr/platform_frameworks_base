{
  runAction(new Action<Void>(ACTION_STOP_NAME){
    @Override public Void run(    ITextToSpeechService service) throws RemoteException {
      if (service.stop(getCallerIdentity()) != Status.SUCCESS) {
        Log.e(TAG,"Stop failed");
      }
      mCallbacks.clear();
      return null;
    }
  }
);
}
