{
synchronized (mLock) {
    if (mSetupConnectionAsyncTask == this) {
      mSetupConnectionAsyncTask=null;
    }
    if (result == null) {
      Log.e(TAG,"Setup task failed");
      disconnect();
      mConnectionCallbacks.onConnectionFailure();
      return;
    }
    mEngineStatus=result;
    mEstablished=true;
  }
  mConnectionCallbacks.onConnectionSuccess();
}
