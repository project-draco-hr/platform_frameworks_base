{
  IccRecords r=mIccRecords.get();
  if (r != null) {
    try {
      Uri uri=Uri.withAppendedPath(Telephony.Carriers.CONTENT_URI,"current");
      ContentValues map=new ContentValues();
      String operatorNumeric=r.getOperatorNumeric();
      map.put(Telephony.Carriers.NUMERIC,operatorNumeric);
      if (DBG)       log("updateCurrentCarrierInProvider from UICC: numeric=" + operatorNumeric);
      mContext.getContentResolver().insert(uri,map);
      return true;
    }
 catch (    SQLException e) {
      Log.e(LOG_TAG,"[CDMALTEPhone] Can't store current operator ret false",e);
    }
  }
 else {
    if (DBG)     log("updateCurrentCarrierInProvider mIccRecords == null ret false");
  }
  return false;
}
