{
  boolean cleared=false;
  int index=mDevicePermissionMap.indexOfKey(uid);
  if (index >= 0) {
    mDevicePermissionMap.removeAt(index);
    cleared=true;
  }
  index=mAccessoryPermissionMap.indexOfKey(uid);
  if (index >= 0) {
    mAccessoryPermissionMap.removeAt(index);
    cleared=true;
  }
  return cleared;
}
