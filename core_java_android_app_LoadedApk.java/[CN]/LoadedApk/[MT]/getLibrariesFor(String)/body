{
  ApplicationInfo ai=null;
  try {
    ai=ActivityThread.getPackageManager().getApplicationInfo(packageName,PackageManager.GET_SHARED_LIBRARY_FILES,UserHandle.myUserId());
  }
 catch (  RemoteException e) {
    throw e.rethrowFromSystemServer();
  }
  if (ai == null) {
    return null;
  }
  return ai.sharedLibraryFiles;
}
