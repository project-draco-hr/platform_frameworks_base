{
  if (ActivityThread.DEBUG_BROADCAST) {
    int seq=intent.getIntExtra("seq",-1);
    Slog.i(ActivityThread.TAG,"Enqueueing broadcast " + intent.getAction() + " seq="+ seq+ " to "+ mReceiver);
  }
  Args args=new Args();
  args.mCurIntent=intent;
  args.mCurCode=resultCode;
  args.mCurData=data;
  args.mCurMap=extras;
  args.mCurOrdered=ordered;
  args.mCurSticky=sticky;
  if (!mActivityThread.post(args)) {
    if (mRegistered && ordered) {
      IActivityManager mgr=ActivityManagerNative.getDefault();
      try {
        if (ActivityThread.DEBUG_BROADCAST)         Slog.i(ActivityThread.TAG,"Finishing sync broadcast to " + mReceiver);
        mgr.finishReceiver(mIIntentReceiver,args.mCurCode,args.mCurData,args.mCurMap,false);
      }
 catch (      RemoteException ex) {
      }
    }
  }
}
