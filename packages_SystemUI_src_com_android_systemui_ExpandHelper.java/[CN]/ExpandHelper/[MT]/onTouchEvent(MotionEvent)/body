{
  final int action=ev.getAction();
  if (DEBUG)   Log.d(TAG,"touch: act=" + (action) + " stretching="+ mStretching);
  if (mStretching) {
    mDetector.onTouchEvent(ev);
  }
switch (action) {
case MotionEvent.ACTION_UP:
case MotionEvent.ACTION_CANCEL:
    mStretching=false;
  clearView();
break;
}
return true;
}
