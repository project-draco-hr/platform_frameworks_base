{
  super(in);
  selectedId=in.readLong();
  firstId=in.readLong();
  viewTop=in.readInt();
  position=in.readInt();
  height=in.readInt();
  filter=in.readString();
  checkState=in.readSparseBooleanArray();
  long[] idState=in.createLongArray();
  if (idState.length > 0) {
    checkIdState=new LongSparseArray<Boolean>();
    checkIdState.setValues(idState,Boolean.TRUE);
  }
}
