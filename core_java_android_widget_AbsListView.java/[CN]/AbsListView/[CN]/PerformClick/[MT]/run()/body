{
  if (mDataChanged)   return;
  final ListAdapter adapter=mAdapter;
  final int motionPosition=mClickMotionPosition;
  if (adapter != null && mItemCount > 0 && motionPosition != INVALID_POSITION && motionPosition < adapter.getCount() && sameWindow()) {
    performItemClick(getChildAt(motionPosition - mFirstPosition),motionPosition,adapter.getItemId(motionPosition));
  }
}
