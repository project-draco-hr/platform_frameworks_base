{
  if ((focusableMode & FOCUSABLES_ACCESSIBILITY) == FOCUSABLES_ACCESSIBILITY && (direction == ACCESSIBILITY_FOCUS_FORWARD || direction == ACCESSIBILITY_FOCUS_BACKWARD)) {
    if (canTakeAccessibilityFocusFromHover()) {
      views.add(this);
    }
  }
 else {
    super.addFocusables(views,direction,focusableMode);
  }
}
