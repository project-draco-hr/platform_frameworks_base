{
  mUri.writeToParcel(out,flags);
  out.writeInt(mFlags);
  out.writeCharSequence(mTitle);
  if (mSummary != null) {
    out.writeInt(1);
    out.writeCharSequence(mSummary);
  }
 else {
    out.writeInt(0);
  }
  if (mExtras != null) {
    out.writeInt(1);
    mExtras.writeToParcel(out,flags);
  }
 else {
    out.writeInt(0);
  }
}
