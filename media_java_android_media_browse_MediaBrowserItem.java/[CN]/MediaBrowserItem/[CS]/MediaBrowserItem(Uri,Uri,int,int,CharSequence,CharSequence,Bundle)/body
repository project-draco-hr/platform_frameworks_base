{
  if (uri == null) {
    throw new IllegalArgumentException("uri can not be null");
  }
  if (title == null) {
    throw new IllegalArgumentException("title can not be null");
  }
  mUri=uri;
  mIconUri=iconUri;
  mIconResId=iconResId;
  mFlags=flags;
  mTitle=title;
  mSummary=summary;
  mExtras=extras;
}
