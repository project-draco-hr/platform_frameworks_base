{
  mUri=Uri.CREATOR.createFromParcel(in);
  mFlags=in.readInt();
  mTitle=in.readCharSequence();
  if (in.readInt() != 0) {
    mSummary=in.readCharSequence();
  }
 else {
    mSummary=null;
  }
  if (in.readInt() != 0) {
    mExtras=Bundle.CREATOR.createFromParcel(in);
  }
 else {
    mExtras=null;
  }
}
