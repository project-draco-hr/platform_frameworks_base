{
  String rslt=SQLiteDebug.captureSql(TEST_DB,"select * from t1 where a=? and b=1",new Object[]{"blah"});
  String expectedVal="select * from t1 where a='blah' and b=1";
  assertTrue(rslt.equals("captured_sql|" + TEST_DB + "|"+ expectedVal));
  rslt=SQLiteDebug.captureSql(TEST_DB,"select * from t1 where a=?",new Object[]{"blah"});
  expectedVal="select * from t1 where a='blah'";
  assertTrue(rslt.equals("captured_sql|" + TEST_DB + "|"+ expectedVal));
  rslt=SQLiteDebug.captureSql(TEST_DB,"select * from t1 where a=1",new Object[]{"blah"});
  assertTrue(rslt.startsWith("too many bindArgs provided."));
  rslt=SQLiteDebug.captureSql(TEST_DB,"update t1 set a=? where b=?",new Object[]{"blah","foo"});
  expectedVal="update t1 set a='blah' where b='foo'";
  assertTrue(rslt.equals("captured_sql|" + TEST_DB + "|"+ expectedVal));
}
