{
  if (data == null) {
    throw new IllegalArgumentException("data is null");
  }
  mData=data.getData();
  mAccountId=data.getAccountId();
  mSubscriptionId=data.getSubscriptionId();
  mMimeType=mimeType;
  if (!isValid()) {
    final String msg="mimeType: " + mMimeType + ","+ "data: "+ mData;
    throw new IllegalArgumentException(msg);
  }
}
