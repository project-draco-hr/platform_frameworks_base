{
  try {
    FileInputStream in=file.openRead();
    try {
      read(in,statsOut);
      statsOut.lastTimeSaved=file.getLastModifiedTime();
    }
  finally {
      try {
        in.close();
      }
 catch (      IOException e) {
      }
    }
  }
 catch (  FileNotFoundException e) {
    Slog.e(TAG,"UsageStats Xml",e);
    throw e;
  }
}
