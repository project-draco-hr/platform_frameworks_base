{
  if (!mHasSelectedDay) {
    return;
  }
  mSelectedDayLine.setBounds(mSelectedLeft - mSelectedDayLineWidth / 2,mWeekSeperatorWidth,mSelectedLeft + mSelectedDayLineWidth / 2,mHeight);
  mSelectedDayLine.draw(canvas);
  mSelectedDayLine.setBounds(mSelectedRight - mSelectedDayLineWidth / 2,mWeekSeperatorWidth,mSelectedRight + mSelectedDayLineWidth / 2,mHeight);
  mSelectedDayLine.draw(canvas);
}
