{
  mDrawPaint.setFakeBoldText(false);
  mDrawPaint.setAntiAlias(true);
  mDrawPaint.setTextSize(mNumberTextSize);
  mDrawPaint.setStyle(Style.FILL);
  mMonthNumDrawPaint.setFakeBoldText(true);
