{
  mHttpRequest.removeHeaders(CONTENT_LENGTH_HEADER);
  if (mBodyProvider != null) {
    try {
      mBodyProvider.reset();
    }
 catch (    IOException ex) {
      if (HttpLog.LOGV)       HttpLog.v("failed to reset body provider " + getUri());
    }
    setBodyProvider(mBodyProvider,mBodyLength);
  }
}
