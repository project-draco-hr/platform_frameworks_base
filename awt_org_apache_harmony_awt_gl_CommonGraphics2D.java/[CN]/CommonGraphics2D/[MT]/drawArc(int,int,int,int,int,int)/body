{
  if (stroke instanceof BasicStroke && ((BasicStroke)stroke).getLineWidth() <= 1 && ((BasicStroke)stroke).getDashArray() == null && (transform.isIdentity() || transform.getType() == AffineTransform.TYPE_TRANSLATION)) {
    Point p=new Point(x,y);
    transform.transform(p,p);
    MultiRectArea mra=JavaArcRasterizer.rasterize(x,y,width,height,sa,ea,clip);
    fillMultiRectArea(mra);
    return;
  }
  draw(new Arc2D.Float(x,y,width,height,sa,ea,Arc2D.OPEN));
}
