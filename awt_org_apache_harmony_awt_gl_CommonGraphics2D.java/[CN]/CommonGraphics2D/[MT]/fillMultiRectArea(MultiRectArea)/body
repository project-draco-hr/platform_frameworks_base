{
  if (clip != null) {
    mra.intersect(clip);
  }
  if (mra.rect[0] < 5) {
    return;
  }
  if (debugOutput) {
    System.err.println("CommonGraphics2D.fillMultiRectArea(" + mra + ")");
  }
  if (paint instanceof Color) {
    fillMultiRectAreaColor(mra);
  }
 else {
    fillMultiRectAreaPaint(mra);
  }
}
