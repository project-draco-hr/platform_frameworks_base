{
  if (mState != STATE_WAITING_FOR_REPORT_POWER_STATUS || mTargetAddress != cmd.getSource()) {
    return false;
  }
  if (cmd.getOpcode() == Constants.MESSAGE_REPORT_POWER_STATUS) {
    int status=cmd.getParams()[0];
    invokeCallback(status);
    finish();
    return true;
  }
  return false;
}
