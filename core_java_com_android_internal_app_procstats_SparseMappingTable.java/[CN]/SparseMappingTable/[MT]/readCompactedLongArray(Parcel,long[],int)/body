{
  final int alen=array.length;
  if (num > alen) {
    logOrThrow("bad array lengths: got " + num + " array is "+ alen);
    return;
  }
  int i;
  for (i=0; i < num; i++) {
    int val=in.readInt();
    if (val >= 0) {
      array[i]=val;
    }
 else {
      int bottom=in.readInt();
      array[i]=(((long)~val) << 32) | bottom;
    }
  }
  while (i < alen) {
    array[i]=0;
    i++;
  }
}
