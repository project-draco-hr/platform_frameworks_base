{
  for (int i=0; i < num; i++) {
    long val=array[i];
    if (val < 0) {
      Slog.w(TAG,"Time val negative: " + val);
      val=0;
    }
    if (val <= Integer.MAX_VALUE) {
      out.writeInt((int)val);
    }
 else {
      int top=~((int)((val >> 32) & 0x7fffffff));
      int bottom=(int)(val & 0xfffffff);
      out.writeInt(top);
      out.writeInt(bottom);
    }
  }
}
