{
  assertConsistency();
  final int idx=binarySearch(id);
  if (idx >= 0) {
    final int key=mTable[idx];
    try {
      final long[] array=mParent.mLongs.get(getArrayFromKey(key));
      return array[getIndexFromKey(key) + index];
    }
 catch (    IndexOutOfBoundsException ex) {
      logOrThrow("id=0x" + Integer.toHexString(id) + " idx="+ idx+ " key=0x"+ Integer.toHexString(key)+ " index="+ index+ " -- "+ dumpInternalState(),ex);
      return 0;
    }
  }
 else {
    return 0;
  }
}
