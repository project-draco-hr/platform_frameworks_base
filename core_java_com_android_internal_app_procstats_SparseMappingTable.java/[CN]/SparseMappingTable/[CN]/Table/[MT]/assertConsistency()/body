{
  if (mSequence == UNINITIALIZED_SEQUENCE) {
    logOrThrow("mSequence == UNINITIALIZED_SEQUENCE in" + " SparseMappingTable.Table.  mParent.mSequence=" + mParent.mSequence);
  }
  if (mSequence != mParent.mSequence) {
    if (mSequence < mParent.mSequence) {
      logOrThrow("Sequence mismatch. SparseMappingTable.resetTable()" + " called but not Table.resetTable() -- " + dumpInternalState());
    }
 else     if (mSequence > mParent.mSequence) {
      logOrThrow("Sequence mismatch. Table.resetTable()" + " called but not SparseMappingTable.resetTable() -- " + dumpInternalState());
    }
  }
}
