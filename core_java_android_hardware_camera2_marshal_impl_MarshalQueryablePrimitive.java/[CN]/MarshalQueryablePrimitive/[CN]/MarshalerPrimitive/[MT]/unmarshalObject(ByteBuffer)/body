{
switch (mNativeType) {
case TYPE_INT32:
    return buffer.getInt();
case TYPE_FLOAT:
  return buffer.getFloat();
case TYPE_INT64:
return buffer.getLong();
case TYPE_RATIONAL:
int numerator=buffer.getInt();
int denominator=buffer.getInt();
return new Rational(numerator,denominator);
case TYPE_DOUBLE:
return buffer.getDouble();
case TYPE_BYTE:
return buffer.get();
default :
throw new UnsupportedOperationException("Can't unmarshal native type " + mNativeType);
}
}
