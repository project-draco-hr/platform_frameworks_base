{
  final int[] NOOP_COMMANDS={TetherInterfaceSM.CMD_TETHER_MODE_DEAD,TetherInterfaceSM.CMD_TETHER_UNREQUESTED,TetherInterfaceSM.CMD_INTERFACE_UP,TetherInterfaceSM.CMD_CELL_DUN_ERROR,TetherInterfaceSM.CMD_IP_FORWARDING_ENABLE_ERROR,TetherInterfaceSM.CMD_IP_FORWARDING_DISABLE_ERROR,TetherInterfaceSM.CMD_START_TETHERING_ERROR,TetherInterfaceSM.CMD_STOP_TETHERING_ERROR,TetherInterfaceSM.CMD_SET_DNS_FORWARDERS_ERROR,TetherInterfaceSM.CMD_TETHER_CONNECTION_CHANGED};
  for (  int command : NOOP_COMMANDS) {
    mTestedSm.sendMessage(command);
    dispatchUntilIdle();
    verifyNoMoreInteractions(mNMService);
    verifyNoMoreInteractions(mStatsService);
    verifyNoMoreInteractions(mTetherHelper);
  }
}
