{
switch (hotspotState) {
case WifiManager.WIFI_AP_STATE_DISABLED:
    return "DISABLED";
case WifiManager.WIFI_AP_STATE_DISABLING:
  return "DISABLING";
case WifiManager.WIFI_AP_STATE_ENABLED:
return "ENABLED";
case WifiManager.WIFI_AP_STATE_ENABLING:
return "ENABLING";
case WifiManager.WIFI_AP_STATE_FAILED:
return "FAILED";
}
return null;
}
