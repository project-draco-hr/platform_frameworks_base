{
synchronized (mLocks) {
    if (time > (mWarningSpewThrottleTime + (60 * 60 * 1000))) {
      mWarningSpewThrottleTime=time;
      mWarningSpewThrottleCount=0;
      return true;
    }
 else     if (mWarningSpewThrottleCount < 30) {
      mWarningSpewThrottleCount++;
      return true;
    }
 else {
      return false;
    }
  }
}
