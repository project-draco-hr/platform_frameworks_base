{
synchronized (mLocks) {
    mIsDocked=(state != Intent.EXTRA_DOCK_STATE_UNDOCKED);
    if (mIsDocked) {
      mHighestLightSensorValue=-1;
    }
    if ((mPowerState & SCREEN_ON_BIT) != 0) {
      int value=(int)mLightSensorValue;
      mLightSensorValue=-1;
      lightSensorChangedLocked(value,false);
    }
  }
}
