{
  if (mWakeUpWhenPluggedOrUnpluggedConfig) {
    if (wasPowered && !mIsPowered && oldPlugType == BatteryManager.BATTERY_PLUGGED_WIRELESS) {
      return false;
    }
    if (!wasPowered && mIsPowered && mPlugType == BatteryManager.BATTERY_PLUGGED_WIRELESS && mBatteryService.getBatteryLevel() >= WIRELESS_CHARGER_TURN_ON_BATTERY_LEVEL_LIMIT) {
      return false;
    }
    return true;
  }
  return false;
}
