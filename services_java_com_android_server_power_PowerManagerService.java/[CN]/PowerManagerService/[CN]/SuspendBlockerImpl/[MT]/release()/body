{
synchronized (this) {
    mReferenceCount-=1;
    if (mReferenceCount == 0) {
      if (DEBUG_SPEW) {
        Slog.d(TAG,"Releasing suspend blocker \"" + mName + "\".");
      }
      nativeReleaseSuspendBlocker(mName);
    }
 else     if (mReferenceCount < 0) {
      Log.wtf(TAG,"Suspend blocker \"" + mName + "\" was released without being acquired!",new Throwable());
      mReferenceCount=0;
    }
  }
}
