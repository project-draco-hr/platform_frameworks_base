{
  int[] ids=FingerprintUtils.getFingerprintIdsForUser(mContext.getContentResolver(),getCurrentUserId());
  List<FingerprintItem> result=new ArrayList<FingerprintItem>();
  for (int i=0; i < ids.length; i++) {
    FingerprintItem item=new FingerprintItem("Finger" + ids[i],ids[i]);
    result.add(item);
  }
  return result;
}
