{
  if (mService != null) {
    try {
      if (!mListening) {
        mService.addListener(mToken,mServiceReceiver,getCurrentUserId());
        mListening=true;
      }
    }
 catch (    RemoteException e) {
      Log.v(TAG,"Remote exception in startListening(): ",e);
    }
  }
 else {
    Log.w(TAG,"startListening(): Service not connected!");
  }
}
