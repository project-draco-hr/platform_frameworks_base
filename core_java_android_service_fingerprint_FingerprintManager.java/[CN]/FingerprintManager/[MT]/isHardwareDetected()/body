{
  if (mService != null) {
    try {
      return mService.isHardwareDetected();
    }
 catch (    RemoteException e) {
      Log.v(TAG,"Remote exception in isFingerprintHardwareDetected(): ",e);
    }
  }
 else {
    Log.w(TAG,"isFingerprintHardwareDetected(): Service not connected!");
  }
  return false;
}
