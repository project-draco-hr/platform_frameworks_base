{
  List<WebViewProviderInfo> webViewProviders=new ArrayList<WebViewProviderInfo>(Arrays.asList(mWebViewUtility.getWebViewPackages()));
  Iterator<WebViewProviderInfo> it=webViewProviders.iterator();
  while (it.hasNext()) {
    WebViewProviderInfo current=it.next();
    if (!current.isValidProvider())     it.remove();
  }
synchronized (this) {
    mCurrentValidWebViewPackages=webViewProviders.toArray(new WebViewProviderInfo[webViewProviders.size()]);
  }
}
