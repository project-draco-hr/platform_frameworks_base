{
  if (Binder.getCallingUid() == Process.SYSTEM_UID) {
    Slog.wtf(TAG,"Trying to load WebView from the SystemServer",new IllegalStateException());
  }
synchronized (this) {
    if (is64Bit) {
      while (!mRelroReady64Bit) {
        try {
          this.wait();
        }
 catch (        InterruptedException e) {
        }
      }
    }
 else {
      while (!mRelroReady32Bit) {
        try {
          this.wait();
        }
 catch (        InterruptedException e) {
        }
      }
    }
  }
}
