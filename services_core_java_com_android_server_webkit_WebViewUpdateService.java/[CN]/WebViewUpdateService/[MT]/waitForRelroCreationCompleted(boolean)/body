{
synchronized (this) {
    if (is64Bit) {
      while (!mRelroReady64Bit) {
        try {
          this.wait();
        }
 catch (        InterruptedException e) {
        }
      }
    }
 else {
      while (!mRelroReady32Bit) {
        try {
          this.wait();
        }
 catch (        InterruptedException e) {
        }
      }
    }
  }
}
