{
  int dialStrLen=dialStr.length();
  int sysSelCodeInt=INVALID_SYSTEM_SELECTION_CODE;
  if ((dialStr.regionMatches(0,IS683A_FEATURE_CODE,0,IS683A_FEATURE_CODE_NUM_DIGITS)) && (dialStrLen >= (IS683A_FEATURE_CODE_NUM_DIGITS + IS683A_SYS_SEL_CODE_NUM_DIGITS))) {
    sysSelCodeInt=Integer.parseInt(dialStr.substring(IS683A_FEATURE_CODE_NUM_DIGITS,IS683A_FEATURE_CODE_NUM_DIGITS + IS683A_SYS_SEL_CODE_NUM_DIGITS));
  }
  if (DBG)   Log.d(LOG_TAG,"extractSelCodeFromOtaSpNum " + sysSelCodeInt);
  return sysSelCodeInt;
}
