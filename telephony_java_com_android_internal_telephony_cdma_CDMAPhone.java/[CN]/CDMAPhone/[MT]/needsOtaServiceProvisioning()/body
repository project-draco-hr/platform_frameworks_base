{
  String cdmaMin=getCdmaMin();
  boolean needsProvisioning;
  if (cdmaMin == null || (cdmaMin.length() < 6)) {
    if (DBG)     Log.d(LOG_TAG,"needsOtaServiceProvisioning: illegal cdmaMin='" + cdmaMin + "' assume provisioning needed.");
    needsProvisioning=true;
  }
 else {
    needsProvisioning=(cdmaMin.equals(UNACTIVATED_MIN_VALUE) || cdmaMin.substring(0,6).equals(UNACTIVATED_MIN2_VALUE)) || SystemProperties.getBoolean("test_cdma_setup",false);
  }
  if (DBG)   Log.d(LOG_TAG,"needsOtaServiceProvisioning: ret=" + needsProvisioning);
  return needsProvisioning;
}
