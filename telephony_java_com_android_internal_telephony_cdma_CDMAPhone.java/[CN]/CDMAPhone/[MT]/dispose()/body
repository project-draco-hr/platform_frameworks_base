{
synchronized (PhoneProxy.lockForRadioTechnologyChange) {
    mRuimRecords.unregisterForRecordsLoaded(h);
    mCM.unregisterForAvailable(h);
    mCM.unregisterForOffOrNotAvailable(h);
    mCM.unregisterForOn(h);
    mCM.unregisterForNVReady(h);
    mSST.unregisterForNetworkAttach(h);
    mCM.unSetOnSuppServiceNotification(h);
    mCM.unSetOnCallRing(h);
    mCM.unregisterForCdmaCallWaiting(h);
    mCT.dispose();
    mDataConnection.dispose();
    mSST.dispose();
    mSMS.dispose();
    mIccFileHandler.dispose();
    mRuimRecords.dispose();
    mRuimCard.dispose();
    mRuimPhoneBookInterfaceManager.dispose();
    mRuimSmsInterfaceManager.dispose();
    mSubInfo.dispose();
    mEriManager.dispose();
  }
}
