{
  if (mIcon == null) {
    return;
  }
  Rect tmpRect=mPositionIconOutput;
  getLineBounds(0,tmpRect);
  mPositionIconAvailable.set(0,0,getWidth(),tmpRect.top);
  Gravity.apply(Gravity.CENTER_VERTICAL | Gravity.LEFT,mIcon.getIntrinsicWidth(),mIcon.getIntrinsicHeight(),mPositionIconAvailable,mPositionIconOutput);
  mIcon.setBounds(mPositionIconOutput);
}
