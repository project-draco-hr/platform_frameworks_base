{
  super.animateValue(fraction);
  if (mSetter != null) {
    try {
      mTmpValueArray[0]=getAnimatedValue();
      mSetter.invoke(mTarget,mTmpValueArray);
    }
 catch (    InvocationTargetException e) {
      Log.e("PropertyAnimator",e.toString());
    }
catch (    IllegalAccessException e) {
      Log.e("PropertyAnimator",e.toString());
    }
  }
}
