{
  if (activeTest != null) {
    if (!activeTest.isAlive()) {
      try {
        activeTest.join();
      }
 catch (      InterruptedException e) {
      }
      activeTest=null;
    }
  }
  if (activeTest == null) {
    if (test_iter.hasNext()) {
      activeTest=test_iter.next();
      activeTest.start();
    }
  }
  count++;
}
