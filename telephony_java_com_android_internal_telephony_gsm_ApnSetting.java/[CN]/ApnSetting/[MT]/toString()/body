{
  StringBuilder sb=new StringBuilder();
  sb.append(carrier).append(", ").append(id).append(", ").append(numeric).append(", ").append(apn).append(", ").append(proxy).append(", ").append(mmsc).append(", ").append(mmsProxy).append(", ").append(mmsPort).append(", ").append(port).append(", ").append(authType);
  for (  String t : types) {
    sb.append(", ").append(t);
  }
  return sb.toString();
}
