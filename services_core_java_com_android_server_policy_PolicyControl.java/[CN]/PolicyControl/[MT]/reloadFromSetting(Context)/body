{
  if (DEBUG)   Slog.d(TAG,"reloadFromSetting()");
  String value=null;
  try {
    value=Settings.Global.getStringForUser(context.getContentResolver(),Settings.Global.POLICY_CONTROL,UserHandle.USER_CURRENT);
    if (sSettingValue != null && sSettingValue.equals(value))     return;
    setFilters(value);
    sSettingValue=value;
  }
 catch (  Throwable t) {
    Slog.w(TAG,"Error loading policy control, value=" + value,t);
  }
}
