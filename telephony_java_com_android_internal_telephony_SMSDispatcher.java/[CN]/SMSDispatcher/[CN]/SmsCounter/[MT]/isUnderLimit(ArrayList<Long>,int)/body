{
  Long ct=System.currentTimeMillis();
  Log.d(TAG,"SMS send size=" + sent.size() + "time="+ ct);
  while (sent.size() > 0 && (ct - sent.get(0)) > mCheckPeriod) {
    sent.remove(0);
  }
  if ((sent.size() + smsWaiting) <= mMaxAllowed) {
    for (int i=0; i < smsWaiting; i++) {
      sent.add(ct);
    }
    return true;
  }
  return false;
}
