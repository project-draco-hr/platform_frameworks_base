{
  mStateManager.onTileAdded();
  mStateManager.onStartListening();
  PackageManager pm=getContext().getPackageManager();
  pm.setComponentEnabledSetting(new ComponentName(getContext(),FakeTileService.class),PackageManager.COMPONENT_ENABLED_STATE_DISABLED,PackageManager.DONT_KILL_APP);
  bindService();
  assertTrue(mStateManager.mReceiverRegistered);
  pm.setComponentEnabledSetting(new ComponentName(getContext(),FakeTileService.class),PackageManager.COMPONENT_ENABLED_STATE_ENABLED,PackageManager.DONT_KILL_APP);
  waitForCallback("onCreate");
}
