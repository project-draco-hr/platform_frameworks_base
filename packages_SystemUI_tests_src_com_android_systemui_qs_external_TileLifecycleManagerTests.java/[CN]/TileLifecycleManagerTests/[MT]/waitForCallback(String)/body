{
  for (int i=0; i < 50; i++) {
    if (mCallbacks.contains(callback)) {
      mCallbacks.remove(callback);
      return;
    }
synchronized (mBroadcastLock) {
      try {
        mBroadcastLock.wait(500);
      }
 catch (      InterruptedException e) {
      }
    }
  }
  if (mCallbacks.contains(callback)) {
    mCallbacks.remove(callback);
    return;
  }
  fail("Didn't receive callback: " + callback);
}
