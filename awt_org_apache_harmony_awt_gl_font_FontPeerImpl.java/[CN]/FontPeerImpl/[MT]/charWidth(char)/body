{
  Paint p;
  AndroidGraphics2D g=AndroidGraphics2D.getInstance();
  if (g == null) {
    throw new RuntimeException("AndroidGraphics2D not instantiated!");
  }
  p=((AndroidGraphics2D)g).getAndroidPaint();
  char[] ca={ch};
  float[] fa=new float[1];
  p.getTextWidths(ca,0,1,fa);
  return (int)fa[0];
}
