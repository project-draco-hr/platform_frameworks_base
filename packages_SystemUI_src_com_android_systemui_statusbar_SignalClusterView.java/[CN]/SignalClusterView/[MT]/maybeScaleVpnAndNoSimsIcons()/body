{
  if (mIconScaleFactor == 1.f) {
    return;
  }
  mVpn.setImageDrawable(new ScalingDrawableWrapper(mVpn.getDrawable(),mIconScaleFactor));
  mNoSims.setImageDrawable(new ScalingDrawableWrapper(mNoSims.getDrawable(),mIconScaleFactor));
  mNoSimsDark.setImageDrawable(new ScalingDrawableWrapper(mNoSimsDark.getDrawable(),mIconScaleFactor));
}
