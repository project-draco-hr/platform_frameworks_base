{
  if (mEthernetVisible && mEthernetGroup != null && mEthernetGroup.getContentDescription() != null)   event.getText().add(mEthernetGroup.getContentDescription());
  if (mWifiVisible && mWifiGroup != null && mWifiGroup.getContentDescription() != null)   event.getText().add(mWifiGroup.getContentDescription());
  for (  PhoneState state : mPhoneStates) {
    state.populateAccessibilityEvent(event);
  }
  return super.dispatchPopulateAccessibilityEventInternal(event);
}
