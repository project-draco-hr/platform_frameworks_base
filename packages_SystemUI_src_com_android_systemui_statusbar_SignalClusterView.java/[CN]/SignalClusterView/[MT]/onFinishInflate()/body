{
  super.onFinishInflate();
  mWideTypeIconStartPadding=getContext().getResources().getDimensionPixelSize(R.dimen.wide_type_icon_start_padding);
  mSecondaryTelephonyPadding=getContext().getResources().getDimensionPixelSize(R.dimen.secondary_telephony_padding);
