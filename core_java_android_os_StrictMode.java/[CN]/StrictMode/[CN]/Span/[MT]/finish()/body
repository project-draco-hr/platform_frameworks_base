{
  ThreadSpanState state=mContainerState;
synchronized (state) {
    if (mName == null) {
      return;
    }
    if (mPrev != null) {
      mPrev.mNext=mNext;
    }
    if (mNext != null) {
      mNext.mPrev=mPrev;
    }
    if (state.mActiveHead == this) {
      state.mActiveHead=mNext;
    }
    this.mCreateMillis=-1;
    this.mName=null;
    this.mPrev=null;
    this.mNext=null;
    state.mActiveSize--;
    if (state.mFreeListSize < 5) {
      this.mNext=state.mFreeListHead;
      state.mFreeListHead=this;
      state.mFreeListSize++;
    }
  }
}
