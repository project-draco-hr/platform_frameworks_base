{
  if (message == null || !message.startsWith("policy=")) {
    return 0;
  }
  int spaceIndex=message.indexOf(' ');
  if (spaceIndex == -1) {
    return 0;
  }
  String policyString=message.substring(7,spaceIndex);
  try {
    return Integer.parseInt(policyString);
  }
 catch (  NumberFormatException e) {
    return 0;
  }
}
