{
  if ("user".equals(Build.TYPE)) {
    return false;
  }
  StrictMode.setThreadPolicyMask(StrictMode.DETECT_DISK_WRITE | StrictMode.DETECT_DISK_READ | StrictMode.DETECT_NETWORK| StrictMode.PENALTY_DROPBOX);
  sVmPolicyMask=StrictMode.DETECT_VM_CURSOR_LEAKS | StrictMode.PENALTY_DROPBOX | StrictMode.PENALTY_LOG;
  return true;
}
