{
  if (IS_USER_BUILD) {
    setCloseGuardEnabled(false);
    return false;
  }
  StrictMode.setThreadPolicyMask(StrictMode.DETECT_DISK_WRITE | StrictMode.DETECT_DISK_READ | StrictMode.DETECT_NETWORK| StrictMode.PENALTY_DROPBOX);
  sVmPolicyMask=StrictMode.DETECT_VM_CURSOR_LEAKS | StrictMode.DETECT_VM_CLOSABLE_LEAKS | StrictMode.PENALTY_DROPBOX;
  setCloseGuardEnabled(vmClosableObjectLeaksEnabled());
  return true;
}
