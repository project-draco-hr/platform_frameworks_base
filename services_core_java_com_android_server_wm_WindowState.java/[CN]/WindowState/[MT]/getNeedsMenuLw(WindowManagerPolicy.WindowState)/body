{
  int index=-1;
  WindowState ws=this;
  WindowList windows=getWindowList();
  while (true) {
    if ((ws.mAttrs.privateFlags & WindowManager.LayoutParams.PRIVATE_FLAG_SET_NEEDS_MENU_KEY) != 0) {
      return (ws.mAttrs.flags & WindowManager.LayoutParams.FLAG_NEEDS_MENU_KEY) != 0;
    }
    if (ws == bottom) {
      return false;
    }
    if (index < 0) {
      index=windows.indexOf(ws);
    }
    index--;
    if (index < 0) {
      return false;
    }
    ws=windows.get(index);
  }
}
