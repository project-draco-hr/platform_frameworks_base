{
  final Task task=getTask();
  if (task == null) {
    return false;
  }
  if (task.isDragResizing()) {
    return true;
  }
  return (mDisplayContent.mDividerControllerLocked.isResizing() || mAppToken != null && !mAppToken.mFrozenBounds.isEmpty()) && !task.inFreeformWorkspace() && !task.isFullscreen();
}
