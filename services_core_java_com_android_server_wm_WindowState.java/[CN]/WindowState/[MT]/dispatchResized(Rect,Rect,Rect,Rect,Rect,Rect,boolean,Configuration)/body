{
  final boolean forceRelayout=isDragResizeChanged() || mResizedWhileNotDragResizing;
  mClient.resized(frame,overscanInsets,contentInsets,visibleInsets,stableInsets,outsets,reportDraw,newConfig,getBackdropFrame(frame),forceRelayout,mPolicy.isNavBarForcedShownLw(this));
  mDragResizingChangeReported=true;
}
