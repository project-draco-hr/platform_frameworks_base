{
  DisplayInfo displayInfo=getDisplayInfo();
  mTmpRect.set(0,0,displayInfo.logicalWidth,displayInfo.logicalHeight);
  boolean resizing=computeDragResizing();
  mClient.resized(frame,overscanInsets,contentInsets,visibleInsets,stableInsets,outsets,reportDraw,newConfig,inFreeformWorkspace() || !resizing ? frame : mTmpRect);
}
