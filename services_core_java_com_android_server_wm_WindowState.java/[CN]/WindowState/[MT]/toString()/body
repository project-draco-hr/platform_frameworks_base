{
  CharSequence title=mAttrs.getTitle();
  if (title == null || title.length() <= 0) {
    title=mAttrs.packageName;
  }
  if (mStringNameCache == null || mLastTitle != title || mWasExiting != mExiting) {
    mLastTitle=title;
    mWasExiting=mExiting;
    mStringNameCache="Window{" + Integer.toHexString(System.identityHashCode(this)) + " u"+ UserHandle.getUserId(mSession.mUid)+ " "+ mLastTitle+ (mExiting ? " EXITING}" : "}");
  }
  return mStringNameCache;
}
