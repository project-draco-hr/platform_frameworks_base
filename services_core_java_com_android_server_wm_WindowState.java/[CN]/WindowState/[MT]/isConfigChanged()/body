{
  final TaskStack stack=getStack();
  final Configuration overrideConfig=(stack != null) ? stack.mOverrideConfig : Configuration.EMPTY;
  final Configuration serviceConfig=mService.mCurConfiguration;
  boolean configChanged=(mConfiguration != serviceConfig && mConfiguration.diff(serviceConfig) != 0) || (mOverrideConfig != overrideConfig && !mOverrideConfig.equals(overrideConfig));
  if ((mAttrs.privateFlags & PRIVATE_FLAG_KEYGUARD) != 0) {
    mConfigHasChanged|=configChanged;
    configChanged=mConfigHasChanged;
  }
  return configChanged;
}
