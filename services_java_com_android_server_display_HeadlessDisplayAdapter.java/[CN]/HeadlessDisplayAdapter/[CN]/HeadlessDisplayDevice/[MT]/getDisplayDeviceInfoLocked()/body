{
  if (mInfo == null) {
    mInfo=new DisplayDeviceInfo();
    mInfo.name=getContext().getResources().getString(com.android.internal.R.string.display_manager_built_in_display_name);
    mInfo.width=640;
    mInfo.height=480;
    mInfo.refreshRate=60;
    mInfo.densityDpi=DisplayMetrics.DENSITY_DEFAULT;
    mInfo.xDpi=160;
    mInfo.yDpi=160;
    mInfo.flags=DisplayDeviceInfo.FLAG_DEFAULT_DISPLAY | DisplayDeviceInfo.FLAG_SECURE;
    mInfo.touch=DisplayDeviceInfo.TOUCH_NONE;
  }
  return mInfo;
}
