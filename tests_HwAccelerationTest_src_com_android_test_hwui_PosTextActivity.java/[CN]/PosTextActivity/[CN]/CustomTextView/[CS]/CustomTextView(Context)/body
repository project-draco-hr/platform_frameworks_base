{
  super(c);
  mText=c.getResources().getString(R.string.complex_string);
  mPos=new float[mText.length() * 2];
  for (int i=0; i < mPos.length; i+=2) {
    mPos[i]=i * 30.0f;
    mPos[i + 1]=i * 10.0f;
  }
  mLargePaint=new Paint();
  mLargePaint.setAntiAlias(true);
  mLargePaint.setTextSize(36.0f);
}
