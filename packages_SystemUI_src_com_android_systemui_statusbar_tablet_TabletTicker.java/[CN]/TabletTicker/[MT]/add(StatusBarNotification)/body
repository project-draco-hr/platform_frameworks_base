{
  if (false) {
    Slog.d(TAG,"add mCurrentNotification=" + mCurrentNotification + " mQueuePos="+ mQueuePos+ " mQueue="+ Arrays.toString(mQueue));
  }
  mQueue[mQueuePos]=notification;
  if (mQueuePos == 0) {
    sendEmptyMessage(MSG_ADVANCE);
  }
  if (mQueuePos < mQueue.length - 1) {
    mQueuePos++;
  }
}
