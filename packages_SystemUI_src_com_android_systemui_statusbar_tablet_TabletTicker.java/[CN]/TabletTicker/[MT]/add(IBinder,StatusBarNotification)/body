{
  if (false) {
    Log.d(TAG,"add 1 mCurrentNotification=" + mCurrentNotification + " mQueuePos="+ mQueuePos+ " mQueue="+ Arrays.toString(mQueue));
  }
  remove(key,false);
  mKeys[mQueuePos]=key;
  mQueue[mQueuePos]=notification;
  if (mQueuePos == 0 && mCurrentNotification == null) {
    sendEmptyMessage(MSG_ADVANCE);
  }
  if (mQueuePos < QUEUE_LENGTH - 1) {
    mQueuePos++;
  }
}
