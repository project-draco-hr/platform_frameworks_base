{
  int foregroundUser;
  long callingIdentity=Binder.clearCallingIdentity();
  boolean valid=false;
  try {
    foregroundUser=ActivityManager.getCurrentUser();
    valid=r.callerUid == foregroundUser && (r.events & events) != 0;
    if (DBG | DBG_LOC) {
      Slog.d(TAG,"validateEventsAndUserLocked: valid=" + valid + " r.callerUid="+ r.callerUid+ " foregroundUser="+ foregroundUser+ " r.events="+ r.events+ " events="+ events);
    }
  }
  finally {
    Binder.restoreCallingIdentity(callingIdentity);
  }
  return valid;
}
