{
  if (logSSC == null || logSSC.length == 0 || (next == 0 && logSSC[next] == null)) {
    log(prompt + ": logSSC is empty");
  }
 else {
    log(prompt + ": logSSC.length=" + logSSC.length+ " next="+ next);
    int i=next;
    if (logSSC[i] == null) {
      i=0;
    }
    do {
      log(logSSC[i].toString());
      if (++i >= logSSC.length) {
        i=0;
      }
    }
 while (i != next);
  }
}
