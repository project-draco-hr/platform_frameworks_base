{
  final VCardParser vCardParser;
  if (VCardConfig.isV30(vCardType)) {
    vCardParser=new VCardParser_V30(true);
  }
 else {
    vCardParser=new VCardParser_V21();
  }
  try {
    vCardParser.parse(is,this);
  }
  finally {
    if (is != null) {
      try {
        is.close();
      }
 catch (      IOException e) {
      }
    }
  }
}
