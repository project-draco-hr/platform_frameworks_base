{
  StringBuffer buffer=new StringBuffer();
  boolean first=true;
  for (  String propValueElem : propValueList) {
    if (first) {
      first=false;
    }
 else {
      buffer.append(';');
    }
    buffer.append(propValueElem);
  }
  return addNodeWithOrder(propName,buffer.toString(),propValueList,null,null,null,null);
}
