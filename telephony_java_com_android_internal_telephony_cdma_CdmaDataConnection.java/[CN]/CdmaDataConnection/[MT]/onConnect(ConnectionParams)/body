{
  if (DBG)   log("CdmaDataConnection Connecting...");
  createTime=-1;
  lastFailTime=-1;
  lastFailCause=FailCause.NONE;
  Message msg=obtainMessage(EVENT_SETUP_DATA_CONNECTION_DONE,cp);
  msg.obj=cp;
  phone.mCM.setupDataCall(Integer.toString(RILConstants.SETUP_DATA_TECH_CDMA),Integer.toString(RILConstants.DATA_PROFILE_DEFAULT),null,null,null,Integer.toString(RILConstants.SETUP_DATA_AUTH_PAP_CHAP),msg);
}
