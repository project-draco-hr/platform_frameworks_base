{
  if (onCompleted == null) {
    return;
  }
  state=State.ACTIVE;
  createTime=System.currentTimeMillis();
  onConnectCompleted=null;
  onCompleted.arg1=cid;
  if (DBG)   log("Notify data connection success at " + createTime);
  AsyncResult.forMessage(onCompleted);
  onCompleted.sendToTarget();
}
