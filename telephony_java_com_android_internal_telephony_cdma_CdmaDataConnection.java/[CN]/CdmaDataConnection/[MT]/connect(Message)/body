{
  if (DBG)   log("CdmaDataConnection Connecting...");
  state=State.ACTIVATING;
  onConnectCompleted=onCompleted;
  createTime=-1;
  lastFailTime=-1;
  lastFailCause=FailCause.NONE;
  receivedDisconnectReq=false;
  phone.mCM.setupDataCall(Integer.toString(RILConstants.CDMA_PHONE),null,null,null,null,obtainMessage(EVENT_SETUP_DATA_CONNECTION_DONE));
}
