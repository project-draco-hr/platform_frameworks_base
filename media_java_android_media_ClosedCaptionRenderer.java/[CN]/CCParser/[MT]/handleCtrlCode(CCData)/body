{
  int ctrlCode=ccData.getCtrlCode();
switch (ctrlCode) {
case RCL:
    mMode=MODE_POP_ON;
  break;
case BS:
getMemory().bs();
break;
case DER:
getMemory().der();
break;
case RU2:
case RU3:
case RU4:
mRollUpSize=(ctrlCode - 0x23);
if (mMode != MODE_ROLL_UP) {
mDisplay.erase();
mNonDisplay.erase();
}
mMode=MODE_ROLL_UP;
break;
case FON:
Log.i(TAG,"Flash On");
break;
case RDC:
mMode=MODE_PAINT_ON;
break;
case TR:
mMode=MODE_TEXT;
mTextMem.erase();
break;
case RTD:
mMode=MODE_TEXT;
break;
case EDM:
mDisplay.erase();
updateDisplay();
break;
case CR:
if (mMode == MODE_ROLL_UP) {
getMemory().rollUp(mRollUpSize);
}
 else {
getMemory().cr();
}
if (mMode == MODE_ROLL_UP) {
updateDisplay();
}
break;
case ENM:
mNonDisplay.erase();
break;
case EOC:
swapMemory();
mMode=MODE_POP_ON;
updateDisplay();
break;
case INVALID:
default :
return false;
}
return true;
}
