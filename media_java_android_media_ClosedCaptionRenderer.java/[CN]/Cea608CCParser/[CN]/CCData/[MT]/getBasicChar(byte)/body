{
  char c;
switch (data) {
case 0x2A:
    c='\u00E1';
  break;
case 0x5C:
c='\u00E9';
break;
case 0x5E:
c='\u00ED';
break;
case 0x5F:
c='\u00F3';
break;
case 0x60:
c='\u00FA';
break;
case 0x7B:
c='\u00E7';
break;
case 0x7C:
c='\u00F7';
break;
case 0x7D:
c='\u00D1';
break;
case 0x7E:
c='\u00F1';
break;
case 0x7F:
c='\u2588';
break;
default :
c=(char)data;
break;
}
return c;
}
