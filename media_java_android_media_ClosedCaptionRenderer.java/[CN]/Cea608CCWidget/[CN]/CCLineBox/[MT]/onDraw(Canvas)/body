{
  if (mEdgeType == CaptionStyle.EDGE_TYPE_UNSPECIFIED || mEdgeType == CaptionStyle.EDGE_TYPE_NONE || mEdgeType == CaptionStyle.EDGE_TYPE_DROP_SHADOW) {
    super.onDraw(c);
    return;
  }
  if (mEdgeType == CaptionStyle.EDGE_TYPE_OUTLINE) {
    drawEdgeOutline(c);
  }
 else {
    drawEdgeRaisedOrDepressed(c);
  }
}
