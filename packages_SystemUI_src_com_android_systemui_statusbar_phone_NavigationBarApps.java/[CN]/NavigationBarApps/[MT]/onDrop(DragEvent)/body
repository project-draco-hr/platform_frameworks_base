{
  if (DEBUG)   Slog.d(TAG,"onDrop");
  if (mDragView == null) {
    return true;
  }
  AppInfo appInfo=getAppFromDragEvent(event);
  if (appInfo == null) {
    removePlaceholderDragViewIfNeeded();
    return false;
  }
  if (mDragView.getTag() != null) {
    endDrag();
    return true;
  }
  updateApp(mDragView,appInfo);
  endDrag();
  return true;
}
