{
  mMaxCount=maxCacheSize;
  mMaxCountSlack=Math.round(sMaxCountSlackPercent * (mMaxCount / 2));
  mPreloadLowerBound=0;
  mPreloadUpperBound=-1;
  mMetaData=new RemoteViewsMetaData();
