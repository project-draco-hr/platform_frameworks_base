{
  boolean result=false;
switch (msg.what) {
case sUnbindServiceMessageType:
    final AppWidgetManager mgr=AppWidgetManager.getInstance(mContext);
  if (mServiceConnection.isConnected()) {
    mgr.unbindRemoteViewsService(mAppWidgetId,mIntent);
  }
result=true;
break;
default :
break;
}
return result;
}
