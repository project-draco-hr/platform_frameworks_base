{
  ArrayList<Integer> window=new ArrayList<Integer>();
  if ((lower == 0 && upper == 0) || lower < 0 || upper < 0) {
    return window;
  }
  if (lower <= upper) {
    for (int i=lower; i <= upper; i++) {
      window.add(i);
    }
  }
 else {
    for (int i=lower; i < count; i++) {
      window.add(i);
    }
    for (int i=0; i <= upper; i++) {
      window.add(i);
    }
  }
  return window;
}
