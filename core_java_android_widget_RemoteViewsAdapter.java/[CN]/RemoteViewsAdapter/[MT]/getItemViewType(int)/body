{
  requestBindService();
  int typeId=0;
synchronized (mCache) {
    if (mCache.containsMetaDataAt(position)) {
      typeId=mCache.getMetaDataAt(position).typeId;
    }
 else {
      return 0;
    }
  }
  final RemoteViewsMetaData metaData=mCache.getMetaData();
synchronized (metaData) {
    return metaData.getMappedViewType(typeId);
  }
}
