{
  UiSelector ret=new UiSelector();
  ret.mSelectorAttributes=mSelectorAttributes.clone();
  if (hasChildSelector())   ret.mSelectorAttributes.put(SELECTOR_CHILD,new UiSelector(getChildSelector()));
  if (hasParentSelector())   ret.mSelectorAttributes.put(SELECTOR_PARENT,new UiSelector(getParentSelector()));
  if (hasPatternSelector())   ret.mSelectorAttributes.put(SELECTOR_PATTERN,new UiSelector(getPatternSelector()));
  return ret;
}
