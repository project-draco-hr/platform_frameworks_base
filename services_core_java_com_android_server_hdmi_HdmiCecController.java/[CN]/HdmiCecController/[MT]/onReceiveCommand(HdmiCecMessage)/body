{
  assertRunOnServiceThread();
  if (isAcceptableAddress(message.getDestination()) && mService.handleCecCommand(message)) {
    return;
  }
  if (message.getDestination() != Constants.ADDR_BROADCAST) {
    int sourceAddress=message.getDestination();
    HdmiCecMessage cecMessage=HdmiCecMessageBuilder.buildFeatureAbortCommand(sourceAddress,message.getSource(),message.getOpcode(),Constants.ABORT_REFUSED);
    sendCommand(cecMessage);
  }
}
