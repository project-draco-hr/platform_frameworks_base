{
  assertRunOnServiceThread();
  if (isAcceptableAddress(message.getDestination()) && mService.handleCecCommand(message)) {
    return;
  }
  int sourceAddress=message.getDestination() != HdmiCec.ADDR_BROADCAST ? message.getDestination() : 0;
  HdmiCecMessage cecMessage=HdmiCecMessageBuilder.buildFeatureAbortCommand(sourceAddress,message.getSource(),message.getOpcode(),HdmiCecMessageBuilder.ABORT_REFUSED);
  sendCommand(cecMessage,null);
}
