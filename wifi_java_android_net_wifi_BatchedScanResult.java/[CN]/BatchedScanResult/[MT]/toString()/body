{
  StringBuffer sb=new StringBuffer();
  sb.append("BatchedScanResult: ").append("truncated: ").append(String.valueOf(truncated)).append("scanResults: [");
  for (  ScanResult s : scanResults) {
    sb.append(" <").append(s.toString()).append("> ");
  }
  sb.append(" ]");
  return sb.toString();
}
