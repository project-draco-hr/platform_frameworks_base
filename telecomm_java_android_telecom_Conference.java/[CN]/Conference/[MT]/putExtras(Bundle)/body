{
  if (extras == null) {
    return;
  }
  Bundle listenersBundle;
synchronized (mExtrasLock) {
    if (mExtras == null) {
      mExtras=new Bundle();
    }
    mExtras.putAll(extras);
    listenersBundle=new Bundle(mExtras);
  }
  for (  Listener l : mListeners) {
    l.onExtrasChanged(this,new Bundle(listenersBundle));
  }
}
