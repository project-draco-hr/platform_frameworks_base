{
switch (state) {
case State.NEW:
    return "NEW";
case State.RINGING:
  return "RINGING";
case State.DIALING:
return "DIALING";
case State.ACTIVE:
return "ACTIVE";
case State.HOLDING:
return "HOLDING";
case State.DISCONNECTED:
return "DISCONNECTED";
case State.FAILED:
return "FAILED";
case State.CANCELED:
return "CANCELED";
default :
Log.wtf(Connection.class,"Unknown state %d",state);
return "UNKNOWN";
}
}
