{
  new Handler().postDelayed(new Runnable(){
    public void run(){
      final ArrayList<MediaBrowserItem> list=new ArrayList();
      for (int i=0; i < 10; i++) {
        list.add(new MediaBrowserItem.Builder(Uri.withAppendedPath(BASE_URI,Integer.toString(i)),MediaBrowserItem.FLAG_BROWSABLE,"Title " + i).setSummary("Summary " + i).build());
      }
      result.sendResult(list);
    }
  }
,2000);
  result.detach();
}
