{
  if (mState != STATE_WAITING_TIMEOUT) {
    return false;
  }
  int opcode=cmd.getOpcode();
  if (opcode == Constants.MESSAGE_FEATURE_ABORT) {
    int originalOpcode=cmd.getParams()[0] & 0xFF;
    if (originalOpcode == Constants.MESSAGE_REPORT_ARC_INITIATED) {
      setArcStatus(false);
      finish();
      return true;
    }
  }
  return false;
}
