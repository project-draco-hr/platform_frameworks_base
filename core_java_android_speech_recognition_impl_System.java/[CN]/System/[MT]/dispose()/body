{
synchronized (System.class) {
    if (nativeObject == 0)     return;
    shutdownRequested=true;
  }
  for (  Object o : registerMap.keySet()) {
    WeakReference weakReference=registerMap.get(o);
    Runnable r=(Runnable)weakReference.get();
    if (r != null)     r.run();
  }
  registerMap.clear();
  disposeProxy();
synchronized (System.class) {
    nativeObject=0;
    instance=null;
  }
}
