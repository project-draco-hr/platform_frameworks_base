{
  StringBuilder sb=new StringBuilder();
  if (areaList == null) {
    return null;
  }
  int i=0;
  for (  Camera.Area area : areaList) {
    if (area == null) {
      sb.append("null");
    }
 else {
      sb.append(stringFromArea(area));
    }
    if (i != areaList.size() - 1) {
      sb.append(", ");
    }
    i++;
  }
  return sb.toString();
}
