{
  if (Intent.ACTION_DOCK_EVENT.equals(intent.getAction())) {
    Bundle extras=intent.getExtras();
    int state=extras.getInt(Intent.EXTRA_DOCK_STATE,Intent.EXTRA_DOCK_STATE_UNDOCKED);
    if (state == Intent.EXTRA_DOCK_STATE_DESK || state == Intent.EXTRA_DOCK_STATE_LE_DESK || state == Intent.EXTRA_DOCK_STATE_HE_DESK) {
      launchDream(context);
    }
  }
}
