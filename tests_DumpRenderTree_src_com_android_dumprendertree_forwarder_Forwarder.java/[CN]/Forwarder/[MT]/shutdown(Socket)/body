{
  try {
    socket.shutdownInput();
  }
 catch (  IOException e) {
    Log.v(LOGTAG,"Socket#shutdownInput",e);
  }
  try {
    socket.shutdownOutput();
  }
 catch (  IOException e) {
    Log.v(LOGTAG,"Socket#shutdownOutput",e);
  }
  try {
    socket.close();
  }
 catch (  IOException e) {
    Log.v(LOGTAG,"Socket#close",e);
  }
}
