{
  mContext.enforceCallingOrSelfPermission(android.Manifest.permission.BACKUP,"getDataManagementIntent");
synchronized (mTransports) {
    final IBackupTransport transport=mTransports.get(transportName);
    if (transport != null) {
      try {
        final Intent intent=transport.dataManagementIntent();
        if (MORE_DEBUG)         Slog.d(TAG,"getDataManagementIntent() returning intent " + intent);
        return intent;
      }
 catch (      RemoteException e) {
      }
    }
  }
  return null;
}
