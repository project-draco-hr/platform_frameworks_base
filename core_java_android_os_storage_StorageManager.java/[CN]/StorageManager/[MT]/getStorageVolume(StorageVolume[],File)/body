{
  try {
    file=file.getCanonicalFile();
  }
 catch (  IOException ignored) {
    return null;
  }
  for (  StorageVolume volume : volumes) {
    File volumeFile=volume.getPathFile();
    try {
      volumeFile=volumeFile.getCanonicalFile();
    }
 catch (    IOException ignored) {
      continue;
    }
    if (FileUtils.contains(volumeFile,file)) {
      return volume;
    }
  }
  return null;
}
