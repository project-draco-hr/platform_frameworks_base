{
  if (context == null) {
    throw new NullPointerException("context == null");
  }
  mContext=context;
  ensureNotOnMainThread();
  mAccountManager=AccountManager.get(mContext);
  mIsBound=mContext.bindService(new Intent(IKeyChainService.class.getName()),mServiceConnection,Context.BIND_AUTO_CREATE);
  if (!mIsBound) {
    throw new AssertionError();
  }
synchronized (mServiceLock) {
    if (mService == null) {
      mServiceLock.wait();
    }
  }
  mGuard.open("close");
}
