{
  for (  ClientInfo c : mClientInfoList.values()) {
    Message msg=Message.obtain();
    msg.what=WifiP2pManager.PING;
    msg.arg1=0;
    msg.arg2=0;
    msg.obj=null;
    try {
      c.mMessenger.send(msg);
    }
 catch (    RemoteException e) {
      if (DBG)       logd("detect dead channel");
      clearClientInfo(c.mMessenger);
    }
  }
}
