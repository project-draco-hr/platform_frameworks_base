{
  super(stateChangedListener,context);
  mDeadlineExpiredAlarmIntent=PendingIntent.getBroadcast(mContext,0,new Intent(ACTION_JOB_EXPIRED),0);
  mNextDelayExpiredAlarmIntent=PendingIntent.getBroadcast(mContext,0,new Intent(ACTION_JOB_DELAY_EXPIRED),0);
  mNextJobExpiredElapsedMillis=Long.MAX_VALUE;
  mNextDelayExpiredElapsedMillis=Long.MAX_VALUE;
  IntentFilter intentFilter=new IntentFilter(ACTION_JOB_EXPIRED);
  intentFilter.addAction(ACTION_JOB_DELAY_EXPIRED);
  mContext.registerReceiver(mAlarmExpiredReceiver,intentFilter);
}
