{
  StringBuilder s=new StringBuilder("QuadF(");
  s.append(p1.x).append(",").append(p1.y);
  s.append(" - ");
  s.append(p2.x).append(",").append(p2.y);
  s.append(" - ");
  s.append(p3.x).append(",").append(p3.y);
  s.append(" - ");
  s.append(p4.x).append(",").append(p4.y);
  s.append(")");
  return s.toString();
}
