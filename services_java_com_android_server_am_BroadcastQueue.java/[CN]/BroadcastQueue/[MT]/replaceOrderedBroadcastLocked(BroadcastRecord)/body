{
  for (int i=mOrderedBroadcasts.size() - 1; i > 0; i--) {
    if (r.intent.filterEquals(mOrderedBroadcasts.get(i).intent)) {
      if (DEBUG_BROADCAST)       Slog.v(TAG,"***** DROPPING ORDERED [" + mQueueName + "]: "+ r.intent);
      mOrderedBroadcasts.set(i,r);
      return true;
    }
  }
  return false;
}
