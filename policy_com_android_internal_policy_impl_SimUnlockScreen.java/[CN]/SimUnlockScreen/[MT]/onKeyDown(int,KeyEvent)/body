{
  if (keyCode == KeyEvent.KEYCODE_BACK) {
    mCallback.goToLockScreen();
    return true;
  }
  final char match=event.getMatch(DIGITS);
  if (match != 0) {
    reportDigit(match - '0');
    return true;
  }
  if (keyCode == KeyEvent.KEYCODE_DEL) {
    if (mEnteredDigits > 0) {
      mPinText.onKeyDown(keyCode,event);
      mEnteredDigits--;
    }
    return true;
  }
  if (keyCode == KeyEvent.KEYCODE_ENTER) {
    checkPin();
    return true;
  }
  return false;
}
