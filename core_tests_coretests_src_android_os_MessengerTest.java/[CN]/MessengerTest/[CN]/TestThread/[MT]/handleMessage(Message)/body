{
  if (msg.arg1 != 100) {
    failure(new RuntimeException("Message.arg1 is not 100: " + msg.arg1));
    return;
  }
  if (msg.arg2 != 1000) {
    failure(new RuntimeException("Message.arg2 is not 1000: " + msg.arg2));
    return;
  }
  if (!mTestMessenger.equals(msg.replyTo)) {
    failure(new RuntimeException("Message.replyTo is not me: " + msg.replyTo));
    return;
  }
  success();
}
