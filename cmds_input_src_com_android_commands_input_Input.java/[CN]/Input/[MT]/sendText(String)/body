{
  StringBuffer buff=new StringBuffer(text);
  boolean escapeFlag=false;
  for (int i=0; i < buff.length(); i++) {
    if (escapeFlag) {
      escapeFlag=false;
      if (buff.charAt(i) == 's') {
        buff.setCharAt(i,' ');
        buff.deleteCharAt(--i);
      }
    }
    if (buff.charAt(i) == '%') {
      escapeFlag=true;
    }
  }
  char[] chars=buff.toString().toCharArray();
  KeyCharacterMap mKeyCharacterMap=KeyCharacterMap.load(KeyCharacterMap.VIRTUAL_KEYBOARD);
  KeyEvent[] events=mKeyCharacterMap.getEvents(chars);
  for (int i=0; i < events.length; i++) {
    KeyEvent event=events[i];
    Log.i("SendKeyEvent",Integer.toString(event.getKeyCode()));
    try {
      (IWindowManager.Stub.asInterface(ServiceManager.getService("window"))).injectKeyEvent(event,true);
    }
 catch (    RemoteException e) {
      Log.i("Input","DeadOjbectException");
    }
  }
}
