{
synchronized (mNotifications) {
    Slog.d(TAG,"addNotification notification=" + notification);
    IBinder key=new Binder();
    mNotifications.put(key,notification);
    if (mBar != null) {
      try {
        mBar.addNotification(key,notification);
      }
 catch (      RemoteException ex) {
      }
    }
    return key;
  }
}
