{
  int slot;
  if (data != null && n == null) {
    slot=getRightIconIndex(data.slot);
    if (slot < 0) {
      throw new SecurityException("invalid status bar icon slot: " + (data.slot != null ? "'" + data.slot + "'" : "null"));
    }
  }
 else {
    slot=-1;
  }
  IBinder key=new Binder();
  addPendingOp(OP_ADD_ICON,key,data,n,-1);
  return key;
}
