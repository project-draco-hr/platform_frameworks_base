{
  enforceStatusBar();
synchronized (mIcons) {
    int index=mIcons.getSlotIndex(slot);
    if (index < 0) {
      throw new SecurityException("invalid status bar icon slot: " + slot);
    }
    mIcons.removeIcon(index);
    if (mBar != null) {
      try {
        mBar.removeIcon(index);
      }
 catch (      RemoteException ex) {
      }
    }
  }
}
