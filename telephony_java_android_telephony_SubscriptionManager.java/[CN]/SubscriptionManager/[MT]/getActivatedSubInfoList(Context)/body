{
  if (VDBG)   logd("[getActivatedSubInfoList]+");
  List<SubInfoRecord> result=null;
  try {
    ISub iSub=ISub.Stub.asInterface(ServiceManager.getService("isub"));
    if (iSub != null) {
      result=iSub.getActivatedSubInfoList();
    }
  }
 catch (  RemoteException ex) {
  }
  return result;
}
