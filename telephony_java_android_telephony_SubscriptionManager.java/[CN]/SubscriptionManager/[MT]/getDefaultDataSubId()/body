{
  long subId=INVALID_SUB_ID;
  try {
    ISub iSub=ISub.Stub.asInterface(ServiceManager.getService("isub"));
    if (iSub != null) {
      subId=iSub.getDefaultDataSubId();
    }
  }
 catch (  RemoteException ex) {
  }
  if (VDBG)   logd("getDefaultDataSubId, sub id = " + subId);
  return subId;
}
