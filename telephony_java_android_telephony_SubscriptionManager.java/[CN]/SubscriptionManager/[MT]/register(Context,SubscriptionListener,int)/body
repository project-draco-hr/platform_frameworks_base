{
  String pkgForDebug=context != null ? context.getPackageName() : "<unknown>";
  if (DBG) {
    logd("SubscriptionManager listen pkgForDebug=" + pkgForDebug + " events=0x"+ Integer.toHexString(events)+ " listener="+ listener);
  }
  try {
    ITelephonyRegistry tr=ITelephonyRegistry.Stub.asInterface(ServiceManager.getService("telephony.registry"));
    if (tr != null) {
      tr.registerSubscriptionListener(pkgForDebug,listener.callback,events);
    }
  }
 catch (  RemoteException ex) {
  }
}
