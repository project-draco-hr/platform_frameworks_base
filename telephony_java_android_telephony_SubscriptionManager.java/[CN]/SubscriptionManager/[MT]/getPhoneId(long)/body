{
  if (VDBG)   logd("[getPhoneId]+ subId=" + subId);
  int result=0;
  try {
    ISub iSub=ISub.Stub.asInterface(ServiceManager.getService("isub"));
    if (iSub != null) {
      result=iSub.getPhoneId(subId);
    }
  }
 catch (  RemoteException ex) {
  }
  if (VDBG)   logd("[getPhoneId]- phonId=" + result);
  return result;
}
