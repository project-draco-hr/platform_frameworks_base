{
  if (!isValidSubId(subId)) {
    logd("[getSlotId]- fail");
  }
  int result=INVALID_SIM_SLOT_INDEX;
  try {
    ISub iSub=ISub.Stub.asInterface(ServiceManager.getService("isub"));
    if (iSub != null) {
      result=iSub.getSlotId(subId);
    }
  }
 catch (  RemoteException ex) {
  }
  return result;
}
