{
  db.execSQL("CREATE TABLE " + TABLE_RECENT_OPEN + " ("+ COL_PATH+ " TEXT,"+ COL_TIMESTAMP+ " INTEGER,"+ ")");
  db.execSQL("CREATE TABLE " + TABLE_RECENT_CREATE + " ("+ COL_PATH+ " TEXT,"+ COL_TIMESTAMP+ " INTEGER,"+ ")");
  db.execSQL("CREATE TABLE " + TABLE_RESUME + " ("+ COL_PACKAGE_NAME+ " TEXT PRIMARY KEY ON CONFLICT REPLACE,"+ COL_PATH+ " TEXT,"+ COL_TIMESTAMP+ " INTEGER,"+ ")");
}
