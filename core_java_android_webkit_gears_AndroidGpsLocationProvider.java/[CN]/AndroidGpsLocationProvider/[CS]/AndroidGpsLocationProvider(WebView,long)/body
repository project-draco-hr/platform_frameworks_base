{
  nativeObject=object;
  locationManager=(LocationManager)webview.getContext().getSystemService(Context.LOCATION_SERVICE);
  if (locationManager == null) {
    Log.e(TAG,"AndroidGpsLocationProvider: could not get location manager.");
    throw new NullPointerException("AndroidGpsLocationProvider: locationManager is null.");
  }
  try {
    locationManager.requestLocationUpdates(LocationManager.GPS_PROVIDER,0,0,this);
  }
 catch (  IllegalArgumentException ex) {
    Log.e(TAG,"AndroidLocationGpsProvider: could not register for updates: " + ex);
    throw ex;
  }
catch (  SecurityException ex) {
    Log.e(TAG,"AndroidGpsLocationProvider: not allowed to register for update: " + ex);
    throw ex;
  }
}
