{
  super.onDetach();
  if (mLauncherListener != null) {
    mLauncherListener.onDetached(this);
  }
  if (mGroupAdapter != null) {
    mRouter.removeCallback(mGroupAdapter);
    mGroupAdapter=null;
  }
  if (mAdapter != null) {
    mRouter.removeCallback(mAdapter);
    mAdapter=null;
  }
  mInflater=null;
  mRouter=null;
}
