{
  if (types != getRouteTypes()) {
    Bundle args=getArguments();
    if (args == null) {
      args=new Bundle();
    }
    args.putInt(ARGUMENT_ROUTE_TYPES,types);
    setArguments(args);
    MediaRouteChooserDialog dialog=(MediaRouteChooserDialog)getDialog();
    if (dialog != null) {
      dialog.setRouteTypes(types);
    }
  }
}
