{
  if (context == null) {
    return;
  }
  PackageManager pm=context.getPackageManager();
  if (pm == null) {
    return;
  }
  ComponentName cName=new ComponentName("com.android.stk","com.android.stk.StkActivity");
  try {
    pm.setComponentEnabledSetting(cName,state,PackageManager.DONT_KILL_APP);
  }
 catch (  Exception e) {
    Log.w("StkAppInstaller","Could not change STK app state");
  }
}
