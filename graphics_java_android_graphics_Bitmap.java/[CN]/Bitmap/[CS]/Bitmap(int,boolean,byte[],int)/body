{
  if (nativeBitmap == 0) {
    throw new RuntimeException("internal error: native bitmap is 0");
  }
  mNativeBitmap=nativeBitmap;
  mIsMutable=isMutable;
  mNinePatchChunk=ninePatchChunk;
  if (density >= 0) {
    mDensity=density;
  }
  if (Finalizers.getQueue() != null) {
    new BitmapFinalizer(this);
  }
}
