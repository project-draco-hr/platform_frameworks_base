{
  setName("GLThread " + getId());
  if (LOG_THREADS) {
    Log.i("GLThread","starting tid=" + getId());
  }
  try {
    guardedRun();
  }
 catch (  InterruptedException e) {
  }
 finally {
synchronized (this) {
      if (LOG_THREADS) {
        Log.i("GLThread","exiting tid=" + getId());
      }
      mDone=true;
      notifyAll();
    }
  }
}
