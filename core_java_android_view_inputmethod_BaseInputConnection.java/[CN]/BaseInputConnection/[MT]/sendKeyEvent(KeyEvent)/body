{
synchronized (mIMM.mH) {
    Handler h=mTargetView != null ? mTargetView.getHandler() : null;
    if (h == null) {
      if (mIMM.mServedView != null) {
        h=mIMM.mServedView.getHandler();
      }
    }
    if (h != null) {
      h.sendMessage(h.obtainMessage(ViewRootImpl.DISPATCH_KEY_FROM_IME,event));
    }
  }
  return false;
}
