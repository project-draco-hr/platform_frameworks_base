{
  mContext=Preconditions.checkNotNull(context,"context");
  mName=mContext.getResources().getIdentifier(Preconditions.checkStringNotEmpty(name,"name"),null,mContext.getPackageName());
  mPackageName=Preconditions.checkStringNotEmpty(packageName);
  mMDNSNames=new HashSet<>(Preconditions.checkCollectionNotEmpty(Preconditions.checkCollectionElementsNotNull(mDNSNames,"mDNSNames"),"mDNSNames"));
  mResolveQueue=NsdResolveQueue.getInstance();
  mPrinters=new HashSet<>();
}
