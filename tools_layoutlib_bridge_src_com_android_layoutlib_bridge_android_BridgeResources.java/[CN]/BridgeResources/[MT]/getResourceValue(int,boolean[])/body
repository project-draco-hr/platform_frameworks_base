{
  Pair<ResourceType,String> resourceInfo=Bridge.resolveResourceId(id);
  if (resourceInfo != null) {
    platformResFlag_out[0]=true;
    return mContext.getRenderResources().getFrameworkResource(resourceInfo.getFirst(),resourceInfo.getSecond());
  }
  if (mProjectCallback != null) {
    resourceInfo=mProjectCallback.resolveResourceId(id);
    if (resourceInfo != null) {
      platformResFlag_out[0]=false;
      return mContext.getRenderResources().getProjectResource(resourceInfo.getFirst(),resourceInfo.getSecond());
    }
  }
  return null;
}
