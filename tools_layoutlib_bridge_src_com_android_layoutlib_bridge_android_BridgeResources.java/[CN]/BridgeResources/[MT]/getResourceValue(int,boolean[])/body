{
  String[] resourceInfo=Bridge.resolveResourceValue(id);
  if (resourceInfo != null) {
    ResourceType resType=ResourceType.getEnum(resourceInfo[1]);
    if (resType == null) {
      return null;
    }
    platformResFlag_out[0]=true;
    return mContext.getRenderResources().getFrameworkResource(resType,resourceInfo[0]);
  }
  if (mProjectCallback != null) {
    resourceInfo=mProjectCallback.resolveResourceValue(id);
    if (resourceInfo != null) {
      ResourceType resType=ResourceType.getEnum(resourceInfo[1]);
      if (resType == null) {
        return null;
      }
      platformResFlag_out[0]=false;
      return mContext.getRenderResources().getProjectResource(resType,resourceInfo[0]);
    }
  }
  return null;
}
