{
  IResourceValue value=getResourceValue(id,mPlatformResourceFlag);
  if (value != null) {
    String v=value.getValue();
    if (v != null) {
      if (ResourceHelper.stringToFloat(v,outValue)) {
        return;
      }
      outValue.type=TypedValue.TYPE_STRING;
      outValue.string=v;
      return;
    }
  }
  throwException(id);
}
