{
  IResourceValue value=getResourceValue(id,mPlatformResourceFlag);
  if (value != null && value.getValue() != null) {
    String v=value.getValue();
    int radix=10;
    if (v.startsWith("0x")) {
      v=v.substring(2);
      radix=16;
    }
    try {
      return Integer.parseInt(v,radix);
    }
 catch (    NumberFormatException e) {
    }
  }
  throwException(id);
  return 0;
}
