{
  IResourceValue value=getResourceValue(id,mPlatformResourceFlag);
  if (value != null) {
    try {
      int color=ResourceHelper.getColor(value.getValue());
      return ColorStateList.valueOf(color);
    }
 catch (    NumberFormatException e) {
      return null;
    }
  }
  throwException(id);
  return null;
}
