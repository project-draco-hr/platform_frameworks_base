{
  ResourceValue resValue=getResourceValue(id,mPlatformResourceFlag);
  if (resValue != null) {
    String value=resValue.getValue();
    if (value != null) {
      File f=new File(value);
      if (f.isFile()) {
        try {
          KXmlParser parser=new KXmlParser();
          parser.setFeature(XmlPullParser.FEATURE_PROCESS_NAMESPACES,true);
          parser.setInput(new FileReader(f));
          return ColorStateList.createFromXml(this,new BridgeXmlBlockParser(parser,mContext,resValue.isFramework()));
        }
 catch (        Exception e) {
          Bridge.getLog().error(null,"Failed to parse file " + value,e);
          return null;
        }
      }
 else {
        try {
          int color=ResourceHelper.getColor(value);
          return ColorStateList.valueOf(color);
        }
 catch (        NumberFormatException e) {
          return null;
        }
      }
    }
  }
  throwException(id);
  return null;
}
