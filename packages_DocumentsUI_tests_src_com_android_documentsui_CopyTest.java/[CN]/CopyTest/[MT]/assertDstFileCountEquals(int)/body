{
  final Uri queryUri=DocumentsContract.buildChildDocumentsUri(AUTHORITY,mRoots.get(1).documentId);
  Cursor c=null;
  int count=0;
  try {
    c=mClient.query(queryUri,null,null,null,null);
    count=c.getCount();
  }
  finally {
    IoUtils.closeQuietly(c);
  }
  assertEquals("Incorrect file count after copy",expected,count);
}
