{
  pw.println("BatteryController state:");
  pw.print("  mLevel=");
  pw.println(mLevel);
  pw.print("  mPluggedIn=");
  pw.println(mPluggedIn);
  pw.print("  mCharging=");
  pw.println(mCharging);
  pw.print("  mCharged=");
  pw.println(mCharged);
  pw.print("  mPowerSave=");
  pw.println(mPowerSave);
}
