{
  if (mState == STATE_NONE || mState != state) {
    return;
  }
  if (++mTimeoutRetry < HdmiConfig.TIMEOUT_RETRY) {
    sendQueryCommand();
    return;
  }
  mTimeoutRetry=0;
  Slog.v(TAG,"Timeout[State=" + mState + ", Processed="+ mProcessedDeviceCount);
  removeDevice(mProcessedDeviceCount);
  checkAndProceedStage();
}
