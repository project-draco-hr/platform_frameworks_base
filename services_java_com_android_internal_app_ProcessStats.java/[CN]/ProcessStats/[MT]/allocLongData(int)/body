{
  int whichLongs=mLongs.size() - 1;
  long[] longs=mLongs.get(whichLongs);
  if (mNextLong + num > longs.length) {
    longs=new long[LONGS_SIZE];
    mLongs.add(longs);
    whichLongs++;
    mNextLong=0;
  }
  int off=(whichLongs << OFFSET_ARRAY_SHIFT) | (mNextLong << OFFSET_INDEX_SHIFT);
  mNextLong+=num;
  return off;
}
