{
  final int size=in.readInt();
  if (size < 0) {
    Slog.w(TAG,"Ignoring existing stats; bad " + what + " table size: "+ size);
    return BAD_TABLE;
  }
  if (size == 0) {
    return null;
  }
  final int[] table=new int[size];
  for (int i=0; i < size; i++) {
    table[i]=in.readInt();
    if (DEBUG)     Slog.i(TAG,"Reading in " + name + " table #"+ i+ ": "+ ProcessStats.printLongOffset(table[i]));
    if (!validateLongOffset(table[i])) {
      Slog.w(TAG,"Ignoring existing stats; bad " + what + " table entry: "+ ProcessStats.printLongOffset(table[i]));
      return null;
    }
  }
  return table;
}
