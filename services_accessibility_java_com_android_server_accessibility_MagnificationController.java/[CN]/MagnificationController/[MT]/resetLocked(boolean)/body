{
  if (!mRegistered) {
    return false;
  }
  final MagnificationSpec spec=mCurrentMagnificationSpec;
  final boolean changed=!spec.isNop();
  if (changed) {
    spec.clear();
    onMagnificationChangedLocked();
  }
  mIdOfLastServiceToMagnify=INVALID_ID;
  mSpecAnimationBridge.updateSentSpec(spec,animate);
  return changed;
}
