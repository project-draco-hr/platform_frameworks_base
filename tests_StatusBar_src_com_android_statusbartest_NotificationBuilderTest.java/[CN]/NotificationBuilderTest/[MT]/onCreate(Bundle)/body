{
  super.onCreate(icicle);
  mNM=(NotificationManager)getSystemService(NOTIFICATION_SERVICE);
  setContentView(R.layout.notification_builder_test);
  if (icicle == null) {
    setDefaults();
  }
  for (  int id : new int[]{R.id.clear_1,R.id.clear_2,R.id.clear_3,R.id.clear_4,R.id.clear_5,R.id.clear_6,R.id.clear_7,R.id.clear_8,R.id.clear_9,R.id.clear_10,R.id.notify_1,R.id.notify_2,R.id.notify_3,R.id.notify_4,R.id.notify_5,R.id.notify_6,R.id.notify_7,R.id.notify_8,R.id.notify_9,R.id.notify_10,R.id.ten,R.id.clear_all}) {
    findViewById(id).setOnClickListener(mClickListener);
  }
}
