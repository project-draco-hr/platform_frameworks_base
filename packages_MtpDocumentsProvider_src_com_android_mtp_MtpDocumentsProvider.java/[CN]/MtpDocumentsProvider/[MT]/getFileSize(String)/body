{
  final Cursor cursor=mDatabase.queryDocument(documentId,MtpDatabase.strings(Document.COLUMN_SIZE,Document.COLUMN_DISPLAY_NAME));
  try {
    if (cursor.moveToNext()) {
      return cursor.getLong(0);
    }
 else {
      throw new FileNotFoundException();
    }
  }
  finally {
    cursor.close();
  }
}
