{
  try {
    final Identifier parentId=Identifier.createFromDocumentId(parentDocumentId);
    final int objectHandle=mMtpManager.createDocument(parentId.mDeviceId,parentId.mStorageId,parentId.mObjectHandle,mimeType,displayName);
    final String documentId=new Identifier(parentId.mDeviceId,parentId.mStorageId,objectHandle).toDocumentId();
    notifyChildDocumentsChange(parentDocumentId);
    return documentId;
  }
 catch (  IOException error) {
    Log.e(TAG,error.getMessage());
    throw new FileNotFoundException(error.getMessage());
  }
}
