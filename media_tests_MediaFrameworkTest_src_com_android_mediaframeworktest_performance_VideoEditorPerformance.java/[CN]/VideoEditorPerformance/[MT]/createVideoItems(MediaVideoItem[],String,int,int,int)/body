{
  for (int i=0; i < NUM_OF_ITERATIONS; i++) {
    try {
      mediaVideoItem[i]=new MediaVideoItem(mVideoEditor,"m" + i,videoItemFileName,renderingMode);
      mediaVideoItem[i].setExtractBoundaries(startTime,endTime);
    }
 catch (    Exception e1) {
      assertTrue("Can not create an object of Video Item with file name = " + videoItemFileName + "------ID:m"+ i+ "       Issue = "+ e1.toString(),false);
    }
  }
}
