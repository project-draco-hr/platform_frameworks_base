{
  ensureOpen();
  if (inputStreamOpened)   throw new IOException("no more input streams available");
  if (isGet) {
    validateConnection();
    isValidateConnected=true;
  }
 else {
    if (privateInput == null) {
      privateInput=new PrivateInputStream(this);
    }
  }
  inputStreamOpened=true;
  return privateInput;
}
