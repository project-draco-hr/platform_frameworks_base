{
  mVerifier.initForExportTest(vcardType);
  ContactEntry entry=mVerifier.addInputEntry();
  entry.addContentValues(StructuredName.CONTENT_ITEM_TYPE).put(StructuredName.IS_PRIMARY,1);
  entry.addContentValues(StructuredName.CONTENT_ITEM_TYPE).put(StructuredName.FAMILY_NAME,"family1").put(StructuredName.DISPLAY_NAME,"display");
  entry.addContentValues(StructuredName.CONTENT_ITEM_TYPE).put(StructuredName.IS_PRIMARY,1).put(StructuredName.FAMILY_NAME,"family2").put(StructuredName.DISPLAY_NAME,"display");
  entry.addContentValues(StructuredName.CONTENT_ITEM_TYPE).put(StructuredName.IS_PRIMARY,1).put(StructuredName.FAMILY_NAME,"family3").put(StructuredName.DISPLAY_NAME,"display");
  entry.addContentValues(StructuredName.CONTENT_ITEM_TYPE).put(StructuredName.FAMILY_NAME,"family4").put(StructuredName.DISPLAY_NAME,"display");
  mVerifier.addPropertyNodesVerifierElem().addExpectedNode("N",Arrays.asList("family2","","","","")).addExpectedNode("FN","display");
}
