{
  Assert.assertTrue(isMediaMounted());
  IMountService ms=getMs();
  try {
    int rc=ms.createSecureContainer("com.android.unittests.AsecTests.testCreateZeroSizeContainer",0,"fat","none",android.os.Process.myUid());
    Assert.assertEquals(StorageResultCode.OperationFailedInternalError,rc);
  }
 catch (  Exception e) {
    failStr(e);
  }
}
