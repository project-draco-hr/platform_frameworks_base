{
  if (!mApply) {
    return;
  }
  NotificationHeaderView header=row.getNotificationHeader();
  if (header == null) {
    mApply=false;
    return;
  }
  Object childData=mExtractor == null ? null : mExtractor.extractData(row);
  mApply=mComparator.compare(mParentView,header.findViewById(mId),mParentData,childData);
}
