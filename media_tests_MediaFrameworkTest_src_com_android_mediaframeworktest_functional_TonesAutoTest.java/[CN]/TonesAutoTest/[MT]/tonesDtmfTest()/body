{
  Log.v(TAG,"DTMF tones test");
  ToneGenerator toneGen;
  int type;
  boolean result=true;
  toneGen=new ToneGenerator(AudioManager.STREAM_MUSIC,100);
  for (type=ToneGenerator.TONE_DTMF_0; type <= ToneGenerator.TONE_DTMF_D; type++) {
    if (toneGen.startTone(type)) {
      Thread.sleep(200);
      toneGen.stopTone();
      Thread.sleep(100);
    }
 else {
      result=false;
      break;
    }
  }
  toneGen.release();
  return result;
}
