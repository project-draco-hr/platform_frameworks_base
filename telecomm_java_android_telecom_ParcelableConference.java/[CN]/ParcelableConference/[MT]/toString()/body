{
  return (new StringBuffer()).append("account: ").append(mPhoneAccount).append(", state: ").append(Connection.stateToString(mState)).append(", capabilities: ").append(Connection.capabilitiesToString(mConnectionCapabilities)).append(", properties: ").append(Connection.propertiesToString(mConnectionProperties)).append(", connectTime: ").append(mConnectTimeMillis).append(", children: ").append(mConnectionIds).append(", VideoState: ").append(mVideoState).append(", VideoProvider: ").append(mVideoProvider).toString();
