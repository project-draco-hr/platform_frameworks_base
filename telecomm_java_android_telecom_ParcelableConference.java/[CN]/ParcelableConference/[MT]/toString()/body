{
  return (new StringBuffer()).append("account: ").append(mPhoneAccount).append(", state: ").append(Connection.stateToString(mState)).append(", capabilities: ").append(Connection.capabilitiesToString(mConnectionCapabilities)).append(", connectTime: ").append(mConnectTimeMillis).append(", children: ").append(mConnectionIds).toString();
}
