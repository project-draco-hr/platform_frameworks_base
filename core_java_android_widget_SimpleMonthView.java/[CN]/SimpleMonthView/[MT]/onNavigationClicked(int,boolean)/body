{
  final int direction;
  if (id == ITEM_ID_NEXT) {
    direction=1;
  }
 else   if (id == ITEM_ID_PREV) {
    direction=-1;
  }
 else {
    return false;
  }
  if (mOnDayClickListener != null) {
    mOnDayClickListener.onNavigationClick(this,direction,animate);
  }
  mTouchHelper.sendEventForVirtualView(id,AccessibilityEvent.TYPE_VIEW_CLICKED);
  return true;
}
