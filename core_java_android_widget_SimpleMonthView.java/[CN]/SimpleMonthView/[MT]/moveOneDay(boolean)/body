{
  ensureFocusedDay();
  boolean focusChanged=false;
  if (positive) {
    if (!isLastDayOfWeek(mHighlightedDay) && mHighlightedDay < mDaysInMonth) {
      mHighlightedDay++;
      focusChanged=true;
    }
  }
 else {
    if (!isFirstDayOfWeek(mHighlightedDay) && mHighlightedDay > 1) {
      mHighlightedDay--;
      focusChanged=true;
    }
  }
  return focusChanged;
}
