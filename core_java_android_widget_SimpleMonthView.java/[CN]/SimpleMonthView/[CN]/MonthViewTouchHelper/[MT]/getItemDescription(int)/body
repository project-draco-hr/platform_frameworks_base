{
  if (id == ITEM_ID_NEXT) {
    return mNextContentDesc;
  }
 else   if (id == ITEM_ID_PREV) {
    return mPrevContentDesc;
  }
 else   if (id >= 1 && id <= mDaysInMonth) {
    mTempCalendar.set(mYear,mMonth,id);
    return DateFormat.format(DATE_FORMAT,mTempCalendar.getTimeInMillis());
  }
  return "";
}
