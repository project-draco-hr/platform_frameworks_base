{
  TypedArray a;
  if (theme != null) {
    a=theme.obtainStyledAttributes(attrs,AnticipateOvershootInterpolator,0,0);
  }
 else {
    a=res.obtainAttributes(attrs,AnticipateOvershootInterpolator);
