{
  Operation operation;
  if (isNavigating) {
    operation=new Operation(){
      @Override public void execute(      IGnssStatusListener listener) throws RemoteException {
        listener.onGnssStarted();
      }
    }
;
  }
 else {
    operation=new Operation(){
      @Override public void execute(      IGnssStatusListener listener) throws RemoteException {
        listener.onGnssStopped();
      }
    }
;
  }
  foreach(operation);
}
