{
  if (HttpLog.LOGV) {
    HttpLog.v("HttpAuthHeader.parseParameters():" + " parameters: " + parameters);
  }
  if (parameters != null) {
    int i;
    do {
      i=parameters.indexOf(',');
      if (i < 0) {
        parseParameter(parameters);
      }
 else {
        parseParameter(parameters.substring(0,i));
        parameters=parameters.substring(i + 1);
      }
    }
 while (i >= 0);
  }
}
