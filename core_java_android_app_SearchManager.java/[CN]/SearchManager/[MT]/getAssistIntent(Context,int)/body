{
  try {
    if (mService == null) {
      return null;
    }
    ComponentName comp=mService.getAssistIntent(userHandle);
    if (comp == null) {
      return null;
    }
    Intent intent=new Intent(Intent.ACTION_ASSIST);
    intent.setComponent(comp);
    return intent;
  }
 catch (  RemoteException re) {
    Log.e(TAG,"getAssistIntent() failed: " + re);
    return null;
  }
}
