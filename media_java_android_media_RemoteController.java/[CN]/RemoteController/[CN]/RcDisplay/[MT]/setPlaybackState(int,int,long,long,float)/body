{
  final RemoteController rc=mController.get();
  if (rc == null) {
    return;
  }
  if (DEBUG) {
    Log.d(TAG,"> new playback state: genId=" + genId + " state="+ state+ " changeTime="+ stateChangeTimeMs+ " pos="+ currentPosMs+ "ms speed="+ speed);
  }
synchronized (mGenLock) {
    if (rc.mClientGenerationIdCurrent != genId) {
      return;
    }
  }
  final PlaybackInfo playbackInfo=new PlaybackInfo(state,stateChangeTimeMs,currentPosMs,speed);
  sendMsg(rc.mEventHandler,MSG_NEW_PLAYBACK_INFO,SENDMSG_REPLACE,genId,0,playbackInfo,0);
}
