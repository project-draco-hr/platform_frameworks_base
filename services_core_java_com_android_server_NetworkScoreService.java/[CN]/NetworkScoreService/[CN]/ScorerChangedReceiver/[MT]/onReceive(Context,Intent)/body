{
  String action=intent.getAction();
  if ((Intent.ACTION_PACKAGE_CHANGED.equals(action) || Intent.ACTION_PACKAGE_REPLACED.equals(action) || Intent.ACTION_PACKAGE_FULLY_REMOVED.equals(action)) && NetworkScorerAppManager.getActiveScorer(mContext) == null) {
    Log.i(TAG,"Package " + mRegisteredPackage + " is no longer valid, disabling scoring");
    setScorerInternal(null);
  }
}
