{
  int value=level.intValue();
  if (value >= Level.SEVERE.intValue()) {
    return Log.ERROR;
  }
 else   if (value >= Level.WARNING.intValue()) {
    return Log.WARN;
  }
 else   if (value >= Level.INFO.intValue()) {
    return Log.INFO;
  }
 else   if (value >= Level.CONFIG.intValue()) {
    return Log.DEBUG;
  }
 else {
    return Log.VERBOSE;
  }
}
