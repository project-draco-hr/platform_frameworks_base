{
  if (wtoken != null && wtoken.startingWindow != null) {
    if (DEBUG_STARTING_WINDOW)     Slog.v(TAG,Debug.getCallers(1) + ": Schedule remove starting " + wtoken+ (wtoken != null ? " startingWindow=" + wtoken.startingWindow : ""));
    Message m=mH.obtainMessage(H.REMOVE_STARTING,wtoken);
    mH.sendMessage(m);
  }
}
