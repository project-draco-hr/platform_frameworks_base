{
  final WindowList windows=pos.getWindowList();
  final int i=windows.indexOf(pos);
  if (DEBUG_FOCUS || DEBUG_WINDOW_MOVEMENT || DEBUG_ADD_REMOVE)   Slog.v(TAG_WM,"Adding window " + window + " at "+ (i + 1)+ " of "+ windows.size()+ " (after "+ pos+ ")");
  windows.add(i + 1,window);
  mWindowsChanged=true;
}
