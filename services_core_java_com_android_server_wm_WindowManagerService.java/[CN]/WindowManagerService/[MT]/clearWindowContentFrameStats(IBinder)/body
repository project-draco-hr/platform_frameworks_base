{
  if (!checkCallingPermission(Manifest.permission.FRAME_STATS,"clearWindowContentFrameStats()")) {
    throw new SecurityException("Requires FRAME_STATS permission");
  }
synchronized (mWindowMap) {
    WindowState windowState=mWindowMap.get(token);
    if (windowState == null) {
      return false;
    }
    SurfaceControl surfaceControl=windowState.mWinAnimator.mSurfaceControl;
    if (surfaceControl == null) {
      return false;
    }
    return surfaceControl.clearContentFrameStats();
  }
}
