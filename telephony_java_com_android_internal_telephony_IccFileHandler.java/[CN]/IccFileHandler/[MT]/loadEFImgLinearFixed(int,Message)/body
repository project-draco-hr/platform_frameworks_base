{
  Message response=obtainMessage(EVENT_READ_IMG_DONE,new LoadLinearFixedContext(IccConstants.EF_IMG,recordNum,onLoaded));
  phone.mCM.iccIOForApp(COMMAND_GET_RESPONSE,IccConstants.EF_IMG,"img",recordNum,READ_RECORD_MODE_ABSOLUTE,GET_RESPONSE_EF_IMG_SIZE_BYTES,null,null,phone.getIccCard().getAid(),response);
}
