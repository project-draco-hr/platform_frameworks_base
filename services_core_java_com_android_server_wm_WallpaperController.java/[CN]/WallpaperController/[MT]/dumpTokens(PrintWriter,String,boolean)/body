{
  if (!mWallpaperTokens.isEmpty()) {
    pw.println();
    pw.print(prefix);
    pw.println("Wallpaper tokens:");
    for (int i=mWallpaperTokens.size() - 1; i >= 0; i--) {
      WindowToken token=mWallpaperTokens.get(i);
      pw.print(prefix);
      pw.print("Wallpaper #");
      pw.print(i);
      pw.print(' ');
      pw.print(token);
      if (dumpAll) {
        pw.println(':');
        token.dump(pw,"    ");
      }
 else {
        pw.println();
      }
    }
  }
}
