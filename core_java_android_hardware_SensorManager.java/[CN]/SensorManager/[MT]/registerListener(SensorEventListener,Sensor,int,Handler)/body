{
  if (listener == null || sensor == null) {
    return false;
  }
  int delay=-1;
switch (rate) {
case SENSOR_DELAY_FASTEST:
    delay=0;
  break;
case SENSOR_DELAY_GAME:
delay=20000;
break;
case SENSOR_DELAY_UI:
delay=66667;
break;
case SENSOR_DELAY_NORMAL:
delay=200000;
break;
default :
delay=rate;
break;
}
return registerListenerImpl(listener,sensor,delay,handler);
}
