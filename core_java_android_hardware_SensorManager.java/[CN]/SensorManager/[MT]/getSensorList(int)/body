{
  List<Sensor> list;
  final List<Sensor> fullList=getFullSensorList();
  if (type == Sensor.TYPE_ALL) {
    list=fullList;
  }
 else {
    list=new ArrayList<Sensor>();
    for (    Sensor i : fullList) {
      if (i.getType() == type)       list.add(i);
    }
  }
  list=Collections.unmodifiableList(list);
  return list;
}
