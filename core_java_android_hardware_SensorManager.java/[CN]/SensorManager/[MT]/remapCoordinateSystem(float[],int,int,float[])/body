{
  if (inR == outR) {
    final float[] temp=mTempMatrix;
synchronized (temp) {
      if (remapCoordinateSystemImpl(inR,X,Y,temp)) {
        final int size=outR.length;
        for (int i=0; i < size; i++)         outR[i]=temp[i];
        return true;
      }
    }
  }
  return remapCoordinateSystemImpl(inR,X,Y,outR);
}
