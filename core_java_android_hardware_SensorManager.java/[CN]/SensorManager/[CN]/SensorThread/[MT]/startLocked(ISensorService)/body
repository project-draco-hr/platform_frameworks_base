{
  try {
    if (mThread == null) {
      ParcelFileDescriptor fd=service.getDataChanel();
      mThread=new Thread(new SensorThreadRunnable(fd,service),SensorThread.class.getName());
      mThread.start();
    }
  }
 catch (  RemoteException e) {
    Log.e(TAG,"RemoteException in startLocked: ",e);
  }
}
