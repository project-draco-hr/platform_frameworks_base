{
  try {
    if (mThread == null) {
      Bundle dataChannel=service.getDataChannel();
      mThread=new Thread(new SensorThreadRunnable(dataChannel),SensorThread.class.getName());
      mThread.start();
    }
  }
 catch (  RemoteException e) {
    Log.e(TAG,"RemoteException in startLocked: ",e);
  }
}
