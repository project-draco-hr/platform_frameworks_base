{
  float[] v;
synchronized (this) {
    v=mValuesPool;
    mValuesPool=null;
  }
  if (v != null) {
    v[0]=values[0];
    v[1]=values[1];
    v[2]=values[2];
    v[3]=values[3];
    v[4]=values[4];
    v[5]=values[5];
  }
 else {
    v=values.clone();
  }
  Message msg=Message.obtain();
  msg.what=sensor;
  msg.obj=v;
  msg.arg1=accuracy;
  mHandler.sendMessage(msg);
}
