{
  Markup m1=new Markup("cardinal");
  m1.setParameter("ccc","-");
  m1.setParameter("aaa","-");
  m1.setParameter("aa","-");
  m1.setParameter("bbb","-");
  String str=m1.toString();
  assertEquals("type: \"cardinal\" " + "aa: \"-\" " + "aaa: \"-\" "+ "bbb: \"-\" "+ "ccc: \"-\"",str);
  Markup m2=Markup.markupFromString(str);
  assertEquals(m1,m2);
}
