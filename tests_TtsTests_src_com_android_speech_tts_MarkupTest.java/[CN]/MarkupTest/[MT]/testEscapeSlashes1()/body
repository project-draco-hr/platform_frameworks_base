{
  Markup m1=new Markup("text").setParameter("something_unknown","\\ \\\\ \t \n \"");
  String str=m1.toString();
  assertEquals("type: \"text\" " + "something_unknown: \"\\\\ \\\\\\\\ \t \n \\\"\"",str);
  Markup m2=Markup.markupFromString(str);
  assertEquals(m1.toString(),m2.toString());
  assertEquals(m1,m2);
}
