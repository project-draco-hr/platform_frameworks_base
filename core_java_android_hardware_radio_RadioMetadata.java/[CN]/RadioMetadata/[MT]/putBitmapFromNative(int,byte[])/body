{
  String key=getKeyFromNativeKey(nativeKey);
  if (!METADATA_KEYS_TYPE.containsKey(key) || METADATA_KEYS_TYPE.get(key) != METADATA_TYPE_BITMAP) {
    return -1;
  }
  Bitmap bmp=null;
  try {
    bmp=BitmapFactory.decodeByteArray(value,0,value.length);
  }
 catch (  Exception e) {
  }
 finally {
    if (bmp == null) {
      return -1;
    }
    mBundle.putParcelable(key,bmp);
    return 0;
  }
}
