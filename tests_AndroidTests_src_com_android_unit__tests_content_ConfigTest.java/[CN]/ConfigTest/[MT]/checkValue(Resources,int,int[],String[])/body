{
  Resources.Theme theme=res.newTheme();
  TypedArray sa=theme.obtainStyledAttributes(resId,styleable);
  for (int i=0; i < styleable.length; i++) {
    String actual=sa.getString(i);
    assertEquals("Returned wrong configuration-based style value: expected " + expectedValues[i] + ", got '"+ actual+ "' from attr "+ i+ " of resource 0x"+ Integer.toHexString(resId),actual,expectedValues[i]);
  }
  sa.recycle();
}
