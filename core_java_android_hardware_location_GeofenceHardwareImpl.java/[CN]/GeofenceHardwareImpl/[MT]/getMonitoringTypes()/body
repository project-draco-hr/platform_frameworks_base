{
  boolean gpsSupported;
  boolean fusedSupported;
synchronized (mSupportedMonitorTypes) {
    gpsSupported=mSupportedMonitorTypes[GeofenceHardware.MONITORING_TYPE_GPS_HARDWARE] != GeofenceHardware.MONITOR_UNSUPPORTED;
    fusedSupported=mSupportedMonitorTypes[GeofenceHardware.MONITORING_TYPE_FUSED_HARDWARE] != GeofenceHardware.MONITOR_UNSUPPORTED;
  }
  if (gpsSupported) {
    if (fusedSupported) {
      return new int[]{GeofenceHardware.MONITORING_TYPE_GPS_HARDWARE,GeofenceHardware.MONITORING_TYPE_FUSED_HARDWARE};
    }
 else {
      return new int[]{GeofenceHardware.MONITORING_TYPE_GPS_HARDWARE};
    }
  }
 else   if (fusedSupported) {
    return new int[]{GeofenceHardware.MONITORING_TYPE_FUSED_HARDWARE};
  }
 else {
    return new int[0];
  }
}
