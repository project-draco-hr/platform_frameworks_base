{
  boolean gpsSupported;
  try {
    gpsSupported=mGpsService.isHardwareGeofenceSupported();
  }
 catch (  RemoteException e) {
    Log.e(TAG,"Remote Exception calling LocationManagerService");
    gpsSupported=false;
  }
  if (gpsSupported) {
    setMonitorAvailability(GeofenceHardware.MONITORING_TYPE_GPS_HARDWARE,GeofenceHardware.MONITOR_CURRENTLY_AVAILABLE);
  }
}
