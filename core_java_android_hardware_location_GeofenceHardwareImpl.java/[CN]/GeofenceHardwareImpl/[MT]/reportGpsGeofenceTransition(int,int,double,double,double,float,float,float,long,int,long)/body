{
  if (DEBUG)   Log.d(TAG,"GeofenceTransition: Flags: " + flags + " Lat: "+ latitude+ " Long: "+ longitude+ " Altitude: "+ altitude+ " Speed: "+ speed+ " Bearing: "+ bearing+ " Accuracy: "+ accuracy+ " Timestamp: "+ timestamp+ " Transition: "+ transition+ " TransitionTimestamp: "+ transitionTimestamp);
  Location location=getLocation(flags,latitude,longitude,altitude,speed,bearing,accuracy,timestamp);
  GeofenceTransition t=new GeofenceTransition(geofenceId,transition,timestamp,location);
  acquireWakeLock();
  Message m=mGeofenceHandler.obtainMessage(GEOFENCE_TRANSITION_CALLBACK,t);
  mGeofenceHandler.sendMessage(m);
}
