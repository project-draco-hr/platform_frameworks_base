{
  if (Build.VERSION.SDK_INT >= Build.VERSION_CODES.LOLLIPOP) {
    assertEquals(mBindingObject.getCheckMark().getColor(),((ColorDrawable)mView.getCheckMarkDrawable()).getColor());
    assertEquals(mBindingObject.getCheckMarkTint(),mView.getCheckMarkTintList().getDefaultColor());
    changeValues();
    assertEquals(mBindingObject.getCheckMark().getColor(),((ColorDrawable)mView.getCheckMarkDrawable()).getColor());
    assertEquals(mBindingObject.getCheckMarkTint(),mView.getCheckMarkTintList().getDefaultColor());
  }
}
