{
  if (appendPostalsForDoCoMoInternal(builder,contentValuesList,StructuredPostal.TYPE_HOME)) {
    return;
  }
  if (appendPostalsForDoCoMoInternal(builder,contentValuesList,StructuredPostal.TYPE_WORK)) {
    return;
  }
  if (appendPostalsForDoCoMoInternal(builder,contentValuesList,StructuredPostal.TYPE_OTHER)) {
    return;
  }
  if (appendPostalsForDoCoMoInternal(builder,contentValuesList,StructuredPostal.TYPE_CUSTOM)) {
    return;
  }
  Log.w(LOG_TAG,"Should not come here. Must have at least one postal data.");
}
