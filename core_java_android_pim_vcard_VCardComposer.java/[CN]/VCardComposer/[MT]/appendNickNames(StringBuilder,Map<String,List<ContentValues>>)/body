{
  final List<ContentValues> contentValuesList=contentValuesListMap.get(Nickname.CONTENT_ITEM_TYPE);
  if (contentValuesList == null) {
    return;
  }
  final boolean useAndroidProperty;
  if (mIsV30) {
    useAndroidProperty=false;
  }
 else   if (mUsesAndroidProperty) {
    useAndroidProperty=true;
  }
 else {
    return;
  }
  for (  ContentValues contentValues : contentValuesList) {
    final String nickname=contentValues.getAsString(Nickname.NAME);
    if (TextUtils.isEmpty(nickname)) {
      continue;
    }
    if (useAndroidProperty) {
      appendAndroidSpecificProperty(builder,Nickname.CONTENT_ITEM_TYPE,contentValues);
    }
 else {
      appendVCardLineWithCharsetAndQPDetection(builder,Constants.PROPERTY_NICKNAME,nickname);
    }
  }
}
