{
  List<ContentValues> contentValuesList=contentValuesListMap.get(Organization.CONTENT_ITEM_TYPE);
  if (contentValuesList != null) {
    for (    ContentValues contentValues : contentValuesList) {
      final String company=contentValues.getAsString(Organization.COMPANY);
      final String title=contentValues.getAsString(Organization.TITLE);
      appendVCardLine(builder,VCARD_PROPERTY_ORG,company,true,mUsesQuotedPrintable);
      appendVCardLine(builder,VCARD_PROPERTY_TITLE,title,true,mUsesQuotedPrintable);
    }
  }
}
