{
  final boolean needCharset=(mUsesQuotedPrintable && !VCardUtils.containsOnlyPrintableAscii(rawData));
  final boolean reallyUseQuotedPrintable=!VCardUtils.containsOnlyNonCrLfPrintableAscii(rawData);
  appendVCardLine(builder,propertyName,parameterList,rawData,needCharset,reallyUseQuotedPrintable);
}
