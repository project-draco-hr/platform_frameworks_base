{
  final IAudioPolicyCallback pcb2=pcb;
  final Thread thread=new Thread(){
    @Override public void run(){
synchronized (mAudioFocusLock) {
        if (mFocusStack.isEmpty()) {
          return;
        }
        try {
          pcb2.notifyAudioFocusGrant(mFocusStack.peek().toAudioFocusInfo(),AudioManager.AUDIOFOCUS_REQUEST_GRANTED);
        }
 catch (        RemoteException e) {
          Log.e(TAG,"Can't call notifyAudioFocusGrant() on IAudioPolicyCallback " + pcb2.asBinder(),e);
        }
      }
    }
  }
;
  thread.start();
}
