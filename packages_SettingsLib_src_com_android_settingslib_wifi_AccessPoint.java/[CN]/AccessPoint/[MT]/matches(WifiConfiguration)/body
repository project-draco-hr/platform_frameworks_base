{
  if (config.isPasspoint() && mConfig != null && mConfig.isPasspoint()) {
    return config.FQDN.equals(mConfig.providerFriendlyName);
  }
 else {
    return ssid.equals(removeDoubleQuotes(config.SSID)) && security == getSecurity(config) && (mConfig == null || mConfig.shared == config.shared);
  }
}
