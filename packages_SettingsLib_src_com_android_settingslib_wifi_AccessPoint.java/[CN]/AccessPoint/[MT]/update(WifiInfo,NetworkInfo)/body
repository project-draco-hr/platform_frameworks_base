{
  boolean reorder=false;
  if (info != null && isInfoForThisAccessPoint(info)) {
    reorder=(mInfo == null);
    mRssi=info.getRssi();
    mInfo=info;
    mNetworkInfo=networkInfo;
    if (mAccessPointListener != null) {
      mAccessPointListener.onAccessPointChanged(this);
    }
  }
 else   if (mInfo != null) {
    reorder=true;
    mInfo=null;
    mNetworkInfo=null;
    if (mAccessPointListener != null) {
      mAccessPointListener.onAccessPointChanged(this);
    }
  }
  return reorder;
}
