{
  if (imsi == null) {
    Log.d(LOG_TAG,"getOperatorNumeric: IMSI == null");
    return null;
  }
  if (mncLength == UNINITIALIZED || mncLength == UNKNOWN) {
    Log.d(LOG_TAG,"getSIMOperatorNumeric: bad mncLength");
    return null;
  }
  if (SystemProperties.getInt(com.android.internal.telephony.TelephonyProperties.PROPERTY_NETWORK_LTE_ON_CDMA,0) == 1) {
    Log.e(LOG_TAG,"getOperatorNumeric: STOPSHIP bad numeric operators in lte");
    return SystemProperties.get("ro.cdma.home.operator.numeric","310004");
  }
  return imsi.substring(0,3 + mncLength);
}
