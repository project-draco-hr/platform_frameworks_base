{
  enforceAccessPermission();
synchronized (mLock) {
    mHdmiControlEnabled=enabled;
  }
  if (!enabled) {
    return;
  }
  runOnServiceThread(new Runnable(){
    @Override public void run(){
      HdmiCecLocalDeviceTv tv=tv();
      if (tv == null) {
        return;
      }
      tv.routingAtEnableTime();
    }
  }
);
}
