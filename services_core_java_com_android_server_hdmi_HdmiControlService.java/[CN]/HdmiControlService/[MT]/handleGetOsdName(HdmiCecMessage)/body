{
  String name=HdmiCec.getDefaultDeviceName(message.getDestination());
  HdmiCecMessage cecMessage=HdmiCecMessageBuilder.buildSetOsdNameCommand(message.getDestination(),message.getSource(),name);
  if (cecMessage != null) {
    sendCecCommand(cecMessage);
  }
 else {
    Slog.w(TAG,"Failed to build <Get Osd Name>:" + name);
  }
}
