{
  if (mActiveRoutingPath == 0) {
    return false;
  }
  for (int i=12; i >= 0; i-=4) {
    int curActivePath=(mActiveRoutingPath >> i) & 0xF;
    if (curActivePath == 0) {
      return true;
    }
 else {
      int curPath=(path >> i) & 0xF;
      if (curPath != curActivePath) {
        return false;
      }
    }
  }
  return false;
}
