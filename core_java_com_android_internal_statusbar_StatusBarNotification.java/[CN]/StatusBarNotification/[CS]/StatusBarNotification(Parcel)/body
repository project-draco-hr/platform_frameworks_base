{
  this.pkg=in.readString();
  this.id=in.readInt();
  if (in.readInt() != 0) {
    this.tag=in.readString();
  }
 else {
    this.tag=null;
  }
  this.uid=in.readInt();
  this.initialPid=in.readInt();
  this.score=in.readInt();
  this.notification=new Notification(in);
  this.user=UserHandle.readFromParcel(in);
  this.notification.setUser(user);
}
