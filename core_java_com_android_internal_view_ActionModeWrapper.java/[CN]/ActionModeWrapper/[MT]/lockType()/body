{
  mTypeLocked=true;
switch (getType()) {
case ActionMode.TYPE_PRIMARY:
default :
    mActionMode=new StandaloneActionMode(mActionModeView.getContext(),mActionModeView,mCallback,mIsFocusable,mMenu);
  break;
case ActionMode.TYPE_FLOATING:
break;
}
if (mActionMode == null) {
return;
}
mActionMode.setTitle(mTitle);
mActionMode.setSubtitle(mSubtitle);
if (mCustomView != null) {
mActionMode.setCustomView(mCustomView);
}
mTitle=null;
mSubtitle=null;
mCustomView=null;
}
