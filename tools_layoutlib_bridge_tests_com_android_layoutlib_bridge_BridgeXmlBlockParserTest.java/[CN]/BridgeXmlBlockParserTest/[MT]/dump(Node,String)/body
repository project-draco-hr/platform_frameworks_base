{
  Node n;
  String[] types={"unknown","ELEMENT_NODE","ATTRIBUTE_NODE","TEXT_NODE","CDATA_SECTION_NODE","ENTITY_REFERENCE_NODE","ENTITY_NODE","PROCESSING_INSTRUCTION_NODE","COMMENT_NODE","DOCUMENT_NODE","DOCUMENT_TYPE_NODE","DOCUMENT_FRAGMENT_NODE","NOTATION_NODE"};
  String s=String.format("%s<%s> %s %s",prefix,types[node.getNodeType()],node.getNodeName(),node.getNodeValue() == null ? "" : node.getNodeValue().trim());
  System.out.println(s);
  n=node.getFirstChild();
  if (n != null) {
    dump(n,prefix + "- ");
  }
  n=node.getNextSibling();
  if (n != null) {
    dump(n,prefix);
  }
}
