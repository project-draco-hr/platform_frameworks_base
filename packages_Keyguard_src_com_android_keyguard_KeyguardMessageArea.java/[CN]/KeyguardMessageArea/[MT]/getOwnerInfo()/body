{
  ContentResolver res=getContext().getContentResolver();
  final boolean ownerInfoEnabled=Settings.Secure.getIntForUser(res,Settings.Secure.LOCK_SCREEN_OWNER_INFO_ENABLED,1,UserHandle.USER_CURRENT) != 0;
  return ownerInfoEnabled && !mShowingMessage ? Settings.Secure.getStringForUser(res,Settings.Secure.LOCK_SCREEN_OWNER_INFO,UserHandle.USER_CURRENT) : null;
}
