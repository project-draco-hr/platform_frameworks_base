{
  try {
    Context targetContext=getInstrumentation().getTargetContext();
    File cacheDir=targetContext.getCacheDir();
    for (int i=0; i < LOAD_TEST_RUNNER_FILES.length; i++) {
      InputStream in=targetContext.getAssets().open(LOAD_TEST_RUNNER_FILES[i]);
      OutputStream out=new FileOutputStream(new File(cacheDir,LOAD_TEST_RUNNER_FILES[i]));
      byte[] buf=new byte[2048];
      int len;
      while ((len=in.read(buf)) >= 0) {
        out.write(buf,0,len);
      }
      out.close();
      in.close();
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
