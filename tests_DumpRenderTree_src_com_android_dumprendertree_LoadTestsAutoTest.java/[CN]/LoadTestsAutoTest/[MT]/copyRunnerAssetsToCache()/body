{
  try {
    String out_dir=getActivity().getApplicationContext().getCacheDir().getPath() + "/";
    for (int i=0; i < LOAD_TEST_RUNNER_FILES.length; i++) {
      InputStream in=getActivity().getAssets().open(LOAD_TEST_RUNNER_FILES[i]);
      OutputStream out=new FileOutputStream(out_dir + LOAD_TEST_RUNNER_FILES[i]);
      byte[] buf=new byte[2048];
      int len;
      while ((len=in.read(buf)) >= 0) {
        out.write(buf,0,len);
      }
      out.close();
      in.close();
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
