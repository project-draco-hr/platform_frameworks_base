{
  int index;
  if (mRetryCount < mRetryArray.size()) {
    index=mRetryCount;
  }
 else {
    index=mRetryArray.size() - 1;
  }
  int retVal;
  if ((index >= 0) && (index < mRetryArray.size())) {
    retVal=mRetryArray.get(index).mDelayTime + nextRandomizationTime(index);
  }
 else {
    retVal=0;
  }
  if (DBG)   log("getRetryTimer: " + retVal);
  return retVal;
}
