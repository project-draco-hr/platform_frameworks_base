{
  if (DEBUG)   Log.v(TAG,"onInterceptTouchEvent()");
  if (System.currentTimeMillis() < mStartTouchTime) {
    return true;
  }
  return mEdgeSwipeHelper.onInterceptTouchEvent(ev) || mSwipeHelper.onInterceptTouchEvent(ev) || mExpandHelper.onInterceptTouchEvent(ev)|| super.onInterceptTouchEvent(ev);
}
