{
  int keyCode=event.getKeyCode();
  if (isInterestingKey(keyCode)) {
    if (keyCode == KeyEvent.KEYCODE_BACK) {
      setVisible(false);
    }
 else {
      dismissControlsDelayed(ZOOM_CONTROLS_TIMEOUT);
    }
    return false;
  }
 else {
    ViewRoot viewRoot=getOwnerViewRoot();
    if (viewRoot != null) {
      viewRoot.dispatchKey(event);
    }
    return true;
  }
}
