{
  try {
    if (mActiveListener != null) {
      mActiveListener.onError(SoundTrigger.STATUS_DEAD_OBJECT);
    }
  }
 catch (  RemoteException e) {
    Slog.w(TAG,"RemoteException in onError",e);
  }
 finally {
    internalClearSoundModelLocked();
    internalClearStateLocked();
    if (mModule != null) {
      mModule.detach();
      mModule=null;
    }
  }
}
