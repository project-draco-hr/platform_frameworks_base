{
synchronized (mLock) {
    pw.print("  module properties=");
    pw.println(mModuleProperties == null ? "null" : mModuleProperties);
    pw.print("  keyphrase ID=");
    pw.println(mKeyphraseId);
    pw.print("  call active=");
    pw.println(mCallActive);
    pw.print("  power save mode active=");
    pw.println(mIsPowerSaveMode);
    pw.print("  service disabled=");
    pw.println(mServiceDisabled);
    if (mKeyphraseModelData != null) {
      pw.println(mKeyphraseModelData.toString());
    }
  }
}
