{
synchronized (mLock) {
    mSealed=true;
    mDestroyed=true;
    for (    FileBridge bridge : mBridges) {
      bridge.forceClose();
    }
  }
  if (stageDir != null) {
    mPm.mInstaller.rmPackageDir(stageDir.getAbsolutePath());
  }
  if (stageCid != null) {
    PackageHelper.destroySdDir(stageCid);
  }
}
