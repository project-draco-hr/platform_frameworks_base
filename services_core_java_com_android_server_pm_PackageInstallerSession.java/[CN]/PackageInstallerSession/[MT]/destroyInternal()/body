{
synchronized (mLock) {
    mSealed=true;
    mDestroyed=true;
    for (    FileBridge bridge : mBridges) {
      bridge.forceClose();
    }
  }
  if (stageDir != null) {
    FileUtils.deleteContents(stageDir);
    stageDir.delete();
  }
  if (stageCid != null) {
    PackageHelper.destroySdDir(stageCid);
  }
}
