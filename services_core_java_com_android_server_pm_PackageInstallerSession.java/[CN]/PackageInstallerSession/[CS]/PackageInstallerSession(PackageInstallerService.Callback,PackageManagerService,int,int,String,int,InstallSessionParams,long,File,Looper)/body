{
  mCallback=callback;
  mPm=pm;
  mHandler=new Handler(looper,mHandlerCallback);
  this.sessionId=sessionId;
  this.userId=userId;
  this.installerPackageName=installerPackageName;
  this.installerUid=installerUid;
  this.params=params;
  this.createdMillis=createdMillis;
  this.sessionStageDir=sessionStageDir;
  mSignatures=params.signatures;
  mMutationsAllowed=true;
  if (pm.checkPermission(android.Manifest.permission.INSTALL_PACKAGES,installerPackageName) == PackageManager.PERMISSION_GRANTED) {
    mPermissionsConfirmed=true;
  }
  if (installerUid == Process.SHELL_UID || installerUid == 0) {
    mPermissionsConfirmed=true;
  }
}
