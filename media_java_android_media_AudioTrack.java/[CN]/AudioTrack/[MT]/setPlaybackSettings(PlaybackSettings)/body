{
  if (settings == null) {
    throw new IllegalArgumentException("settings is null");
  }
  float[] floatArray;
  int[] intArray;
  try {
    floatArray=new float[]{settings.getSpeed(),settings.getPitch()};
    intArray=new int[]{settings.getAudioFallbackMode(),PlaybackSettings.AUDIO_STRETCH_MODE_DEFAULT};
  }
 catch (  IllegalStateException e) {
    throw new IllegalArgumentException(e);
  }
  native_set_playback_settings(floatArray,intArray);
}
