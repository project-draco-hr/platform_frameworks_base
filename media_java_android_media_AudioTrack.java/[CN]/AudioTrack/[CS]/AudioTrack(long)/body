{
  mAttributes=null;
  mAppOps=null;
  mNativeTrackInJavaObj=0;
  mJniData=0;
  Looper looper;
  if ((looper=Looper.myLooper()) == null) {
    looper=Looper.getMainLooper();
  }
  mInitializationLooper=looper;
  if (nativeTrackInJavaObj != 0) {
    deferred_connect(nativeTrackInJavaObj);
  }
 else {
    mState=STATE_UNINITIALIZED;
  }
}
