{
  StringBuilder ret=new StringBuilder(length * 2);
  for (int i=offset; i < offset + length; i++) {
    byte b;
    int v;
    v=data[i] & 0xf;
    if (v > 9)     break;
    ret.append((char)('0' + v));
    v=(data[i] >> 4) & 0xf;
    if (v > 9)     break;
    ret.append((char)('0' + v));
  }
  return ret.toString();
}
