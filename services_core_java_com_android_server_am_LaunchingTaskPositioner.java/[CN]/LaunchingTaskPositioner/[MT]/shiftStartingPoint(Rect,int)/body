{
switch (shiftPolicy) {
case SHIFT_POLICY_HORIZONTAL_LEFT:
    posposal.offset(-mDefaultFreeformStepHorizontal,0);
  break;
case SHIFT_POLICY_HORIZONTAL_RIGHT:
posposal.offset(mDefaultFreeformStepHorizontal,0);
break;
default :
posposal.offset(mDefaultFreeformStepHorizontal,mDefaultFreeformStepVertical);
break;
}
}
