{
  final SQLiteDatabase db=getDatabase();
  mValues.clear();
  DatabaseUtils.cursorStringToContentValues(diffsCursor,SubscribedFeeds.Feeds._SYNC_ID,mValues);
  DatabaseUtils.cursorStringToContentValues(diffsCursor,SubscribedFeeds.Feeds._SYNC_ACCOUNT,mValues);
  DatabaseUtils.cursorStringToContentValues(diffsCursor,SubscribedFeeds.Feeds._SYNC_VERSION,mValues);
  db.replace(mDeletedTable,SubscribedFeeds.Feeds._SYNC_ID,mValues);
}
