{
  mValues.clear();
  DatabaseUtils.cursorStringToContentValues(diffsCursor,SubscribedFeeds.Feeds._SYNC_ID,mValues);
  DatabaseUtils.cursorStringToContentValues(diffsCursor,SubscribedFeeds.Feeds._SYNC_TIME,mValues);
  DatabaseUtils.cursorStringToContentValues(diffsCursor,SubscribedFeeds.Feeds._SYNC_VERSION,mValues);
  mValues.put(SubscribedFeeds.Feeds._SYNC_DIRTY,conflicts ? 1 : 0);
  final SQLiteDatabase db=getDatabase();
  db.update(mTable,mValues,SubscribedFeeds.Feeds._ID + '=' + localPersonID,null);
}
