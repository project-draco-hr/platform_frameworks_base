{
  if (platformVersion == 0) {
    return "5:00";
  }
  if (platformVersion < GINGERBREAD) {
    return "2:20";
  }
  if (platformVersion < ICE_CREAM_SANDWICH) {
    return "2:30";
  }
  if (platformVersion < JELLY_BEAN) {
    return "4:00";
  }
  if (platformVersion < KITKAT) {
    return "4:30";
  }
  if (platformVersion <= KITKAT_WATCH) {
    return "4:40";
  }
  return "4:04";
}
