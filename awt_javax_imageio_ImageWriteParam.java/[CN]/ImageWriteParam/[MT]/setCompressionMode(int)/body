{
  checkWriteCompressed();
switch (mode) {
case MODE_EXPLICIT:
{
      compressionMode=mode;
      unsetCompression();
      break;
    }
case MODE_COPY_FROM_METADATA:
case MODE_DISABLED:
case MODE_DEFAULT:
{
    compressionMode=mode;
    break;
  }
default :
{
  throw new IllegalArgumentException("Illegal value for mode!");
}
}
}
