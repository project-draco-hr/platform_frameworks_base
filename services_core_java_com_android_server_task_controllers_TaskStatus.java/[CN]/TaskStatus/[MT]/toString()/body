{
  return String.valueOf(hashCode()).substring(0,3) + ".." + ":["+ task.getService().getPackageName()+ ",tId="+ task.getId()+ ",R=("+ earliestRunTimeElapsedMillis+ ","+ latestRunTimeElapsedMillis+ ")"+ ",N="+ task.getNetworkCapabilities()+ ",C="+ task.isRequireCharging()+ ",I="+ task.isRequireDeviceIdle()+ ",F="+ numFailures+ (isReady() ? "(READY)" : "")+ "]";
}
