{
  return (!hasChargingConstraint() || chargingConstraintSatisfied.get()) && (!hasTimingDelayConstraint() || timeDelayConstraintSatisfied.get()) && (!hasConnectivityConstraint() || connectivityConstraintSatisfied.get())&& (!hasMeteredConstraint() || meteredConstraintSatisfied.get())&& (!hasIdleConstraint() || idleConstraintSatisfied.get())&& (!hasDeadlineConstraint() || deadlineConstraintSatisfied.get());
}
