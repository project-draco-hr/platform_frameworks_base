{
  StringBuilder builder=new StringBuilder("rename");
  builder.append(' ');
  builder.append(oldname);
  builder.append(' ');
  builder.append(newname);
  builder.append(' ');
  if (useEncryptedFilesystem) {
    builder.append('1');
  }
 else {
    builder.append('0');
  }
  return execute(builder.toString());
}
