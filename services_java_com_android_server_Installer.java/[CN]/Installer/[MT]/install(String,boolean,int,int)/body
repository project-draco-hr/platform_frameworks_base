{
  StringBuilder builder=new StringBuilder("install");
  builder.append(' ');
  builder.append(name);
  builder.append(' ');
  if (useEncryptedFilesystem) {
    builder.append('1');
  }
 else {
    builder.append('0');
  }
  builder.append(' ');
  builder.append(uid);
  builder.append(' ');
  builder.append(gid);
  return execute(builder.toString());
}
