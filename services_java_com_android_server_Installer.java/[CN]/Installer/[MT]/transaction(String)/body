{
  if (!connect()) {
    Log.e(TAG,"connection failed");
    return "-1";
  }
  if (!writeCommand(cmd)) {
    Log.e(TAG,"write command failed? reconnect!");
    if (!connect() || !writeCommand(cmd)) {
      return "-1";
    }
  }
  if (readReply()) {
    String s=new String(buf,0,buflen);
    return s;
  }
 else {
    return "-1";
  }
}
