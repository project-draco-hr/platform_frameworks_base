{
  long totalTime=0;
  int printedScreen=-1;
  for (int iscreen=0; iscreen < ProcessStats.ADJ_COUNT; iscreen+=ProcessStats.ADJ_SCREEN_MOD) {
    int printedMem=-1;
    for (int imem=0; imem < ProcessStats.ADJ_MEM_FACTOR_COUNT; imem++) {
      int state=imem + iscreen;
      long time=getDuration(serviceType,curState,curStartTime,state,now);
      String running="";
      if (curState == state && pw != null) {
        running=" (running)";
      }
      if (time != 0) {
        if (pw != null) {
          pw.print(prefix);
          DumpUtils.printScreenLabel(pw,printedScreen != iscreen ? iscreen : STATE_NOTHING);
          printedScreen=iscreen;
          DumpUtils.printMemLabel(pw,printedMem != imem ? imem : STATE_NOTHING,(char)0);
          printedMem=imem;
          pw.print(": ");
          TimeUtils.formatDuration(time,pw);
          pw.println(running);
        }
        totalTime+=time;
      }
    }
  }
  if (totalTime != 0 && pw != null) {
    pw.print(prefix);
    pw.print("    TOTAL: ");
    TimeUtils.formatDuration(totalTime,pw);
    pw.println();
  }
  return totalTime;
}
