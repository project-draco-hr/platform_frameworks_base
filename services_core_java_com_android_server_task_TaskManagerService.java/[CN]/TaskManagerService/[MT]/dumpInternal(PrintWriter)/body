{
synchronized (mTasks) {
    pw.print("Registered tasks:");
    if (mTasks.size() > 0) {
      SparseArray<TaskStatus> tasks=mTasks.getTasks();
      for (int i=0; i < tasks.size(); i++) {
        TaskStatus ts=tasks.get(i);
        pw.println();
        ts.dump(pw,"  ");
      }
    }
 else {
      pw.println();
      pw.println("No tasks scheduled.");
    }
  }
  pw.println();
}
