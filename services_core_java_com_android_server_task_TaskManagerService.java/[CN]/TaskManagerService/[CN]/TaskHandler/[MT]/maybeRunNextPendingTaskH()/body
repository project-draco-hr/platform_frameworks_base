{
  TaskStatus nextPending;
synchronized (mPendingTasks) {
    nextPending=mPendingTasks.poll();
  }
  if (nextPending == null) {
    return;
  }
synchronized (mActiveServices) {
    for (    TaskServiceContext tsc : mActiveServices) {
      if (tsc.isAvailable()) {
        if (tsc.executeRunnableTask(nextPending)) {
          return;
        }
      }
    }
  }
}
