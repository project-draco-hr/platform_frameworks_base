{
  RenderScript pRS=RenderScript.create(mCtx);
  ScriptC_noroot s=new ScriptC_noroot(pRS,mRes,R.raw.noroot);
  pRS.setMessageHandler(mRsMessage);
  initializeGlobals(pRS,s);
  s.forEach_foo(A,A);
  s.invoke_verify_foo();
  s.invoke_noroot_test();
  pRS.finish();
  waitForMessage();
  pRS.destroy();
}
