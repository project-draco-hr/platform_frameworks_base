{
  ArrayList<SmsMessage> messages=new ArrayList<SmsMessage>();
  if (records != null) {
    int count=records.size();
    for (int i=0; i < count; i++) {
      SmsRawData data=records.get(i);
      if (data != null) {
        SmsMessage sms=SmsMessage.createFromEfRecord(i + 1,data.getBytes());
        messages.add(sms);
      }
    }
  }
  return messages;
}
