{
  if (!mInteractive) {
    if (DEBUG)     Slog.v(TAG,"finishing on keyEvent");
    finish();
    return true;
  }
 else   if (event.getKeyCode() == KeyEvent.KEYCODE_BACK) {
    if (DEBUG)     Slog.v(TAG,"finishing on back key");
    finish();
    return true;
  }
  return mWindow.superDispatchKeyEvent(event);
}
