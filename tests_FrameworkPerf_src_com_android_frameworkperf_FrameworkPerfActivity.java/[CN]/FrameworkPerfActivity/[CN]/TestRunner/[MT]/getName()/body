{
  String fgName=mForegroundOp.getName();
  String bgName=mBackgroundOp.getName();
  StringBuilder res=new StringBuilder();
  if (fgName != null) {
    res.append(fgName);
    res.append("Fg");
  }
  if (bgName != null) {
    res.append(bgName);
    res.append("Bg");
  }
  return res.toString();
}
