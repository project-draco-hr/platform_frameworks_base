{
  if (callbackType < CALLBACK_TYPE_ALL_MATCHES || callbackType > (CALLBACK_TYPE_FIRST_MATCH | CALLBACK_TYPE_MATCH_LOST) || (callbackType & CALLBACK_TYPE_ALL_MATCHES) != CALLBACK_TYPE_ALL_MATCHES) {
    throw new IllegalArgumentException("invalid callback type - " + callbackType);
  }
  mCallbackType=callbackType;
  return this;
}
