{
  if (deviceSupportsTelephony()) {
    Resources r=mContext.getResources();
    mRSSIState.iconId=enabled && (mobileSignalIconId > 0) ? mobileSignalIconId : R.drawable.ic_qs_signal_no_signal;
    mRSSIState.label=enabled ? enabledDesc : r.getString(R.string.quick_settings_rssi_emergency_only);
    mRSSICallback.refreshView(mRSSITile,mRSSIState);
  }
}
