{
  Resources r=mContext.getResources();
  mWifiState.iconId=enabled && (wifiSignalIconId > 0) ? wifiSignalIconId : R.drawable.ic_qs_wifi_no_network;
  mWifiState.label=enabled ? enabledDesc : r.getString(R.string.quick_settings_wifi_no_network);
  mWifiCallback.refreshView(mWifiTile,mWifiState);
}
