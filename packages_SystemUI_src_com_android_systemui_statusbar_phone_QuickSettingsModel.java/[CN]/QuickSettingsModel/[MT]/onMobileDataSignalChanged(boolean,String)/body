{
  if (deviceSupportsTelephony()) {
    Resources r=mContext.getResources();
    mRSSIState.enabled=enabled;
    mRSSIState.iconId=(enabled ? R.drawable.ic_qs_rssi_enabled : R.drawable.ic_qs_rssi_normal);
    mRSSIState.label=(enabled ? description : r.getString(R.string.quick_settings_rssi_emergency_only));
    mRSSICallback.refreshView(mRSSITile,mRSSIState);
  }
}
