{
  double[][] array=new double[2][2];
  array[0][0]=0;
  array[0][1]=0;
  array[1][0]=0;
  array[1][1]=0;
  for (int i=0; i < points.length; i++) {
    float x=points[i];
    i++;
    float y=points[i];
    array[0][0]+=x * x;
    array[0][1]+=x * y;
    array[1][0]=array[0][1];
    array[1][1]+=y * y;
  }
  array[0][0]/=(points.length / 2);
  array[0][1]/=(points.length / 2);
  array[1][0]/=(points.length / 2);
  array[1][1]/=(points.length / 2);
  return array;
}
