{
  int i;
  if (arr1 == null || arr2 == null) {
    if (arr1 == null) {
      Log.w(LOG_TAG,"arr1 is null");
    }
 else {
      Log.w(LOG_TAG,"arr2 is null");
    }
    return false;
  }
  if (arr1.length != arr2.length) {
    return false;
  }
  for (i=0; i < arr1.length; i++) {
    if (arr1[i] != arr2[i])     return false;
  }
  return true;
}
