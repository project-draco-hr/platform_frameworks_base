{
  if (DEBUG)   Slog.v(TAG,"startDream name=" + name + " pid="+ Binder.getCallingPid()+ ", uid="+ Binder.getCallingUid());
synchronized (mLock) {
    final long ident=Binder.clearCallingIdentity();
    try {
      bindDreamComponentL(name,true);
    }
  finally {
      Binder.restoreCallingIdentity(ident);
    }
  }
}
