{
  if (v == this && hasFocus && getTabCount() > 0) {
    getChildTabViewAt(mSelectedTab).requestFocus();
    return;
  }
  if (hasFocus) {
    int i=0;
    int numTabs=getTabCount();
    while (i < numTabs) {
      if (getChildTabViewAt(i) == v) {
        setCurrentTab(i);
        mSelectionChangedListener.onTabSelectionChanged(i,false);
        break;
      }
      i++;
    }
  }
}
