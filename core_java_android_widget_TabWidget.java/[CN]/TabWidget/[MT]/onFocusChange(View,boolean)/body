{
  if (v == this && hasFocus) {
    getChildAt(mSelectedTab).requestFocus();
    return;
  }
  if (hasFocus) {
    int i=0;
    while (i < getChildCount()) {
      if (getChildAt(i) == v) {
        setCurrentTab(i);
        mSelectionChangedListener.onTabSelectionChanged(i,false);
        break;
      }
      i++;
    }
  }
}
