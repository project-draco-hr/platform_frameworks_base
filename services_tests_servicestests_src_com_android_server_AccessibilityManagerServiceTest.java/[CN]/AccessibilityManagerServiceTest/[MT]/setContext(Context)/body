{
  super.setContext(context);
  if (MyFirstMockAccessibilityService.sComponentName == null) {
    MyFirstMockAccessibilityService.sComponentName=new ComponentName(context.getPackageName(),MyFirstMockAccessibilityService.class.getName()).flattenToShortString();
  }
  if (MySecondMockAccessibilityService.sComponentName == null) {
    MySecondMockAccessibilityService.sComponentName=new ComponentName(context.getPackageName(),MySecondMockAccessibilityService.class.getName()).flattenToShortString();
  }
}
