{
  if (mFirstTime) {
    mFirstTime=false;
    mProperty.setValue(mObject,target);
    mCurrentValue=target;
    return true;
  }
  if (!mAnimating || rate > mRate || (target <= mCurrentValue && mCurrentValue <= mTargetValue) || (mTargetValue <= mCurrentValue && mCurrentValue <= target)) {
    mRate=rate;
  }
  final boolean changed=(mTargetValue != target);
  mTargetValue=target;
  if (!mAnimating && target != mCurrentValue) {
    mAnimating=true;
    mAnimatedValue=mCurrentValue;
    mLastFrameTimeNanos=System.nanoTime();
    postCallback();
  }
  return changed;
}
