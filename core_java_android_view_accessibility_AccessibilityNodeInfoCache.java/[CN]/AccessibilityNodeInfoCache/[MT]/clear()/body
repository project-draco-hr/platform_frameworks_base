{
  if (ENABLED) {
synchronized (mLock) {
      if (DEBUG) {
        Log.i(LOG_TAG,"clear()");
      }
      final int nodeCount=mCacheImpl.size();
      for (int i=0; i < nodeCount; i++) {
        AccessibilityNodeInfo info=mCacheImpl.valueAt(i);
        info.recycle();
      }
      mCacheImpl.clear();
    }
  }
}
