{
  if (mService != null) {
    try {
      return mService.getAuthenticatorId(mContext.getOpPackageName());
    }
 catch (    RemoteException e) {
      Log.v(TAG,"Remote exception in getAuthenticatorId(): ",e);
    }
  }
 else {
    Log.w(TAG,"getAuthenticatorId(): Service not connected!");
  }
  return 0;
}
