{
  try {
    int res=mPM.installExistingPackageAsUser(packageName,UserHandle.myUserId());
    if (res == INSTALL_FAILED_INVALID_URI) {
      throw new NameNotFoundException("Package " + packageName + " doesn't exist");
    }
    return res;
  }
 catch (  RemoteException e) {
    throw new NameNotFoundException("Package " + packageName + " doesn't exist");
  }
}
