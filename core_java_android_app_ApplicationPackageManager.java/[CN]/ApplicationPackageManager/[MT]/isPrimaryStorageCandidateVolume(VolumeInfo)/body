{
  if (VolumeInfo.ID_PRIVATE_INTERNAL.equals(vol.getId())) {
    return true;
  }
  if (!vol.isMountedWritable()) {
    return false;
  }
  if ((vol.getType() == VolumeInfo.TYPE_PUBLIC) && vol.getDisk().isDefaultPrimary()) {
    return true;
  }
  return (vol.getType() == VolumeInfo.TYPE_PRIVATE);
}
