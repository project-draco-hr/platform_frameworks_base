{
  try {
    ParceledListSlice<ResolveInfo> parceledList=mPM.queryIntentReceivers(intent,intent.resolveTypeIfNeeded(mContext.getContentResolver()),flags,userId);
    if (parceledList == null) {
      return Collections.emptyList();
    }
    return parceledList.getList();
  }
 catch (  RemoteException e) {
    throw e.rethrowFromSystemServer();
  }
}
