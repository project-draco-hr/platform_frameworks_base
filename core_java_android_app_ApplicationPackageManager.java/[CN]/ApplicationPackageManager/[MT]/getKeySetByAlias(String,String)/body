{
  Preconditions.checkNotNull(packageName);
  Preconditions.checkNotNull(alias);
  IBinder keySetToken;
  try {
    keySetToken=mPM.getKeySetByAlias(packageName,alias);
  }
 catch (  RemoteException e) {
    return null;
  }
  if (keySetToken == null) {
    return null;
  }
  return new KeySet(keySetToken);
}
