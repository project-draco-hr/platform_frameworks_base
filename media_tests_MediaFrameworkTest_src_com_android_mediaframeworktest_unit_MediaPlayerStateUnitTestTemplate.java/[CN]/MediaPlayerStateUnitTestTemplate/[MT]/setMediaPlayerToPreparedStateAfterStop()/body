{
  try {
    mMediaPlayer.reset();
    mMediaPlayer.setDataSource(TEST_PATH);
    mMediaPlayer.prepare();
    mMediaPlayer.start();
    mMediaPlayer.stop();
    mMediaPlayer.prepare();
  }
 catch (  Exception e) {
    Log.v(TAG,"setMediaPlayerToPreparedStateAfterStop: Exception " + e.getClass().getName() + " was thrown.");
    assertTrue(false);
  }
}
