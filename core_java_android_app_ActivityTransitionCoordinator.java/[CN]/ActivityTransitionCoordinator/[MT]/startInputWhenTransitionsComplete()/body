{
  if (mViewsTransitionComplete && mSharedElementTransitionComplete) {
    final View decor=getDecor();
    if (decor != null) {
      final ViewRootImpl viewRoot=decor.getViewRootImpl();
      if (viewRoot != null) {
        viewRoot.setPausedForTransition(false);
      }
    }
    onTransitionsComplete();
  }
}
