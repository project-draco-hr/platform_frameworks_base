{
  if (transition1 == null) {
    return transition2;
  }
 else   if (transition2 == null) {
    return transition1;
  }
 else {
    TransitionSet transitionSet=new TransitionSet();
    transitionSet.addTransition(transition1);
    transitionSet.addTransition(transition2);
    return transitionSet;
  }
}
