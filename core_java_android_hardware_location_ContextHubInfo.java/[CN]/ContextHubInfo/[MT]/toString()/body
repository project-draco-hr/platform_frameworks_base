{
  String retVal="";
  retVal+="Id : " + mId;
  retVal+=", Name : " + mName;
  retVal+="\n\tVendor : " + mVendor;
  retVal+=", ToolChain : " + mToolchain;
  retVal+="\n\tPlatformVersion : " + mPlatformVersion;
  retVal+=", StaticSwVersion : " + mStaticSwVersion;
  retVal+="\n\tPeakMips : " + mPeakMips;
