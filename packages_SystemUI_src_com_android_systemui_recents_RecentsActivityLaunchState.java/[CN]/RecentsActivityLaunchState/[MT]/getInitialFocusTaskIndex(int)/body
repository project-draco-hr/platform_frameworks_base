{
  RecentsDebugFlags debugFlags=Recents.getDebugFlags();
  if (launchedFromAppWithThumbnail) {
    if (debugFlags.isFastToggleRecentsEnabled()) {
      return numTasks - 1;
    }
    return numTasks - 2;
  }
 else {
    if (debugFlags.isFastToggleRecentsEnabled()) {
      return -1;
    }
    return numTasks - 1;
  }
}
