{
  if (Constants.DebugFlags.App.EnableFastToggleRecents && !launchedWithAltTab) {
    if (launchedFromHome) {
      return numTasks - 1;
    }
 else {
      return numTasks - 2;
    }
  }
  if (launchedWithAltTab && launchedFromAppWithThumbnail) {
    return numTasks - 2;
  }
 else   if ((launchedWithAltTab && launchedFromHome) || (!launchedWithAltTab && launchedFromAppWithThumbnail)) {
    return numTasks - 1;
  }
 else {
    return -1;
  }
}
