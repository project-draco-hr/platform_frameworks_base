{
switch (msg.what) {
case SHOW:
    showH(msg.arg1);
  break;
case DISMISS:
dismissH(msg.arg1);
break;
case RECHECK:
recheckH((VolumeRow)msg.obj);
break;
case RECHECK_ALL:
recheckH(null);
break;
case SET_STREAM_IMPORTANT:
setStreamImportantH(msg.arg1,msg.arg2 != 0);
break;
case RESCHEDULE_TIMEOUT:
rescheduleTimeoutH();
break;
}
}
