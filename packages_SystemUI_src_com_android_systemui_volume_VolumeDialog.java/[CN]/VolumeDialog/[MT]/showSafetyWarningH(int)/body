{
  if ((flags & (AudioManager.FLAG_SHOW_UI | AudioManager.FLAG_SHOW_UI_WARNINGS)) != 0 || mShowing) {
synchronized (mSafetyWarningLock) {
      if (mSafetyWarning != null) {
        return;
      }
      mSafetyWarning=new SafetyWarningDialog(mContext,mController.getAudioManager()){
        @Override protected void cleanUp(){
synchronized (mSafetyWarningLock) {
            mSafetyWarning=null;
          }
          recheckH(null);
        }
      }
;
      mSafetyWarning.show();
    }
    recheckH(null);
  }
  rescheduleTimeoutH();
}
