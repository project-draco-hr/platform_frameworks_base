{
  String psStat=getPsStat("/" + pid);
  String[] statBreakDown=psStat.split(" +");
  long psTime;
  if (pid.isEmpty()) {
    psTime=Long.parseLong(statBreakDown[1]) + Long.parseLong(statBreakDown[2]) + Long.parseLong(statBreakDown[3])+ Long.parseLong(statBreakDown[4]);
  }
 else {
    psTime=Long.parseLong(statBreakDown[13]) + Long.parseLong(statBreakDown[14]);
  }
  return psTime;
}
