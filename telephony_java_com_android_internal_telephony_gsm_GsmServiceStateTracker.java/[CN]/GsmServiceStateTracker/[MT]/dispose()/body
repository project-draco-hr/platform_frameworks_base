{
  cm.unregisterForAvailable(this);
  cm.unregisterForRadioStateChanged(this);
  cm.unregisterForNetworkStateChanged(this);
  cm.unregisterForSIMReady(this);
  phone.mSIMRecords.unregisterForRecordsLoaded(this);
