{
  String ret="unknown";
switch (type) {
case DATA_ACCESS_GPRS:
    ret="GPRS";
  break;
case DATA_ACCESS_EDGE:
ret="EDGE";
break;
case DATA_ACCESS_UMTS:
ret="UMTS";
break;
case DATA_ACCESS_HSDPA:
ret="HSDPA";
break;
case DATA_ACCESS_HSUPA:
ret="HSUPA";
break;
case DATA_ACCESS_HSPA:
ret="HSPA";
break;
default :
Log.e(LOG_TAG,"Wrong network type: " + Integer.toString(type));
break;
}
return ret;
}
