{
  if (packageName == null) {
    return false;
  }
synchronized (mLock) {
    if (mDeviceOwnerPackage != null) {
      if ((mDeviceOwnerUserId == userId) && (packageName.equals(mDeviceOwnerPackage))) {
        return true;
      }
    }
    if (mProfileOwnerPackages != null) {
      if (packageName.equals(mProfileOwnerPackages.get(userId))) {
        return true;
      }
    }
  }
  return false;
}
