{
synchronized (sPolicies) {
    for (    Policy policy : sPolicies) {
      String seinfo=policy.getMatchedSeinfo(pkg);
      if (seinfo != null) {
        pkg.applicationInfo.seinfo=seinfo;
        if (DEBUG_POLICY_INSTALL) {
          Slog.i(TAG,"package (" + pkg.packageName + ") labeled with "+ "seinfo="+ seinfo);
        }
        return true;
      }
    }
  }
  if (DEBUG_POLICY_INSTALL) {
    Slog.i(TAG,"package (" + pkg.packageName + ") doesn't match any policy; "+ "seinfo will remain null");
  }
  return false;
}
