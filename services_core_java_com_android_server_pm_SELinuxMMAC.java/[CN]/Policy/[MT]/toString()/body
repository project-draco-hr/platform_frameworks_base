{
  StringBuilder sb=new StringBuilder();
  for (  Signature cert : mCerts) {
    sb.append("cert=" + cert.toCharsString().substring(0,11) + "... ");
  }
  if (mSeinfo != null) {
    sb.append("seinfo=" + mSeinfo);
  }
  for (  String name : mPkgMap.keySet()) {
    sb.append(" " + name + "="+ mPkgMap.get(name));
  }
  return sb.toString();
}
