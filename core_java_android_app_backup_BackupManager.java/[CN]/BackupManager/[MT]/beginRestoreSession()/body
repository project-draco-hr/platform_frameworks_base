{
  RestoreSession session=null;
  checkServiceBinder();
  if (sService != null) {
    try {
      IRestoreSession binder=sService.beginRestoreSession(null,null);
      if (binder != null) {
        session=new RestoreSession(mContext,binder);
      }
    }
 catch (    RemoteException e) {
      Log.w(TAG,"beginRestoreSession() couldn't connect");
    }
  }
  return session;
}
