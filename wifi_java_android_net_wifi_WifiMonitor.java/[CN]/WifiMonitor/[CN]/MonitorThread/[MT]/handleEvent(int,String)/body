{
switch (event) {
case DISCONNECTED:
    handleNetworkStateChange(NetworkInfo.DetailedState.DISCONNECTED,remainder);
  break;
case CONNECTED:
handleNetworkStateChange(NetworkInfo.DetailedState.CONNECTED,remainder);
break;
case SCAN_RESULTS:
mWifiStateMachine.notifyScanResultsAvailable();
break;
case UNKNOWN:
break;
}
}
