{
  float dx=v1.x - v2.x;
  float dy=v1.y - v2.y;
  float dz=v1.z - v2.z;
  float len=(float)java.lang.Math.sqrt(dx * dx + dy * dy + dz * dz);
  dx/=len;
  dy/=len;
  dz/=len;
  nx=dx * dz;
  ny=dy * dz;
  nz=(float)java.lang.Math.sqrt(dx * dx + dy * dy);
  len=(float)java.lang.Math.sqrt(nx * nx + ny * ny + nz * nz);
  nx/=len;
  ny/=len;
  nz/=len;
}
