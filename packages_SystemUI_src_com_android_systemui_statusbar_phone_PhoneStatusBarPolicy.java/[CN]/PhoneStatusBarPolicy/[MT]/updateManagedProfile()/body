{
  if (DEBUG)   Log.v(TAG,"updateManagedProfile: mManagedProfileFocused: " + mManagedProfileFocused + " mKeyguardVisible: "+ mKeyguardVisible);
  boolean showIcon=mManagedProfileFocused && !mKeyguardVisible;
  if (mManagedProfileIconVisible != showIcon) {
    mService.setIconVisibility(SLOT_MANAGED_PROFILE,showIcon);
    mManagedProfileIconVisible=showIcon;
  }
}
