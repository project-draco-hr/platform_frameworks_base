{
  if (DBG)   Slog.d(TAG,"resolveService: " + resolveId + " "+ service);
  try {
    mNativeConnector.execute("mdnssd","resolve",resolveId,service.getServiceName(),service.getServiceType(),"local.");
  }
 catch (  NativeDaemonConnectorException e) {
    Slog.e(TAG,"Failed to resolveService " + e);
    return false;
  }
  return true;
}
