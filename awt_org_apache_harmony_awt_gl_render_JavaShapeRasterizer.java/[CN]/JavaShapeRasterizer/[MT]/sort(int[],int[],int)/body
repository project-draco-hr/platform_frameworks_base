{
  for (int i=0; i < length - 1; i++) {
    int num=i;
    int min=master[num];
    for (int j=i + 1; j < length; j++) {
      if (master[j] < min) {
        num=j;
        min=master[num];
      }
    }
    if (num != i) {
      master[num]=master[i];
      master[i]=min;
      min=slave[num];
      slave[num]=slave[i];
      slave[i]=min;
    }
  }
}
