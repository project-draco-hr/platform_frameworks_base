{
synchronized (mLock) {
    int requestId;
    try {
      requestId=mRemoteDevice.submitRequest(request,repeating);
    }
 catch (    CameraRuntimeException e) {
      throw e.asChecked();
    }
catch (    RemoteException e) {
      return;
    }
    mCaptureListenerMap.put(requestId,new CaptureListenerHolder(listener,request,repeating));
    if (repeating) {
      mRepeatingRequestIdStack.add(requestId);
    }
  }
}
