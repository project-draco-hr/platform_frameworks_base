{
  CameraMetadataNative info=new CameraMetadataNative();
  try {
    mRemoteDevice.getCameraInfo(info);
  }
 catch (  CameraRuntimeException e) {
    throw e.asChecked();
  }
catch (  RemoteException e) {
    return null;
  }
  CameraProperties properties=new CameraProperties(info);
  return properties;
}
