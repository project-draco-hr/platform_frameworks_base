{
synchronized (mLock) {
    checkIfCameraClosed();
    while (!mRepeatingRequestIdStack.isEmpty()) {
      int requestId=mRepeatingRequestIdStack.pop();
      try {
        mRemoteDevice.cancelRequest(requestId);
      }
 catch (      CameraRuntimeException e) {
        throw e.asChecked();
      }
catch (      RemoteException e) {
        return;
      }
    }
  }
}
