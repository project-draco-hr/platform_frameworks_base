{
  if (isError) {
    mFutureErrorSet.add(frameNumber);
  }
 else {
    if (frameNumber != mCompletedFrameNumber + 1) {
      throw new AssertionError(String.format("result frame number %d comes out of order, should be %d + 1",frameNumber,mCompletedFrameNumber));
    }
    mCompletedFrameNumber++;
  }
  update();
}
