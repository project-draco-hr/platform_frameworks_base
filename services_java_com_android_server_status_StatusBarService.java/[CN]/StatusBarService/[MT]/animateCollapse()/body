{
  if (SPEW) {
    Log.d(TAG,"animateCollapse(): mExpanded=" + mExpanded + " mExpandedVisible="+ mExpandedVisible+ " mAnimating="+ mAnimating+ " mAnimVel="+ mAnimVel);
  }
  if (!mExpandedVisible) {
    return;
  }
  if (mAnimating) {
    return;
  }
  int y=mDisplay.getHeight() - 1;
  prepareTracking(y);
  performFling(y,-2000.0f,true);
}
