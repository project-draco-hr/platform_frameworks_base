{
  int threadId=getCallingThreadId();
  Session threadSession=mSessionMapper.get(threadId);
  if (threadSession == null) {
    android.util.Slog.d(LOGGING_TAG,"Log.createSubsession was called with no session " + "active.");
    return null;
  }
  Session newSubsession=new Session(threadSession.getNextChildId(),threadSession.getShortMethodName(),System.currentTimeMillis(),threadId,isStartedFromActiveSession,null);
  threadSession.addChild(newSubsession);
  newSubsession.setParentSession(threadSession);
  if (!isStartedFromActiveSession) {
    android.util.Slog.v(LOGGING_TAG,Session.CREATE_SUBSESSION + " " + newSubsession.toString());
  }
 else {
    android.util.Slog.v(LOGGING_TAG,Session.CREATE_SUBSESSION + " (Invisible subsession)");
  }
  return newSubsession;
}
