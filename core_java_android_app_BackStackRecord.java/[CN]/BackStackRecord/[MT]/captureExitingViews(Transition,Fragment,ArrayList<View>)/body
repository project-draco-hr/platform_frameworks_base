{
  if (exitTransition != null) {
    View root=outFragment.getView();
    viewList.clear();
    root.captureTransitioningViews(viewList);
    if (viewList.isEmpty()) {
      exitTransition=null;
    }
 else {
      addTransitioningViews(exitTransition,viewList);
    }
  }
  return exitTransition;
}
