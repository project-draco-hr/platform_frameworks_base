{
  StringBuilder sb=new StringBuilder(128);
  sb.append("BackStackEntry{");
  sb.append(Integer.toHexString(System.identityHashCode(this)));
  if (mIndex >= 0) {
    sb.append(" #");
    sb.append(mIndex);
  }
  if (mName != null) {
    sb.append(" ");
    sb.append(mName);
  }
  sb.append("}");
  return sb.toString();
}
