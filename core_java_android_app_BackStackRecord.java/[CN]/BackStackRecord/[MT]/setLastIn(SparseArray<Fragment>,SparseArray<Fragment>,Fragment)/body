{
  if (fragment != null) {
    int containerId=fragment.mContainerId;
    if (containerId != 0) {
      if (!fragment.isAdded()) {
        lastInFragments.put(containerId,fragment);
      }
      if (firstOutFragments.get(containerId) == fragment) {
        firstOutFragments.remove(containerId);
      }
    }
  }
}
