{
  if (phone != null && !mPhones.contains(phone)) {
    if (mPhones.isEmpty()) {
      mDefaultPhone=phone;
    }
    mPhones.add(phone);
    mRingingCalls.add(phone.getRingingCall());
    mBackgroundCalls.add(phone.getBackgroundCall());
    mForegroundCalls.add(phone.getForegroundCall());
    registerForPhoneStates(phone);
    return true;
  }
  return false;
}
