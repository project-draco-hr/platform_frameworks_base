{
  Phone activePhone=null;
  Phone heldPhone=null;
  if (hasActiveFgCall()) {
    activePhone=getActiveFgCall().getPhone();
  }
  if (heldCall != null) {
    heldPhone=heldCall.getPhone();
  }
  if (activePhone != heldPhone) {
    activePhone.switchHoldingAndActive();
  }
  heldPhone.switchHoldingAndActive();
}
