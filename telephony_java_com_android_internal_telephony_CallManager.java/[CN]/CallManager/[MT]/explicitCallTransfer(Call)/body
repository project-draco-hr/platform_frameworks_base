{
  if (VDBG) {
    Log.d(LOG_TAG," explicitCallTransfer(" + heldCall + ")");
    Log.d(LOG_TAG,this.toString());
  }
  if (canTransfer(heldCall)) {
    heldCall.getPhone().explicitCallTransfer();
  }
  if (VDBG) {
    Log.d(LOG_TAG,"End explicitCallTransfer(" + heldCall + ")");
    Log.d(LOG_TAG,this.toString());
  }
}
