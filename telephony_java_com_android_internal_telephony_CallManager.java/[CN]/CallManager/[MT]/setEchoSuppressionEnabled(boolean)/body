{
  if (VDBG) {
    Log.d(LOG_TAG," setEchoSuppression(" + enabled + ")");
    Log.d(LOG_TAG,this.toString());
  }
  if (hasActiveFgCall()) {
    getActiveFgCall().getPhone().setEchoSuppressionEnabled(enabled);
  }
  if (VDBG) {
    Log.d(LOG_TAG,"End setEchoSuppression(" + enabled + ")");
    Log.d(LOG_TAG,this.toString());
  }
}
