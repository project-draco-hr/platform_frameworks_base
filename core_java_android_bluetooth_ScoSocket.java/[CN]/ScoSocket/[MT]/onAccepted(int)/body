{
  if (VDBG)   log("onAccepted() " + this);
  if (mState != STATE_ACCEPT) {
    if (DBG)     log("Strange state " + this);
    return;
  }
  if (result >= 0) {
    mState=STATE_CONNECTED;
  }
 else {
    mState=STATE_CLOSED;
  }
  mHandler.obtainMessage(mAcceptedCode,mState,-1,this).sendToTarget();
}
