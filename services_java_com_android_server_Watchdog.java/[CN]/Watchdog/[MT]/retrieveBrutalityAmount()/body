{
  mMinScreenOff=(mReqMinScreenOff >= 0 ? mReqMinScreenOff : Settings.Gservices.getInt(mResolver,Settings.Gservices.MEMCHECK_MIN_SCREEN_OFF,MEMCHECK_DEFAULT_MIN_SCREEN_OFF)) * 1000;
  mMinAlarm=(mReqMinNextAlarm >= 0 ? mReqMinNextAlarm : Settings.Gservices.getInt(mResolver,Settings.Gservices.MEMCHECK_MIN_ALARM,MEMCHECK_DEFAULT_MIN_ALARM)) * 1000;
}
