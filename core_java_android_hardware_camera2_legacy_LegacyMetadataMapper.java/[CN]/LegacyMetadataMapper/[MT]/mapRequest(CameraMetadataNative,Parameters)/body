    if (p.getMaxNumMeteringAreas() > 0) {
      availableKeys.add(CaptureResult.CONTROL_AE_REGIONS);
    }
    if (p.getMaxNumFocusAreas() > 0) {
      availableKeys.add(CaptureResult.CONTROL_AF_REGIONS);
    }
