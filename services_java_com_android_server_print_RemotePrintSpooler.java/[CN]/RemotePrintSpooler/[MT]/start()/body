{
  throwIfCalledOnMainThread();
synchronized (mLock) {
    throwIfDestroyedLocked();
    mCanUnbind=false;
  }
  try {
    getRemoteInstanceLazy();
  }
 catch (  TimeoutException te) {
    Slog.e(LOG_TAG,"Error starting the spooler.",te);
  }
 finally {
    if (DEBUG) {
      Slog.i(LOG_TAG,"[user: " + mUserHandle.getIdentifier() + "] start()");
    }
synchronized (mLock) {
      mCanUnbind=true;
      mLock.notifyAll();
    }
  }
}
