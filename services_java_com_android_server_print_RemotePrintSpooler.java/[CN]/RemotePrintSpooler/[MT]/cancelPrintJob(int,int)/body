{
  throwIfCalledOnMainThread();
synchronized (mLock) {
    throwIfDestroyedLocked();
    mCanUnbind=false;
  }
  if (DEBUG) {
    Slog.i(LOG_TAG,"[user: " + mUserHandle.getIdentifier() + "] cancelPrintJob()");
  }
  try {
    return mCancelPrintJobCaller.cancelPrintJob(getRemoteInstanceLazy(),printJobId,appId);
  }
 catch (  RemoteException re) {
    Slog.e(LOG_TAG,"Error canceling print job.",re);
  }
catch (  TimeoutException te) {
    Slog.e(LOG_TAG,"Error canceling print job.",te);
  }
 finally {
synchronized (mLock) {
      mCanUnbind=true;
      mLock.notifyAll();
    }
  }
  return false;
}
