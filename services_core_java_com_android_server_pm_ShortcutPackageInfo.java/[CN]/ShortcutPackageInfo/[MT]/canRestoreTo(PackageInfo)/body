{
  if (target.versionCode < mVersionCode) {
    Slog.w(TAG,String.format("Package current version %d < backed up version %d",target.versionCode,mVersionCode));
    return false;
  }
  if (!signaturesMatch(mSigHashes,target)) {
    Slog.w(TAG,"Package signature mismtach");
    return false;
  }
  return true;
}
