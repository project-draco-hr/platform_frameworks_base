{
  LruCache<String,String> cache=new LruCache<String,String>(3){
    @Override protected void entryRemoved(    boolean evicted,    String key,    String oldValue,    String newValue){
      assertFalse(Thread.holdsLock(this));
    }
  }
;
  cache.put("a","A");
  cache.put("a","A2");
  cache.put("b","B");
  cache.put("c","C");
  cache.put("d","D");
  cache.remove("a");
  cache.evictAll();
}
