{
  if (tag != null && tag.equals(mTag)) {
    return this;
  }
  final View[] where=mChildren;
  final int len=mChildrenCount;
  for (int i=0; i < len; i++) {
    View v=where[i];
    if ((v.mPrivateFlags & PFLAG_IS_ROOT_NAMESPACE) == 0) {
      v=v.findViewWithTag(tag);
      if (v != null) {
        return v;
      }
    }
  }
  return null;
}
