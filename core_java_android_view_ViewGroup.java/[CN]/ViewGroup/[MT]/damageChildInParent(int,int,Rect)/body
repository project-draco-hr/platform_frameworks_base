{
  if ((mPrivateFlags & PFLAG_DRAWN) != 0 || (mPrivateFlags & PFLAG_DRAWING_CACHE_VALID) != 0) {
    dirty.offset(left - mScrollX,top - mScrollY);
    if ((mGroupFlags & FLAG_CLIP_CHILDREN) == 0) {
      dirty.union(0,0,mRight - mLeft,mBottom - mTop);
    }
    if ((mGroupFlags & FLAG_CLIP_CHILDREN) == 0 || dirty.intersect(0,0,mRight - mLeft,mBottom - mTop)) {
      if (!getMatrix().isIdentity()) {
        transformRect(dirty);
      }
      return mParent;
    }
  }
  return null;
}
