{
  if (DBG) {
    System.out.println(this + " requestChildFocus()");
  }
  if (getDescendantFocusability() == FOCUS_BLOCK_DESCENDANTS) {
    return;
  }
  super.unFocus(focused);
  if (mFocused != child) {
    if (mFocused != null) {
      mFocused.unFocus(focused);
    }
    mFocused=child;
  }
  if (mParent != null) {
    mParent.requestChildFocus(this,focused);
  }
}
