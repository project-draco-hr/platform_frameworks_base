{
  if (predicate.apply(this)) {
    return this;
  }
  final View[] where=mChildren;
  final int len=mChildrenCount;
  for (int i=0; i < len; i++) {
    View v=where[i];
    if ((v.mPrivateFlags & IS_ROOT_NAMESPACE) == 0) {
      v=v.findViewByPredicate(predicate);
      if (v != null) {
        return v;
      }
    }
  }
  return null;
}
