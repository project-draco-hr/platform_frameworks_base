{
  if (!checkPartialLayoutParams(child,layoutParamsClass))   return axisFilter;
  if (child.didLayoutParamsChange()) {
    return axisFilter;
  }
  final LayoutParams lp=child.getLayoutParams();
  final int wrapAxisFilter=((lp.width == WRAP_CONTENT ? FLAG_LAYOUT_AXIS_HORIZONTAL : 0) | (lp.height == WRAP_CONTENT ? FLAG_LAYOUT_AXIS_VERTICAL : 0)) & axisFilter;
  if (wrapAxisFilter == axisFilter) {
    return wrapAxisFilter;
  }
  final int matchAxisFilter=((lp.width == MATCH_PARENT ? FLAG_LAYOUT_AXIS_HORIZONTAL : 0) | (lp.height == MATCH_PARENT ? FLAG_LAYOUT_AXIS_VERTICAL : 0)) & axisFilter;
  if (matchAxisFilter != 0 || wrapAxisFilter != 0) {
    final ViewParent parent=getParent();
    if (parent != null) {
      return getParent().findDependentLayoutAxes(this,matchAxisFilter) | wrapAxisFilter;
    }
    return matchAxisFilter | wrapAxisFilter;
  }
  return 0;
}
