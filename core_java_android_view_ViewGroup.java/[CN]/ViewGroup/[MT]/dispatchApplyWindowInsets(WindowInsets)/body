{
  insets=super.dispatchApplyWindowInsets(insets);
  if (!insets.isConsumed()) {
    final int count=getChildCount();
    for (int i=0; i < count; i++) {
      insets=getChildAt(i).dispatchApplyWindowInsets(insets);
      if (insets.isConsumed()) {
        break;
      }
    }
  }
  return insets;
}
