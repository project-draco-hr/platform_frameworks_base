{
  if (sDebugLines == null) {
    sDebugLines=new float[16];
  }
  sDebugLines[0]=x1;
  sDebugLines[1]=y1;
  sDebugLines[2]=x2;
  sDebugLines[3]=y1;
  sDebugLines[4]=x2;
  sDebugLines[5]=y1;
  sDebugLines[6]=x2;
  sDebugLines[7]=y2;
  sDebugLines[8]=x2;
  sDebugLines[9]=y2;
  sDebugLines[10]=x1;
  sDebugLines[11]=y2;
  sDebugLines[12]=x1;
  sDebugLines[13]=y2;
  sDebugLines[14]=x1;
  sDebugLines[15]=y1;
  canvas.drawLines(sDebugLines,paint);
}
