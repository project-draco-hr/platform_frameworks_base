{
  if (mTransientViews == null) {
    return;
  }
  final int size=mTransientViews.size();
  for (int i=0; i < size; ++i) {
    if (view == mTransientViews.get(i)) {
      mTransientViews.remove(i);
      mTransientIndices.remove(i);
      view.mParent=null;
      view.dispatchDetachedFromWindow();
      invalidate(true);
      return;
    }
  }
}
