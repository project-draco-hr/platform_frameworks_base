{
  ChildListForAccessibility list=null;
synchronized (sPoolLock) {
    if (sPool != null) {
      list=sPool;
      sPool=list.mNext;
      list.mNext=null;
      list.mIsPooled=false;
      sPoolSize--;
    }
 else {
      list=new ChildListForAccessibility();
    }
    list.init(parent,sort);
    return list;
  }
}
