{
  if (clipRect == null) {
    if (mClippedMatrixState.mClipRect != null) {
      mClippedMatrixState.mClipRect=null;
      invalidateSelf();
    }
  }
 else {
    if (mClippedMatrixState.mClipRect == null) {
      mClippedMatrixState.mClipRect=new Rect(clipRect);
    }
 else {
      mClippedMatrixState.mClipRect.set(clipRect);
    }
    invalidateSelf();
  }
}
