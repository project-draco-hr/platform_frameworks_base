{
  for (int quality=CamcorderProfile.QUALITY_HIGH_SPEED_480P; quality <= CamcorderProfile.QUALITY_HIGH_SPEED_2160P; quality++) {
    if (CamcorderProfile.hasProfile(quality)) {
      CamcorderProfile profile=CamcorderProfile.get(quality);
      if (size.equals(new Size(profile.videoFrameWidth,profile.videoFrameHeight))) {
        return profile.videoFrameRate;
      }
    }
  }
  return 0;
}
