{
  mContext=context;
  mHalDeviceId=halDeviceId;
  mToken=token;
  mReceiver=receiver;
  mCallingUserId=callingUserId;
  mGroupId=groupId;
  mIsRestricted=restricted;
  mOwner=owner;
  try {
    token.linkToDeath(this,0);
  }
 catch (  RemoteException e) {
    Slog.w(TAG,"caught remote exception in linkToDeath: ",e);
  }
}
