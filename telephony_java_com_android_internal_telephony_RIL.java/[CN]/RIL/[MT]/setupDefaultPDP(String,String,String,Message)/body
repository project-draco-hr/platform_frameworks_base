{
  int radioTechnology;
  int authType;
  String profile="";
  radioTechnology=RILConstants.SETUP_DATA_TECH_GSM;
  authType=(user != null) ? RILConstants.SETUP_DATA_AUTH_PAP_CHAP : RILConstants.SETUP_DATA_AUTH_NONE;
  setupDataCall(Integer.toString(radioTechnology),profile,apn,user,password,Integer.toString(authType),result);
