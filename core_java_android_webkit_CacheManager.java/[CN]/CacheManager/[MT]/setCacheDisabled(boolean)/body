{
  if (disabled == mDisabled) {
    return;
  }
  mDisabled=disabled;
  if (mDisabled) {
    removeAllCacheFiles();
  }
  if (!mDisabled && JniUtil.useChromiumHttpStack()) {
    Log.w(LOGTAG,"CacheManager enabled, but it will not work as " + "expected because the Chrome HTTP stack is in use.");
  }
}
