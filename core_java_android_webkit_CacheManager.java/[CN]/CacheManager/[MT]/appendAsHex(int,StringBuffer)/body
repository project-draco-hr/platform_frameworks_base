{
  String hex=Integer.toHexString(i);
switch (hex.length()) {
case 1:
    ret.append("0000000");
  break;
case 2:
ret.append("000000");
break;
case 3:
ret.append("00000");
break;
case 4:
ret.append("0000");
break;
case 5:
ret.append("000");
break;
case 6:
ret.append("00");
break;
case 7:
ret.append("0");
break;
}
ret.append(hex);
}
