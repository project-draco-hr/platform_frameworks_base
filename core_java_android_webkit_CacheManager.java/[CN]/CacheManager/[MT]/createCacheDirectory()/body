{
  if (!mBaseDir.exists()) {
    if (!mBaseDir.mkdirs()) {
      Log.w(LOGTAG,"Unable to create webviewCache directory");
      return false;
    }
    FileUtils.setPermissions(mBaseDir.toString(),FileUtils.S_IRWXU | FileUtils.S_IRWXG,-1,-1);
    WebViewWorker.getHandler().sendEmptyMessage(WebViewWorker.MSG_CLEAR_CACHE);
    return true;
  }
  return false;
}
