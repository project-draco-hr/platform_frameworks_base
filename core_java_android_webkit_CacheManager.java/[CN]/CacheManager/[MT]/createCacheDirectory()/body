{
  if (!mBaseDir.exists()) {
    if (!mBaseDir.mkdirs()) {
      Log.w(LOGTAG,"Unable to create webviewCache directory");
      return false;
    }
    FileUtils.setPermissions(mBaseDir.toString(),FileUtils.S_IRWXU | FileUtils.S_IRWXG | FileUtils.S_IXOTH,-1,-1);
    WebViewCore.endCacheTransaction();
    mDataBase.clearCache();
    WebViewCore.startCacheTransaction();
    return true;
  }
  return false;
}
