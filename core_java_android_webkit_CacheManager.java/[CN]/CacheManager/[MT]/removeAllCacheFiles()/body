{
  final Runnable clearCache=new Runnable(){
    public void run(){
      try {
        String[] files=mBaseDir.list();
        for (int i=0; i < files.length; i++) {
          new File(mBaseDir,files[i]).delete();
        }
      }
 catch (      SecurityException e) {
      }
      mDataBase.clearCache();
    }
  }
;
  new Thread(clearCache).start();
  return true;
}
