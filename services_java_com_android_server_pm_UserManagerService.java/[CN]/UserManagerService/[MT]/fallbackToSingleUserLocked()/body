{
  UserInfo primary=new UserInfo(0,mContext.getResources().getString(com.android.internal.R.string.owner_name),null,UserInfo.FLAG_ADMIN | UserInfo.FLAG_PRIMARY | UserInfo.FLAG_INITIALIZED);
  mUsers.put(0,primary);
  mNextSerialNumber=MIN_USER_ID;
  updateUserIdsLocked();
  writeUserListLocked();
  writeUserLocked(primary);
}
