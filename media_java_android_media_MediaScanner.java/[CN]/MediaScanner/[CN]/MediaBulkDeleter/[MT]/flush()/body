{
  int size=whereArgs.size();
  if (size > 0) {
    String[] foo=new String[size];
    foo=whereArgs.toArray(foo);
    int numrows=mProvider.delete(mPackageName,mBaseUri,MediaStore.MediaColumns._ID + " IN (" + whereClause.toString()+ ")",foo);
    whereClause.setLength(0);
    whereArgs.clear();
  }
}
