{
  if (mFileType == MediaFile.FILE_TYPE_MP3 || mFileType == MediaFile.FILE_TYPE_MP4 || mFileType == MediaFile.FILE_TYPE_M4A || mFileType == MediaFile.FILE_TYPE_3GPP || mFileType == MediaFile.FILE_TYPE_3GPP2 || mFileType == MediaFile.FILE_TYPE_OGG || mFileType == MediaFile.FILE_TYPE_AAC || mFileType == MediaFile.FILE_TYPE_MID || mFileType == MediaFile.FILE_TYPE_WMA) {
    return true;
  }
  return false;
}
