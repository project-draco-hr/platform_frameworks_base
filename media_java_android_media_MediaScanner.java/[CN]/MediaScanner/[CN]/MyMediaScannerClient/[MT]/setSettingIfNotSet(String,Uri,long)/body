{
  if (wasSettingAlreadySet(settingName)) {
    return;
  }
  String existingSettingValue=Settings.System.getString(mContext.getContentResolver(),settingName);
  if (TextUtils.isEmpty(existingSettingValue)) {
    ContentResolver cr=mContext.getContentResolver();
    Settings.System.putString(cr,settingName,ContentUris.withAppendedId(uri,rowId).toString());
    Settings.System.putInt(cr,settingSetIndicatorName(settingName),1);
  }
}
