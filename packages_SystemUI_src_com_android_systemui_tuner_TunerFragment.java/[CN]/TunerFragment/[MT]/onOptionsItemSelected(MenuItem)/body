{
switch (item.getItemId()) {
case android.R.id.home:
    getActivity().finish();
  return true;
case MENU_REMOVE:
TunerService.showResetRequest(getContext(),new Runnable(){
  @Override public void run(){
    getActivity().finish();
  }
}
);
return true;
}
return super.onOptionsItemSelected(item);
}
