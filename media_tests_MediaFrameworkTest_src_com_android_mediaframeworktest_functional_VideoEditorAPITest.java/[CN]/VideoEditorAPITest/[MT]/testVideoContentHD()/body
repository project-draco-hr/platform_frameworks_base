{
  final String videoItemFileName1=INPUT_FILE_PATH + "H264_BP_1920x1080_30fps_1200Kbps_1_10.mp4";
  final int renderingMode=MediaItem.RENDERING_MODE_BLACK_BORDER;
  final MediaVideoItem mediaVideoItem1;
  boolean flagForException=false;
  try {
    mediaVideoItem1=mVideoEditorHelper.createMediaItem(mVideoEditor,"m1",videoItemFileName1,renderingMode);
  }
 catch (  IllegalArgumentException e) {
    flagForException=true;
  }
  assertTrue("VideoContent 1920x1080",flagForException);
}
