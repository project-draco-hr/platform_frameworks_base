{
  byte[] storedDigest=new byte[DIGEST_SIZE_BYTES];
synchronized (mLock) {
    byte[] digest=computeDigestLocked(storedDigest);
    if (digest == null || !Arrays.equals(storedDigest,digest)) {
      Slog.i(TAG,"Formatting FRP partition...");
      formatPartitionLocked();
      return false;
    }
  }
  return true;
}
