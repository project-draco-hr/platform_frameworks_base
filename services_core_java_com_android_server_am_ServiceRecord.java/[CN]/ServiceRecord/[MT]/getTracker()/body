{
  if (tracker != null) {
    return tracker;
  }
  if ((serviceInfo.applicationInfo.flags & ApplicationInfo.FLAG_PERSISTENT) == 0) {
    tracker=ams.mProcessStats.getServiceStateLocked(serviceInfo.packageName,serviceInfo.applicationInfo.uid,serviceInfo.processName,serviceInfo.name);
    tracker.applyNewOwner(this);
  }
  return tracker;
}
