{
  if (foregroundId == 0) {
    return;
  }
  final int localForegroundId=foregroundId;
  final int localUserId=userId;
  final String localPackageName=packageName;
  ams.mHandler.post(new Runnable(){
    @Override public void run(){
      NotificationManagerInternal nmi=LocalServices.getService(NotificationManagerInternal.class);
      if (nmi == null) {
        return;
      }
      nmi.removeForegroundServiceFlagFromNotification(localPackageName,localForegroundId,localUserId);
    }
  }
);
}
