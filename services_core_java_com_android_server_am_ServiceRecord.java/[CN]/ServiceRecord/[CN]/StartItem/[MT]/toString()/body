{
  if (stringName != null) {
    return stringName;
  }
  StringBuilder sb=new StringBuilder(128);
  sb.append("ServiceRecord{").append(Integer.toHexString(System.identityHashCode(sr))).append(' ').append(sr.shortName).append(" StartItem ").append(Integer.toHexString(System.identityHashCode(this))).append(" id=").append(id).append('}');
  return stringName=sb.toString();
}
