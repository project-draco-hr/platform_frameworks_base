{
  mBootCompleted=true;
  List<UserInfo> users=getAllUsers();
  if (users != null) {
    for (    UserInfo user : users) {
      mSyncStorageEngine.doDatabaseCleanup(AccountManagerService.getSingleton().getAccounts(user.id),user.id);
    }
  }
  if (mReadyToRunLatch != null) {
    mReadyToRunLatch.countDown();
  }
}
