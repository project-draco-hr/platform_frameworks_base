{
  byte[] extrasData=cursor.getBlob(COLUMN_EXTRAS);
  Bundle extras;
  Parcel parcel=Parcel.obtain();
  try {
    parcel.unmarshall(extrasData,0,extrasData.length);
    parcel.setDataPosition(0);
    extras=parcel.readBundle();
  }
 catch (  RuntimeException e) {
    extras=new Bundle();
  }
 finally {
    parcel.recycle();
  }
  SyncOperation syncOperation=new SyncOperation(new Account(cursor.getString(COLUMN_ACCOUNT),cursor.getString(COLUMN_ACCOUNT_TYPE)),cursor.getInt(COLUMN_SOURCE),cursor.getString(COLUMN_AUTHORITY),extras,0);
  syncOperation.rowId=cursor.getLong(COLUMN_ID);
  return syncOperation;
}
