{
  StringBuilder sb=new StringBuilder();
  dumpSyncState(sb);
  sb.append("\n");
  if (isSyncEnabled()) {
    dumpSyncHistory(sb);
  }
  pw.println(sb.toString());
  pw.println("SyncAdapters:");
  for (  RegisteredServicesCache.ServiceInfo info : mSyncAdapters.getAllServices()) {
    pw.println("  " + info);
  }
}
