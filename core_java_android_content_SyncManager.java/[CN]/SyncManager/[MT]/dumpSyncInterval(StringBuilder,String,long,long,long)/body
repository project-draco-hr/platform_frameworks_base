{
  sb.append(String.format(" | %-8s | %6d | %8.1f | %8.1f",label,numSyncs,((float)duration / numSyncs) / 1000,(float)duration / 1000));
  if (interval > 0) {
    sb.append(String.format(" | %13.2f |\n",((float)duration / interval) * 100.0));
  }
 else {
    sb.append(String.format(" | %13s |\n","na"));
  }
}
