{
  int len=source.length();
  char[] buf=obtain(len);
  if (start != 0) {
    getChars(source,0,start,buf,0);
  }
  if (end != len) {
    getChars(source,end,len,buf,end);
  }
  if (start != end) {
    buf[start]='\u2026';
    for (int i=start + 1; i < end; i++) {
      buf[i]='\uFEFF';
    }
  }
  String ret=new String(buf,0,len);
  recycle(buf);
  return ret;
}
