{
  String fList[]=mDir.list();
  if (fList == null) {
    return null;
  }
  ArrayList<String> fileList=new ArrayList<String>();
  for (  String file : fList) {
    if (!file.startsWith(FILE_PREFIX)) {
      continue;
    }
    if (file.endsWith(".bak")) {
      (new File(mDir,file)).delete();
      continue;
    }
    fileList.add(file);
  }
  return fileList;
}
