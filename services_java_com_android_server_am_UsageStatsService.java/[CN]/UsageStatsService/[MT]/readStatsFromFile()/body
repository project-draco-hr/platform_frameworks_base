{
  File newFile=mFile;
synchronized (mFileLock) {
    try {
      if (newFile.exists()) {
        readStatsFLOCK(newFile);
      }
 else {
        checkFileLimitFLOCK();
        newFile.createNewFile();
      }
    }
 catch (    IOException e) {
      Log.w(TAG,"Error : " + e + " reading data from file:"+ newFile);
    }
  }
}
