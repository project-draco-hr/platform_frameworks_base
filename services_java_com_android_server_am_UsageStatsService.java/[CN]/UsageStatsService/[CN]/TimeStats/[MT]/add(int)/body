{
  count++;
  if (size > MAX_LAUNCH_TIME_SAMPLES) {
    return;
  }
  if (size >= avail) {
    avail=((avail + 2) * 3) / 2;
    int[] newarray=new int[avail];
    if (array != null) {
      System.arraycopy(array,0,newarray,0,size);
    }
    array=newarray;
  }
  array[size]=val;
  size++;
  haveStats=false;
}
