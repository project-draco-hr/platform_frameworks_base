{
  try {
    String resourcePath=ProcessBindable.class.getPackage().getName().replace('.','/') + "/binding_properties.bin";
    Enumeration<URL> resources=getClass().getClassLoader().getResources(resourcePath);
    while (resources.hasMoreElements()) {
      URL url=resources.nextElement();
      System.out.println("Merging binding adapters from " + url);
      InputStream inputStream=null;
      try {
        inputStream=url.openStream();
        ObjectInputStream in=new ObjectInputStream(inputStream);
        Intermediate properties=(Intermediate)in.readObject();
        if (properties != null) {
          properties.captureProperties(intermediateProperties);
        }
      }
 catch (      IOException e) {
        System.err.println("Could not merge in Bindables from " + url + ": "+ e.getLocalizedMessage());
      }
catch (      ClassNotFoundException e) {
        System.err.println("Could not read Binding properties intermediate file: " + e.getLocalizedMessage());
      }
 finally {
        try {
          inputStream.close();
        }
 catch (        IOException e2) {
          System.err.println("Error closing intermediate Bindables store: " + e2.getLocalizedMessage());
        }
      }
    }
  }
 catch (  IOException e) {
    System.err.println("Could not read Binding properties intermediate file: " + e.getLocalizedMessage());
  }
}
