{
switch (element.getKind()) {
case FIELD:
    return stripPrefixFromField((VariableElement)element);
case METHOD:
  return stripPrefixFromMethod((ExecutableElement)element);
default :
processingEnv.getMessager().printMessage(Diagnostic.Kind.ERROR,"@Bindable is not allowed on " + element.getKind(),element);
return null;
}
}
