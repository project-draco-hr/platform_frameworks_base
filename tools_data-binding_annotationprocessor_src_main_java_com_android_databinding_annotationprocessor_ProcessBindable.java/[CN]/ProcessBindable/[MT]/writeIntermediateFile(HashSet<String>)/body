{
  try {
    File intermediate=getIntermediateFile();
    ObjectOutputStream out=new ObjectOutputStream(new FileOutputStream(intermediate));
    out.writeObject(properties);
    out.close();
  }
 catch (  IOException e) {
    processingEnv.getMessager().printMessage(Diagnostic.Kind.ERROR,"Could not write to intermediate file: " + e.getLocalizedMessage());
  }
}
