{
  try {
    FileObject intermediate=processingEnv.getFiler().createResource(StandardLocation.CLASS_OUTPUT,ProcessBindable.class.getPackage().getName(),"binding_properties.bin");
    ObjectOutputStream out=new ObjectOutputStream(intermediate.openOutputStream());
    out.writeObject(properties);
    out.close();
  }
 catch (  IOException e) {
    processingEnv.getMessager().printMessage(Diagnostic.Kind.ERROR,"Could not write to intermediate file: " + e.getLocalizedMessage());
  }
}
