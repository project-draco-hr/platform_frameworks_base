{
  boolean isEdu=false;
  if (isEdu) {
    return true;
  }
  List<UserInfo> users=mUserManager.getUsers(true);
  int N=users.size();
  int switchableUsers=0;
  for (int i=0; i < N; i++) {
    if (users.get(i).supportsSwitchTo()) {
      switchableUsers++;
    }
  }
  return switchableUsers > 1;
}
