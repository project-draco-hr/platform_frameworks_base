{
  if (mRtAnimator != null) {
    mRtAnimator.end();
    mRtAnimator=null;
  }
  if (canRunAsync()) {
    mRtAnimator=new RenderNodeAnimator(mX,mY,mStartRadius,mEndRadius);
    mRtAnimator.setDuration(getDuration());
    mRtAnimator.setInterpolator(getInterpolator());
    mRtAnimator.setTarget(mClipView);
    mRtAnimator.start();
  }
 else {
    super.start();
  }
}
