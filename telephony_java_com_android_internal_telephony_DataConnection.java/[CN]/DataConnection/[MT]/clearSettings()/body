{
  log("DataConnection.clearSettings()");
  this.state=State.INACTIVE;
  this.createTime=-1;
  this.lastFailTime=-1;
  this.lastFailCause=FailCause.NONE;
  receivedDisconnectReq=false;
  onConnectCompleted=null;
  interfaceName=null;
  ipAddress=null;
  gatewayAddress=null;
  dnsServers[0]=null;
  dnsServers[1]=null;
}
