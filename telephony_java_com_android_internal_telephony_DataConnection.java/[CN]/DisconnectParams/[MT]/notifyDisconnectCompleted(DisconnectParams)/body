{
  if (DBG)   log("NotifyDisconnectCompleted");
  if (dp.onCompletedMsg != null) {
    Message msg=dp.onCompletedMsg;
    log(String.format("msg.what=%d msg.obj=%s",msg.what,((msg.obj instanceof String) ? (String)msg.obj : "<no-reason>")));
    AsyncResult.forMessage(msg);
    msg.sendToTarget();
  }
  if (dp.lockObj != null) {
synchronized (dp.lockObj) {
      dp.lockObj.notify();
    }
  }
  clearSettings();
}
