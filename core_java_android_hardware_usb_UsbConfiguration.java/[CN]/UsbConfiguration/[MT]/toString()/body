{
  StringBuilder builder=new StringBuilder("UsbConfiguration[mId=" + mId + ",mName="+ mName+ ",mAttributes="+ mAttributes+ ",mMaxPower="+ mMaxPower+ ",mInterfaces=[");
  for (int i=0; i < mInterfaces.length; i++) {
    builder.append("\n");
    builder.append(mInterfaces[i].toString());
  }
  builder.append("]");
  return builder.toString();
}
