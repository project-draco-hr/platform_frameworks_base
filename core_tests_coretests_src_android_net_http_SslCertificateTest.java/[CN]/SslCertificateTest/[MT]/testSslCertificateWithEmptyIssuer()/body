{
  CertificateFactory certificateFactory=CertificateFactory.getInstance("X.509");
  X509Certificate x509Certificate=(X509Certificate)certificateFactory.generateCertificate(new ByteArrayInputStream(Issue1597Certificate.getBytes()));
  assertEquals(x509Certificate.getIssuerDN().getName(),"");
  SslCertificate sslCertificate=new SslCertificate(x509Certificate);
  assertEquals(sslCertificate.getIssuedBy().getDName(),"");
}
