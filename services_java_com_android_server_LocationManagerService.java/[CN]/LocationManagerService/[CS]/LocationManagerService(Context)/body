{
  super();
  mContext=context;
  Resources resources=context.getResources();
  mNetworkLocationProviderPackageName=resources.getString(com.android.internal.R.string.config_networkLocationProvider);
  mGeocodeProviderPackageName=resources.getString(com.android.internal.R.string.config_geocodeProvider);
  mPackageMonitor.register(context,true);
  if (LOCAL_LOGV) {
    Slog.v(TAG,"Constructed LocationManager Service");
  }
}
