{
  String result;
  if (mListener != null) {
    result="Receiver{" + Integer.toHexString(System.identityHashCode(this)) + " Listener "+ mKey+ "}";
  }
 else {
    result="Receiver{" + Integer.toHexString(System.identityHashCode(this)) + " Intent "+ mKey+ "}";
  }
  result+="mUpdateRecords: " + mUpdateRecords;
  return result;
}
