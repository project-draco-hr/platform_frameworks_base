{
  int seq=mReportedGpsUids.get(uid,-1);
  if (seq == curSeq) {
    mReportedGpsUids.put(uid,nextSeq);
    return true;
  }
 else   if (seq != nextSeq) {
    try {
      mBatteryStats.noteStartGps(uid);
      mReportedGpsUids.put(uid,nextSeq);
      return true;
    }
 catch (    RemoteException e) {
    }
  }
  return false;
}
