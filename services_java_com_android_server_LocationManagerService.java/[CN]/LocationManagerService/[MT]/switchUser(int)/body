{
  mBlacklist.switchUser(userId);
synchronized (mLock) {
    mLastLocation.clear();
    for (    LocationProviderInterface p : mProviders) {
      updateProviderListenersLocked(p.getName(),false,mCurrentUserId);
      p.switchUser(userId);
    }
    mCurrentUserId=userId;
    updateProvidersLocked();
  }
}
