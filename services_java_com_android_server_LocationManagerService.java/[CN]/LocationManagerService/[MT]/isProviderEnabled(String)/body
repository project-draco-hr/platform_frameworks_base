{
  try {
synchronized (mLock) {
      return _isProviderEnabledLocked(provider);
    }
  }
 catch (  SecurityException se) {
    throw se;
  }
catch (  IllegalArgumentException iae) {
    throw iae;
  }
catch (  Exception e) {
    Slog.e(TAG,"isProviderEnabled got exception:",e);
    return false;
  }
}
