{
synchronized (mLock) {
    for (    String black : mBlacklist) {
      if (packageName.startsWith(black)) {
        if (inWhitelist(packageName)) {
          continue;
        }
 else {
          if (LOCAL_LOGV)           Log.d(TAG,"dropping location (blacklisted): " + packageName + " matches "+ black);
          return true;
        }
      }
    }
  }
  return false;
}
