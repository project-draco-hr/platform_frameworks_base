{
  if (lastLoc == null) {
    return true;
  }
  long minTime=record.mMinTime;
  if (loc.getTime() - lastLoc.getTime() < minTime - MAX_PROVIDER_SCHEDULING_JITTER) {
    return false;
  }
  double minDistance=record.mMinDistance;
  if (minDistance > 0.0) {
    if (loc.distanceTo(lastLoc) <= minDistance) {
      return false;
    }
  }
  return true;
}
