{
  try {
synchronized (mLocationListeners) {
      requestLocationUpdatesLocked(provider,minTime,minDistance,new Receiver(listener,Binder.getCallingUid()));
    }
  }
 catch (  SecurityException se) {
    throw se;
  }
catch (  Exception e) {
    Log.e(TAG,"requestUpdates got exception:",e);
  }
}
