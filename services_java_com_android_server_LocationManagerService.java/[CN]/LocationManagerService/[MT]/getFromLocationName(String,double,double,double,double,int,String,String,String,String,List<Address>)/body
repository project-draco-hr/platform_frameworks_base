{
synchronized (mLocationListeners) {
    if (mNetworkLocationProvider != null) {
      return mNetworkLocationProvider.getFromLocationName(locationName,lowerLeftLatitude,lowerLeftLongitude,upperRightLatitude,upperRightLongitude,maxResults,language,country,variant,appName,addrs);
    }
 else {
      return null;
    }
  }
}
