{
  try {
    Locale locale=new Locale(language,country,variant);
    mMasfClient.forwardGeocode(locale,appName,locationName,lowerLeftLatitude,lowerLeftLongitude,upperRightLatitude,upperRightLongitude,maxResults,addrs);
    return null;
  }
 catch (  IOException e) {
    return e.getMessage();
  }
catch (  Exception e) {
    Log.e(TAG,"getFromLocationName got exception:",e);
    return null;
  }
}
