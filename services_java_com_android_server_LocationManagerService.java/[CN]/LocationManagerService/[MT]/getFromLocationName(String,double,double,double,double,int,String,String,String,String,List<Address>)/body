{
  if (mGeocodeProvider != null) {
    try {
      return mGeocodeProvider.getFromLocationName(locationName,lowerLeftLatitude,lowerLeftLongitude,upperRightLatitude,upperRightLongitude,maxResults,language,country,variant,appName,addrs);
    }
 catch (    RemoteException e) {
      Log.e(TAG,"getFromLocationName failed",e);
      mGeocodeProvider=null;
    }
  }
  return null;
}
