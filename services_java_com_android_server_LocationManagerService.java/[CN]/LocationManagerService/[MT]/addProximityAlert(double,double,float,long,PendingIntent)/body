{
  validatePendingIntent(intent);
  try {
synchronized (mLock) {
      addProximityAlertLocked(latitude,longitude,radius,expiration,intent);
    }
  }
 catch (  SecurityException se) {
    throw se;
  }
catch (  IllegalArgumentException iae) {
    throw iae;
  }
catch (  Exception e) {
    Slog.e(TAG,"addProximityAlert got exception:",e);
  }
}
