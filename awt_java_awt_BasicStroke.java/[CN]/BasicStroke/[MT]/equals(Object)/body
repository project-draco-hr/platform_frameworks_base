{
  if (obj == this) {
    return true;
  }
  if (obj instanceof BasicStroke) {
    BasicStroke bs=(BasicStroke)obj;
    return bs.width == width && bs.cap == cap && bs.join == join && bs.miterLimit == miterLimit && bs.dashPhase == dashPhase && java.util.Arrays.equals(bs.dash,dash);
  }
  return false;
}
