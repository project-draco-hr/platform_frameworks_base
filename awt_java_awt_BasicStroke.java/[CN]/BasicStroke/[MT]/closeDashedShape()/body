{
  if (sp != null) {
    addCap(sp,fmx,fmy,sp.xMove,sp.yMove);
    sp.closePath();
    dst.append(sp);
  }
  if (lp.typeSize > 0) {
    if (!dasher.isClosed()) {
      addCap(lp,scx,scy,rp.xLast,rp.yLast);
      lp.combine(rp);
      addCap(lp,smx,smy,lp.xMove,lp.yMove);
      lp.closePath();
    }
    dst.append(lp);
  }
}
