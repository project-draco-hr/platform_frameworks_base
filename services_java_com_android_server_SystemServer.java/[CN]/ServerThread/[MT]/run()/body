      boolean reboot=(shutdownAction.charAt(0) == '1');
      final String reason;
      if (shutdownAction.length() > 1) {
        reason=shutdownAction.substring(1,shutdownAction.length());
      }
 else {
        reason=null;
      }
      ShutdownThread.rebootOrShutdown(reboot,reason);
    }
  }
