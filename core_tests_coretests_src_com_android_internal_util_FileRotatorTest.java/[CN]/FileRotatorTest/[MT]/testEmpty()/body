{
  final FileRotator rotate1=new FileRotator(mBasePath,PREFIX,DAY_IN_MILLIS,WEEK_IN_MILLIS);
  final FileRotator rotate2=new FileRotator(mBasePath,ANOTHER_PREFIX,DAY_IN_MILLIS,WEEK_IN_MILLIS);
  final RecordingReader reader=new RecordingReader();
  long currentTime=TEST_TIME;
  rotate1.combineActive(reader,writer("foo"),currentTime);
  reader.assertRead();
  assertReadAll(rotate1,"foo");
  assertReadAll(rotate2);
}
