{
  super();
  flags=f;
  binder=b;
  tag=t;
  uid=u == MY_UID ? Process.SYSTEM_UID : u;
  if (u != MY_UID || (!"KEEP_SCREEN_ON_FLAG".equals(tag) && !"KeyInputQueue".equals(tag))) {
    monitorType=(f & LOCK_MASK) == PowerManager.PARTIAL_WAKE_LOCK ? BatteryStats.WAKE_TYPE_PARTIAL : BatteryStats.WAKE_TYPE_FULL;
  }
 else {
    monitorType=-1;
  }
  try {
    b.linkToDeath(this,0);
  }
 catch (  RemoteException e) {
    binderDied();
  }
}
