{
  if (mContext.checkCallingOrSelfPermission(android.Manifest.permission.DEVICE_POWER) != PackageManager.PERMISSION_GRANTED) {
    if (shouldLog(time)) {
      Slog.w(TAG,"Caller does not have DEVICE_POWER permission.  pid=" + Binder.getCallingPid() + " uid="+ Binder.getCallingUid());
    }
    return;
  }
  userActivity(time,-1,noChangeLights,OTHER_EVENT,false);
}
