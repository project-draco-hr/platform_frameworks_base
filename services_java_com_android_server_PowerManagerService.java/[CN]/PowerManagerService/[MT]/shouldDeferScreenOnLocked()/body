{
  if (mPreparingForScreenOn) {
    if (DEBUG_SCREEN_ON)     Slog.i(TAG,"updateLights: delaying screen on due to mPreparingForScreenOn");
    return true;
  }
 else {
    for (int i=0; i < mBroadcastQueue.length; i++) {
      if (mBroadcastQueue[i] == 1) {
        if (DEBUG_SCREEN_ON)         Slog.i(TAG,"updateLights: delaying screen on due to notification queue");
        return true;
      }
    }
  }
  return false;
}
