{
  try {
    if (mScreenBrightnessOverride >= 0) {
      return mScreenBrightnessOverride;
    }
 else     if (mLightSensorScreenBrightness >= 0 && mUseSoftwareAutoBrightness && mAutoBrightessEnabled) {
      return mLightSensorScreenBrightness;
    }
    final int brightness=Settings.System.getInt(mContext.getContentResolver(),SCREEN_BRIGHTNESS);
    return Math.max(brightness,mScreenBrightnessDim);
  }
 catch (  SettingNotFoundException snfe) {
    return Power.BRIGHTNESS_ON;
  }
}
