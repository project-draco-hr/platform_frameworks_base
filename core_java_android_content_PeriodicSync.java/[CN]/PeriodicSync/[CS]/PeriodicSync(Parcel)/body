{
  this.isService=(in.readInt() != 0);
  if (this.isService) {
    this.service=in.readParcelable(null);
    this.account=null;
    this.authority=null;
  }
 else {
    this.account=in.readParcelable(null);
    this.authority=in.readString();
    this.service=null;
  }
  this.extras=in.readBundle();
  this.period=in.readLong();
  this.flexTime=in.readLong();
}
