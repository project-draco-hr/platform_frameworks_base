    assertThat(nonAutoEnUS,isIn(result));
    assertThat(nonAutoJa,isIn(result));
    assertThat(nonAutoIn,not(isIn(result)));
    assertThat(nonAutoEnabledWhenDefaultIsNotAsciiCalableSubtype,not(isIn(result)));
    assertThat(nonAutoEnabledWhenDefaultIsNotAsciiCalableSubtype,not(isIn(result)));
