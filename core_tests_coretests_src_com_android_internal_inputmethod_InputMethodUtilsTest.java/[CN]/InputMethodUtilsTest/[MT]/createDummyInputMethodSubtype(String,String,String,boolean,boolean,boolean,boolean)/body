{
  final StringBuilder subtypeExtraValue=new StringBuilder();
  if (isEnabledWhenDefaultIsNotAsciiCapable) {
    subtypeExtraValue.append(EXTRA_VALUE_PAIR_SEPARATOR);
    subtypeExtraValue.append(EXTRA_VALUE_ENABLED_WHEN_DEFAULT_IS_NOT_ASCII_CAPABLE);
  }
  if (isAsciiCapable) {
    subtypeExtraValue.append(EXTRA_VALUE_PAIR_SEPARATOR);
    subtypeExtraValue.append(EXTRA_VALUE_ASCII_CAPABLE);
  }
  return new InputMethodSubtypeBuilder().setSubtypeNameResId(0).setSubtypeIconResId(0).setSubtypeLocale(locale).setLanguageTag(languageTag).setSubtypeMode(mode).setSubtypeExtraValue(subtypeExtraValue.toString()).setIsAuxiliary(isAuxiliary).setOverridesImplicitlyEnabledSubtype(overridesImplicitlyEnabledSubtype).setIsAsciiCapable(isAsciiCapable).build();
}
