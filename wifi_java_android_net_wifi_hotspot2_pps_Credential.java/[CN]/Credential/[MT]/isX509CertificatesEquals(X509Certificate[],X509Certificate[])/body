{
  if (certs1 == null && certs2 == null) {
    return true;
  }
  if (certs1 == null || certs2 == null) {
    return false;
  }
  if (certs1.length != certs2.length) {
    return false;
  }
  for (int i=0; i < certs1.length; i++) {
    if (!isX509CertificateEquals(certs1[i],certs2[i])) {
      return false;
    }
  }
  return true;
}
