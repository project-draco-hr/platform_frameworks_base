{
  initTestFiles();
  bots.directory.clickDocument("file1.png");
  device.waitForIdle();
  bots.main.menuDelete().click();
  bots.directory.waitForDeleteSnackbar();
  bots.directory.assertDocumentsAbsent("file1.png");
  bots.directory.waitForDeleteSnackbarGone();
  bots.directory.assertDocumentsAbsent("file1.png");
}
