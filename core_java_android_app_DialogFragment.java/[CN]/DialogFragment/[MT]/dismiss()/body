{
  if (mDialog != null) {
    mDialog.dismiss();
  }
  if (mBackStackId >= 0) {
    getActivity().popBackStack(mBackStackId,Activity.POP_BACK_STACK_INCLUSIVE);
    mBackStackId=-1;
  }
 else {
    FragmentTransaction ft=getActivity().openFragmentTransaction();
    ft.remove(this);
    ft.commit();
  }
}
