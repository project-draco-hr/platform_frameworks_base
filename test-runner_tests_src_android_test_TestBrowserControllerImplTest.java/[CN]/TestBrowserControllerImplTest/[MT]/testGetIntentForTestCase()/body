{
  TestSuite testSuite=new TestSuite();
  testSuite.addTest(new DummyTestCase());
  mTestBrowserController.setTestSuite(testSuite);
  Intent intent=mTestBrowserController.getIntentForTestAt(1);
  verifyIntent(intent,DummyTestCase.class,"com.android.testharness");
  assertEquals(TestBrowserControllerImpl.TEST_RUNNER_ACTIVITY_CLASS_NAME,intent.getComponent().getClassName());
  assertEquals("testDummyTest",intent.getStringExtra(TestBrowserController.BUNDLE_EXTRA_TEST_METHOD_NAME));
}
