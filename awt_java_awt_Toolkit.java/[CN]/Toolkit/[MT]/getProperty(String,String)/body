{
  if (propName == null) {
    throw new NullPointerException(Messages.getString("awt.7D"));
  }
  staticLockAWT();
  try {
    String retVal=null;
    if (properties != null) {
      try {
        retVal=properties.getString(propName);
      }
 catch (      MissingResourceException e) {
      }
catch (      ClassCastException e) {
      }
    }
    return (retVal == null) ? defVal : retVal;
  }
  finally {
    staticUnlockAWT();
  }
}
