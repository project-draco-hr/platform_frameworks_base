{
  PanelFeatureState st=getPanelState(featureId,true);
  if (!preparePanel(st,new KeyEvent(KeyEvent.ACTION_DOWN,KeyEvent.KEYCODE_MENU))) {
    return false;
  }
  if (st.menu == null) {
    return false;
  }
  boolean res=st.menu.performIdentifierAction(id,flags);
  if (mActionBar == null) {
    closePanel(st,true);
  }
  return res;
}
