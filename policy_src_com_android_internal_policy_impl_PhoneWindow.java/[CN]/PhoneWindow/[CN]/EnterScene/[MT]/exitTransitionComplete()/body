{
  if (mAllDone) {
    return;
  }
  mAllDone=true;
  sharedElementTransitionComplete(null);
  mHandler.removeCallbacks(this);
  if (!mAllowEnterOverlap) {
    runOnUiThread(mHandler,new Runnable(){
      @Override public void run(){
        beginEnterScene();
        fadeInBackground();
      }
    }
);
  }
}
