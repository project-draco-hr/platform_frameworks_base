{
  boolean handled=performPanelShortcut(mPreparedPanel,ev.getKeyCode(),ev,Menu.FLAG_PERFORM_NO_CLOSE);
  if (handled) {
    if (mPreparedPanel != null) {
      mPreparedPanel.isHandled=true;
    }
    return true;
  }
  final Callback cb=getCallback();
  return cb != null && !isDestroyed() && mFeatureId < 0 ? cb.dispatchKeyShortcutEvent(ev) : super.dispatchKeyShortcutEvent(ev);
}
