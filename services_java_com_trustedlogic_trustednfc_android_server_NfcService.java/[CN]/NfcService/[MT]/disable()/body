{
  boolean isSuccess=false;
  mContext.enforceCallingPermission(android.Manifest.permission.NFC_ADMIN,"NFC_ADMIN permission required to disable NFC service");
  if (isEnabled()) {
    isSuccess=mManager.deinitialize();
    if (isSuccess) {
      mIsNfcEnabled=false;
    }
  }
  updateNfcOnSetting();
  return isSuccess;
}
