{
  final int callbackType=settings.getCallbackType();
  if ((callbackType != ScanSettings.CALLBACK_TYPE_ALL_MATCHES || (callbackType == ScanSettings.CALLBACK_TYPE_ALL_MATCHES && settings.getReportDelaySeconds() > 0)) && !mBluetoothAdapter.isOffloadedFilteringSupported()) {
    return false;
  }
  return true;
}
