{
  boolean ret=true;
  int callbackType;
  callbackType=settings.getCallbackType();
  if (((callbackType == ScanSettings.CALLBACK_TYPE_ON_LOST) || (callbackType == ScanSettings.CALLBACK_TYPE_ON_FOUND) || (callbackType == ScanSettings.CALLBACK_TYPE_ON_UPDATE && settings.getReportDelayNanos() > 0) && (!mBluetoothAdapter.isOffloadedFilteringSupported()))) {
    ret=false;
  }
  return ret;
}
