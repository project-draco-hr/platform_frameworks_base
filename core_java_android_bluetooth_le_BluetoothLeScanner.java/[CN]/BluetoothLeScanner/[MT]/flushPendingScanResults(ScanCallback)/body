{
  checkAdapterState();
  if (callback == null) {
    throw new IllegalArgumentException("callback cannot be null!");
  }
synchronized (mLeScanClients) {
    BleScanCallbackWrapper wrapper=mLeScanClients.get(callback);
    if (wrapper == null) {
      return;
    }
    wrapper.flushPendingBatchResults();
  }
}
