{
synchronized (this) {
    if (mScannerId <= 0) {
      Log.e(TAG,"Error state, mLeHandle: " + mScannerId);
      return;
    }
    try {
      mBluetoothGatt.flushPendingBatchResults(mScannerId);
    }
 catch (    RemoteException e) {
      Log.e(TAG,"Failed to get pending scan results",e);
    }
  }
}
