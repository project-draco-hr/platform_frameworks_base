{
synchronized (this) {
    if (mLeHandle <= 0) {
      Log.e(TAG,"Error state, mLeHandle: " + mLeHandle);
      return;
    }
    try {
      mBluetoothGatt.flushPendingBatchResults(mLeHandle,false);
    }
 catch (    RemoteException e) {
      Log.e(TAG,"Failed to get pending scan results",e);
    }
  }
}
