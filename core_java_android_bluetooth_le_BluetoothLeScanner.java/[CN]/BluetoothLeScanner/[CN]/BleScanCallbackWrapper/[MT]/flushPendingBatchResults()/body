{
synchronized (this) {
    if (mClientIf <= 0) {
      Log.e(TAG,"Error state, mLeHandle: " + mClientIf);
      return;
    }
    try {
      mBluetoothGatt.flushPendingBatchResults(mClientIf,false);
    }
 catch (    RemoteException e) {
      Log.e(TAG,"Failed to get pending scan results",e);
    }
  }
}
