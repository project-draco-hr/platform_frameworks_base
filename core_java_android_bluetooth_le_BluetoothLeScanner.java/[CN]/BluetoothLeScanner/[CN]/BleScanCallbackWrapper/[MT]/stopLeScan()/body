{
synchronized (this) {
    if (mClientIf <= 0) {
      Log.e(TAG,"Error state, mLeHandle: " + mClientIf);
      return;
    }
    try {
      mBluetoothGatt.stopScan(mClientIf,false);
      mBluetoothGatt.unregisterClient(mClientIf);
    }
 catch (    RemoteException e) {
      Log.e(TAG,"Failed to stop scan and unregister",e);
    }
    mClientIf=-1;
  }
}
