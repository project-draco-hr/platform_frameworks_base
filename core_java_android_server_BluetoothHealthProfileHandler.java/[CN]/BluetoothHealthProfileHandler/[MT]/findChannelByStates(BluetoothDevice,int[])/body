{
  List<HealthChannel> channels=new ArrayList<HealthChannel>();
  for (  HealthChannel chan : mHealthChannels) {
    if (chan.mDevice.equals(device)) {
      for (      int state : states) {
        if (chan.mState == state) {
          channels.add(chan);
        }
      }
    }
  }
  return channels;
}
