{
  broadcastHealthDeviceStateChange(device,prevState,state);
  debugLog("Health Device Callback: " + device + " State Change: "+ prevState+ "->"+ state);
  try {
    config.getCallback().onHealthChannelStateChange(config,device,prevState,state,fd);
  }
 catch (  RemoteException e) {
    errorLog("Error while making health channel state change callback: " + e);
  }
}
