{
synchronized (mServiceRunningLock) {
    if (mServiceRunning) {
      try {
        if (DEBUG)         Log.d(TAG,"Stopping Face Unlock");
        mService.stopUi();
      }
 catch (      RemoteException e) {
        Log.e(TAG,"Caught exception stopping Face Unlock: " + e.toString());
      }
      mServiceRunning=false;
    }
  }
}
