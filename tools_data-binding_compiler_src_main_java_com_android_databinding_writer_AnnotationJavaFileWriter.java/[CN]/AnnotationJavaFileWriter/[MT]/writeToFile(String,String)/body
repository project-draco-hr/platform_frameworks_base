{
  Writer writer=null;
  try {
    JavaFileObject javaFileObject=mProcessingEnvironment.getFiler().createSourceFile(canonicalName);
    writer=javaFileObject.openWriter();
    writer.write(contents);
  }
 catch (  IOException e) {
    mProcessingEnvironment.getMessager().printMessage(Diagnostic.Kind.ERROR,"Could not write to " + canonicalName + ": "+ e.getLocalizedMessage());
  }
 finally {
    if (writer != null) {
      IOUtils.closeQuietly(writer);
    }
  }
}
