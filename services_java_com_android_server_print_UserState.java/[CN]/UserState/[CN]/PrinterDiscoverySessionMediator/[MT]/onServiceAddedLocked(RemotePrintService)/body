{
  if (mIsDestroyed) {
    Log.w(LOG_TAG,"Not updating added service - session destroyed");
    return;
  }
  mHandler.obtainMessage(SessionHandler.MSG_CREATE_PRINTER_DISCOVERY_SESSION,service).sendToTarget();
  if (mDiscoveryObservers.getRegisteredCallbackCount() > 0) {
    mHandler.obtainMessage(SessionHandler.MSG_START_PRINTER_DISCOVERY,service).sendToTarget();
  }
}
