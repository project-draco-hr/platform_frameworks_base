{
  if (mIsDestroyed) {
    Log.w(LOG_TAG,"Not updating pritner - session destroyed");
    return;
  }
  RemotePrintService service=mActiveServices.get(printerId.getServiceName());
  if (service != null) {
    SomeArgs args=SomeArgs.obtain();
    args.arg1=service;
    args.arg2=printerId;
    mHandler.obtainMessage(SessionHandler.MSG_REQUEST_PRINTER_UPDATE,args).sendToTarget();
  }
}
