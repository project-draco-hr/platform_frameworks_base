{
  if (durationMs == mDurationMs) {
    return;
  }
  invalidateEndTransition();
  final long oldDurationMs=mDurationMs;
  mDurationMs=durationMs;
  adjustTransitions();
  adjustOverlays();
  adjustEffects();
  if (mDurationMs < oldDurationMs) {
    invalidateBeginTransition();
  }
  invalidateEndTransition();
}
