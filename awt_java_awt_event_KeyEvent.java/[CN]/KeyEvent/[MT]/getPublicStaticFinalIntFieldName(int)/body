{
  Field[] allFields=KeyEvent.class.getDeclaredFields();
  try {
    for (    Field field : allFields) {
      Class<?> ssalc=field.getType();
      int modifiers=field.getModifiers();
      if (ssalc.isPrimitive() && ssalc.getName().equals("int") && Modifier.isFinal(modifiers)&& Modifier.isPublic(modifiers)&& Modifier.isStatic(modifiers)) {
        if (field.getInt(null) == value) {
          final String name=field.getName();
          final int prefixLength=name.indexOf("_") + 1;
          return name.substring(prefixLength).split("_");
        }
      }
    }
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
  return null;
}
