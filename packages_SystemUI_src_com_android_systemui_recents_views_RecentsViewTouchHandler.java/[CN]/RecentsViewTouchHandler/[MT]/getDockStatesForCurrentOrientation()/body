{
  boolean isLandscape=mRv.getResources().getConfiguration().orientation == Configuration.ORIENTATION_LANDSCAPE;
  RecentsConfiguration config=Recents.getConfiguration();
  TaskStack.DockState[] dockStates=isLandscape ? (config.isLargeScreen ? DockRegion.TABLET_LANDSCAPE : DockRegion.PHONE_LANDSCAPE) : (config.isLargeScreen ? DockRegion.TABLET_PORTRAIT : DockRegion.PHONE_PORTRAIT);
  return dockStates;
}
