{
  if (extent == WIDTH) {
    float scale=Utilities.clamp01((float)instance.width() / other.width());
    return Math.max(minValue,(int)(scale * value));
  }
 else   if (extent == HEIGHT) {
    float scale=Utilities.clamp01((float)instance.height() / other.height());
    return Math.max(minValue,(int)(scale * value));
  }
  return value;
}
