{
  transformOut.reset();
  if (mNumFreeformTasks == 0) {
    return transformOut;
  }
  if (stackScroll > mStackEndScrollP) {
    float stackOverscroll=(Math.max(0,stackScroll - mStackEndScrollP)) / mFreeformWorkspacePOffset;
    int overscrollYOffset=(int)(stackOverscroll * mFreeformWorkspaceOffset);
    transformOut.scale=1f;
    transformOut.alpha=1f;
    transformOut.translationY=mStackRect.height() + mFreeformWorkspaceGapOffset - overscrollYOffset;
    transformOut.rect.set(mFreeformRect);
    transformOut.rect.offset(0,transformOut.translationY);
    Utilities.scaleRectAboutCenter(transformOut.rect,transformOut.scale);
    transformOut.visible=true;
  }
  return transformOut;
}
