{
  if (mFreeformLayoutAlgorithm.isTransformAvailable(task,this)) {
    mFreeformLayoutAlgorithm.getTransform(task,transformOut,this);
    return transformOut;
  }
 else {
    if (task == null || !mTaskIndexMap.containsKey(task.key)) {
      transformOut.reset();
      return transformOut;
    }
    getStackTransform(mTaskIndexMap.get(task.key),stackScroll,focusState,transformOut,frontTransform,false,forceUpdate);
    return transformOut;
  }
}
