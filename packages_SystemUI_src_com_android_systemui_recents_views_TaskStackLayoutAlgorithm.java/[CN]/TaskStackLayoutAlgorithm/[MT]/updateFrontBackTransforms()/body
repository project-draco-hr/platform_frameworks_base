{
  if (mStackRect.isEmpty()) {
    return;
  }
  float min=Utilities.mapRange(mFocusState,mUnfocusedRange.relativeMin,mFocusedRange.relativeMin);
  float max=Utilities.mapRange(mFocusState,mUnfocusedRange.relativeMax,mFocusedRange.relativeMax);
  getStackTransform(min,min,0f,mFocusState,mBackOfStackTransform,null,true,true);
  getStackTransform(max,max,0f,mFocusState,mFrontOfStackTransform,null,true,true);
  mBackOfStackTransform.visible=true;
  mFrontOfStackTransform.visible=true;
}
