{
  CameraProperties properties=new CameraProperties();
  CameraMetadata info=new CameraMetadata();
  try {
    mRemoteDevice.getCameraInfo(info);
  }
 catch (  CameraRuntimeException e) {
    throw e.asChecked();
  }
catch (  RemoteException e) {
    return null;
  }
  properties.swap(info);
  return properties;
}
