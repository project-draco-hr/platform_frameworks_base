{
  if (mResizingBackgroundDrawable == null) {
    mResizingBackgroundDrawable=getResizingBackgroundDrawable(getContext(),mWindow.mBackgroundResource,mWindow.mBackgroundFallbackResource,mWindow.isTranslucent() || mWindow.isShowingWallpaper());
    if (mResizingBackgroundDrawable == null) {
      Log.w(mLogTag,"Failed to find background drawable for PhoneWindow=" + mWindow);
    }
  }
  if (mCaptionBackgroundDrawable == null) {
    mCaptionBackgroundDrawable=getContext().getDrawable(R.drawable.decor_caption_title_focused);
  }
  if (mResizingBackgroundDrawable != null) {
    mLastBackgroundDrawableCb=mResizingBackgroundDrawable.getCallback();
    mResizingBackgroundDrawable.setCallback(null);
  }
}
