{
  if (mClosed) {
    throw new IllegalStateException("Already closed");
  }
  if (mWrapped != null) {
    int fd=mWrapped.detachFd();
    mClosed=true;
    mGuard.close();
    return fd;
  }
  int fd=getFd();
  mClosed=true;
  mGuard.close();
  Parcel.clearFileDescriptor(mFileDescriptor);
  return fd;
}
