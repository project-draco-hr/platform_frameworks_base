{
  if (mWrapped != null) {
    try {
      mWrapped.writeToParcel(out,flags);
    }
  finally {
      releaseResources();
    }
  }
 else {
    out.writeFileDescriptor(mFd);
    if (mCommFd != null) {
      out.writeInt(1);
      out.writeFileDescriptor(mCommFd);
    }
 else {
      out.writeInt(0);
    }
    if ((flags & PARCELABLE_WRITE_RETURN_VALUE) != 0 && !mClosed) {
      closeWithStatus(Status.SILENCE,null);
    }
  }
}
