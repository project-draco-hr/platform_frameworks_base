{
  if (Constants.DebugFlags.App.EnableBackgroundTaskLoading) {
    Drawable icon=mIconCache.get(t);
    Bitmap thumbnail=mThumbnailCache.get(t);
    Console.log(Constants.DebugFlags.App.TaskDataLoader,"[RecentsTaskLoader|loadTask]",t + " icon: " + icon+ " thumbnail: "+ thumbnail+ " thumbnailCacheSize: "+ mThumbnailCache.size());
    boolean requiresLoad=false;
    if (icon == null) {
      icon=mDefaultIcon;
      requiresLoad=true;
    }
    if (thumbnail == null) {
      thumbnail=mDefaultThumbnail;
      requiresLoad=true;
    }
    if (requiresLoad) {
      mLoadQueue.addTask(t);
    }
    t.notifyTaskLoaded(thumbnail,icon);
  }
}
