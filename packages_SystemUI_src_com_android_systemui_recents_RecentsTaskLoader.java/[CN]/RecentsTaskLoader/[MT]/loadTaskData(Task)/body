{
  if (Constants.DebugFlags.App.EnableBackgroundTaskLoading) {
    t.icon=mIconCache.get(t);
    t.thumbnail=mThumbnailCache.get(t);
    Console.log(Constants.DebugFlags.App.TaskDataLoader,"[RecentsTaskLoader|loadTask]",t + " icon: " + t.icon+ " thumbnail: "+ t.thumbnail);
    boolean requiresLoad=false;
    if (t.icon == null) {
      t.icon=mDefaultIcon;
      requiresLoad=true;
    }
    if (t.thumbnail == null) {
      t.thumbnail=mDefaultThumbnail;
      requiresLoad=true;
    }
    if (requiresLoad) {
      mLoadQueue.addTask(t);
    }
  }
}
