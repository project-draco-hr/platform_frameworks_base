{
  long t1=System.currentTimeMillis();
  SystemServicesProxy ssp=mSystemServicesProxy;
  List<ActivityManager.RecentTaskInfo> tasks=ssp.getRecentTasks(25,UserHandle.CURRENT.getIdentifier());
  Collections.reverse(tasks);
  Console.log(Constants.Log.App.TimeSystemCalls,"[RecentsTaskLoader|getRecentTasks]","" + (System.currentTimeMillis() - t1) + "ms");
  Console.log(Constants.Log.App.TaskDataLoader,"[RecentsTaskLoader|tasks]","" + tasks.size());
  return tasks;
}
