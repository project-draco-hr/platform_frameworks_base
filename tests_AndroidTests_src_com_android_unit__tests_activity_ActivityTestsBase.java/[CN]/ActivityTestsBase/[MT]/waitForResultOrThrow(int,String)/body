{
  int res=waitForResult(timeoutMs,expected);
  if (res == Activity.RESULT_CANCELED) {
    if (mResultStack != null) {
      throw new RuntimeException(mData != null ? mData.toString() : "Unable to launch",mResultStack);
    }
 else {
      throw new RuntimeException(mData != null ? mData.toString() : "Unable to launch");
    }
  }
  return res;
}
