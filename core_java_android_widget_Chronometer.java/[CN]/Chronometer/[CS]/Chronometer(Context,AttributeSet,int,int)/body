{
  super(context,attrs,defStyleAttr,defStyleRes);
  final TypedArray a=context.obtainStyledAttributes(attrs,com.android.internal.R.styleable.Chronometer,defStyleAttr,defStyleRes);
  setFormat(a.getString(R.styleable.Chronometer_format));
  setCountDown(a.getBoolean(R.styleable.Chronometer_countDown,false));
  a.recycle();
  init();
}
