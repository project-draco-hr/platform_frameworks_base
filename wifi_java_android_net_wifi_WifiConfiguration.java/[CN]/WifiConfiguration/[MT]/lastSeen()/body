{
  ScanResult mostRecent=null;
  if (scanResultCache == null) {
    return null;
  }
  for (  ScanResult result : scanResultCache.values()) {
    if (mostRecent == null) {
      if (result.seen != 0)       mostRecent=result;
    }
 else {
      if (result.seen > mostRecent.seen) {
        mostRecent=result;
      }
    }
  }
  return mostRecent;
}
