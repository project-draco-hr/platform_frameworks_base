{
  if (allowedKeyManagement == null)   return false;
  if (allowedKeyManagement.cardinality() > 1) {
    if (allowedKeyManagement.cardinality() != 2) {
      return false;
    }
    if (allowedKeyManagement.get(KeyMgmt.WPA_EAP) == false) {
      return false;
    }
    if ((allowedKeyManagement.get(KeyMgmt.IEEE8021X) == false) && (allowedKeyManagement.get(KeyMgmt.WPA_PSK) == false)) {
      return false;
    }
  }
  if (TextUtils.isEmpty(FQDN) == false) {
    if (TextUtils.isEmpty(SSID) == false) {
      return false;
    }
    if (TextUtils.isEmpty(providerFriendlyName)) {
      return false;
    }
    if (enterpriseConfig == null || enterpriseConfig.getEapMethod() == WifiEnterpriseConfig.Eap.NONE) {
      return false;
    }
  }
  return true;
}
