{
  final Intent intent=target.getResolvedIntent();
  final ResolveInfo resolve=target.getResolveInfo();
  if (intent != null && Intent.ACTION_GET_CONTENT.equals(intent.getAction()) && resolve != null && resolve.priority > 0 && resolve.activityInfo != null && DocumentsContract.PACKAGE_DOCUMENTS_UI.equals(resolve.activityInfo.packageName)) {
    return true;
  }
  return false;
}
