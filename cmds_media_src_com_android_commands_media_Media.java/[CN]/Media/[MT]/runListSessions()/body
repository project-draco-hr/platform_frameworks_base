{
  System.out.println("Sessions:");
  try {
    List<IBinder> sessions=mSessionService.getSessions(null,ActivityManager.getCurrentUser());
    for (    IBinder session : sessions) {
      MediaController controller=new MediaController(ISessionController.Stub.asInterface(session));
      if (controller != null) {
        MediaSessionInfo info=controller.getSessionInfo();
        System.out.println("  id=" + info.getId() + ", package="+ info.getPackageName());
      }
    }
  }
 catch (  Exception e) {
    System.out.println("***Error listing sessions***");
  }
}
