{
  final String videoItemFilename=INPUT_FILE_PATH + "Text_FileRenamedTo3gp.3gp";
  boolean flagForException=false;
  try {
    new MediaVideoItem(mVideoEditor,"m1",videoItemFilename,MediaItem.RENDERING_MODE_BLACK_BORDER);
  }
 catch (  IllegalArgumentException e) {
    flagForException=true;
  }
  assertTrue("Media Properties for a Dirty  File ",flagForException);
}
