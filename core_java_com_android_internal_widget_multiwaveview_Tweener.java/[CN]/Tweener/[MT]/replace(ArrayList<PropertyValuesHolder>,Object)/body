{
  for (  final Object killobject : args) {
    Tweener tween=sTweens.get(killobject);
    if (tween != null) {
      tween.animator.cancel();
      if (props != null) {
        tween.animator.setValues(props.toArray(new PropertyValuesHolder[props.size()]));
      }
 else {
        sTweens.remove(tween);
      }
    }
  }
}
