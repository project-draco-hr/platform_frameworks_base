{
  if (mNetworkCallback != null) {
    mCm.unregisterNetworkCallback(mNetworkCallback);
    mNetworkCallback=null;
  }
switch (result) {
case DISMISSED:
    mCaptivePortal.reportCaptivePortalDismissed();
  break;
case UNWANTED:
mCaptivePortal.ignoreNetwork();
break;
case WANTED_AS_IS:
mCaptivePortal.useNetwork();
break;
}
finish();
}
