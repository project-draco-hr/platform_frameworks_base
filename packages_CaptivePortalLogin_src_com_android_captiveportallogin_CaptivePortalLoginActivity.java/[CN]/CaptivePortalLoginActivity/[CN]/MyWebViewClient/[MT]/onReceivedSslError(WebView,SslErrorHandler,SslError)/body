{
  Log.w(TAG,"SSL error; displaying SSL warning.");
  final String html=String.format(SSL_ERROR_HTML,getString(R.string.ssl_error_warning),getString(R.string.ssl_error_example),mBrowserBailOutToken,getString(R.string.ssl_error_continue));
  view.loadDataWithBaseURL(INTERNAL_ASSETS,html,"text/HTML","UTF-8",null);
}
