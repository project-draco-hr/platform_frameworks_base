{
synchronized (mLock) {
    if (mActiveConnection == this && Binder.getCallingUid() == mUid) {
      if (DEBUG_KEY) {
        Slog.d(TAG,this + ": sendKeyUp," + " token="+ token+ ", keyCode="+ keyCode);
      }
      final long idToken=Binder.clearCallingIdentity();
      try {
        if (mProviderMethods != null) {
          mProviderMethods.sendKeyUp(TvRemoteProviderProxy.this,token,keyCode);
        }
      }
  finally {
        Binder.restoreCallingIdentity(idToken);
      }
    }
 else {
      if (DEBUG) {
        Slog.w(TAG,"sendKeyUp, Invalid connection or incorrect uid: " + Binder.getCallingUid());
      }
    }
  }
}
