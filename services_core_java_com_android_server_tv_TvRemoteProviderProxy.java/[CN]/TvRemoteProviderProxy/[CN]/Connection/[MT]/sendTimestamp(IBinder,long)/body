{
synchronized (mLock) {
    if (mActiveConnection == this && Binder.getCallingUid() == mUid) {
      final long idToken=Binder.clearCallingIdentity();
      try {
        if (mProviderMethods != null) {
          mProviderMethods.sendTimeStamp(TvRemoteProviderProxy.this,token,timestamp);
        }
      }
  finally {
        Binder.restoreCallingIdentity(idToken);
      }
    }
 else {
      if (DEBUG) {
        Slog.w(TAG,"sendTimeStamp, Invalid connection or incorrect uid: " + Binder.getCallingUid());
      }
    }
  }
}
