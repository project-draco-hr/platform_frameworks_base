{
  if (event == null) {
    throw new IllegalArgumentException("event must not be null");
  }
  if (injectorPid < 0 || injectorUid < 0) {
    throw new IllegalArgumentException("injectorPid and injectorUid must not be negative.");
  }
  if (timeoutMillis <= 0) {
    throw new IllegalArgumentException("timeoutMillis must be positive");
  }
  return nativeInjectInputEvent(event,injectorPid,injectorUid,syncMode,timeoutMillis,WindowManagerPolicy.FLAG_DISABLE_KEY_REPEAT);
}
