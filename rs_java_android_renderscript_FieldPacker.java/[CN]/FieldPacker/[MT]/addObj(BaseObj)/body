{
  if (obj != null) {
    if (RenderScript.sPointerSize == 8) {
      addI64(obj.getID(null));
      addI64(0);
      addI64(0);
      addI64(0);
    }
 else {
      addI32((int)obj.getID(null));
    }
  }
 else {
    if (RenderScript.sPointerSize == 8) {
      addI64(0);
      addI64(0);
      addI64(0);
      addI64(0);
    }
 else {
      addI32(0);
    }
  }
}
