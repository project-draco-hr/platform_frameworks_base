{
  if ((mFadingMode & IN) != IN || endValues == null) {
    return null;
  }
  if (DBG) {
    View startView=(startValues != null) ? startValues.view : null;
    Log.d(LOG_TAG,"Fade.onAppear: startView, startVis, endView, endVis = " + startView + ", "+ view);
  }
  return createAnimation(view,0,1);
}
