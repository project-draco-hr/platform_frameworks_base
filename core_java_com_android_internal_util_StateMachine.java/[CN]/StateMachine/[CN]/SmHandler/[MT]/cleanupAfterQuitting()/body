{
  mSm.quitting();
  if (mSm.mSmThread != null) {
    getLooper().quit();
    mSm.mSmThread=null;
  }
  mSm.mSmHandler=null;
  mSm=null;
  mMsg=null;
  mProcessedMessages.cleanup();
  mStateStack=null;
  mTempStateStack=null;
  mStateInfo.clear();
  mInitialState=null;
  mDestState=null;
  mDeferredMessages.clear();
}
