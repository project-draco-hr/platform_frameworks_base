{
  StringBuilder sb=new StringBuilder();
  sb.append(getName() + ":\n");
  sb.append(" total records=" + getLogRecCount() + "\n");
  for (int i=0; i < getLogRecSize(); i++) {
    sb.append(" rec[" + i + "]: "+ getLogRec(i).toString()+ "\n");
  }
  sb.append("curState=" + getCurrentState().getName());
  return sb.toString();
}
