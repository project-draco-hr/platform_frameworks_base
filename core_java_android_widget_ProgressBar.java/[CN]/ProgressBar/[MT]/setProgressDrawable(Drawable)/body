{
  if (d != null) {
    d.setCallback(this);
    int drawableHeight=drawable.getMinimumHeight();
    if (mMaxHeight < drawableHeight) {
      mMaxHeight=drawableHeight;
      requestLayout();
    }
  }
  mProgressDrawable=d;
  if (!mIndeterminate) {
    mCurrentDrawable=d;
    postInvalidate();
  }
}
