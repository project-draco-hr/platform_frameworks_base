{
  int[] offsets=null;
  if (mFormat == PixelFormat.YCbCr_420_SP) {
    offsets=new int[]{top * mStrides[0] + left,mHeight * mStrides[0] + top / 2 * mStrides[1] + left / 2 * 2};
    return offsets;
  }
  if (mFormat == PixelFormat.YCbCr_422_I) {
    offsets=new int[]{top * mStrides[0] + left / 2 * 4};
    return offsets;
  }
  return offsets;
}
