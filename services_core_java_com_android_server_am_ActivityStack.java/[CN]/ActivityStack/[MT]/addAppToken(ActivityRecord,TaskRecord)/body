{
  final Rect bounds=task.getLaunchBounds();
  final Configuration config=mWindowManager.addAppToken(task.mActivities.indexOf(r),r.appToken,r.task.taskId,mStackId,r.info.screenOrientation,r.fullscreen,(r.info.flags & ActivityInfo.FLAG_SHOW_FOR_ALL_USERS) != 0,r.userId,r.info.configChanges,task.voiceSession != null,r.mLaunchTaskBehind,bounds);
  if (config != null) {
    task.updateOverrideConfiguration(config,bounds);
  }
  r.taskConfigOverride=task.mOverrideConfig;
}
