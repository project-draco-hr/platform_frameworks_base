{
  task.stack=this;
  if (toTop) {
    insertTaskAtTop(task,null);
  }
 else {
    mTaskHistory.add(0,task);
    updateTaskMovement(task,false);
  }
  if (!moving && task.voiceSession != null) {
    try {
      task.voiceSession.taskStarted(task.intent,task.taskId);
    }
 catch (    RemoteException e) {
    }
  }
}
