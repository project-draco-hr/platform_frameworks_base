{
  final Rect bounds=task.getLaunchBounds();
  final Configuration config=task.updateOverrideConfiguration(mStackId,bounds);
  mWindowManager.addAppToken(task.mActivities.indexOf(r),r.appToken,r.task.taskId,mStackId,r.info.screenOrientation,r.fullscreen,(r.info.flags & ActivityInfo.FLAG_SHOW_FOR_ALL_USERS) != 0,r.userId,r.info.configChanges,task.voiceSession != null,r.mLaunchTaskBehind,bounds,config);
  r.taskConfigOverride=task.mOverrideConfig;
}
