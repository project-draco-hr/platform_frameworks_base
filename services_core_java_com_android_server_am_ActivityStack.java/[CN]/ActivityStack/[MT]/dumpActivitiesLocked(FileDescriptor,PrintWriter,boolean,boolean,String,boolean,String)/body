{
  boolean printed=false;
  for (int taskNdx=mTaskHistory.size() - 1; taskNdx >= 0; --taskNdx) {
    final TaskRecord task=mTaskHistory.get(taskNdx);
    printed|=ActivityStackSupervisor.dumpHistoryList(fd,pw,mTaskHistory.get(taskNdx).mActivities,"    ","Hist",true,!dumpAll,dumpClient,dumpPackage,needSep,header,"    Task id #" + task.taskId);
    if (printed) {
      header=null;
    }
  }
  return printed;
}
