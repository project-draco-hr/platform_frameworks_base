{
  Printer pr=new PrintWriterPrinter(pw);
  dumpLocal(pw,prefix);
  String p2=prefix + "  ";
  final int N=size();
  for (int i=0; i < N; i++) {
    BroadcastFilter bf=get(i);
    pw.print(prefix);
    pw.print("Filter #");
    pw.print(i);
    pw.print(": BroadcastFilter{");
    pw.print(Integer.toHexString(System.identityHashCode(bf)));
    pw.println('}');
    bf.dumpInReceiverList(pw,pr,p2);
  }
}
