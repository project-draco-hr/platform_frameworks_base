{
  pw.print(prefix);
  pw.print("app=");
  pw.print(app != null ? app.toShortString() : null);
  pw.print(" pid=");
  pw.print(pid);
  pw.print(" uid=");
  pw.print(uid);
  pw.print(" user=");
  pw.println(userId);
  if (curBroadcast != null || linkedToDeath) {
    pw.print(prefix);
    pw.print("curBroadcast=");
    pw.print(curBroadcast);
    pw.print(" linkedToDeath=");
    pw.println(linkedToDeath);
  }
}
