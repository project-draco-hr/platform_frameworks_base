{
  if (mConditionFlag != -1) {
    return readSoFar.get(mConditionFlag) || mParents.get(0).areAllPathsSatisfied(readSoFar);
  }
 else {
    final BitSet clone=(BitSet)readSoFar.clone();
    readSoFar.and(mBitSet);
    if (!readSoFar.isEmpty()) {
      return true;
    }
    if (mParents.isEmpty()) {
      return false;
    }
    for (    Node parent : mParents) {
      if (!parent.areAllPathsSatisfied(readSoFar)) {
        return false;
      }
    }
    return true;
  }
}
