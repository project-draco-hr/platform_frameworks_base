{
  if (mResourceData[index] == null) {
    return defValue;
  }
  String s=mResourceData[index].getValue();
  try {
    return ResourceHelper.getColor(s);
  }
 catch (  NumberFormatException e) {
    Bridge.getLog().warning(null,String.format("Unable to convert \"%s\" into a color in attribute \"%2$s\"",s,mNames[index]));
  }
  return defValue;
}
