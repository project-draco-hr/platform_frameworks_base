{
  if (mResourceData[index] == null) {
    return defValue;
  }
  String value=mResourceData[index].getValue();
  if (value == null) {
    return defValue;
  }
  if (ResourceHelper.stringToFloat(value,mValue)) {
    return mValue.getFraction(base,pbase);
  }
  Bridge.getLog().warning(LayoutLog.TAG_RESOURCES_FORMAT,String.format("\"%1$s\" in attribute \"%2$s\" cannont be converted to a fraction.",value,mNames[index]),null);
  return defValue;
}
