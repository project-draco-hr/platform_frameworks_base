{
  if (mData[index] == null) {
    return defValue;
  }
  String value=mData[index].getValue();
  if (value == null) {
    return defValue;
  }
  if (ResourceHelper.stringToFloat(value,mValue)) {
    return mValue.getFraction(base,pbase);
  }
  Bridge.getLog().warning(null,String.format("Unable to resolve fraction value \"%1$s\" in attribute \"%2$s\"",value,mNames[index]));
  return defValue;
}
