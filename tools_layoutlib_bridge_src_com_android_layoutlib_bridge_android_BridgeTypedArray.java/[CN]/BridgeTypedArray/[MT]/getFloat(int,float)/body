{
  if (mData[index] == null) {
    return defValue;
  }
  String s=mData[index].getValue();
  if (s != null) {
    try {
      return Float.parseFloat(s);
    }
 catch (    NumberFormatException e) {
      Bridge.getLog().warning(null,String.format("Unable to convert \"%s\" into a float in attribute \"%2$s\"",s,mNames[index]));
    }
  }
  return defValue;
}
