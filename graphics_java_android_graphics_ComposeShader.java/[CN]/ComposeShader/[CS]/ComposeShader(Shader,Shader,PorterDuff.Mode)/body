{
  mType=TYPE_PORTERDUFFMODE;
  mShaderA=shaderA;
  mShaderB=shaderB;
  mPorterDuffMode=mode;
  native_instance=nativeCreate2(shaderA.native_instance,shaderB.native_instance,mode.nativeInt);
  native_shader=nativePostCreate2(native_instance,shaderA.native_shader,shaderB.native_shader,mode.nativeInt);
}
