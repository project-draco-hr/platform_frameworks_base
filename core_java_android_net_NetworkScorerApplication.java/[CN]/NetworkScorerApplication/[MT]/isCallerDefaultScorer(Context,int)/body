{
  String defaultApp=getActiveScorer(context);
  if (defaultApp == null) {
    return false;
  }
  AppOpsManager appOpsMgr=(AppOpsManager)context.getSystemService(Context.APP_OPS_SERVICE);
  try {
    appOpsMgr.checkPackage(callingUid,defaultApp);
    return true;
  }
 catch (  SecurityException e) {
    return false;
  }
}
