{
  super.onDetachedFromWindow();
  if (DEBUG)   Log.d(mTag,"onDetachedFromWindow");
  checkForAttachedZenChange();
  mAttached=false;
  mAttachedZen=-1;
  mSessionZen=-1;
  setSessionExitCondition(null);
  if (!mEmbedded) {
    setExpanded(false);
  }
  setRequestingConditions(false);
  mTransitionHelper.clear();
}
