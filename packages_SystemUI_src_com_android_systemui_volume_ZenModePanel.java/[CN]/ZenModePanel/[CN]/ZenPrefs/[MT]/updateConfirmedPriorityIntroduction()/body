{
  final boolean confirmed=Prefs.getBoolean(mContext,Prefs.Key.DND_CONFIRMED_PRIORITY_INTRODUCTION,false);
  if (confirmed == mConfirmedPriorityIntroduction)   return;
  mConfirmedPriorityIntroduction=confirmed;
  if (DEBUG)   Log.d(mTag,"Confirmed priority introduction: " + mConfirmedPriorityIntroduction);
}
