{
  Map<String,IResourceValue> typeMap;
  if (frameworkOnly == false) {
    typeMap=mProjectResources.get(resType);
    if (typeMap != null) {
      IResourceValue item=typeMap.get(resName);
      if (item != null) {
        return item;
      }
    }
  }
  typeMap=mFrameworkResources.get(resType);
  if (typeMap != null) {
    IResourceValue item=typeMap.get(resName);
    if (item != null) {
      return item;
    }
  }
  return null;
}
