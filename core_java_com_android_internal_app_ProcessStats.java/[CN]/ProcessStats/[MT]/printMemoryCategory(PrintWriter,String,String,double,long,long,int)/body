{
  if (memWeight != 0) {
    long mem=(long)(memWeight * 1024 / totalTime);
    pw.print(prefix);
    pw.print(label);
    pw.print(": ");
    DebugUtils.printSizeValue(pw,mem);
    pw.print(" (");
    pw.print(samples);
    pw.print(" samples)");
    pw.println();
    return curTotalMem + mem;
  }
  return curTotalMem;
}
