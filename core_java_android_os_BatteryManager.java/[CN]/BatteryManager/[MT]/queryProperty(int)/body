{
  long ret;
  if (mBatteryPropertiesRegistrar == null) {
    IBinder b=ServiceManager.getService("batteryproperties");
    mBatteryPropertiesRegistrar=IBatteryPropertiesRegistrar.Stub.asInterface(b);
    if (mBatteryPropertiesRegistrar == null)     return Long.MIN_VALUE;
  }
  try {
    BatteryProperty prop=new BatteryProperty();
    if (mBatteryPropertiesRegistrar.getProperty(id,prop) == 0)     ret=prop.getLong();
 else     ret=Long.MIN_VALUE;
  }
 catch (  RemoteException e) {
    ret=Long.MIN_VALUE;
  }
  return ret;
}
