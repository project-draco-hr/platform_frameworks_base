{
  if (mHandler == null || pw == null) {
    return;
  }
  DumpUtils.dumpAsync(mHandler,new DumpUtils.Dump(){
    @Override public void dump(    PrintWriter pw){
      pw.print("  component=");
      pw.println(mCurrentDreamComponent);
      pw.print("  token=");
      pw.println(mCurrentDreamToken);
      pw.print("  dream=");
      pw.println(mCurrentDream);
    }
  }
,pw,200);
}
