{
  dest.writeLong(mTimestamp);
  dest.writeCharSequence(mNumber);
  dest.writeLong(mId);
  dest.writeLong(mDuration);
  dest.writeCharSequence(mSource);
  dest.writeCharSequence(mProviderData);
  if (mUri == null) {
    dest.writeInt(0);
  }
 else {
    dest.writeInt(1);
    mUri.writeToParcel(dest,flags);
  }
  if (mIsRead) {
    dest.writeInt(1);
  }
 else {
    dest.writeInt(0);
  }
  if (mHasContent) {
    dest.writeInt(1);
  }
 else {
    dest.writeInt(0);
  }
}
