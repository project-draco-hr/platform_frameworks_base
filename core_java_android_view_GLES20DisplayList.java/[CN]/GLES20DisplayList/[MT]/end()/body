{
  if (mCanvas != null) {
    if (mFinalizer != null) {
      mCanvas.end(mFinalizer.mNativeDisplayList);
    }
 else {
      mFinalizer=new DisplayListFinalizer(mCanvas.end(0));
      GLES20Canvas.setDisplayListName(mFinalizer.mNativeDisplayList,mName);
    }
    mCanvas.recycle();
    mCanvas=null;
    mValid=true;
  }
}
