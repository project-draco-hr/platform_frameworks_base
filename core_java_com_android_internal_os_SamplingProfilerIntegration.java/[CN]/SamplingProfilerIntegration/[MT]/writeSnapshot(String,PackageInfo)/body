{
  if (!enabled) {
    return;
  }
  if (pending.compareAndSet(false,true)) {
    snapshotWriter.execute(new Runnable(){
      public void run(){
        try {
          writeSnapshotFile(processName,packageInfo);
        }
  finally {
          pending.set(false);
        }
      }
    }
);
  }
}
