{
  if (!checkNotifyPermission("notifyCellLocation()")) {
    return;
  }
synchronized (mRecords) {
    mCellLocation=cellLocation;
    for (    Record r : mRecords) {
      if ((r.events & PhoneStateListener.LISTEN_CELL_LOCATION) != 0) {
        sendCellLocation(r,cellLocation);
      }
    }
  }
}
