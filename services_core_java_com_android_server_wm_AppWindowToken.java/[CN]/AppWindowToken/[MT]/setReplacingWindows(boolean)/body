{
  if (DEBUG_ADD_REMOVE)   Slog.d(TAG_WM,"Marking app token " + appWindowToken + " with replacing windows.");
  for (int i=allAppWindows.size() - 1; i >= 0; i--) {
    final WindowState w=allAppWindows.get(i);
    w.setReplacing(animate);
  }
  if (animate) {
    if (DEBUG_APP_TRANSITIONS)     Slog.v(TAG_WM,"setReplacingWindow() Setting dummy animation on: " + this);
    mAppAnimator.setDummyAnimation();
  }
}
