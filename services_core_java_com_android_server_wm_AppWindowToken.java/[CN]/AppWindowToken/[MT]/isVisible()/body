{
  final int N=allAppWindows.size();
  for (int i=0; i < N; i++) {
    WindowState win=allAppWindows.get(i);
    if (!win.mAppFreezing && (win.mViewVisibility == View.VISIBLE || mAnimatingWithSavedSurface || (win.mWinAnimator.isAnimating() && !service.mAppTransition.isTransitionSet())) && !win.mDestroying&& win.isDrawnLw()) {
      return true;
    }
  }
  return false;
}
