{
  Pair<ResourceType,String> resourceInfo=Bridge.resolveResourceId(id);
  if (resourceInfo != null) {
    platformResFlag_out[0]=true;
    String attributeName=resourceInfo.getSecond();
    return Pair.of(attributeName,mContext.getRenderResources().getFrameworkResource(resourceInfo.getFirst(),attributeName));
  }
  if (mLayoutlibCallback != null) {
    resourceInfo=mLayoutlibCallback.resolveResourceId(id);
    if (resourceInfo != null) {
      platformResFlag_out[0]=false;
      String attributeName=resourceInfo.getSecond();
      return Pair.of(attributeName,mContext.getRenderResources().getProjectResource(resourceInfo.getFirst(),attributeName));
    }
  }
  return null;
}
