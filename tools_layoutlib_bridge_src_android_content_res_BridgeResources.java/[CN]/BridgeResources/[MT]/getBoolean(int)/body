{
  Pair<String,ResourceValue> value=getResourceValue(id,mPlatformResourceFlag);
  if (value != null) {
    ResourceValue resValue=value.getSecond();
    if (resValue != null) {
      String v=resValue.getValue();
      if (v != null) {
        return Boolean.parseBoolean(v);
      }
    }
  }
  throwException(id);
  return false;
}
