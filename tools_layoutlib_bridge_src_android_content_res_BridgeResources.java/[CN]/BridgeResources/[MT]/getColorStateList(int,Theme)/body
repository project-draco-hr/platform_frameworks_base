{
  Pair<String,ResourceValue> resValue=getResourceValue(id,mPlatformResourceFlag);
  if (resValue != null) {
    ColorStateList stateList=ResourceHelper.getColorStateList(resValue.getSecond(),mContext);
    if (stateList != null) {
      return stateList.obtainForTheme(theme);
    }
  }
  throwException(id);
  return null;
}
