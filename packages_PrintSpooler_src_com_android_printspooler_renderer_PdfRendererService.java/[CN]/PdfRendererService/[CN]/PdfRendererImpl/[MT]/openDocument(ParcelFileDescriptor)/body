{
synchronized (mLock) {
    throwIfOpened();
    if (DEBUG) {
      Log.i(LOG_TAG,"openDocument()");
    }
    try {
      mRenderer=new PdfRenderer(source);
      return mRenderer.getPageCount();
    }
 catch (    IOException ioe) {
      throw new RemoteException("Cannot open file");
    }
  }
}
