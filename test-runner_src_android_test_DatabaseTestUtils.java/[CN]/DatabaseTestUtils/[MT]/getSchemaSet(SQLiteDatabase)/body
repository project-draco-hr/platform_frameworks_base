{
  Set<String> schemaSet=Sets.newHashSet();
  Cursor entityCursor=db.rawQuery("SELECT sql FROM sqlite_master",null);
  try {
    while (entityCursor.moveToNext()) {
      String sql=entityCursor.getString(0);
      schemaSet.add(sql);
    }
  }
  finally {
    entityCursor.close();
  }
  return schemaSet;
}
