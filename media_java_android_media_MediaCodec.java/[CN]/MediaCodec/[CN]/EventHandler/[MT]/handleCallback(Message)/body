{
  if (mCallback == null) {
    return;
  }
switch (msg.arg1) {
case CB_INPUT_AVAILABLE:
{
      mCallback.onInputBufferAvailable(mCodec,msg.arg2);
      break;
    }
case CB_OUTPUT_AVAILABLE:
{
    mCallback.onOutputBufferAvailable(mCodec,msg.arg2,(MediaCodec.BufferInfo)msg.obj);
    break;
  }
case CB_ERROR:
{
  mCallback.onError(mCodec,msg.arg2,(Integer)msg.obj);
  break;
}
case CB_OUTPUT_FORMAT_CHANGE:
{
mCallback.onOutputFormatChanged(mCodec,new MediaFormat((Map<String,Object>)msg.obj));
break;
}
default :
{
break;
}
}
}
