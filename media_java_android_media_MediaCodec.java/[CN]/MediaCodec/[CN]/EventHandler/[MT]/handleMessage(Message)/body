{
switch (msg.what) {
case EVENT_CALLBACK:
{
      handleCallback(msg);
      break;
    }
case EVENT_SET_CALLBACK:
{
    mCallback=(MediaCodec.Callback)msg.obj;
    break;
  }
case EVENT_FRAME_RENDERED:
synchronized (mListenerLock) {
  FrameRenderedInfo info=(FrameRenderedInfo)msg.obj;
  if (mOnFrameRenderedListener != null) {
    mOnFrameRenderedListener.onFrameRendered(mCodec,info.mPresentationTimeUs,info.mNanoTime);
  }
  break;
}
default :
{
break;
}
}
}
