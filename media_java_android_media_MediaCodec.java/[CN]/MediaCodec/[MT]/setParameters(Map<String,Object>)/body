{
  if (params == null) {
    return;
  }
  String[] keys=new String[params.size()];
  Object[] values=new Object[params.size()];
  int i=0;
  for (  Map.Entry<String,Object> entry : params.entrySet()) {
    keys[i]=entry.getKey();
    values[i]=entry.getValue();
    ++i;
  }
  setParameters(keys,values);
}
