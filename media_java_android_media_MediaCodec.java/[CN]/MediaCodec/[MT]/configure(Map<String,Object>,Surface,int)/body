{
  String[] keys=null;
  Object[] values=null;
  if (format != null) {
    keys=new String[format.size()];
    values=new Object[format.size()];
    int i=0;
    for (    Map.Entry<String,Object> entry : format.entrySet()) {
      keys[i]=entry.getKey();
      values[i]=entry.getValue();
      ++i;
    }
  }
  native_configure(keys,values,surface,flags);
}
