{
  boolean started=false;
synchronized (this) {
    if (mClientIf == -1) {
      return false;
    }
    try {
      wait(LE_CALLBACK_TIMEOUT_MILLIS);
    }
 catch (    InterruptedException e) {
      Log.e(TAG,"Callback reg wait interrupted: ",e);
    }
    started=(mClientIf > 0 && isAdvertising);
  }
  return started;
}
