{
  Log.d(TAG,"onClientRegistered() - status=" + status + " clientIf="+ clientIf);
synchronized (this) {
    if (status == BluetoothGatt.GATT_SUCCESS) {
      mLeHandle=clientIf;
      try {
        mBluetoothGatt.startMultiAdvertising(mLeHandle,mAdvertisement,mScanResponse,mSettings);
      }
 catch (      RemoteException e) {
        Log.e(TAG,"fail to start le advertise: " + e);
        mLeHandle=-1;
        notifyAll();
      }
    }
 else {
      mLeHandle=-1;
      notifyAll();
    }
  }
}
