{
switch (languageValue) {
case LANGUAGE_ENGLISH:
    return "en";
case LANGUAGE_FRENCH:
  return "fr";
case LANGUAGE_SPANISH:
return "es";
case LANGUAGE_JAPANESE:
return "ja";
case LANGUAGE_KOREAN:
return "ko";
case LANGUAGE_CHINESE:
return "zh";
case LANGUAGE_HEBREW:
return "he";
default :
return null;
}
}
