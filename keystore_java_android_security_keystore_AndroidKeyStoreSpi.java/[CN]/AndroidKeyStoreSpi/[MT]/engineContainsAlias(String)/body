{
  if (alias == null) {
    throw new NullPointerException("alias == null");
  }
  return mKeyStore.contains(Credentials.USER_PRIVATE_KEY + alias,mUid) || mKeyStore.contains(Credentials.USER_SECRET_KEY + alias,mUid) || mKeyStore.contains(Credentials.USER_CERTIFICATE + alias,mUid)|| mKeyStore.contains(Credentials.CA_CERTIFICATE + alias,mUid);
}
