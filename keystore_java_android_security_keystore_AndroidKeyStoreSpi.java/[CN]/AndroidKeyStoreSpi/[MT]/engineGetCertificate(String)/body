{
  if (alias == null) {
    throw new NullPointerException("alias == null");
  }
  byte[] certificate=mKeyStore.get(Credentials.USER_CERTIFICATE + alias);
  if (certificate != null) {
    return toCertificate(certificate);
  }
  certificate=mKeyStore.get(Credentials.CA_CERTIFICATE + alias);
  if (certificate != null) {
    return toCertificate(certificate);
  }
  return null;
}
