{
  X509Certificate cert=toCertificate(encodedCert);
  if (cert == null) {
    return null;
  }
  String privateKeyAlias=Credentials.USER_PRIVATE_KEY + alias;
  if (mKeyStore.contains(privateKeyAlias)) {
    return wrapIntoKeyStoreCertificate(privateKeyAlias,cert);
  }
 else {
    return cert;
  }
}
