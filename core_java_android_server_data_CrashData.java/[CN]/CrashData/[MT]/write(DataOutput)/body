{
  if (this.state == null) {
    out.writeInt(0);
  }
 else {
    out.writeInt(1);
  }
  out.writeUTF(this.id);
  out.writeUTF(this.activity);
  out.writeLong(this.time);
  buildData.write(out);
  throwableData.write(out);
  if (this.state != null) {
    out.writeInt(this.state.length);
    out.write(this.state,0,this.state.length);
  }
}
