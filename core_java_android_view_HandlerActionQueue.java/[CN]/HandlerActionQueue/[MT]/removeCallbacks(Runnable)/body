{
synchronized (this) {
    final int count=mCount;
    int j=0;
    final HandlerAction[] actions=mActions;
    for (int i=0; i < count; i++) {
      if (actions[i].matches(action)) {
        continue;
      }
      if (j != i) {
        actions[j]=actions[i];
      }
      j++;
    }
    mCount=j;
    for (; j < count; j++) {
      actions[j]=null;
    }
  }
}
