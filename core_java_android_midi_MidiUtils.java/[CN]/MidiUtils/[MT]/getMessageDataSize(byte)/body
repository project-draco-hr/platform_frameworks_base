{
switch (b & 0xF0) {
case 0x80:
case 0x90:
case 0xA0:
case 0xB0:
case 0xE0:
    return 2;
case 0xC0:
case 0xD0:
  return 1;
case 0xF0:
switch (b & 0x0F) {
case 0x00:
  Log.e(TAG,"System Exclusive not supported yet");
return -1;
case 0x01:
case 0x03:
return 1;
case 0x02:
return 2;
default :
return 0;
}
default :
Log.e(TAG,"unknown MIDI command " + b);
return -1;
}
}
