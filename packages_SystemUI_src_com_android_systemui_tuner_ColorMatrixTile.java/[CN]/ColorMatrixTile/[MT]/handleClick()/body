{
  mIndex++;
  if (mIndex == DisplayController.AUTO_INDEX) {
    mDisplayController.setAuto(true);
  }
 else {
    mDisplayController.setAuto(false);
    if (!mDisplayController.isCustomEnabled() && (mIndex == DisplayController.CUSTOM_INDEX)) {
      mIndex++;
    }
    if (mIndex == mValues.length - 1) {
      mIndex=0;
    }
    Settings.Secure.putStringForUser(mContext.getContentResolver(),Settings.Secure.ACCESSIBILITY_DISPLAY_COLOR_MATRIX,mValues[mIndex],ActivityManager.getCurrentUser());
  }
  refreshState();
}
