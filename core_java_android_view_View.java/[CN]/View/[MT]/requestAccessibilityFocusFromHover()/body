{
  if (includeForAccessibility() && isActionableForAccessibility()) {
    requestAccessibilityFocus();
  }
 else {
    if (mParent != null) {
      View nextFocus=mParent.findViewToTakeAccessibilityFocusFromHover(this,this);
      if (nextFocus != null) {
        nextFocus.requestAccessibilityFocus();
      }
    }
  }
}
