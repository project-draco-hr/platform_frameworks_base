{
  if (outline != null && !outline.isValid()) {
    throw new IllegalArgumentException("Outline must not be invalid");
  }
  mPrivateFlags3|=PFLAG3_OUTLINE_DEFINED;
  if (outline == null) {
    mOutline=null;
  }
 else {
    if (mOutline == null) {
      mOutline=new Outline(outline);
    }
  }
  mRenderNode.setOutline(mOutline);
}
