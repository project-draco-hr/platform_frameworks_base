{
  SparseArray<Object> tags=null;
synchronized (sTagsLock) {
    if (sTags == null) {
      sTags=new WeakHashMap<View,SparseArray<Object>>();
    }
 else {
      tags=sTags.get(view);
    }
  }
  if (tags == null) {
    tags=new SparseArray<Object>(2);
synchronized (sTagsLock) {
      sTags.put(view,tags);
    }
  }
  tags.put(key,tag);
}
