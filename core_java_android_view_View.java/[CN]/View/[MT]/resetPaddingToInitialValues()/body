{
  if (isRtlCompatibilityMode()) {
    mPaddingLeft=mUserPaddingLeftInitial;
    mPaddingRight=mUserPaddingRightInitial;
  }
 else {
    if (isLayoutRtl()) {
      mPaddingLeft=mUserPaddingRightInitial;
      mPaddingRight=mUserPaddingLeftInitial;
    }
 else {
      mPaddingLeft=mUserPaddingLeftInitial;
      mPaddingRight=mUserPaddingRightInitial;
    }
  }
}
