{
  boolean result=false;
  if (KeyEvent.isConfirmKey(event.getKeyCode())) {
    if ((mViewFlags & ENABLED_MASK) == DISABLED) {
      return true;
    }
    if (((mViewFlags & CLICKABLE) == CLICKABLE || (mViewFlags & LONG_CLICKABLE) == LONG_CLICKABLE) && (event.getRepeatCount() == 0)) {
      setPressed(true);
      checkForLongClick(0);
      return true;
    }
  }
  return result;
}
