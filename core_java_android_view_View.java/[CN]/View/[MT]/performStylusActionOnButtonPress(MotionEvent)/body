{
  if (isStylusButtonPressable() && !mInStylusButtonPress && !mHasPerformedLongPress&& event.isButtonPressed(MotionEvent.BUTTON_STYLUS_SECONDARY)) {
    if (performStylusButtonPress()) {
      mInStylusButtonPress=true;
      setPressed(true,event.getX(),event.getY());
      removeTapCallback();
      removeLongPressCallback();
      return true;
    }
  }
  return false;
}
