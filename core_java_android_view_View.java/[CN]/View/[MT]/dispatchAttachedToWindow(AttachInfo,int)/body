{
  mAttachInfo=info;
  mWindowAttachCount++;
  if (mFloatingTreeObserver != null) {
    info.mTreeObserver.merge(mFloatingTreeObserver);
    mFloatingTreeObserver=null;
  }
  performCollectViewAttributes(visibility);
  onAttachedToWindow();
  int vis=info.mWindowVisibility;
  if (vis != GONE) {
    onWindowVisibilityChanged(vis);
  }
}
