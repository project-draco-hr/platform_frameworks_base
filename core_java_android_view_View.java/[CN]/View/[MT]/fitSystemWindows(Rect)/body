{
  if ((mPrivateFlags3 & PFLAG3_APPLYING_INSETS) == 0) {
    try {
      mPrivateFlags3|=PFLAG3_FITTING_SYSTEM_WINDOWS;
      return !dispatchApplyWindowInsets(new WindowInsets(insets)).hasInsets();
    }
  finally {
      mPrivateFlags3&=~PFLAG3_FITTING_SYSTEM_WINDOWS;
    }
  }
 else {
    return fitSystemWindowsInt(insets);
  }
}
