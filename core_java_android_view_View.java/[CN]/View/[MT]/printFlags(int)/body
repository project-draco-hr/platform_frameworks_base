{
  String output="";
  int numFlags=0;
  if ((flags & FOCUSABLE_MASK) == FOCUSABLE) {
    output+="TAKES_FOCUS";
    numFlags++;
  }
switch (flags & VISIBILITY_MASK) {
case INVISIBLE:
    if (numFlags > 0) {
      output+=" ";
    }
  output+="INVISIBLE";
break;
case GONE:
if (numFlags > 0) {
output+=" ";
}
output+="GONE";
break;
default :
break;
}
return output;
}
