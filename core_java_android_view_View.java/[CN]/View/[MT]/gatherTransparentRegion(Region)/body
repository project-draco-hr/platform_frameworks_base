{
  if (region != null) {
    final int pflags=mPrivateFlags;
    if ((pflags & SKIP_DRAW) == 0) {
      getLocationInWindow(mLocation);
      region.op(mLocation[0],mLocation[1],mLocation[0] + mRight - mLeft,mLocation[1] + mBottom - mTop,Region.Op.DIFFERENCE);
    }
 else     if ((pflags & ONLY_DRAWS_BACKGROUND) != 0 && mBGDrawable != null) {
      applyDrawableToTransparentRegion(mBGDrawable,region);
    }
  }
  return true;
}
