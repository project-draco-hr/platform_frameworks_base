{
  CharSequence text=getIterableTextForAccessibility();
  if (text == null || text.length() == 0) {
    return false;
  }
  TextSegmentIterator iterator=getIteratorForGranularity(granularity);
  if (iterator == null) {
    return false;
  }
  final int current=getAccessibilityCursorPosition();
  final int[] range=iterator.following(current);
  if (range == null) {
    return false;
  }
  final int start=range[0];
  final int end=range[1];
  setAccessibilityCursorPosition(end);
  sendViewTextTraversedAtGranularityEvent(AccessibilityNodeInfo.ACTION_NEXT_AT_MOVEMENT_GRANULARITY,granularity,start,end);
  return true;
}
