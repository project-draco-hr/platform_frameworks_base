{
  if (getSharesGlobalCamera() != sharesGlobalCamera) {
    if (sharesGlobalCamera) {
      mPrivateFlags3|=PFLAG3_SHARES_GLOBAL_CAMERA;
    }
 else {
      mPrivateFlags3&=~PFLAG3_SHARES_GLOBAL_CAMERA;
    }
    if (mDisplayList != null) {
      mDisplayList.setSharesGlobalCamera(sharesGlobalCamera);
    }
  }
}
