{
  ViewParent ancestor=mParent;
  while (ancestor instanceof ViewGroup) {
    final ViewGroup vgAncestor=(ViewGroup)ancestor;
    if (vgAncestor.getDescendantFocusability() == ViewGroup.FOCUS_BLOCK_DESCENDANTS || vgAncestor.shouldBlockFocusForTouchscreen()) {
      return true;
    }
 else {
      ancestor=vgAncestor.getParent();
    }
  }
  return false;
}
