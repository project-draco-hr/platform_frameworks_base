{
  if (event.isConfirmKey()) {
    if ((mViewFlags & ENABLED_MASK) == DISABLED) {
      return true;
    }
    if ((mViewFlags & CLICKABLE) == CLICKABLE && isPressed()) {
      setPressed(false);
      if (!mHasPerformedLongPress) {
        removeLongPressCallback();
        return performClick();
      }
    }
  }
  return false;
}
