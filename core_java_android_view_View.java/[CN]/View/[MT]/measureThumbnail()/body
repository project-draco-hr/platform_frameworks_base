{
  mPrivateFlags&=~MEASURED_DIMENSION_SET;
  onMeasureDragThumbnail();
  if ((mPrivateFlags & MEASURED_DIMENSION_SET) != MEASURED_DIMENSION_SET) {
    throw new IllegalStateException("onMeasureDragThumbnail() did not set the" + " measured dimension by calling setDragThumbnailDimension()");
  }
  if (ViewDebug.DEBUG_DRAG) {
    Log.d(VIEW_LOG_TAG,"Drag thumb measured: w=" + mThumbnailWidth + " h="+ mThumbnailHeight);
  }
}
