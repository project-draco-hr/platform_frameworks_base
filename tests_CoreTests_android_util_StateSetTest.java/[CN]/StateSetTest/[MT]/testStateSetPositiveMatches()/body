{
  int[] stateSpec=new int[2];
  int[] stateSet=new int[3];
  stateSpec[0]=1;
  stateSet[0]=1;
  assertTrue(StateSet.stateSetMatches(stateSpec,stateSet));
  stateSet[0]=2;
  assertFalse(StateSet.stateSetMatches(stateSpec,stateSet));
  stateSpec[1]=2;
  assertFalse(StateSet.stateSetMatches(stateSpec,stateSet));
  stateSet[1]=1;
  assertTrue(StateSet.stateSetMatches(stateSpec,stateSet));
  stateSet[2]=12345;
  assertTrue(StateSet.stateSetMatches(stateSpec,stateSet));
}
