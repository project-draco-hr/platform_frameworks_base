{
  if (source.getPath().endsWith(".apk")) {
    findClassesInApk(source.getPath(),packageName,classNames,subpackageNames);
  }
 else {
    File[] files=source.listFiles();
    if (files != null) {
      for (      File file : files) {
        scanForApkFiles(file,packageName,classNames,subpackageNames);
      }
    }
  }
}
