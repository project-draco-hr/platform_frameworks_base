{
  String authority=getAuthority();
  if (authority == null) {
    return null;
  }
  int userInfoSeparator=authority.indexOf('@');
  int portSeparator=authority.indexOf(':',userInfoSeparator);
  return portSeparator == NOT_FOUND ? authority.substring(userInfoSeparator + 1) : authority.substring(userInfoSeparator + 1,portSeparator);
}
