{
  if (value == null) {
    return nullValue();
  }
  String string=value.toString();
  if (!lenient && (string.equals("-Infinity") || string.equals("Infinity") || string.equals("NaN"))) {
    throw new IllegalArgumentException("Numeric values must be finite, but was " + value);
  }
  beforeValue(false);
  out.append(string);
  return this;
}
