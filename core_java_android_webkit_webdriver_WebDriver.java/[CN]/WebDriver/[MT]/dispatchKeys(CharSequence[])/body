{
  KeyCharacterMap chararcterMap=KeyCharacterMap.load(KeyCharacterMap.VIRTUAL_KEYBOARD);
  for (int i=0; i < keys.length; i++) {
    CharSequence s=keys[i];
    for (int j=0; j < s.length(); j++) {
      KeyEvent[] events=chararcterMap.getEvents(new char[]{s.charAt(j)});
      for (      KeyEvent e : events) {
        mWebView.dispatchKeyEvent(e);
      }
    }
  }
}
