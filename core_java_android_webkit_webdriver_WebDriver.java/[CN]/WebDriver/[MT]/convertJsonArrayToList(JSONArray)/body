{
  List<Object> toReturn=Lists.newArrayList();
  for (int i=0; i < json.length(); i++) {
    try {
      toReturn.add(convertJsonToJavaObject(json.get(i)));
    }
 catch (    JSONException e) {
      throw new RuntimeException("Failed to parse JSON: " + json.toString(),e);
    }
  }
  return toReturn;
}
