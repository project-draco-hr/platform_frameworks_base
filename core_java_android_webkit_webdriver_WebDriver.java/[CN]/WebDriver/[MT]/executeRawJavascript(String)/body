{
  String result=executeCommand(CMD_EXECUTE_SCRIPT,"window.webdriver.resultReady(" + script + ")",JS_EXECUTION_TIMEOUT);
  try {
    JSONObject json=new JSONObject(result);
    throwIfError(json);
    Object value=json.get(VALUE);
    return convertJsonToJavaObject(value);
  }
 catch (  JSONException e) {
    throw new RuntimeException("Failed to parse JavaScript result: " + result.toString(),e);
  }
}
