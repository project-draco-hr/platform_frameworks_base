{
  StringBuilder toReturn=new StringBuilder("\"");
  for (int i=0; i < toWrap.length(); i++) {
    char c=toWrap.charAt(i);
    if (c == '\"') {
      toReturn.append("\\\"");
    }
 else     if (c == '\\') {
      toReturn.append("\\\\");
    }
 else {
      toReturn.append(c);
    }
  }
  toReturn.append("\"");
  return toReturn.toString();
}
