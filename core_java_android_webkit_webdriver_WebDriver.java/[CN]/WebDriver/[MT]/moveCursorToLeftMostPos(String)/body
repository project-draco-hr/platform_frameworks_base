{
  if (value != null && !value.equals("")) {
    long downTime=SystemClock.uptimeMillis();
    KeyEvent down=new KeyEvent(downTime,SystemClock.uptimeMillis(),KeyEvent.ACTION_DOWN,KeyEvent.KEYCODE_DPAD_RIGHT,0);
    KeyEvent up=new KeyEvent(downTime,SystemClock.uptimeMillis(),KeyEvent.ACTION_UP,KeyEvent.KEYCODE_DPAD_RIGHT,value.length());
    mWebView.dispatchKeyEvent(down);
    mWebView.dispatchKeyEvent(up);
  }
}
