{
  Log.v("TtsService","TtsService.setLanguage(" + lang + ", "+ country+ ", "+ variant+ ")");
  if (sIsKillingSynth) {
    Log.v("TtsService","killing synth:: aborting setLanguage()");
    return TextToSpeech.ERROR;
  }
  if (isDefaultEnforced()) {
    return sNativeSynth.setLanguage(getDefaultLanguage(),getDefaultCountry(),getDefaultLocVariant());
  }
 else {
    return sNativeSynth.setLanguage(lang,country,variant);
  }
}
