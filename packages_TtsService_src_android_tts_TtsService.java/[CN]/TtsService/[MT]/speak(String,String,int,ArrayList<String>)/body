{
  Log.v(SERVICE_TAG,"TTS service received " + text);
  if (queueMode == TextToSpeech.QUEUE_FLUSH) {
    stop(callingApp);
  }
 else   if (queueMode == 2) {
    stopAll(callingApp);
  }
  mSpeechQueue.add(new SpeechItem(callingApp,text,params,SpeechItem.TEXT));
  if (!mIsSpeaking) {
    processSpeechQueue();
  }
  return TextToSpeech.SUCCESS;
}
