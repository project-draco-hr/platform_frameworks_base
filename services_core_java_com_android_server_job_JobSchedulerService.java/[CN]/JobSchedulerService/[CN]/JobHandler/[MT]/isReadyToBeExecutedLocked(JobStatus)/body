{
  final boolean jobReady=job.isReady();
  final boolean jobPending=mPendingJobs.contains(job);
  final boolean jobActive=isCurrentlyActiveLocked(job);
  final boolean userRunning=mStartedUsers.contains(job.getUserId());
  if (DEBUG) {
    Slog.v(TAG,"isReadyToBeExecutedLocked: " + job.toShortString() + " ready="+ jobReady+ " pending="+ jobPending+ " active="+ jobActive+ " userRunning="+ userRunning);
  }
  return userRunning && jobReady && !jobPending&& !jobActive;
}
