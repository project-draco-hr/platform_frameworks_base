{
  if (backoffCount > 0 || idleCount >= MIN_IDLE_COUNT || connectivityCount >= MIN_CONNECTIVITY_COUNT || chargingCount >= MIN_CHARGING_COUNT || contentCount >= MIN_CONTENT_COUNT || (runnableJobs != null && runnableJobs.size() >= MIN_READY_JOBS_COUNT)) {
    if (DEBUG) {
      Slog.d(TAG,"maybeQueueReadyJobsForExecutionLockedH: Running jobs.");
    }
    noteJobsPending(runnableJobs);
    mPendingJobs.addAll(runnableJobs);
  }
 else {
    if (DEBUG) {
      Slog.d(TAG,"maybeQueueReadyJobsForExecutionLockedH: Not running anything.");
    }
  }
  reset();
}
