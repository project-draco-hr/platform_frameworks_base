{
  ArrayList<JobInfo> outList=new ArrayList<JobInfo>();
synchronized (mJobs) {
    ArraySet<JobStatus> jobs=mJobs.getJobs();
    for (int i=0; i < jobs.size(); i++) {
      JobStatus job=jobs.valueAt(i);
      if (job.getUid() == uid) {
        outList.add(job.getJob());
      }
    }
  }
  return outList;
}
