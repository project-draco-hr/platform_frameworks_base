{
synchronized (mJobs) {
    List<JobStatus> jobsForUser=mJobs.getJobsByUser(userHandle);
    for (int i=0; i < jobsForUser.size(); i++) {
      JobStatus toRemove=jobsForUser.get(i);
      if (DEBUG) {
        Slog.d(TAG,"Cancelling: " + toRemove);
      }
      cancelJobLocked(toRemove);
    }
  }
}
