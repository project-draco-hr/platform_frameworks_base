{
  if (codes != null) {
    try {
      int mcc=Integer.parseInt(codes.substring(0,3));
      int mnc=Integer.parseInt(codes.substring(3));
      if (homeValues) {
        homeMobileCountryCode=mcc;
        homeMobileNetworkCode=mnc;
      }
 else {
        mobileCountryCode=mcc;
        mobileNetworkCode=mnc;
      }
    }
 catch (    IndexOutOfBoundsException ex) {
      Log.e(TAG,"AndroidRadioDataProvider: Invalid operator numeric data: " + ex);
    }
catch (    NumberFormatException ex) {
      Log.e(TAG,"AndroidRadioDataProvider: Operator numeric format error: " + ex);
    }
  }
}
