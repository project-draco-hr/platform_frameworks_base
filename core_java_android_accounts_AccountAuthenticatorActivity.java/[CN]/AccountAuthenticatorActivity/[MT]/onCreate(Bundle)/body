{
  super.onCreate(icicle);
  if (icicle == null) {
    Intent intent=getIntent();
    mAccountAuthenticatorResponse=intent.getParcelableExtra(AccountManager.KEY_ACCOUNT_AUTHENTICATOR_RESPONSE);
  }
 else {
    mAccountAuthenticatorResponse=icicle.getParcelable(AccountManager.KEY_ACCOUNT_AUTHENTICATOR_RESPONSE);
  }
  if (mAccountAuthenticatorResponse != null) {
    mAccountAuthenticatorResponse.onRequestContinued();
  }
}
