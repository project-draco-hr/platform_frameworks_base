{
  super.onCreate(icicle);
  if (icicle == null) {
    Intent intent=getIntent();
    mAccountAuthenticatorResponse=intent.getParcelableExtra(Constants.ACCOUNT_AUTHENTICATOR_RESPONSE_KEY);
  }
 else {
    mAccountAuthenticatorResponse=icicle.getParcelable(Constants.ACCOUNT_AUTHENTICATOR_RESPONSE_KEY);
  }
  if (mAccountAuthenticatorResponse != null) {
    mAccountAuthenticatorResponse.onRequestContinued();
  }
}
