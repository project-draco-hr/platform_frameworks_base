{
  if (!path.hasNext()) {
    return null;
  }
  String tag=path.next();
  OMANode child;
  if (tag.equals("?")) {
    child=mChildren.getSingletonValue();
  }
 else {
    child=mChildren.get(tag);
  }
  if (child == null) {
    return null;
  }
 else   if (path.hasNext()) {
    return child.getListValue(path);
  }
 else {
    return child;
  }
}
