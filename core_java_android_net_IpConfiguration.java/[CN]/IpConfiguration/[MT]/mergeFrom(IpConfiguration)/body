{
  if (source == null) {
    Log.e(TAG,"source is null");
    return;
  }
  linkProperties=source.linkProperties;
  if (source.ipAssignment != IpAssignment.UNASSIGNED) {
    ipAssignment=source.ipAssignment;
  }
  if (source.proxySettings != ProxySettings.UNASSIGNED) {
    proxySettings=source.proxySettings;
  }
}
