{
  if (transferInts) {
    setPixels(0,updateFromScanline,imageWidth,numScanlines,cm,intOut,updateFromScanline * imageWidth,imageWidth);
  }
 else {
    setPixels(0,updateFromScanline,imageWidth,numScanlines,cm,byteOut,updateFromScanline * imageWidth * dataElementsPerPixel,imageWidth * dataElementsPerPixel);
  }
}
