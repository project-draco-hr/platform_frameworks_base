{
  if (!(in instanceof BackupDataInputStream)) {
    throw new IllegalStateException("input stream must be a BackupDataInputStream");
  }
  int result=-1;
  File parent=f.getParentFile();
  parent.mkdirs();
  result=writeFile_native(mPtr,f.getAbsolutePath(),((BackupDataInputStream)in).mData.mBackupReader);
  if (result != 0) {
    if (!mExceptionLogged) {
      Log.e(TAG,"Failed restoring file '" + f + "' for app '"+ mContext.getPackageName()+ "\' result=0x"+ Integer.toHexString(result));
      mExceptionLogged=true;
    }
  }
}
