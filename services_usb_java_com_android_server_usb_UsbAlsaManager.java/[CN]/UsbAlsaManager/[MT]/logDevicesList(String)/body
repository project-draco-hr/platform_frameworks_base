{
  if (DEBUG) {
    for (    HashMap.Entry<UsbDevice,UsbAudioDevice> entry : mAudioDevices.entrySet()) {
      Slog.i(TAG,"UsbDevice-------------------");
      Slog.i(TAG,"" + (entry != null ? entry.getKey() : "[none]"));
      Slog.i(TAG,"UsbAudioDevice--------------");
      Slog.i(TAG,"" + entry.getValue());
    }
  }
}
