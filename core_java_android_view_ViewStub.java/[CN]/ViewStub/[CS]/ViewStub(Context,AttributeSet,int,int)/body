{
  TypedArray a=context.obtainStyledAttributes(attrs,com.android.internal.R.styleable.ViewStub,defStyleAttr,defStyleRes);
  mInflatedId=a.getResourceId(R.styleable.ViewStub_inflatedId,NO_ID);
