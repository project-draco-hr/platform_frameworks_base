{
  boolean changed=false;
  Iterator<TaskStatus> it=mTasksSet.iterator();
  while (it.hasNext()) {
    TaskStatus ts=it.next();
    if (ts.getUid() == uid && ts.getTaskId() == taskId) {
      it.remove();
      changed=true;
    }
  }
  if (changed) {
    maybeWriteStatusToDiskAsync();
  }
  return changed;
}
