{
synchronized (mSyncRoot) {
    if (shouldRegisterNonEssentialDisplayAdaptersLocked()) {
      registerDisplayAdapterLocked(new OverlayDisplayAdapter(mSyncRoot,mContext,mHandler,mDisplayAdapterListener,mUiHandler));
      mWifiDisplayAdapter=new WifiDisplayAdapter(mSyncRoot,mContext,mHandler,mDisplayAdapterListener);
      registerDisplayAdapterLocked(mWifiDisplayAdapter);
    }
  }
}
