{
  if (Build.VERSION.SDK_INT >= Build.VERSION_CODES.JELLY_BEAN_MR1) {
    TextView view=mBinder.getTextDrawableStartEnd();
    assertEquals(mBindingObject.getDrawableStart(),((ColorDrawable)view.getCompoundDrawablesRelative()[0]).getColor());
    assertEquals(mBindingObject.getDrawableEnd(),((ColorDrawable)view.getCompoundDrawablesRelative()[2]).getColor());
    changeValues();
    assertEquals(mBindingObject.getDrawableStart(),((ColorDrawable)view.getCompoundDrawablesRelative()[0]).getColor());
    assertEquals(mBindingObject.getDrawableEnd(),((ColorDrawable)view.getCompoundDrawablesRelative()[2]).getColor());
  }
}
