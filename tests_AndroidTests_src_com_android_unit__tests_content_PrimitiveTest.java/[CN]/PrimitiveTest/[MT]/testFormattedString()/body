{
  checkString(R.string.formattedStringNone,mResources.getString(R.string.formattedStringNone),"Format[]");
  checkString(R.string.formattedStringOne,mResources.getString(R.string.formattedStringOne),"Format[%d]");
  checkString(R.string.formattedStringTwo,mResources.getString(R.string.formattedStringTwo),"Format[%3$d,%2$s]");
  checkString(R.string.formattedStringNone,mResources.getString(R.string.formattedStringNone),"Format[]");
  checkString(R.string.formattedStringOne,mResources.getString(R.string.formattedStringOne,42),"Format[42]");
  checkString(R.string.formattedStringTwo,mResources.getString(R.string.formattedStringTwo,"unused","hi",43),"Format[43,hi]");
}
