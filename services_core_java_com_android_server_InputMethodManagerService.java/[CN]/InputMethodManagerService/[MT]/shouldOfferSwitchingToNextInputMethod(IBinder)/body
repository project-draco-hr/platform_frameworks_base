{
  if (!calledFromValidUser()) {
    return false;
  }
synchronized (mMethodMap) {
    final ImeSubtypeListItem nextSubtype=mSwitchingController.getNextInputMethodLocked(false,mMethodMap.get(mCurMethodId),mCurrentSubtype);
    if (nextSubtype == null) {
      return false;
    }
    return true;
  }
}
