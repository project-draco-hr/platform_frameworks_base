{
  pw.println("Conn.");
  pw.println("connected: " + mNetworkConnected + " unmetered: "+ mNetworkUnmetered);
  for (  TaskStatus ts : mTrackedTasks) {
    pw.println(String.valueOf(ts.hashCode()).substring(0,3) + ".." + ": C="+ ts.hasConnectivityConstraint()+ ", UM="+ ts.hasUnmeteredConstraint());
  }
}
