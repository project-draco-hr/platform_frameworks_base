{
switch (nativeType) {
case TYPE_BYTE:
    return Byte.TYPE;
case TYPE_INT32:
  return Integer.TYPE;
case TYPE_FLOAT:
return Float.TYPE;
case TYPE_INT64:
return Long.TYPE;
case TYPE_DOUBLE:
return Double.TYPE;
case TYPE_RATIONAL:
return Rational.class;
}
throw new UnsupportedOperationException("Unknown type, can't map to Java type " + nativeType);
}
