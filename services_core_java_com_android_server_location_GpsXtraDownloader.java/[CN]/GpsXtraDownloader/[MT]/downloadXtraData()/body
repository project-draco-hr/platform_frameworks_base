{
  byte[] result=null;
  int startIndex=mNextServerIndex;
  if (mXtraServers == null) {
    return null;
  }
  while (result == null) {
    result=doDownload(mXtraServers[mNextServerIndex]);
    mNextServerIndex++;
    if (mNextServerIndex == mXtraServers.length) {
      mNextServerIndex=0;
    }
    if (mNextServerIndex == startIndex)     break;
  }
  return result;
}
