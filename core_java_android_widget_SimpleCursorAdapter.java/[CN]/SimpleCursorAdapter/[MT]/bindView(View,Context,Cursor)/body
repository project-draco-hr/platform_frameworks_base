{
  for (int i=0; i < mTo.length; i++) {
    final View v=view.findViewById(mTo[i]);
    if (v != null) {
      String text=cursor.getString(mFrom[i]);
      if (text == null) {
        text="";
      }
      boolean bound=false;
      if (mViewBinder != null) {
        bound=mViewBinder.setViewValue(v,cursor,mFrom[i]);
      }
      if (!bound) {
        if (v instanceof TextView) {
          setViewText((TextView)v,text);
        }
 else         if (v instanceof ImageView) {
          setViewImage((ImageView)v,text);
        }
 else {
          throw new IllegalStateException(v.getClass().getName() + " is not a " + " view that can be bounds by this SimpleCursorAdapter");
        }
      }
    }
  }
}
