{
  super(context,density,"/bars/phone_system_bar.xml","phone_system_bar.xml");
  setGravity(mGravity | Gravity.RIGHT);
  setBackgroundColor(0xFF000000);
  loadIcon(1,"stat_sys_wifi_signal_4_fully.png",density);
