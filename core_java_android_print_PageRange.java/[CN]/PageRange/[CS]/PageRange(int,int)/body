{
  if (start < 0) {
    throw new IllegalArgumentException("start cannot be less than zero.");
  }
  if (end < 0) {
    throw new IllegalArgumentException("end cannot be less than zero.");
  }
  if (start > end) {
    throw new IllegalArgumentException("start must be lesser than end.");
  }
  mStart=start;
  mEnd=end;
}
