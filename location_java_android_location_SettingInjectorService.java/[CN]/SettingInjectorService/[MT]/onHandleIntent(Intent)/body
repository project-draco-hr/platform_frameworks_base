{
  String summary;
  try {
    summary=onGetSummary();
  }
 catch (  RuntimeException e) {
    sendStatus(intent,null,true);
    throw e;
  }
  boolean enabled;
  try {
    enabled=onGetEnabled();
  }
 catch (  RuntimeException e) {
    sendStatus(intent,summary,true);
    throw e;
  }
  sendStatus(intent,summary,enabled);
}
