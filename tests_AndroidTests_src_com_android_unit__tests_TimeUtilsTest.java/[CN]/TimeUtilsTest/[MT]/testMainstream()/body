{
  String[] mainstream=new String[]{"America/New_York","America/Chicago","America/Denver","America/Los_Angeles","America/Anchorage","Pacific/Honolulu"};
  for (  String name : mainstream) {
    TimeZone tz=TimeZone.getTimeZone(name);
    Calendar c=Calendar.getInstance(tz);
    TimeZone guess;
    c.set(2008,Calendar.OCTOBER,20,12,00,00);
    guess=guess(c,"us");
    assertEquals(name,guess.getID());
    c.set(2009,Calendar.JANUARY,20,12,00,00);
    guess=guess(c,"us");
    assertEquals(name,guess.getID());
  }
}
