{
  final int N=profiles.size();
  final StringBuffer buffer=new StringBuffer();
  buffer.append('[');
  for (int i=0; i < N; i++) {
    if (i != 0) {
      buffer.append(',');
    }
    buffer.append(BluetoothUtil.profileToString(profiles.keyAt(i)));
  }
  buffer.append(']');
  return buffer.toString();
}
