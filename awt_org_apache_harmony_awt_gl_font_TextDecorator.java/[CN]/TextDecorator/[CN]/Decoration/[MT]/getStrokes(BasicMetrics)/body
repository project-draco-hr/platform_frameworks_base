{
  if (!haveStrokes) {
    if (strikeThrough) {
      strikeThroughStroke=new BasicStroke(metrics.strikethroughThickness,BasicStroke.CAP_BUTT,BasicStroke.JOIN_MITER,10);
    }
    if (ulOn) {
      ulStroke=new BasicStroke(metrics.underlineThickness,BasicStroke.CAP_BUTT,BasicStroke.JOIN_MITER,10);
    }
    haveStrokes=true;
  }
}
