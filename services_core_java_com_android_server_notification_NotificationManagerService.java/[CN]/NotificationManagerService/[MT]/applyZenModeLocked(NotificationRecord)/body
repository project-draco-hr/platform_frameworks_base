{
  record.setIntercepted(mZenModeHelper.shouldIntercept(record));
  if (record.isIntercepted()) {
    int suppressed=(mZenModeHelper.shouldSuppressLight() ? SUPPRESSED_EFFECT_LIGHTS : 0) | (mZenModeHelper.shouldSuppressPeek() ? SUPPRESSED_EFFECT_PEEK : 0);
    record.setSuppressedVisualEffects(suppressed);
  }
}
