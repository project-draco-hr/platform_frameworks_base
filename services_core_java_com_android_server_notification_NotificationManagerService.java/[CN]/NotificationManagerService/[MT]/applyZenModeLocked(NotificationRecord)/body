{
  record.setIntercepted(mZenModeHelper.shouldIntercept(record));
  if (record.isIntercepted()) {
    int suppressed=(mZenModeHelper.shouldSuppressWhenScreenOff() ? SUPPRESSED_EFFECT_SCREEN_OFF : 0) | (mZenModeHelper.shouldSuppressWhenScreenOn() ? SUPPRESSED_EFFECT_SCREEN_ON : 0);
    record.setSuppressedVisualEffects(suppressed);
  }
}
