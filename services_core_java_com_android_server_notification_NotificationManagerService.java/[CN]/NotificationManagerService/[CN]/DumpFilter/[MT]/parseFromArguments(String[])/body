{
  if (args != null && args.length == 2 && "p".equals(args[0]) && args[1] != null && !args[1].trim().isEmpty()) {
    final DumpFilter filter=new DumpFilter();
    filter.pkgFilter=args[1].trim().toLowerCase();
    return filter;
  }
  if (args != null && args.length == 1 && "zen".equals(args[0])) {
    final DumpFilter filter=new DumpFilter();
    filter.zen=true;
    return filter;
  }
  return null;
}
