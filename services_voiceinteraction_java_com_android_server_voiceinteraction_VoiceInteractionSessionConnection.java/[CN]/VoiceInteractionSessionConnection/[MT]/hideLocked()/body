{
  if (mBound) {
    if (mShown) {
      mShown=false;
      mShowArgs=null;
      mShowFlags=0;
      mHaveAssistData=false;
      mAssistData=null;
      if (mSession != null) {
        try {
          mSession.hide();
        }
 catch (        RemoteException e) {
        }
      }
    }
    if (mFullyBound) {
      mContext.unbindService(mFullConnection);
      mFullyBound=false;
    }
    return true;
  }
  return false;
}
