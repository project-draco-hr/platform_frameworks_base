{
  mSession=session;
  mInteractor=interactor;
  if (mShown) {
    try {
      session.show(mShowArgs,mShowFlags);
      mShowArgs=null;
      mShowFlags=0;
    }
 catch (    RemoteException e) {
    }
    if (mHaveAssistData) {
      try {
        session.handleAssist(mAssistData);
        mAssistData=null;
        mHaveAssistData=false;
      }
 catch (      RemoteException e) {
      }
    }
  }
  return true;
}
