{
  try {
    List<PermissionGroupInfo> pgs=mPm.getAllPermissionGroups(0);
    int count=pgs.size();
    for (int p=0; p < count; p++) {
      PermissionGroupInfo pgi=pgs.get(p);
      System.out.print("permission group:");
      System.out.println(pgi.name);
    }
  }
 catch (  RemoteException e) {
  }
}
