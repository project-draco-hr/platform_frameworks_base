{
  TypedArray a=context.obtainStyledAttributes(com.android.internal.R.styleable.Theme);
  background=a.getResourceId(com.android.internal.R.styleable.Theme_panelBackground,0);
  fullBackground=a.getResourceId(com.android.internal.R.styleable.Theme_panelFullBackground,0);
  windowAnimations=a.getResourceId(com.android.internal.R.styleable.Theme_windowAnimationStyle,0);
  a.recycle();
}
