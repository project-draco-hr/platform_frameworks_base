{
  mPanelChordingKey=event.getKeyCode();
  PanelFeatureState st=getPanelState(featureId,true);
  if (!st.isOpen) {
    if (getContext().getResources().getConfiguration().keyboard == Configuration.KEYBOARD_NOKEYS) {
      mKeycodeMenuTimeoutHandler.removeMessages(MSG_MENU_LONG_PRESS);
      mKeycodeMenuTimeoutHandler.sendMessageDelayed(mKeycodeMenuTimeoutHandler.obtainMessage(MSG_MENU_LONG_PRESS),ViewConfiguration.getLongPressTimeout());
    }
    return preparePanel(st,event);
  }
  return false;
}
