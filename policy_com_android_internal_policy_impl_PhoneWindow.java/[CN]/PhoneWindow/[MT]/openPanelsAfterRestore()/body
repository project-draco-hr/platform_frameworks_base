{
  PanelFeatureState[] panels=mPanels;
  if (panels == null) {
    return;
  }
  PanelFeatureState st;
  for (int i=panels.length - 1; i >= 0; i--) {
    st=panels[i];
    if ((st != null) && !st.isOpen && st.wasLastOpen) {
      st.isInExpandedMode=st.wasLastExpanded;
      openPanel(st,null);
    }
  }
}
