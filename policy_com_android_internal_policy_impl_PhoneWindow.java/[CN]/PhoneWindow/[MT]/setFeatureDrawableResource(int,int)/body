{
  if (resId != 0) {
    DrawableFeatureState st=getDrawableState(featureId,true);
    if (st.resid != resId) {
      st.resid=resId;
      st.uri=null;
      st.local=getContext().getResources().getDrawable(resId);
      updateDrawable(featureId,st,false);
    }
  }
 else {
    setFeatureDrawable(featureId,null);
  }
}
