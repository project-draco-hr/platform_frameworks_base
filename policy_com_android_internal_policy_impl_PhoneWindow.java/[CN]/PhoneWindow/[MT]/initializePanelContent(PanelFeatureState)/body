{
  if (st.createdPanelView != null) {
    st.shownPanelView=st.createdPanelView;
    return true;
  }
  final MenuBuilder menu=(MenuBuilder)st.menu;
  if (menu == null) {
    return false;
  }
  st.shownPanelView=menu.getMenuView((st.isInExpandedMode) ? MenuBuilder.TYPE_EXPANDED : MenuBuilder.TYPE_ICON,st.decorView);
  if (st.shownPanelView != null) {
    final int defaultAnimations=((MenuView)st.shownPanelView).getWindowAnimations();
    if (defaultAnimations != 0) {
      st.windowAnimations=defaultAnimations;
    }
    return true;
  }
 else {
    return false;
  }
}
