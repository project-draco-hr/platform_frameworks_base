{
  if (mRsTexture != null) {
    return mRsTexture;
  }
  RenderScriptGL rs=SceneManager.getRS();
  Resources res=SceneManager.getRes();
  if (rs == null || res == null) {
    return null;
  }
  String shortName=mFileName.substring(mFileName.lastIndexOf('/') + 1);
  mRsTexture=SceneManager.loadCubemap(mFileDir + shortName,rs,res);
  return mRsTexture;
}
