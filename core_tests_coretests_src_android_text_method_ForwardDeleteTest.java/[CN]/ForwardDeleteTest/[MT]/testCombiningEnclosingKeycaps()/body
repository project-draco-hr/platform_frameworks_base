{
  EditorState state=new EditorState();
  state.setByString("| '1' U+20E3");
  forwardDelete(state,0);
  state.assertEquals("|");
  state.setByString("| '1' U+20E3 U+20E3");
  forwardDelete(state,0);
  state.assertEquals("|");
  state.setByString("| U+20E3");
  forwardDelete(state,0);
  state.assertEquals("|");
  state.setByString("| U+20E3 U+20E3");
  forwardDelete(state,0);
  state.assertEquals("|");
}
