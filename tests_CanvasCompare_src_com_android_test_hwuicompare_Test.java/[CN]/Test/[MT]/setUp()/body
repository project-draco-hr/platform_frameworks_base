{
  super.setUp();
  mBundle=new Bundle();
  mActivity=getActivity();
  mActivity.setCallback(new TestCallback(){
    @Override void report(    String key,    float value){
      mBundle.putFloat(key,value);
    }
    @Override void complete(){
synchronized (mBundle) {
        mBundle.notify();
      }
    }
  }
);
}
