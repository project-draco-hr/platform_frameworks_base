{
  String strCallType;
  if (type == Calls.INCOMING_TYPE) {
    strCallType="INCOMING";
  }
 else   if (type == Calls.OUTGOING_TYPE) {
    strCallType="OUTGOING";
  }
 else   if (type == Calls.MISSED_TYPE) {
    strCallType="MISSED";
  }
 else {
    strCallType="";
  }
  timeStamp="X-IRMC-CALL-DATETIME;" + strCallType + ":"+ date;
}
