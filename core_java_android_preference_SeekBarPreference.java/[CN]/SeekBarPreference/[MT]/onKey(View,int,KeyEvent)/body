{
  if (event.getAction() != KeyEvent.ACTION_UP) {
    if (keyCode == KeyEvent.KEYCODE_PLUS || keyCode == KeyEvent.KEYCODE_EQUALS) {
      setProgress(getProgress() + 1);
      return true;
    }
    if (keyCode == KeyEvent.KEYCODE_MINUS) {
      setProgress(getProgress() - 1);
      return true;
    }
  }
  return false;
}
