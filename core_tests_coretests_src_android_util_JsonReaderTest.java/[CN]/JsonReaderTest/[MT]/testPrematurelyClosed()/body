{
  try {
    JsonReader reader=new JsonReader(new StringReader("{\"a\":[]}"));
    reader.beginObject();
    reader.close();
    reader.nextName();
    fail();
  }
 catch (  IllegalStateException expected) {
  }
  try {
    JsonReader reader=new JsonReader(new StringReader("{\"a\":[]}"));
    reader.close();
    reader.beginObject();
    fail();
  }
 catch (  IllegalStateException expected) {
  }
  try {
    JsonReader reader=new JsonReader(new StringReader("{\"a\":true}"));
    reader.beginObject();
    reader.nextName();
    reader.peek();
    reader.close();
    reader.nextBoolean();
    fail();
  }
 catch (  IllegalStateException expected) {
  }
}
