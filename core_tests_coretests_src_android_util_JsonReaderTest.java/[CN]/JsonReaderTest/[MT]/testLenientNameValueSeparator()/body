{
  JsonReader reader=new JsonReader(new StringReader("{\"a\"=true}"));
  reader.setLenient(true);
  reader.beginObject();
  assertEquals("a",reader.nextName());
  assertEquals(true,reader.nextBoolean());
  reader=new JsonReader(new StringReader("{\"a\"=>true}"));
  reader.setLenient(true);
  reader.beginObject();
  assertEquals("a",reader.nextName());
  assertEquals(true,reader.nextBoolean());
}
