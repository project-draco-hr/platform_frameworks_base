{
  JsonReader reader=new JsonReader(new StringReader("[// comment \n true]"));
  reader.beginArray();
  try {
    reader.nextBoolean();
    fail();
  }
 catch (  IOException expected) {
  }
  reader=new JsonReader(new StringReader("[# comment \n true]"));
  reader.beginArray();
  try {
    reader.nextBoolean();
    fail();
  }
 catch (  IOException expected) {
  }
  reader=new JsonReader(new StringReader("[/* comment */ true]"));
  reader.beginArray();
  try {
    reader.nextBoolean();
    fail();
  }
 catch (  IOException expected) {
  }
}
