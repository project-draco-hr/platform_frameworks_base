{
  String json="[-1,99,-0,0,0e1,0e+1,0e-1,0E1,0E+1,0E-1,0.0,1.0,-1.0,1.0e0,1.0e+1,1.0e-1]";
  JsonReader reader=new JsonReader(new StringReader(json));
  reader.beginArray();
  for (int i=0; i < 16; i++) {
    assertEquals(JsonToken.NUMBER,reader.peek());
    reader.nextDouble();
  }
  reader.endArray();
}
