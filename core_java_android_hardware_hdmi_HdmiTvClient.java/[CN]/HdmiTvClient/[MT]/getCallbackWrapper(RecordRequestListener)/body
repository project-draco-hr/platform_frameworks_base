{
  return new IHdmiRecordRequestListener.Stub(){
    @Override public byte[] onRecordRequestReceived(    int recorderAddress) throws RemoteException {
      HdmiRecordSources.RecordSource source=listener.onRecordRequestReceived(recorderAddress);
      if (source == null) {
        return EmptyArray.BYTE;
      }
      byte[] data=new byte[source.getDataSize(true)];
      source.toByteArray(true,data,0);
      return data;
    }
  }
;
}
