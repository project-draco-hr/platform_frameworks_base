{
  final int version=in.readInt();
switch (version) {
case VERSION_INIT:
{
      bucketDuration=in.readLong();
      bucketStart=readLongArray(in);
      rx=readLongArray(in);
      tx=readLongArray(in);
      bucketCount=bucketStart.length;
      break;
    }
default :
{
    throw new ProtocolException("unexpected version: " + version);
  }
}
}
