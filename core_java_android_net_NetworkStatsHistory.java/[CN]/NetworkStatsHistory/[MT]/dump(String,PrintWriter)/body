{
  pw.print(prefix);
  pw.print("NetworkStatsHistory: bucketDuration=");
  pw.println(bucketDuration);
  for (int i=0; i < bucketCount; i++) {
    pw.print(prefix);
    pw.print("  bucketStart=");
    pw.print(bucketStart[i]);
    pw.print(" rx=");
    pw.print(rx[i]);
    pw.print(" tx=");
    pw.println(tx[i]);
  }
}
