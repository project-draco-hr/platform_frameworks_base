{
  pw.print(prefix);
  pw.print("NetworkStatsHistory: networkType=");
  pw.print(networkType);
  pw.print(" identity=");
  pw.print(identity);
  pw.print(" uid=");
  pw.println(uid);
  for (int i=0; i < bucketCount; i++) {
    pw.print(prefix);
    pw.print("  timestamp=");
    pw.print(bucketStart[i]);
    pw.print(" rx=");
    pw.print(rx[i]);
    pw.print(" tx=");
    pw.println(tx[i]);
  }
}
