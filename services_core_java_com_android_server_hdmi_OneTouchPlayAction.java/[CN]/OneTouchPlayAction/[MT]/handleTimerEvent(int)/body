{
  if (mState != state) {
    return;
  }
  if (state == STATE_WAITING_FOR_REPORT_POWER_STATUS) {
    if (mPowerStatusCounter++ < LOOP_COUNTER_MAX) {
      queryDevicePowerStatus();
      addTimer(mState,FeatureAction.TIMEOUT_MS);
    }
 else {
      invokeCallback(HdmiCec.RESULT_TIMEOUT);
      finish();
    }
  }
}
