{
  if (mIsDoCoMo) {
    builder.append(VCardConstants.PARAM_TYPE_VOICE);
  }
 else {
    String phoneType=VCardUtils.getPhoneTypeString(type);
    if (phoneType != null) {
      appendTypeParameter(phoneType);
    }
 else {
      Log.e(LOG_TAG,"Unknown or unsupported (by vCard) Phone type: " + type);
    }
  }
}
