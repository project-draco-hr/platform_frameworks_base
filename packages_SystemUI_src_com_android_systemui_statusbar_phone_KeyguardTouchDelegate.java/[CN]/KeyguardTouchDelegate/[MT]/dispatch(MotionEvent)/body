{
  final IKeyguardService service=mService;
  if (service != null) {
    try {
      service.dispatch(event);
      return true;
    }
 catch (    RemoteException e) {
      Slog.e(TAG,"RemoteException sending event to keyguard!",e);
    }
  }
 else {
    Slog.w(TAG,"dispatch(event): NO SERVICE!");
  }
  return false;
}
