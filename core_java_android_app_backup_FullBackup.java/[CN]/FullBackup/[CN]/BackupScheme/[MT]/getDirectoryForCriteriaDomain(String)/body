{
  if (TextUtils.isEmpty(domain)) {
    return null;
  }
  if ("file".equals(domain)) {
    return FILES_DIR;
  }
 else   if ("database".equals(domain)) {
    return DATABASE_DIR;
  }
 else   if ("root".equals(domain)) {
    return ROOT_DIR;
  }
 else   if ("sharedpref".equals(domain)) {
    return SHAREDPREF_DIR;
  }
 else   if ("external".equals(domain)) {
    return EXTERNAL_DIR;
  }
 else {
    return null;
  }
}
