{
  if (DEBUG_RECYCLE)   mStack=null;
  freeBuffer();
  final Parcel[] pool;
  if (mOwnsNativeParcelObject) {
    pool=sOwnedPool;
  }
 else {
    mNativePtr=0;
    pool=sHolderPool;
  }
synchronized (pool) {
    for (int i=0; i < POOL_SIZE; i++) {
      if (pool[i] == null) {
        pool[i]=this;
        return;
      }
    }
  }
}
