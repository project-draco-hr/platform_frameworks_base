{
  int insets=mWindowManagerFuncs.getDockedDividerInsetsLw();
  if (mNavigationBar != null && ((mNavigationBarOnBottom && win.getFrameLw().top + insets >= mNavigationBar.getFrameLw().top) || (!mNavigationBarOnBottom && win.getFrameLw().left + insets >= mNavigationBar.getFrameLw().left))) {
    return 0;
  }
  if (transit == TRANSIT_ENTER || transit == TRANSIT_SHOW) {
    return R.anim.fade_in;
  }
 else   if (transit == TRANSIT_EXIT) {
    return R.anim.fade_out;
  }
 else {
    return 0;
  }
}
