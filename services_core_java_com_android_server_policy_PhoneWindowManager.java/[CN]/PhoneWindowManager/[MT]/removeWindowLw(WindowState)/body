{
  if (mStatusBar == win) {
    mStatusBar=null;
    mStatusBarController.setWindow(null);
    mKeyguardDelegate.showScrim();
  }
 else   if (mKeyguardScrim == win) {
    Log.v(TAG,"Removing keyguard scrim");
    mKeyguardScrim=null;
  }
  if (mNavigationBar == win) {
    mNavigationBar=null;
    mNavigationBarController.setWindow(null);
  }
}
