{
  boolean bindKeyguardNow=false;
synchronized (mLock) {
    if (mKeyguardDelegate != null) {
      bindKeyguardNow=true;
    }
 else {
      mDeferBindKeyguard=true;
    }
  }
  if (bindKeyguardNow) {
    mKeyguardDelegate.bindService(mContext);
    mKeyguardDelegate.onBootCompleted();
  }
synchronized (mLock) {
    mSystemBooted=true;
  }
  startedWakingUp();
  screenTurningOn(null);
}
