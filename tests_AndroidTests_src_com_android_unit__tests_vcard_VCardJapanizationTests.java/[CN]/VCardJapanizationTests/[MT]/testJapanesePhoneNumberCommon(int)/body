{
  VCardVerifier verifier=new VCardVerifier(vcardType);
  ContactEntry entry=verifier.addInputEntry();
  entry.buildData(Phone.CONTENT_ITEM_TYPE).put(Phone.NUMBER,"0312341234").put(Phone.TYPE,Phone.TYPE_HOME);
  entry.buildData(Phone.CONTENT_ITEM_TYPE).put(Phone.NUMBER,"09012341234").put(Phone.TYPE,Phone.TYPE_MOBILE);
  verifier.addPropertyNodesVerifierElemWithEmptyName().addNodeWithoutOrder("TEL","03-1234-1234",new TypeSet("HOME")).addNodeWithoutOrder("TEL","090-1234-1234",new TypeSet("WORK"));
}
