{
  VCardVerifier verifier=new VCardVerifier(VCardConfig.VCARD_TYPE_V30_JAPANESE_SJIS);
  ContactEntry entry=verifier.addInputEntry();
  entry.buildData(StructuredName.CONTENT_ITEM_TYPE).put(StructuredName.FAMILY_NAME,"\u3075\u308B\u3069").put(StructuredName.GIVEN_NAME,"\u3091\u308A\u304B").put(StructuredName.MIDDLE_NAME,"B").put(StructuredName.PREFIX,"Dr.").put(StructuredName.SUFFIX,"Ph.D");
  verifier.addPropertyNodesVerifierElem().addNodeWithoutOrder("FN","Dr. \u3075\u308B\u3069 B \u3091\u308A\u304B Ph.D",mContentValuesForSJis).addNodeWithoutOrder("N","\u3075\u308B\u3069;\u3091\u308A\u304B;B;Dr.;Ph.D",Arrays.asList("\u3075\u308B\u3069","\u3091\u308A\u304B","B","Dr.","Ph.D"),null,mContentValuesForSJis,null,null);
  verifier.verify();
}
