{
  VCardVerifier verifier=new VCardVerifier(VCardConfig.VCARD_TYPE_DOCOMO);
  ContactEntry entry=verifier.addInputEntry();
  entry.buildData(StructuredName.CONTENT_ITEM_TYPE).put(StructuredName.FAMILY_NAME,"\u3075\u308B\u3069").put(StructuredName.GIVEN_NAME,"\u3091\u308A\u304B").put(StructuredName.MIDDLE_NAME,"B").put(StructuredName.PREFIX,"Dr.").put(StructuredName.SUFFIX,"Ph.D");
  final String fullName="Dr. \u3075\u308B\u3069 B \u3091\u308A\u304B Ph.D";
  verifier.addPropertyNodesVerifierElem().addNodeWithoutOrder("N",fullName + ";;;;",Arrays.asList(fullName,"","","",""),null,mContentValuesForSJis,null,null).addNodeWithoutOrder("FN",fullName,mContentValuesForSJis).addNodeWithoutOrder("SOUND",";;;;",new TypeSet("X-IRMC-N")).addNodeWithoutOrder("TEL","",new TypeSet("HOME")).addNodeWithoutOrder("EMAIL","",new TypeSet("HOME")).addNodeWithoutOrder("ADR","",new TypeSet("HOME")).addNodeWithoutOrder("X-CLASS","PUBLIC").addNodeWithoutOrder("X-REDUCTION","").addNodeWithoutOrder("X-NO","").addNodeWithoutOrder("X-DCM-HMN-MODE","");
  verifier.verify();
}
