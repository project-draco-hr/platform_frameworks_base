{
  if (DEBUG)   Slog.i(TAG,"Initiating bind of OBB service on " + this);
  Intent obbIntent=new Intent().setComponent(new ComponentName("com.android.sharedstoragebackup","com.android.sharedstoragebackup.ObbBackupService"));
  BackupManagerService.this.mContext.bindService(obbIntent,this,Context.BIND_AUTO_CREATE);
}
