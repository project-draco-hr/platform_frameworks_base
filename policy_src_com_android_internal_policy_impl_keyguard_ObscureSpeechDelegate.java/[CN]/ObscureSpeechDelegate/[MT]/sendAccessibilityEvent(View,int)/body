{
  super.sendAccessibilityEvent(host,eventType);
  if ((eventType == AccessibilityEvent.TYPE_VIEW_ACCESSIBILITY_FOCUSED) && !sAnnouncedHeadset && shouldObscureSpeech()) {
    sAnnouncedHeadset=true;
    host.announceForAccessibility(host.getContext().getString(R.string.keyboard_headset_required_to_hear_password));
  }
}
