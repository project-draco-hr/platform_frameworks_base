{
  String name=null;
  try {
switch (msg.what) {
case ON_CONNECTED:
      name="onConnected";
    onConnected();
  break;
case ON_REQUEST_CONDITIONS:
name="onRequestConditions";
onRequestConditions(msg.arg1);
break;
case ON_SUBSCRIBE:
name="onSubscribe";
onSubscribe((Uri)msg.obj);
break;
case ON_UNSUBSCRIBE:
name="onUnsubscribe";
onUnsubscribe((Uri)msg.obj);
break;
}
}
 catch (Throwable t) {
Log.w(TAG,"Error running " + name,t);
}
}
