{
  Cursor c=null;
  try {
    c=mMediaProvider.query(mPackageName,mObjectsUri,FORMAT_PROJECTION,ID_WHERE,new String[]{Integer.toString(handle)},null,null);
    if (c != null && c.moveToNext()) {
      return c.getInt(1);
    }
 else {
      return -1;
    }
  }
 catch (  RemoteException e) {
    Log.e(TAG,"RemoteException in getObjectFilePath",e);
    return -1;
  }
 finally {
    if (c != null) {
      c.close();
    }
  }
}
