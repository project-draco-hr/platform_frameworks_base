{
synchronized (mLock) {
    throwIfCursorIsClosed();
    CursorWindow window;
    if (mCursor instanceof AbstractWindowedCursor) {
      AbstractWindowedCursor windowedCursor=(AbstractWindowedCursor)mCursor;
      window=windowedCursor.getWindow();
      if (window == null) {
        window=new CursorWindow(mProviderName,false);
        windowedCursor.setWindow(window);
      }
      mCursor.moveToPosition(startPos);
    }
 else {
      window=mWindowForNonWindowedCursor;
      if (window == null) {
        window=new CursorWindow(mProviderName,false);
        mWindowForNonWindowedCursor=window;
      }
      mCursor.moveToPosition(startPos);
      if (!mWindowForNonWindowedCursorWasFilled || startPos < window.getStartPosition() || startPos >= window.getStartPosition() + window.getNumRows()) {
        mCursor.fillWindow(startPos,window);
        mWindowForNonWindowedCursorWasFilled=true;
      }
    }
    if (window != null) {
      window.acquireReference();
    }
    return window;
  }
}
