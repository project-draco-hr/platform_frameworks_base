{
  mCursor.moveToPosition(startPos);
  if (mWindow != null) {
    if (startPos < mWindow.getStartPosition() || startPos >= (mWindow.getStartPosition() + mWindow.getNumRows())) {
      mCursor.fillWindow(startPos,mWindow);
    }
    return mWindow;
  }
 else {
    return ((AbstractWindowedCursor)mCursor).getWindow();
  }
}
