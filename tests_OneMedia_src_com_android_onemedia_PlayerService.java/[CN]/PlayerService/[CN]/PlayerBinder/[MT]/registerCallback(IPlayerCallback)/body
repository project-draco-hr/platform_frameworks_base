{
  if (!mCbs.contains(cb)) {
    mCbs.add(cb);
    cb.asBinder().linkToDeath(new IBinder.DeathRecipient(){
      @Override public void binderDied(){
        mCbs.remove(cb);
      }
    }
,0);
  }
  try {
    cb.onSessionChanged(getSessionToken());
  }
 catch (  RemoteException e) {
    mCbs.remove(cb);
    throw e;
  }
}
