{
  View v;
  if (convertView == null) {
    v=mInflater.inflate(resource,parent,false);
    final int[] to=mTo;
    final int count=to.length;
    final View[] holder=new View[count];
    for (int i=0; i < count; i++) {
      holder[i]=v.findViewById(to[i]);
    }
    v.setTag(holder);
  }
 else {
    v=convertView;
  }
  bindView(position,v);
  return v;
}
