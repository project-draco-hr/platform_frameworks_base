{
  if ((container == null) || (vendorValues == null))   return false;
  for (  String value : vendorValues) {
    if (containsString(container,value) || containsString(container.toLowerCase(Locale.US),value.toLowerCase(Locale.US)) || containsString(container.toUpperCase(Locale.US),value.toUpperCase(Locale.US)))     return true;
  }
  return false;
}
