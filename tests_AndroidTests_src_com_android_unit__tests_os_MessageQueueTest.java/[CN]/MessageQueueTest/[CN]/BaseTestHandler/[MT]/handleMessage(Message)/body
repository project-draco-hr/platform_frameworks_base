{
  if (mCount <= mLastMessage) {
    if (msg.what != mCount) {
      failure(new RuntimeException("Expected message #" + mCount + ", received #"+ msg.what));
    }
 else     if (mCount == mLastMessage) {
      success();
    }
    mCount++;
  }
 else {
    failure(new RuntimeException("Message received after done, #" + msg.what));
  }
}
