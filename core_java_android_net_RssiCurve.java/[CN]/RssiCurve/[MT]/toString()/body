{
  StringBuilder sb=new StringBuilder();
  sb.append("RssiCurve[start=").append(start).append(",bucketWidth=").append(bucketWidth);
  sb.append(",buckets=");
  for (int i=0; i < rssiBuckets.length; i++) {
    sb.append(rssiBuckets[i]);
    if (i < rssiBuckets.length - 1) {
      sb.append(",");
    }
  }
  sb.append("]");
  return sb.toString();
}
