{
  if (dumpAll) {
    pw.print(prefix);
    pw.print("myID=");
    pw.print(Integer.toHexString(System.identityHashCode(this)));
    pw.print(" mCommonProcess=");
    pw.print(Integer.toHexString(System.identityHashCode(mCommonProcess)));
    pw.print(" mPackage=");
    pw.println(mPackage);
    if (mMultiPackage) {
      pw.print(prefix);
      pw.print("mMultiPackage=");
      pw.println(mMultiPackage);
    }
    if (this != mCommonProcess) {
      pw.print(prefix);
      pw.print("Common Proc: ");
      pw.print(mCommonProcess.mName);
      pw.print("/");
      pw.print(mCommonProcess.mUid);
      pw.print(" pkg=");
      pw.println(mCommonProcess.mPackage);
    }
  }
  if (mActive) {
    pw.print(prefix);
    pw.print("mActive=");
    pw.println(mActive);
  }
  if (mDead) {
    pw.print(prefix);
    pw.print("mDead=");
    pw.println(mDead);
  }
  if (mNumActiveServices != 0 || mNumStartedServices != 0) {
    pw.print(prefix);
    pw.print("mNumActiveServices=");
    pw.print(mNumActiveServices);
    pw.print(" mNumStartedServices=");
    pw.println(mNumStartedServices);
  }
}
