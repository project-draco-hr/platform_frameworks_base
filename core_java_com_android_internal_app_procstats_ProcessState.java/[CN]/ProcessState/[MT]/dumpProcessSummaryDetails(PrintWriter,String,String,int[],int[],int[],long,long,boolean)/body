{
  ProcessStats.ProcessDataCollection totals=new ProcessStats.ProcessDataCollection(screenStates,memStates,procStates);
  computeProcessData(totals,now);
  final double percentage=(double)totals.totalTime / (double)totalTime * 100;
  if (percentage >= 0.005 || totals.numPss != 0) {
    if (prefix != null) {
      pw.print(prefix);
    }
    if (label != null) {
      pw.print(label);
    }
    totals.print(pw,totalTime,full);
    if (prefix != null) {
      pw.println();
    }
  }
}
