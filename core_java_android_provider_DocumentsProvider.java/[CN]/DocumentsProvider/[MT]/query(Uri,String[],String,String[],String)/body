{
  try {
switch (mMatcher.match(uri)) {
case MATCH_ROOT:
      return queryRoots(projection);
case MATCH_RECENT:
    return queryRecentDocuments(getRootId(uri),projection);
case MATCH_DOCUMENT:
  return queryDocument(getDocumentId(uri),projection);
case MATCH_CHILDREN:
return queryChildDocuments(getDocumentId(uri),projection,sortOrder);
case MATCH_SEARCH:
return querySearchDocuments(getDocumentId(uri),getSearchDocumentsQuery(uri),projection);
default :
throw new UnsupportedOperationException("Unsupported Uri " + uri);
}
}
 catch (FileNotFoundException e) {
Log.w(TAG,"Failed during query",e);
return null;
}
}
