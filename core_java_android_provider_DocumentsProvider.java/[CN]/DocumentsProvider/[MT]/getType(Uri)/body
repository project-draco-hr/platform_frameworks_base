{
  try {
switch (mMatcher.match(uri)) {
case MATCH_ROOT:
      return DocumentsContract.Root.MIME_TYPE_ITEM;
case MATCH_DOCUMENT:
case MATCH_DOCUMENT_VIA:
    enforceVia(uri);
  return getDocumentType(getDocumentId(uri));
default :
return null;
}
}
 catch (FileNotFoundException e) {
Log.w(TAG,"Failed during getType",e);
return null;
}
}
