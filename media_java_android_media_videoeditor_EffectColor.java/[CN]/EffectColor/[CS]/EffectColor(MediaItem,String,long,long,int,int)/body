{
  super(mediaItem,effectId,startTimeMs,durationMs);
switch (type) {
case TYPE_COLOR:
case TYPE_GRADIENT:
{
      mColor=color;
      break;
    }
case TYPE_SEPIA:
case TYPE_NEGATIVE:
case TYPE_FIFTIES:
{
    mColor=-1;
    break;
  }
default :
{
  throw new IllegalArgumentException("Invalid type: " + type);
}
}
mType=type;
}
