{
  if (mBlockTouches) {
    return false;
  }
  initDownStates(event);
  if (mHeadsUpTouchHelper.onInterceptTouchEvent(event)) {
    mIsExpansionFromHeadsUp=true;
    return true;
  }
  if (!isFullyCollapsed() && onQsIntercept(event)) {
    return true;
  }
  return super.onInterceptTouchEvent(event);
}
