{
  if (mBlockTouches) {
    return false;
  }
  initDownStates(event);
  if (mHeadsUpTouchHelper.onInterceptTouchEvent(event)) {
    mExpansionIsFromHeadsUp=true;
    return true;
  }
  if (!isShadeCollapsed() && onQsIntercept(event)) {
    return true;
  }
  return super.onInterceptTouchEvent(event);
}
