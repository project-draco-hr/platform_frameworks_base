{
  mKeyguardStatusBar.animate().alpha(0f).setStartDelay(mStatusBar.getKeyguardFadingAwayDelay()).setDuration(mStatusBar.getKeyguardFadingAwayDuration() / 2).setInterpolator(PhoneStatusBar.ALPHA_OUT).setUpdateListener(mStatusBarAnimateAlphaListener).withEndAction(mAnimateKeyguardStatusBarInvisibleEndRunnable).start();
}
