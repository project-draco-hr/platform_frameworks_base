{
  mKeyguardStatusBar.animate().alpha(0f).setStartDelay(mStatusBar.isKeyguardFadingAway() ? mStatusBar.getKeyguardFadingAwayDelay() : 0).setDuration(mStatusBar.isKeyguardFadingAway() ? mStatusBar.getKeyguardFadingAwayDuration() / 2 : StackStateAnimator.ANIMATION_DURATION_STANDARD).setInterpolator(PhoneStatusBar.ALPHA_OUT).setUpdateListener(mStatusBarAnimateAlphaListener).withEndAction(mAnimateKeyguardStatusBarInvisibleEndRunnable).start();
}
