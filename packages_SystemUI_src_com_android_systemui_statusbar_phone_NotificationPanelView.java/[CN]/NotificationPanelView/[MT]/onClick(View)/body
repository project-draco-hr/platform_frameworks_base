{
  if (v == mHeader) {
    onQsExpansionStarted();
    if (mQsExpanded) {
      flingSettings(0,false,null,true);
    }
 else     if (mQsExpansionEnabled) {
      EventLogTags.writeSysuiLockscreenGesture(EventLogConstants.SYSUI_TAP_TO_OPEN_QS,0,0);
      flingSettings(0,true,null,true);
    }
  }
}
