{
  super.onExpandingFinished();
  mNotificationStackScroller.onExpansionStopped();
  mIsExpanding=false;
  mScrollYOverride=-1;
  if (mExpandedHeight == 0f) {
    mHeader.setListening(false);
    mQsPanel.setListening(false);
  }
 else {
    mHeader.setListening(true);
    mQsPanel.setListening(true);
  }
}
