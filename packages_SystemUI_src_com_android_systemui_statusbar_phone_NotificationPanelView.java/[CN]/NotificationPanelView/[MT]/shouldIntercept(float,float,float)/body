{
  if (!mQsExpansionEnabled) {
    return false;
  }
  View headerView=mStatusBar.getBarState() == StatusBarState.KEYGUARD && !mQsExpanded ? mKeyguardStatusView : mHeader;
  boolean onHeader=x >= headerView.getLeft() && x <= headerView.getRight() && y >= headerView.getTop() && y <= headerView.getBottom();
  if (mQsExpanded) {
    return onHeader || (mScrollView.isScrolledToBottom() && yDiff < 0);
  }
 else {
    return onHeader;
  }
}
