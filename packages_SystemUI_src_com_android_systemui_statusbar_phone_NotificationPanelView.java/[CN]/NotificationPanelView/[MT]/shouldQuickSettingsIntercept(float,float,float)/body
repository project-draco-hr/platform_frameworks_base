{
  if (!mQsExpansionEnabled) {
    return false;
  }
  boolean onHeader=x >= mHeader.getLeft() && x <= mHeader.getRight() && y >= mHeader.getTop() && y <= mHeader.getBottom();
  if (mQsExpanded) {
    return onHeader || (mScrollView.isScrolledToBottom() && yDiff < 0);
  }
 else {
    return onHeader;
  }
}
