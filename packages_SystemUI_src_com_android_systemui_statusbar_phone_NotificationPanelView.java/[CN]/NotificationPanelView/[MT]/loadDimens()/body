{
  super.loadDimens();
  mNotificationTopPadding=getResources().getDimensionPixelSize(R.dimen.notifications_top_padding);
  mMinStackHeight=getResources().getDimensionPixelSize(R.dimen.collapsed_stack_height);
  mFlingAnimationUtils=new FlingAnimationUtils(getContext(),0.4f);
  mStatusBarMinHeight=getResources().getDimensionPixelSize(com.android.internal.R.dimen.status_bar_height);
  mQsPeekHeight=getResources().getDimensionPixelSize(R.dimen.qs_peek_height);
  mClockPositionAlgorithm.loadDimens(getResources());
}
