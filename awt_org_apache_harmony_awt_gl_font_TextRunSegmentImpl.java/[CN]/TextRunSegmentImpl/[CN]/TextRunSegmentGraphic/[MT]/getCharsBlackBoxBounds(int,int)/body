{
  start-=this.start;
  limit-=this.start;
  if (limit > length) {
    limit=length;
  }
  Rectangle2D charBounds=ga.getBounds();
  charBounds.setRect(charBounds.getX() + ga.getAdvance() * start + x,charBounds.getY() + y,charBounds.getWidth() + ga.getAdvance() * (limit - start),charBounds.getHeight());
  return charBounds;
}
