{
  TextView view;
  if (convertView == null) {
    view=(TextView)mInflater.inflate(android.R.layout.simple_list_item_1,parent,false);
  }
 else {
    view=(TextView)convertView;
  }
  MtpStorageInfo info=mStorageList.get(position);
  if (info != null) {
    view.setText(info.getDescription());
  }
 else {
    view.setText("???");
  }
  return view;
}
