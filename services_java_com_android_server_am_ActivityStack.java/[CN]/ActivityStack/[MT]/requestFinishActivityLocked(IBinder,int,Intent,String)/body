{
  int index=indexOfTokenLocked(token);
  if (DEBUG_RESULTS)   Slog.v(TAG,"Finishing activity @" + index + ": token="+ token+ ", result="+ resultCode+ ", data="+ resultData);
  if (index < 0) {
    return false;
  }
  ActivityRecord r=mHistory.get(index);
  finishActivityLocked(r,index,resultCode,resultData,reason);
  return true;
}
