{
  for (int taskNdx=mTaskHistory.size() - 1; taskNdx >= 0; --taskNdx) {
    final TaskRecord task=mTaskHistory.get(taskNdx);
    pw.print("  Task ");
    pw.print(taskNdx);
    pw.print(": id #");
    pw.println(task.taskId);
    ActivityStackSupervisor.dumpHistoryList(fd,pw,mTaskHistory.get(taskNdx).mActivities,"    ","Hist",true,!dumpAll,dumpClient,dumpPackage);
  }
}
