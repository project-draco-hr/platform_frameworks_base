{
  int i=mHistory.size();
  while (i > 0) {
    i--;
    ActivityRecord candidate=mHistory.get(i);
    if (candidate.finishing) {
      continue;
    }
    if (candidate.task.taskId != task) {
      break;
    }
    if (candidate.realActivity.equals(r.realActivity)) {
      return i;
    }
  }
  return -1;
}
