{
  Slog.w(TAG,"Mismatch! " + caller + "  mHistory="+ mHistory);
  ArrayList<ArrayList<ActivityRecord>> nestedRecords=new ArrayList<ArrayList<ActivityRecord>>();
  for (  TaskRecord task : mTaskHistory) {
    nestedRecords.add(task.mActivities);
  }
  Slog.w(TAG,"Mismatch! " + caller + " mTaskHistory"+ nestedRecords);
  Slog.w(TAG,"Mismatch! " + caller + " lastHistoryModifier="+ mLastHistoryModifier+ " Caller="+ Debug.getCallers(4));
  if (rebuild) {
    rebuildTaskHistory();
  }
}
