{
  ActivityRecord newAr=newIsInStackLocked(token);
  ActivityRecord r=ActivityRecord.forToken(token);
  if (mHistory.contains(r)) {
    if (VALIDATE_TASK_REPLACE && newAr != r)     Slog.w(TAG,"isInStackLocked: mismatch: newAr=" + newAr + " r="+ r);
    return r;
  }
  if (VALIDATE_TASK_REPLACE && newAr != null)   Slog.w(TAG,"isInStackLocked: mismatch: newAr!=null");
  return null;
}
