{
  if (VALIDATE_TOKENS) {
    validateAppTokensLocked();
  }
  if (mCurrentUser == userId) {
    return;
  }
  mCurrentUser=userId;
  int index=mTaskHistory.size();
  for (int i=0; i < index; ++i) {
    TaskRecord task=mTaskHistory.get(i);
    if (task.userId == userId) {
      mTaskHistory.remove(i);
      mTaskHistory.add(task);
      --index;
    }
  }
}
