{
  if (colorChannel < 0 || colorChannel >= COUNT) {
    throw new IllegalArgumentException("Color channel out of range");
  }
switch (colorChannel) {
case RED:
    return mRed;
case GREEN_EVEN:
  return mGreenEven;
case GREEN_ODD:
return mGreenOdd;
case BLUE:
return mBlue;
default :
throw new AssertionError("Unhandled case " + colorChannel);
}
}
