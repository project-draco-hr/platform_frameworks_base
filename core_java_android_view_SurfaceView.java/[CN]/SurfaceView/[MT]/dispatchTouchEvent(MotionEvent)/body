{
  if (mScaled) {
    MotionEvent scaledBack=MotionEvent.obtain(event);
    mTranslator.translateEventInScreenToAppWindow(event);
    try {
      return super.dispatchTouchEvent(scaledBack);
    }
  finally {
      scaledBack.recycle();
    }
  }
 else {
    return super.dispatchTouchEvent(event);
  }
}
