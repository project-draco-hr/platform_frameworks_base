{
  StringBuilder sb=new StringBuilder();
  sb.append("Audio");
  if (isAudioOnly(videoState)) {
    sb.append(" Only");
  }
 else {
    if (isTransmissionEnabled(videoState)) {
      sb.append(" Tx");
    }
    if (isReceptionEnabled(videoState)) {
      sb.append(" Rx");
    }
    if (isPaused(videoState)) {
      sb.append(" Pause");
    }
  }
  return sb.toString();
}
