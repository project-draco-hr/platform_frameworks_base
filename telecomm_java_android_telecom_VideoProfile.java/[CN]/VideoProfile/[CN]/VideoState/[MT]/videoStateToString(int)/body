{
  StringBuilder sb=new StringBuilder();
  sb.append("Audio");
  if (VideoProfile.VideoState.isTransmissionEnabled(videoState)) {
    sb.append(" Tx");
  }
  if (VideoProfile.VideoState.isReceptionEnabled(videoState)) {
    sb.append(" Rx");
  }
  if (VideoProfile.VideoState.isPaused(videoState)) {
    sb.append(" Pause");
  }
  return sb.toString();
}
