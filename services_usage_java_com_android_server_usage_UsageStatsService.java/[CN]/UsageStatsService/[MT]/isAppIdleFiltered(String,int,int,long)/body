{
  final UserUsageStatsService userService;
  final long screenOnTime;
synchronized (mLock) {
    if (timeNow == -1) {
      timeNow=checkAndGetTimeLocked();
    }
    userService=getUserDataAndInitializeIfNeededLocked(userId,timeNow);
    screenOnTime=getScreenOnTimeLocked();
  }
  return isAppIdleFiltered(packageName,UserHandle.getAppId(uidForAppId),userId,userService,timeNow,screenOnTime);
}
