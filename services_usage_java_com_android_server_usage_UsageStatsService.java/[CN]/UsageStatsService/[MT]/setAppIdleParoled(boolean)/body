{
synchronized (mLock) {
    if (mAppIdleParoled != paroled) {
      mAppIdleParoled=paroled;
      if (DEBUG)       Slog.d(TAG,"Changing paroled to " + mAppIdleParoled);
      if (paroled) {
        mLastAppIdleParoledTime=checkAndGetTimeLocked();
        postNextParoleTimeout();
      }
      postParoleStateChanged();
    }
  }
}
