{
  if (approx) {
    if (relaxed)     mScript_approx_relaxed.invoke_init_filter(mInPixelsAllocation.getType().getX(),mInPixelsAllocation.getType().getY(),center_x,center_y,scale);
 else     mScript_approx_full.invoke_init_filter(mInPixelsAllocation.getType().getX(),mInPixelsAllocation.getType().getY(),center_x,center_y,scale);
  }
 else   if (relaxed)   mScript_relaxed.invoke_init_filter(mInPixelsAllocation.getType().getX(),mInPixelsAllocation.getType().getY(),center_x,center_y,scale);
 else   mScript_full.invoke_init_filter(mInPixelsAllocation.getType().getX(),mInPixelsAllocation.getType().getY(),center_x,center_y,scale);
}
