{
  if (isServer) {
    Log.d(TAG,"StopMonitor on server");
    String[] stopmonitorCmd={"/system/bin/am","startservice","-a","stop","com.google.android.experimental.svcmonitor/.SvcMonitor"};
    Process ps=new ProcessBuilder().command(stopmonitorCmd).redirectErrorStream(true).start();
  }
 else {
    Log.d(TAG,"No need to stop Svcmonitor on client");
  }
}
