{
  if (rs.mSampler_MIRRORED_REPEAT_NEAREST == null) {
synchronized (rs) {
      if (rs.mSampler_MIRRORED_REPEAT_NEAREST == null) {
        Builder b=new Builder(rs);
        b.setMinification(Value.NEAREST);
        b.setMagnification(Value.NEAREST);
