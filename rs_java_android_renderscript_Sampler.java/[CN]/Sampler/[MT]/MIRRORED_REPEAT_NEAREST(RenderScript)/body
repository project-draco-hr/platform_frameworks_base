        b.setWrapT(Value.MIRRORED_REPEAT);
        rs.mSampler_MIRRORED_REPEAT_NEAREST=b.create();
      }
    }
  }
  return rs.mSampler_MIRRORED_REPEAT_NEAREST;
}
