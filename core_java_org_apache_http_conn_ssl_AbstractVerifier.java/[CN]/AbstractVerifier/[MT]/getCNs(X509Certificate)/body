{
  DistinguishedNameParser dnParser=new DistinguishedNameParser(cert.getSubjectX500Principal());
  List<String> cnList=dnParser.getAllMostSpecificFirst("cn");
  if (!cnList.isEmpty()) {
    String[] cns=new String[cnList.size()];
    cnList.toArray(cns);
    return cns;
  }
 else {
    return null;
  }
}
