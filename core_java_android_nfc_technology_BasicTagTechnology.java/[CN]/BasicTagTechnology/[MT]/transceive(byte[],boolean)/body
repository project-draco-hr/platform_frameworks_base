{
  checkConnected();
  try {
    byte[] response=mTagService.transceive(mTag.getServiceHandle(),data,raw);
    if (response == null) {
      throw new IOException("transceive failed");
    }
    return response;
  }
 catch (  RemoteException e) {
    Log.e(TAG,"NFC service dead",e);
    throw new IOException("NFC service died");
  }
}
