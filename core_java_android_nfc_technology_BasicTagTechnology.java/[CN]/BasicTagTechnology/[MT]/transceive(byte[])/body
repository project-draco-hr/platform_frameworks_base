{
  try {
    byte[] response=mTagService.transceive(mTag.getServiceHandle(),data);
    if (response == null) {
      throw new IOException("transcieve failed");
    }
    return response;
  }
 catch (  RemoteException e) {
    attemptDeadServiceRecovery(e);
    throw new IOException("NFC service died");
  }
}
