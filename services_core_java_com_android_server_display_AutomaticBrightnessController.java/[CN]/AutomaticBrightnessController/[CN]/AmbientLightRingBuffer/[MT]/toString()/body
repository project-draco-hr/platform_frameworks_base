{
  StringBuffer buf=new StringBuffer();
  buf.append('[');
  for (int i=0; i < mCount; i++) {
    final long next=i + 1 < mCount ? getTime(i + 1) : SystemClock.uptimeMillis();
    if (i != 0) {
      buf.append(", ");
    }
    buf.append(getLux(i));
    buf.append(" / ");
    buf.append(next - getTime(i));
    buf.append("ms");
  }
  buf.append(']');
  return buf.toString();
}
