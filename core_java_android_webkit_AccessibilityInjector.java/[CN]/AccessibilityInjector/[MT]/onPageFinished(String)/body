{
  if (!isAccessibilityEnabled()) {
    mAccessibilityScriptInjected=false;
    toggleFallbackAccessibilityInjector(false);
    return;
  }
  if (!shouldInjectJavaScript(url)) {
    mAccessibilityScriptInjected=false;
    toggleFallbackAccessibilityInjector(true);
    if (DEBUG)     Log.d(TAG,"[" + mWebView.hashCode() + "] Using fallback accessibility support");
    return;
  }
  toggleFallbackAccessibilityInjector(false);
  if (!mAccessibilityScriptInjected) {
    mAccessibilityScriptInjected=true;
    final String injectionUrl=getScreenReaderInjectionUrl();
    mWebView.loadUrl(injectionUrl);
    if (DEBUG)     Log.d(TAG,"[" + mWebView.hashCode() + "] Loading screen reader into WebView");
  }
 else {
    if (DEBUG)     Log.w(TAG,"[" + mWebView.hashCode() + "] Attempted to inject screen reader twice");
  }
}
