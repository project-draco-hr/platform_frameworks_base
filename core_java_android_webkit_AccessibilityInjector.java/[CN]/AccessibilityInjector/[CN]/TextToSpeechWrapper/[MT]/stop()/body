{
synchronized (mTextToSpeech) {
    if (!mReady) {
      if (DEBUG) {
        Log.w(WRAP_TAG,"[" + hashCode() + "] Attempted to stop before initialize");
      }
      return TextToSpeech.ERROR;
    }
 else {
      if (DEBUG) {
        Log.i(WRAP_TAG,"[" + hashCode() + "] Stop called from JS binder");
      }
    }
    return mTextToSpeech.stop();
  }
}
