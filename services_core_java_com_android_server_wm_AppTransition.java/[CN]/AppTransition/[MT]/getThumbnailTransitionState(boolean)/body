{
  if (enter) {
    if (mNextAppTransitionScaleUp) {
      return THUMBNAIL_TRANSITION_ENTER_SCALE_UP;
    }
 else {
      return THUMBNAIL_TRANSITION_ENTER_SCALE_DOWN;
    }
  }
 else {
    if (mNextAppTransitionScaleUp) {
      return THUMBNAIL_TRANSITION_EXIT_SCALE_UP;
    }
 else {
      return THUMBNAIL_TRANSITION_EXIT_SCALE_DOWN;
    }
  }
}
