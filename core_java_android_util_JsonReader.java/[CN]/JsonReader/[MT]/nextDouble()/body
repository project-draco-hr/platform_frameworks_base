{
  quickPeek();
  if (value == null) {
    throw new IllegalStateException("Expected a double but was " + peek());
  }
  double result=Double.parseDouble(value);
  if ((result >= 1.0d && value.startsWith("0")) || Double.isNaN(result) || Double.isInfinite(result)) {
    throw new NumberFormatException("JSON forbids octal prefixes, NaN and infinities: " + value);
  }
  advance();
  return result;
}
