{
  if (firstElement) {
    replaceTop(JsonScope.NONEMPTY_ARRAY);
  }
 else {
switch (nextNonWhitespace()) {
case ']':
      pop();
    hasToken=true;
  return token=JsonToken.END_ARRAY;
case ';':
checkLenient();
case ',':
break;
default :
throw syntaxError("Unterminated array");
}
}
switch (nextNonWhitespace()) {
case ']':
if (firstElement) {
pop();
hasToken=true;
return token=JsonToken.END_ARRAY;
}
case ';':
case ',':
checkLenient();
pos--;
hasToken=true;
value="null";
return token=JsonToken.NULL;
default :
pos--;
return nextValue();
}
}
