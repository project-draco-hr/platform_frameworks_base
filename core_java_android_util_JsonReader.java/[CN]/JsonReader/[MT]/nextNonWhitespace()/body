{
  while (pos < limit || fillBuffer(1)) {
    int c=buffer[pos++];
switch (c) {
case '\t':
case ' ':
case '\n':
case '\r':
      continue;
case '/':
    if (pos == limit && !fillBuffer(1)) {
      return c;
    }
  checkLenient();
char peek=buffer[pos];
switch (peek) {
case '*':
pos++;
if (!skipTo("*/")) {
throw syntaxError("Unterminated comment");
}
pos+=2;
continue;
case '/':
pos++;
skipToEndOfLine();
continue;
default :
return c;
}
case '#':
checkLenient();
skipToEndOfLine();
continue;
default :
return c;
}
}
throw new EOFException("End of input");
}
