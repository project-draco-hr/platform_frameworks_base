{
  if (!mMutated && super.mutate() == this) {
    mLayerState=new LayerState(mLayerState,this,null);
    final ChildDrawable[] array=mLayerState.mChildren;
    final int N=mLayerState.mNum;
    for (int i=0; i < N; i++) {
      array[i].mDrawable.mutate();
    }
    mMutated=true;
  }
  return this;
}
