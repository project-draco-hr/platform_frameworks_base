{
  if (!mCheckedConstantState && mChildren != null) {
    mCanConstantState=true;
    final int N=mNum;
    for (int i=0; i < N; i++) {
      if (mChildren[i].mDrawable.getConstantState() == null) {
        mCanConstantState=false;
        break;
      }
    }
    mCheckedConstantState=true;
  }
  return mCanConstantState;
}
