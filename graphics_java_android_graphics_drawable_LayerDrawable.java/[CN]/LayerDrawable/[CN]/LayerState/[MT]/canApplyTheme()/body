{
  if (mThemeAttrs != null) {
    return true;
  }
  final ChildDrawable[] array=mChildren;
  final int N=mNum;
  for (int i=0; i < N; i++) {
    final ChildDrawable layer=array[i];
    if (layer.mThemeAttrs != null || layer.mDrawable.canApplyTheme()) {
      return true;
    }
  }
  return false;
}
