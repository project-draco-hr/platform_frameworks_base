{
switch (msg.what) {
case EVENT_RECORDS_LOADED:
    onRecordsLoaded();
  break;
case EVENT_NV_READY:
onNVReady();
break;
case EVENT_CDMA_DATA_DETACHED:
onCdmaDataAttached();
break;
case EVENT_DATA_STATE_CHANGED:
onDataStateChanged((AsyncResult)msg.obj);
break;
default :
super.handleMessage(msg);
break;
}
}
