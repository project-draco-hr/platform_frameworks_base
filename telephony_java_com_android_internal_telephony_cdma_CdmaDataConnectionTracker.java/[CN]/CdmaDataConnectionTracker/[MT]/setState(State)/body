{
  if (DBG)   log("setState: " + s);
  if (state != s) {
    if (s == State.INITING) {
      Checkin.updateStats(phone.getContext().getContentResolver(),Checkin.Stats.Tag.PHONE_CDMA_DATA_ATTEMPTED,1,0.0);
    }
    if (s == State.CONNECTED) {
      Checkin.updateStats(phone.getContext().getContentResolver(),Checkin.Stats.Tag.PHONE_CDMA_DATA_CONNECTED,1,0.0);
    }
  }
  state=s;
}
