{
  checkManageUsersPermission("query users");
synchronized (mUsersLock) {
    final int userSize=mUsers.size();
    for (int i=0; i < userSize; i++) {
      UserInfo ui=mUsers.valueAt(i).info;
      if (ui.isPrimary() && !mRemovingUserIds.get(ui.id)) {
        return ui;
      }
    }
  }
  return null;
}
