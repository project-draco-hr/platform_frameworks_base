{
  final int callingUid=Binder.getCallingUid();
  return UserHandle.isSameApp(callingUid,Process.SYSTEM_UID) || callingUid == Process.ROOT_UID || ActivityManager.checkComponentPermission(android.Manifest.permission.MANAGE_USERS,callingUid,-1,true) == PackageManager.PERMISSION_GRANTED || ActivityManager.checkComponentPermission(android.Manifest.permission.CREATE_USERS,callingUid,-1,true) == PackageManager.PERMISSION_GRANTED;
}
