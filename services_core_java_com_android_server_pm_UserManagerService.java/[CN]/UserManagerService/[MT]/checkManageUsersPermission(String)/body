{
  final int uid=Binder.getCallingUid();
  if (!UserHandle.isSameApp(uid,Process.SYSTEM_UID) && uid != Process.ROOT_UID && ActivityManager.checkComponentPermission(android.Manifest.permission.MANAGE_USERS,uid,-1,true) != PackageManager.PERMISSION_GRANTED) {
    throw new SecurityException("You need MANAGE_USERS permission to: " + message);
  }
}
