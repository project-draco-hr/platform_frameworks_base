{
  final String key=data.getKey();
  if (isKeyInList(key,mFiles)) {
    if (key.equals(WALLPAPER_IMAGE)) {
      File f=new File(STAGE_FILE);
      if (writeFile(f,data)) {
        BitmapFactory.Options options=new BitmapFactory.Options();
        options.inJustDecodeBounds=true;
        BitmapFactory.decodeFile(STAGE_FILE,options);
        if (DEBUG)         Slog.v(TAG,"Restoring wallpaper image w=" + options.outWidth + " h="+ options.outHeight);
        double widthRatio=mDesiredMinWidth / options.outWidth;
        double heightRatio=mDesiredMinHeight / options.outHeight;
        if (widthRatio > 0.8 && widthRatio < 1.25 && heightRatio > 0.8 && heightRatio < 1.25) {
          if (DEBUG)           Slog.v(TAG,"wallpaper dimension match; using");
          f.renameTo(new File(WALLPAPER_IMAGE));
        }
 else {
          if (DEBUG)           Slog.v(TAG,"dimensions too far off: wr=" + widthRatio + " hr="+ heightRatio);
          f.delete();
        }
      }
    }
 else {
      File f=new File(key);
      writeFile(f,data);
    }
  }
}
