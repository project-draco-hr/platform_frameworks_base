{
  if ("0".equals(s)) {
    return Surface.ROTATION_0;
  }
  if ("90".equals(s)) {
    return Surface.ROTATION_90;
  }
  if ("180".equals(s)) {
    return Surface.ROTATION_180;
  }
  if ("270".equals(s)) {
    return Surface.ROTATION_270;
  }
  throw new IllegalArgumentException("Unsupported surface rotation string '" + s + "'");
}
