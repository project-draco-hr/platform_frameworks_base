{
  if (mValueWaiting) {
    try {
      mField.set(mFilter,mValue);
    }
 catch (    IllegalAccessException e) {
      throw new RuntimeException("Access to field '" + mField.getName() + "' was denied!");
    }
    mValueWaiting=false;
    if (context != null) {
      mFilter.notifyFieldPortValueUpdated(mName,context);
    }
  }
}
