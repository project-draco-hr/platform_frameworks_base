{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (!(obj instanceof Geofence))   return false;
  Geofence other=(Geofence)obj;
  if (mRadius != other.mRadius)   return false;
  if (mLatitude != other.mLatitude)   return false;
  if (mLongitude != other.mLongitude)   return false;
  if (mType != other.mType)   return false;
  return true;
}
