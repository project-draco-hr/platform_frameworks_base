{
  Key<Byte> key=CameraCharacteristics.REQUEST_PIPELINE_MAX_DEPTH;
  Byte maxDepth=getValueFromKeyNonNull(key);
  if (maxDepth == null) {
    return REQUEST_PIPELINE_MAX_DEPTH_MAX;
  }
  checkTrueForKey(key," max pipeline depth should be no larger than " + REQUEST_PIPELINE_MAX_DEPTH_MAX,maxDepth <= REQUEST_PIPELINE_MAX_DEPTH_MAX);
  return maxDepth;
}
