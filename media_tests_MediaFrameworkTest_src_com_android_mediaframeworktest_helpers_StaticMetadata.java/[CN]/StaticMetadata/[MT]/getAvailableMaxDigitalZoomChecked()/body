{
  Key<Float> key=CameraCharacteristics.SCALER_AVAILABLE_MAX_DIGITAL_ZOOM;
  Float maxZoom=getValueFromKeyNonNull(key);
  if (maxZoom == null) {
    return 1.0f;
  }
  checkTrueForKey(key," max digital zoom should be no less than 1",maxZoom >= 1.0f && !Float.isNaN(maxZoom) && !Float.isInfinite(maxZoom));
  return maxZoom;
}
