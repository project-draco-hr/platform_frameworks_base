{
  Object array=getValueFromKeyNonNull(key);
  if (array == null) {
    return null;
  }
  if (size != IGNORE_SIZE_CHECK) {
    int actualLength=Array.getLength(array);
    if (actualLength != size) {
      failKeyCheck(key,String.format("had the wrong number of elements (%d), expected (%d)",actualLength,size));
      return null;
    }
  }
  @SuppressWarnings("unchecked") T val=(T)Array.get(array,element);
  if (val == null) {
    failKeyCheck(key,"had a null element at index" + element);
    return null;
  }
  return val;
}
