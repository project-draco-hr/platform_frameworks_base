{
  double sin=Math.sin(angle);
  double cos=Math.cos(angle);
  if (Math.abs(cos) < ZERO) {
    cos=0.0;
    sin=sin > 0.0 ? 1.0 : -1.0;
  }
 else   if (Math.abs(sin) < ZERO) {
    sin=0.0;
    cos=cos > 0.0 ? 1.0 : -1.0;
  }
  m00=m11=cos;
  m01=-sin;
  m10=sin;
  m02=m12=0.0;
  type=TYPE_UNKNOWN;
}
