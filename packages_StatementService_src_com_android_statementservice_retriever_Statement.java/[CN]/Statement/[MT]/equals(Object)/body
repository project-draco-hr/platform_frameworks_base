{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  Statement statement=(Statement)o;
  if (!mRelation.equals(statement.mRelation)) {
    return false;
  }
  if (!mTarget.equals(statement.mTarget)) {
    return false;
  }
  if (!mSource.equals(statement.mSource)) {
    return false;
  }
  return true;
}
