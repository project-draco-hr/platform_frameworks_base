{
switch (keyCode) {
case KeyEvent.KEYCODE_VOLUME_UP:
case KeyEvent.KEYCODE_VOLUME_DOWN:
    adjustSuggestedStreamVolume(keyCode == KeyEvent.KEYCODE_VOLUME_UP ? AudioManager.ADJUST_RAISE : AudioManager.ADJUST_LOWER,stream,AudioManager.FLAG_SHOW_UI | AudioManager.FLAG_VIBRATE);
  break;
case KeyEvent.KEYCODE_VOLUME_MUTE:
break;
}
}
