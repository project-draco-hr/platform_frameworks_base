{
  IAudioService service=getService();
  try {
    if (mUseMasterVolume) {
      return service.getMasterVolume();
    }
 else {
      return service.getLastAudibleStreamVolume(streamType);
    }
  }
 catch (  RemoteException e) {
    Log.e(TAG,"Dead object in getLastAudibleStreamVolume",e);
    return 0;
  }
}
