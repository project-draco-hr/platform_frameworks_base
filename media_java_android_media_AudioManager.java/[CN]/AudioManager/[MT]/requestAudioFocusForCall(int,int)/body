{
  IAudioService service=getService();
  try {
    service.requestAudioFocus(new AudioAttributes.Builder().setInternalLegacyStreamType(streamType).build(),durationHint,mICallBack,null,MediaFocusControl.IN_VOICE_COMM_FOCUS_ID,mApplicationContext.getOpPackageName(),AUDIOFOCUS_FLAG_LOCK,null);
  }
 catch (  RemoteException e) {
    Log.e(TAG,"Can't call requestAudioFocusForCall() on AudioService:",e);
  }
}
