{
  IAudioService service=getService();
  try {
    if (mUseMasterVolume) {
      return service.getMasterMaxVolume();
    }
 else {
      return service.getStreamMaxVolume(streamType);
    }
  }
 catch (  RemoteException e) {
    Log.e(TAG,"Dead object in getStreamMaxVolume",e);
    return 0;
  }
}
