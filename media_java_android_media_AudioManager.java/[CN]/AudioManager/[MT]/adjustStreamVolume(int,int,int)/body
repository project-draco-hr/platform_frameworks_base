{
  IAudioService service=getService();
  try {
    if (mUseMasterVolume) {
      service.adjustMasterVolume(direction,flags,mApplicationContext.getOpPackageName());
    }
 else {
      service.adjustStreamVolume(streamType,direction,flags,mApplicationContext.getOpPackageName());
    }
  }
 catch (  RemoteException e) {
    Log.e(TAG,"Dead object in adjustStreamVolume",e);
  }
}
