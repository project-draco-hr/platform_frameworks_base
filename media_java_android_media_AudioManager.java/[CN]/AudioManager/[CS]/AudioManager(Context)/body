{
  setContext(context);
  mUseMasterVolume=getContext().getResources().getBoolean(com.android.internal.R.bool.config_useMasterVolume);
  mUseVolumeKeySounds=getContext().getResources().getBoolean(com.android.internal.R.bool.config_useVolumeKeySounds);
  mUseFixedVolume=getContext().getResources().getBoolean(com.android.internal.R.bool.config_useFixedVolume);
  sAudioPortEventHandler.init();
}
