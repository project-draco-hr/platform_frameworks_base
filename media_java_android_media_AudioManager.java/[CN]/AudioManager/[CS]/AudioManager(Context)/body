{
  mContext=context;
  mUseMasterVolume=mContext.getResources().getBoolean(com.android.internal.R.bool.config_useMasterVolume);
  mUseVolumeKeySounds=mContext.getResources().getBoolean(com.android.internal.R.bool.config_useVolumeKeySounds);
  mUseFixedVolume=mContext.getResources().getBoolean(com.android.internal.R.bool.config_useFixedVolume);
  sAudioPortEventHandler.init();
}
