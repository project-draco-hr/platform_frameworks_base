{
  if (mTerminated) {
    Log.w(TAG,"updatePublish: called on terminated session");
    return;
  }
 else {
    WifiNanManager mgr=mMgr.get();
    if (mgr == null) {
      Log.w(TAG,"updatePublish: called post GC on WifiNanManager");
      return;
    }
    mgr.updatePublish(mSessionId,publishConfig);
  }
}
