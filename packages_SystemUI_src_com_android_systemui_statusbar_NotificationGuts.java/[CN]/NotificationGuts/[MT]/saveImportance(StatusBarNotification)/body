{
  int progress=mSeekBar.getProgress();
  try {
    if (mApplyToTopic.isChecked()) {
      mINotificationManager.setTopicImportance(sbn.getPackageName(),sbn.getUid(),mTopic,progress);
    }
 else {
      mINotificationManager.setAppImportance(sbn.getPackageName(),sbn.getUid(),progress);
    }
  }
 catch (  RemoteException e) {
  }
}
