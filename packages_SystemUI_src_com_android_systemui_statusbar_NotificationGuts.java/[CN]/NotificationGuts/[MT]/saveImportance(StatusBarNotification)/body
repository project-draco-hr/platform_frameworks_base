{
  int progress=mSeekBar.getProgress();
  MetricsLogger.action(mContext,MetricsEvent.ACTION_SAVE_IMPORTANCE,progress - mStartingImportance);
  try {
    mINotificationManager.setImportance(sbn.getPackageName(),sbn.getUid(),mApplyToTopic.isChecked() ? mTopic : null,progress);
  }
 catch (  RemoteException e) {
  }
}
