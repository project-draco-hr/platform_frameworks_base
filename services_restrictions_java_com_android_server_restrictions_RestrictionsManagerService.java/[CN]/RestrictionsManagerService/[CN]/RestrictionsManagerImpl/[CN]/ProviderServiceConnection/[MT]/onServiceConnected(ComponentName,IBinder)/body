{
  if (DEBUG) {
    Log.i(LOG_TAG,"connected to " + name);
  }
  mRestrictionsProvider=IRestrictionsProvider.Stub.asInterface(service);
  if (!mAbort) {
    try {
      run();
    }
 catch (    RemoteException re) {
      Log.w("RestrictionsProvider","Remote exception: " + re);
    }
  }
  close();
}
