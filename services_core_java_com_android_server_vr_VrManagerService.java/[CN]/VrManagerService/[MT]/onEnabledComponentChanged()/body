{
synchronized (mLock) {
    if (mCurrentVrService == null) {
      return;
    }
    consumeAndApplyPendingStateLocked();
    if (mCurrentVrService == null) {
      return;
    }
    updateCurrentVrServiceLocked(mVrModeEnabled,mCurrentVrService.getComponent(),mCurrentVrService.getUserId(),null);
  }
}
