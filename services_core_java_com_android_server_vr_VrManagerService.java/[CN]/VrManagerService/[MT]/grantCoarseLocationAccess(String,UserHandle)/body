{
  PackageManager pm=mContext.getPackageManager();
  boolean prev=(PackageManager.PERMISSION_GRANTED == pm.checkPermission(android.Manifest.permission.ACCESS_COARSE_LOCATION,pkg));
  mPreviousCoarseLocationPackage=null;
  if (!prev) {
    pm.grantRuntimePermission(pkg,android.Manifest.permission.ACCESS_COARSE_LOCATION,userId);
    mPreviousCoarseLocationPackage=pkg;
  }
}
