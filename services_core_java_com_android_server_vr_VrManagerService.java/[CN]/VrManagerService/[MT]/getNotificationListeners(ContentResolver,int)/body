{
  String flat=Settings.Secure.getStringForUser(resolver,Settings.Secure.ENABLED_NOTIFICATION_LISTENERS,userId);
  ArraySet<String> current=new ArraySet<>();
  if (flat != null) {
    String[] allowed=flat.split(":");
    for (    String s : allowed) {
      current.add(s);
    }
  }
  return current;
}
