{
  Resources res=view.getContext().getResources();
  Locale locale=res.getConfiguration().locale;
  AutoText instance;
synchronized (sLock) {
    instance=sInstance;
    if (!locale.equals(instance.mLocale)) {
      instance=new AutoText(res);
      sInstance=instance;
    }
  }
  return instance.lookup(src,start,end);
}
