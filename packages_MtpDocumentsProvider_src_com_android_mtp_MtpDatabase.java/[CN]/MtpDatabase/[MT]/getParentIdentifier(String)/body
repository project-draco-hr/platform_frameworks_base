{
  final Cursor cursor=mDatabase.query(TABLE_DOCUMENTS,strings(COLUMN_PARENT_DOCUMENT_ID),SELECTION_DOCUMENT_ID,strings(documentId),null,null,null,"1");
  try {
    if (cursor.moveToNext()) {
      return createIdentifier(cursor.getString(0));
    }
 else {
      throw new FileNotFoundException("Cannot find a row having ID = " + documentId);
    }
  }
  finally {
    cursor.close();
  }
}
