{
  Cursor c;
  String[] phoneNumbers=new String[2];
  phoneNumbers[0]="+79161234567";
  phoneNumbers[1]="89161234567";
  c=mDatabase.rawQuery("SELECT CASE WHEN PHONE_NUMBERS_EQUAL(?, ?) THEN 'equal' ELSE 'not equal' END",phoneNumbers);
  assertTrue(c.moveToFirst());
  assertEquals("equal",c.getString(0));
  c.close();
  phoneNumbers[0]="+33123456789";
  phoneNumbers[1]="0123456789";
  c=mDatabase.rawQuery("SELECT CASE WHEN PHONE_NUMBERS_EQUAL(?, ?) THEN 'equal' ELSE 'not equal' END",phoneNumbers);
  assertTrue(c.moveToFirst());
  assertEquals("equal",c.getString(0));
  c.close();
  phoneNumbers[0]="+31771234567";
  phoneNumbers[1]="0771234567";
  c=mDatabase.rawQuery("SELECT CASE WHEN PHONE_NUMBERS_EQUAL(?, ?) THEN 'equal' ELSE 'not equal' END",phoneNumbers);
  assertTrue(c.moveToFirst());
  assertEquals("equal",c.getString(0));
  c.close();
  phoneNumbers[0]="+66811234567";
  phoneNumbers[1]="166811234567";
  c=mDatabase.rawQuery("SELECT CASE WHEN PHONE_NUMBERS_EQUAL(?, ?) THEN 'equal' ELSE 'not equal' END",phoneNumbers);
  assertTrue(c.moveToFirst());
  assertEquals("equal",c.getString(0));
  c.close();
  phoneNumbers[0]="+33123456789";
  phoneNumbers[1]="+1123456789";
  c=mDatabase.rawQuery("SELECT CASE WHEN PHONE_NUMBERS_EQUAL(?, ?) THEN 'equal' ELSE 'not equal' END",phoneNumbers);
  assertTrue(c.moveToFirst());
  assertEquals("not equal",c.getString(0));
  c.close();
  phoneNumbers[0]="5125551212";
  phoneNumbers[1]="+15125551212";
  c=mDatabase.rawQuery("SELECT CASE WHEN PHONE_NUMBERS_EQUAL(?, ?) THEN 'equal' ELSE 'not equal' END",phoneNumbers);
  assertTrue(c.moveToFirst());
  assertEquals("equal",c.getString(0));
  c.close();
  phoneNumbers[0]="5125551212";
  phoneNumbers[1]="6505551212";
  c=mDatabase.rawQuery("SELECT CASE WHEN PHONE_NUMBERS_EQUAL(?, ?) THEN 'equal' ELSE 'not equal' END",phoneNumbers);
  assertTrue(c.moveToFirst());
  assertEquals("not equal",c.getString(0));
  c.close();
}
