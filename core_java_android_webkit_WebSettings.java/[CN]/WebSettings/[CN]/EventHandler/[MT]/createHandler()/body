{
  setRenderPriority();
  mHandler=new Handler(){
    @Override public void handleMessage(    Message msg){
switch (msg.what) {
case SYNC:
synchronized (WebSettings.this) {
          if (mBrowserFrame.mNativeFrame != 0) {
            nativeSync(mBrowserFrame.mNativeFrame);
          }
          mSyncPending=false;
        }
      break;
case PRIORITY:
{
      setRenderPriority();
      break;
    }
}
}
}
;
}
