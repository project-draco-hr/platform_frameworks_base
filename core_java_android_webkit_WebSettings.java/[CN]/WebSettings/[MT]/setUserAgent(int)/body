{
  String uaString=null;
  if (ua == 1) {
    if (DESKTOP_USERAGENT.equals(mUserAgent)) {
      return;
    }
 else {
      uaString=DESKTOP_USERAGENT;
    }
  }
 else   if (ua == 2) {
    if (IPHONE_USERAGENT.equals(mUserAgent)) {
      return;
    }
 else {
      uaString=IPHONE_USERAGENT;
    }
  }
 else   if (ua != 0) {
    return;
  }
  setUserAgentString(uaString);
}
