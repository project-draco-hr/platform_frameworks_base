{
  for (int i=mWaitingActivityLaunched.size() - 1; i >= 0; i--) {
    WaitResult w=mWaitingActivityLaunched.get(i);
    w.timeout=timeout;
    if (r != null) {
      w.who=new ComponentName(r.info.packageName,r.info.name);
    }
    w.thisTime=thisTime;
    w.totalTime=totalTime;
  }
  notify();
}
