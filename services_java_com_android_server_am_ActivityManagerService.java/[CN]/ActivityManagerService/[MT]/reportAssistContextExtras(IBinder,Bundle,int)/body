{
  PendingAssistExtras pae=(PendingAssistExtras)token;
synchronized (pae) {
    if (index == TOP_ACTIVITY_ASSIST_EXTRAS_INDEX) {
      pae.activityExtras=extras;
    }
 else {
      pae.servicesExtras[index]=extras;
      pae.numRespondedServices++;
    }
    pae.numPending--;
    if (pae.numPending == 0) {
      pae.notifyAll();
    }
  }
}
