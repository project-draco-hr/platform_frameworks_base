{
synchronized (this) {
    if (mStackSupervisor.getStack(relativeStackId) == null) {
      return -1;
    }
    int stackId=mStackSupervisor.createStack();
    mWindowManager.createStack(stackId,relativeStackId,position,weight);
    if (taskId > 0) {
      moveTaskToStack(taskId,stackId,true);
    }
    return stackId;
  }
}
