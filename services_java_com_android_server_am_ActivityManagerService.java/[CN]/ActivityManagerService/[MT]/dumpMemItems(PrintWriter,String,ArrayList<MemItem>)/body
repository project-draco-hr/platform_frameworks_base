{
  Collections.sort(items,new Comparator<MemItem>(){
    @Override public int compare(    MemItem lhs,    MemItem rhs){
      if (lhs.pss < rhs.pss) {
        return 1;
      }
 else       if (lhs.pss > rhs.pss) {
        return -1;
      }
      return 0;
    }
  }
);
  for (int i=0; i < items.size(); i++) {
    MemItem mi=items.get(i);
    pw.print(prefix);
    pw.printf("%8d Kb: ",mi.pss);
    pw.println(mi.label);
  }
}
