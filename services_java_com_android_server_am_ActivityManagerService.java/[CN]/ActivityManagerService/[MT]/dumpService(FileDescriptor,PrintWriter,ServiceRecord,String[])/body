{
  pw.println("------------------------------------------------------------" + "-------------------");
  pw.println("APP SERVICE: " + r.name.flattenToString());
  if (r.app != null && r.app.thread != null) {
    try {
      pw.flush();
      r.app.thread.dumpService(fd,r,args);
      pw.print("\n");
      pw.flush();
    }
 catch (    RemoteException e) {
      pw.println("got a RemoteException while dumping the service");
    }
  }
}
