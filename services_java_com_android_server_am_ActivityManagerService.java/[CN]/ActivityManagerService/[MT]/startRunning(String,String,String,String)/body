{
synchronized (this) {
    if (mStartRunning) {
      return;
    }
    mStartRunning=true;
    mTopComponent=pkg != null && cls != null ? new ComponentName(pkg,cls) : null;
    mTopAction=action != null ? action : Intent.ACTION_MAIN;
    mTopData=data;
    if (!mSystemReady) {
      return;
    }
  }
  systemReady();
}
