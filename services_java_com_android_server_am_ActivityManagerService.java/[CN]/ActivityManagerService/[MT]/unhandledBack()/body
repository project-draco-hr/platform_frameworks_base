{
  enforceCallingPermission(android.Manifest.permission.FORCE_BACK,"unhandledBack()");
synchronized (this) {
    int count=mMainStack.mHistory.size();
    if (DEBUG_SWITCH)     Slog.d(TAG,"Performing unhandledBack(): stack size = " + count);
    if (count > 1) {
      final long origId=Binder.clearCallingIdentity();
      mMainStack.finishActivityLocked((ActivityRecord)mMainStack.mHistory.get(count - 1),count - 1,Activity.RESULT_CANCELED,null,"unhandled-back",true);
      Binder.restoreCallingIdentity(origId);
    }
  }
}
