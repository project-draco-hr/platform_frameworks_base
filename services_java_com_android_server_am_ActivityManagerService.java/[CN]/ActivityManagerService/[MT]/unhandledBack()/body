{
  enforceCallingPermission(android.Manifest.permission.FORCE_BACK,"unhandledBack()");
synchronized (this) {
    int count=mHistory.size();
    if (Config.LOGD)     Log.d(TAG,"Performing unhandledBack(): stack size = " + count);
    if (count > 1) {
      final long origId=Binder.clearCallingIdentity();
      finishActivityLocked((HistoryRecord)mHistory.get(count - 1),count - 1,Activity.RESULT_CANCELED,null,"unhandled-back");
      Binder.restoreCallingIdentity(origId);
    }
  }
}
