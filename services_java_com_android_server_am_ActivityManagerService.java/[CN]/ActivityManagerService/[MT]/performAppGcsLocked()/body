{
  final int N=mProcessesToGc.size();
  if (N <= 0) {
    return;
  }
  if (canGcNow()) {
    while (mProcessesToGc.size() > 0) {
      ProcessRecord proc=mProcessesToGc.remove(0);
      if (proc.curRawAdj > VISIBLE_APP_ADJ) {
        performAppGcLocked(proc);
        scheduleAppGcsLocked();
        return;
      }
    }
  }
}
