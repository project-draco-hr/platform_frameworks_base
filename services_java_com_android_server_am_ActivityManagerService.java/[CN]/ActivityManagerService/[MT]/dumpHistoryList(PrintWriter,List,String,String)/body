{
  TaskRecord lastTask=null;
  for (int i=list.size() - 1; i >= 0; i--) {
    HistoryRecord r=(HistoryRecord)list.get(i);
    if (lastTask != r.task) {
      lastTask=r.task;
      lastTask.dump(pw,prefix + "  ");
    }
    pw.println(prefix + "    " + label+ " #"+ i+ ":");
    r.dump(pw,prefix + "      ");
  }
}
