{
  final int N=mPendingActivityLaunches.size();
  if (N <= 0) {
    return;
  }
  for (int i=0; i < N; i++) {
    PendingActivityLaunch pal=mPendingActivityLaunches.get(i);
    startActivityUncheckedLocked(pal.r,pal.sourceRecord,pal.grantedUriPermissions,pal.grantedMode,pal.onlyIfNeeded,doResume && i == (N - 1));
  }
  mPendingActivityLaunches.clear();
}
