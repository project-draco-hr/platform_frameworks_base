{
  enforceNotIsolatedCaller("startActivityAndWait");
  WaitResult res=new WaitResult();
  int userId=Binder.getOrigCallingUser();
  mMainStack.startActivityMayWait(caller,-1,intent,resolvedType,grantedUriPermissions,grantedMode,resultTo,resultWho,requestCode,onlyIfNeeded,debug,openglTrace,profileFile,profileFd,autoStopProfiler,res,null,userId);
  return res;
}
