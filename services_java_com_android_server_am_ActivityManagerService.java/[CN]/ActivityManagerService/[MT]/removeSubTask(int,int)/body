{
synchronized (this) {
    enforceCallingPermission(android.Manifest.permission.REMOVE_TASKS,"removeSubTask()");
    long ident=Binder.clearCallingIdentity();
    try {
      TaskRecord tr=recentTaskForIdLocked(taskId);
      if (tr != null) {
        return tr.stack.removeTaskActivitiesLocked(taskId,subTaskIndex,true) != null;
      }
      return false;
    }
  finally {
      Binder.restoreCallingIdentity(ident);
    }
  }
}
