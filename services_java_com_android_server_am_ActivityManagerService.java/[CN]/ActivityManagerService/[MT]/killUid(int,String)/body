{
  if (Binder.getCallingUid() != Process.SYSTEM_UID) {
    throw new SecurityException("killUid only available to the system");
  }
synchronized (this) {
    killPackageProcessesLocked(null,UserHandle.getAppId(uid),UserHandle.getUserId(uid),ProcessList.FOREGROUND_APP_ADJ - 1,false,true,true,false,reason != null ? reason : "kill uid");
  }
}
