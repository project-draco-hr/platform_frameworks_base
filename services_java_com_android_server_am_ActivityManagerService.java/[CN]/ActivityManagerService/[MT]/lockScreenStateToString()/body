{
switch (mLockScreenState) {
case LOCK_SCREEN_NEVER_SHOWN:
    return "LOCK_SCREEN_NEVER_SHOWN";
case LOCK_SCREEN_FIRST_SHOWN:
  return "LOCK_SCREEN_FIRST_SHOWN";
case LOCK_SCREEN_HIDDEN:
return "LOCK_SCREEN_HIDDEN";
case LOCK_SCREEN_SHOWING:
return "LOCK_SCREEN_SHOWING";
default :
return "unknown (" + mLockScreenState + ")";
}
}
