{
  Log.w(TAG,report);
  try {
    if (watcher != null) {
      Bundle results=new Bundle();
      results.putString(Instrumentation.REPORT_KEY_IDENTIFIER,"ActivityManagerService");
      results.putString("Error",report);
      watcher.instrumentationStatus(cn,-1,results);
    }
  }
 catch (  RemoteException e) {
    Log.w(TAG,e);
  }
}
