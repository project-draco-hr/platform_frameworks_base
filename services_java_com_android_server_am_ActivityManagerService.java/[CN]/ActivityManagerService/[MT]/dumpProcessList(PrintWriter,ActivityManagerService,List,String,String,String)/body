{
  int numPers=0;
  final int N=list.size() - 1;
  for (int i=N; i >= 0; i--) {
    ProcessRecord r=(ProcessRecord)list.get(i);
    pw.println(String.format("%s%s #%2d: %s",prefix,(r.persistent ? persistentLabel : normalLabel),i,r.toString()));
    if (r.persistent) {
      numPers++;
    }
  }
  return numPers;
}
