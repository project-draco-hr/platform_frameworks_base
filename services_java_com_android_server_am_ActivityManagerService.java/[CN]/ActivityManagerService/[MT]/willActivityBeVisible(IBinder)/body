{
synchronized (this) {
    int i;
    for (i=mMainStack.mHistory.size() - 1; i >= 0; i--) {
      ActivityRecord r=(ActivityRecord)mMainStack.mHistory.get(i);
      if (r.appToken == token) {
        return true;
      }
      if (r.fullscreen && !r.finishing) {
        return false;
      }
    }
    return true;
  }
}
