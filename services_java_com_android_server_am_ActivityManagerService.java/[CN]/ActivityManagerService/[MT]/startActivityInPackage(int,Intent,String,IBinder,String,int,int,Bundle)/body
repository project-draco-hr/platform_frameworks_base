{
  final int callingUid=Binder.getCallingUid();
  if (callingUid != 0 && callingUid != Process.myUid()) {
    throw new SecurityException("startActivityInPackage only available to the system");
  }
  int ret=mMainStack.startActivityMayWait(null,uid,intent,resolvedType,resultTo,resultWho,requestCode,startFlags,null,null,null,null,options,UserHandle.getUserId(uid));
  return ret;
}
