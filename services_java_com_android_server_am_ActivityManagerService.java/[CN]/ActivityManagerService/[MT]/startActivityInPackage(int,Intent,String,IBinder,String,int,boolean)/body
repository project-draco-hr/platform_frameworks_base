{
  int userId=Binder.getOrigCallingUser();
  final int callingUid=Binder.getCallingUid();
  if (callingUid != 0 && callingUid != Process.myUid()) {
    throw new SecurityException("startActivityInPackage only available to the system");
  }
  int ret=mMainStack.startActivityMayWait(null,uid,intent,resolvedType,null,0,resultTo,resultWho,requestCode,onlyIfNeeded,false,false,null,null,false,null,null,userId);
  return ret;
}
