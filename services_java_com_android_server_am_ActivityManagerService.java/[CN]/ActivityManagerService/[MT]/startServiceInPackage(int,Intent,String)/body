{
synchronized (this) {
    if (DEBUG_SERVICE)     Slog.v(TAG,"startServiceInPackage: " + service + " type="+ resolvedType);
    final long origId=Binder.clearCallingIdentity();
    ComponentName res=mServices.startServiceLocked(null,service,resolvedType,-1,uid,UserHandle.getUserId(uid));
    Binder.restoreCallingIdentity(origId);
    return res;
  }
}
