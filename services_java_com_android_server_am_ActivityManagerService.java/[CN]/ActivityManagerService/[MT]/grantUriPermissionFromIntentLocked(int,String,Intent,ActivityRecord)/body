{
  if (DEBUG_URI_PERMISSION)   Slog.v(TAG,"Grant URI perm to " + (intent != null ? intent.getData() : null) + " from "+ intent+ "; flags=0x"+ Integer.toHexString(intent != null ? intent.getFlags() : 0));
  if (intent == null) {
    return;
  }
  Uri data=intent.getData();
  if (data == null) {
    return;
  }
  grantUriPermissionLocked(callingUid,targetPkg,data,intent.getFlags(),activity);
}
