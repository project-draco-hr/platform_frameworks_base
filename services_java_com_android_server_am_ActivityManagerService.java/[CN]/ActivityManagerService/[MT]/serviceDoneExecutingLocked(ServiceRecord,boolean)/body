{
  if (DEBUG_SERVICE)   Slog.v(TAG,"<<< DONE EXECUTING " + r + ": nesting="+ r.executeNesting+ ", inStopping="+ inStopping+ ", app="+ r.app);
  r.executeNesting--;
  if (r.executeNesting <= 0 && r.app != null) {
    r.app.executingServices.remove(r);
    if (r.app.executingServices.size() == 0) {
      mHandler.removeMessages(SERVICE_TIMEOUT_MSG,r.app);
    }
    if (inStopping) {
      if (DEBUG_SERVICE)       Slog.v(TAG,"doneExecuting remove stopping " + r);
      mStoppingServices.remove(r);
    }
    updateOomAdjLocked(r.app);
  }
}
