{
  for (int i=mParallelBroadcasts.size() - 1; i >= 0; i--) {
    if (r.intent.filterEquals(mParallelBroadcasts.get(i).intent)) {
      if (DEBUG_BROADCAST)       Slog.v(TAG,"***** DROPPING PARALLEL [" + mQueueName + "]: "+ r.intent);
      mParallelBroadcasts.set(i,r);
      return true;
    }
  }
  return false;
}
