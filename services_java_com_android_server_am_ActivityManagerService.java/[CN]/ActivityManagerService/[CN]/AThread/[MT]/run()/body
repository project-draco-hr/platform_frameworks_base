{
  Looper.prepare();
  android.os.Process.setThreadPriority(android.os.Process.THREAD_PRIORITY_FOREGROUND);
  ActivityManagerService m=new ActivityManagerService();
synchronized (this) {
    mService=m;
    notifyAll();
  }
synchronized (this) {
    while (!mReady) {
      try {
        wait();
      }
 catch (      InterruptedException e) {
      }
    }
  }
  Looper.loop();
}
