{
  if (mDataPaths == null) {
    return false;
  }
  final int numDataPaths=mDataPaths.size();
  for (int i=0; i < numDataPaths; i++) {
    final PatternMatcher pe=mDataPaths.get(i);
    if (pe.getType() == path.getType() && pe.getPath().equals(path.getPath())) {
      return true;
    }
  }
  return false;
}
