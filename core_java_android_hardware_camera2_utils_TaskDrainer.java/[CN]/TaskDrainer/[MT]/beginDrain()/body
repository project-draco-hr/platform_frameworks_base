{
synchronized (mLock) {
    if (!mDraining) {
      if (VERBOSE) {
        Log.v(TAG + "[" + mName+ "]","beginDrain started");
      }
      mDraining=true;
      checkIfDrainFinished();
    }
 else {
      if (VERBOSE) {
        Log.v(TAG + "[" + mName+ "]","beginDrain ignored");
      }
    }
  }
}
