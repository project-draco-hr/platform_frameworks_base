{
synchronized (mLock) {
    if (VERBOSE) {
      Log.v(TAG + "[" + mName+ "]","taskStarted " + task);
    }
    if (mDraining) {
      throw new IllegalStateException("Can't start more tasks after draining has begun");
    }
    if (!mTaskSet.add(task)) {
      throw new IllegalStateException("Task " + task + " was already started");
    }
  }
}
