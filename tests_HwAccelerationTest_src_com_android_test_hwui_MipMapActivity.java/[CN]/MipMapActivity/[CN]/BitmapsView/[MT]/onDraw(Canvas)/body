{
  super.onDraw(canvas);
  canvas.save();
  canvas.scale(0.3f,0.3f);
  canvas.drawBitmap(mBitmap1,0,0,mBitmapPaint);
  canvas.restore();
  canvas.save();
  canvas.translate(mBitmap1.getWidth() * 0.3f + 96.0f,0.0f);
  canvas.scale(0.3f,0.3f);
  canvas.drawBitmap(mBitmap2,0,0,mBitmapPaint);
  canvas.restore();
}
