{
synchronized (mStatsLock) {
    final NetworkStatsHistory combined=new NetworkStatsHistory(mSettings.getNetworkBucketDuration(),estimateNetworkBuckets(),fields);
    for (    NetworkIdentitySet ident : source.keySet()) {
      if (templateMatches(template,ident)) {
        final NetworkStatsHistory history=source.get(ident);
        if (history != null) {
          combined.recordEntireHistory(history);
        }
      }
    }
    return combined;
  }
}
