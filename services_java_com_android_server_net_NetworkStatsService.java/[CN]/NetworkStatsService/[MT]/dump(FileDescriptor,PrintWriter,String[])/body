{
  mContext.enforceCallingOrSelfPermission(DUMP,TAG);
  pw.println("Active interfaces:");
  for (  InterfaceInfo info : mActiveIface.values()) {
    info.dump("  ",pw);
  }
  pw.println("Known historical stats:");
  for (  NetworkStatsHistory stats : mIfaceStats.values()) {
    stats.dump("  ",pw);
  }
}
