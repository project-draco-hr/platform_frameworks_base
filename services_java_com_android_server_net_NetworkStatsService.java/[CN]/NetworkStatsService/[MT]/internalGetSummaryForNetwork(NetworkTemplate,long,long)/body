{
  if (!mSettings.getReportXtOverDev()) {
    return mDevStatsCached.getSummary(template,start,end);
  }
  final long firstAtomicBucket=mXtStatsCached.getFirstAtomicBucketMillis();
  final NetworkStats dev=mDevStatsCached.getSummary(template,Math.min(start,firstAtomicBucket),Math.min(end,firstAtomicBucket));
  final NetworkStats xt=mXtStatsCached.getSummary(template,Math.max(start,firstAtomicBucket),Math.max(end,firstAtomicBucket));
  xt.combineAllValues(dev);
  return xt;
}
