{
  if (TextUtils.isEmpty(label)) {
    return false;
  }
  if (label.equalsIgnoreCase("ata")) {
    return false;
  }
  if (label.toLowerCase().contains("generic")) {
    return false;
  }
  if (label.toLowerCase().startsWith("usb")) {
    return false;
  }
  if (label.toLowerCase().startsWith("multiple")) {
    return false;
  }
  return true;
}
