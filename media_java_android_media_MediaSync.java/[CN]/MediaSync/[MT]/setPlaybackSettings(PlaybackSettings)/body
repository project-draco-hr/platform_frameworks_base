{
  float rate;
  try {
    rate=settings.getSpeed();
    if (mAudioTrack != null) {
      try {
        if (rate == 0.0) {
          mAudioTrack.pause();
        }
 else {
          mAudioTrack.setPlaybackSettings(settings);
          mAudioTrack.play();
        }
      }
 catch (      IllegalStateException e) {
        throw e;
      }
    }
synchronized (mAudioLock) {
      mPlaybackRate=rate;
    }
    if (mPlaybackRate != 0.0 && mAudioThread != null) {
      postRenderAudio(0);
    }
    native_setPlaybackRate(mPlaybackRate);
  }
 catch (  IllegalStateException e) {
    if (mAudioTrack != null) {
      mAudioTrack.setPlaybackSettings(settings);
    }
  }
}
