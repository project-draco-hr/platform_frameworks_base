{
  long updateTime=lastWidgetUpdateTime;
  int N=widgets.size();
  for (int i=0; i < N; i++) {
    Widget widget=widgets.get(i);
    if (widget.appWidgetId == appWidgetId && widget.lastUpdateTime > updateTime) {
      return cloneIfLocalBinder(widget.getEffectiveViewsLocked());
    }
  }
  return null;
}
