{
  String opt;
  String gdbPort=null;
  while ((opt=nextOption()) != null) {
    if (opt.equals("--gdb")) {
      gdbPort=nextArgRequired();
    }
 else {
      System.err.println("Error: Unknown option: " + opt);
      return;
    }
  }
  MyActivityController controller=new MyActivityController(gdbPort);
  controller.run();
}
