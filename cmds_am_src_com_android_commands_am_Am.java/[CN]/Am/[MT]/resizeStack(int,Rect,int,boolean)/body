{
  if (bounds == null) {
    showError("Error: invalid input bounds");
    return;
  }
  try {
    mAm.resizeStack(stackId,bounds,false,false,animate);
    Thread.sleep(delayMs);
  }
 catch (  RemoteException e) {
    showError("Error: resizing stack " + e);
  }
catch (  InterruptedException e) {
  }
}
