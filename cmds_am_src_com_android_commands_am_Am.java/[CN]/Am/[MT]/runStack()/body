{
  String op=nextArgRequired();
  if (op.equals("create")) {
    runStackCreate();
  }
 else   if (op.equals("movetask")) {
    runStackMoveTask();
  }
 else   if (op.equals("resize")) {
    runStackBoxResize();
  }
 else   if (op.equals("boxes")) {
    runStackBoxes();
  }
 else   if (op.equals("box")) {
    runStackBoxInfo();
  }
 else {
    showError("Error: unknown command '" + op + "'");
    return;
  }
}
