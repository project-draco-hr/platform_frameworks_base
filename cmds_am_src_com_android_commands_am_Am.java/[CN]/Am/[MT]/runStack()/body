{
  String op=nextArgRequired();
  if (op.equals("create")) {
    runStackCreate();
  }
 else   if (op.equals("movetask")) {
    runStackMoveTask();
  }
 else   if (op.equals("resize")) {
    runStackResize();
  }
 else   if (op.equals("list")) {
    runStackList();
  }
 else   if (op.equals("info")) {
    runStackInfo();
  }
 else {
    showError("Error: unknown command '" + op + "'");
    return;
  }
}
