{
  String str="IndexColorModel: #pixel_bits = " + pixel_bits + " numComponents = "+ numComponents+ " color space = "+ cs+ " transparency = ";
  if (transparency == Transparency.OPAQUE) {
    str=str + "Transparency.OPAQUE";
  }
 else   if (transparency == Transparency.BITMASK) {
    str=str + "Transparency.BITMASK";
  }
 else {
    str=str + "Transparency.TRANSLUCENT";
  }
  str=str + " transIndex = " + transparentIndex+ " has alpha = "+ hasAlpha+ " isAlphaPre = "+ isAlphaPremultiplied;
  return str;
}
