{
  try {
    StringBuffer sb=new StringBuffer();
    sb.append(String.format("RA %s -> %s %ds ",IPv6AddresstoString(IPV6_SRC_ADDR_OFFSET),IPv6AddresstoString(IPV6_DEST_ADDR_OFFSET),uint16(mPacket.getShort(ICMP6_RA_ROUTER_LIFETIME_OFFSET))));
    for (    int i : mPrefixOptionOffsets) {
      prefixOptionToString(sb,i);
    }
    for (    int i : mRdnssOptionOffsets) {
      rdnssOptionToString(sb,i);
    }
    return sb.toString();
  }
 catch (  BufferUnderflowException|IndexOutOfBoundsException e) {
    return "<Malformed RA>";
  }
}
