{
  Context c=null;
  if (packageName != null) {
    try {
      c=baseContext.createPackageContext(packageName,0);
    }
 catch (    NameNotFoundException e) {
      c=baseContext;
    }
  }
 else {
    c=baseContext;
  }
  String pkg;
  String type;
  String name;
  Resources r=c.getResources();
  try {
    return r.getResourceName(id);
  }
 catch (  Resources.NotFoundException e) {
    return "<name unknown>";
  }
}
