{
  checkCallerIsSystemOrSameApp(pkg);
  userId=ActivityManager.handleIncomingUser(Binder.getCallingPid(),Binder.getCallingUid(),userId,true,true,"cancelNotificationWithTag",pkg);
  cancelNotification(pkg,tag,id,0,Binder.getCallingUid() == Process.SYSTEM_UID ? 0 : Notification.FLAG_FOREGROUND_SERVICE,false,userId);
}
