{
  return new Iterator<StatusBarNotification>(){
    StatusBarNotification mNext=findNext();
    private StatusBarNotification findNext(){
      while (iter.hasNext()) {
        StatusBarNotification nr=iter.next();
        if ((pkg == null || nr.pkg == pkg) && (userId == UserHandle.USER_ALL || nr.getUserId() == userId)) {
          return nr;
        }
      }
      return null;
    }
    @Override public boolean hasNext(){
      return mNext == null;
    }
    @Override public StatusBarNotification next(){
      StatusBarNotification next=mNext;
      if (next == null) {
        throw new NoSuchElementException();
      }
      mNext=findNext();
      return next;
    }
    @Override public void remove(){
      iter.remove();
    }
  }
;
}
