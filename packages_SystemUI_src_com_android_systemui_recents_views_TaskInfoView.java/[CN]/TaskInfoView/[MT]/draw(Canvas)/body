{
  int saveCount=0;
  if (mCircularClipEnabled) {
    saveCount=canvas.save(Canvas.CLIP_SAVE_FLAG);
    mClipPath.reset();
    mClipPath.addCircle(mClipOrigin.x,mClipOrigin.y,mClipRadius * mMaxClipRadius,Path.Direction.CW);
    canvas.clipPath(mClipPath);
  }
  super.draw(canvas);
  if (mCircularClipEnabled) {
    canvas.restoreToCount(saveCount);
  }
}
