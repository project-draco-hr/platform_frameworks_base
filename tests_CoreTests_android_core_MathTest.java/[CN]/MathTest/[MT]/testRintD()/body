{
  assertEquals("Failed to round properly - up to odd",3.0,Math.rint(2.9),0D);
  assertTrue("Failed to round properly - NaN",Double.isNaN(Math.rint(Double.NaN)));
  assertEquals("Failed to round properly down  to even",2.0,Math.rint(2.1),0D);
  assertTrue("Failed to round properly " + 2.5 + " to even",Math.rint(2.5) == 2.0);
}
