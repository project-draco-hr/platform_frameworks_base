{
  if (outline == null || outline.isEmpty()) {
    nSetOutlineEmpty(mNativeRenderNode);
  }
 else   if (outline.mRect != null) {
    nSetOutlineRoundRect(mNativeRenderNode,outline.mRect.left,outline.mRect.top,outline.mRect.right,outline.mRect.bottom,outline.mRadius);
  }
 else   if (outline.mPath != null) {
    nSetOutlineConvexPath(mNativeRenderNode,outline.mPath.mNativePath);
  }
}
