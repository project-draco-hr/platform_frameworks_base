{
  if (!mExitNotified && mExitComplete) {
    mExitNotified=true;
    mResultReceiver.send(MSG_EXIT_TRANSITION_COMPLETE,null);
    mResultReceiver=null;
    ViewGroup decorView=getDecor();
    if (!mIsReturning && decorView != null) {
      decorView.suppressLayout(false);
    }
    finishIfNecessary();
  }
}
