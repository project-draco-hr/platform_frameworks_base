{
  if (mResources != null) {
    return mResources;
  }
  if (mOverrideConfiguration == null) {
    mResources=super.getResources();
    return mResources;
  }
 else {
    Context resc=createConfigurationContext(mOverrideConfiguration);
    mResources=resc.getResources();
    return mResources;
  }
}
