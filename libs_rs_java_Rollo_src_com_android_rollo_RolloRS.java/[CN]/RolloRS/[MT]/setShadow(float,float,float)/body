{
  mAllocStateBuf[STATE_POS_X]=(int)(x * 1000);
  mAllocStateBuf[STATE_POS_Y]=(int)(y * 1000);
  mAllocStateBuf[STATE_PRESSURE]=(int)(size * 1000);
  Log.e("rs","shadow x=" + Integer.toString(mAllocStateBuf[STATE_POS_X]) + "  y="+ Integer.toString(mAllocStateBuf[STATE_POS_X])+ "  s="+ Integer.toString(mAllocStateBuf[STATE_PRESSURE]));
  mAllocState.data(mAllocStateBuf);
}
