{
  if (ID_PRIVATE_INTERNAL.equals(id)) {
    return Resources.getSystem().getString(com.android.internal.R.string.storage_internal);
  }
 else   if (!TextUtils.isEmpty(nickname)) {
    return nickname;
  }
 else   if (!TextUtils.isEmpty(fsLabel)) {
    return fsLabel;
  }
 else {
    return null;
  }
}
