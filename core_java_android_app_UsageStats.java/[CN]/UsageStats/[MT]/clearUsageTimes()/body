{
  for (int i=mPackages.size() - 1; i >= 0; i--) {
    if (mPackages.valueAt(i).clearUsageTimes()) {
      mPackages.removeAt(i);
    }
  }
  for (int i=mConfigurations.size() - 1; i >= 0; i--) {
    if (mConfigurations.valueAt(i).clearUsageTimes()) {
      mConfigurations.removeAt(i);
    }
  }
}
