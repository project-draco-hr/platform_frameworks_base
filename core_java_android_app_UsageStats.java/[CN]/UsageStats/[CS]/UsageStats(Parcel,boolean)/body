{
  int N=source.readInt();
  for (int i=0; i < N; i++) {
    PackageStats pkg=extended ? onNewPackageStats(source) : new PackageStats(source);
    mPackages.put(pkg.getPackageName(),pkg);
  }
  N=source.readInt();
  for (int i=0; i < N; i++) {
    ConfigurationStats config=extended ? onNewConfigurationStats(source) : new ConfigurationStats(source);
    mConfigurations.put(config.getConfiguration(),config);
  }
}
