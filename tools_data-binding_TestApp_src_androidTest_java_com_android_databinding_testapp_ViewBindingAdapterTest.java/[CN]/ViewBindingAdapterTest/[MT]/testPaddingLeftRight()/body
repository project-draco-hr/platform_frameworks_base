{
  View view=mBinder.getPaddingLeftRight();
  assertEquals(mViewBindingObject.getPaddingLeft(),view.getPaddingLeft());
  assertEquals(mViewBindingObject.getPaddingRight(),view.getPaddingRight());
  changeValues();
  assertEquals(mViewBindingObject.getPaddingLeft(),view.getPaddingLeft());
  assertEquals(mViewBindingObject.getPaddingRight(),view.getPaddingRight());
}
