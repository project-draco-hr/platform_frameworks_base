{
  View view=mBinder.getPaddingTopBottom();
  assertEquals(mViewBindingObject.getPaddingTop(),view.getPaddingTop());
  assertEquals(mViewBindingObject.getPaddingBottom(),view.getPaddingBottom());
  changeValues();
  assertEquals(mViewBindingObject.getPaddingTop(),view.getPaddingTop());
  assertEquals(mViewBindingObject.getPaddingBottom(),view.getPaddingBottom());
}
