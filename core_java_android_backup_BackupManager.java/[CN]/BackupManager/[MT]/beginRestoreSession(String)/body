{
  if (!EVEN_THINK_ABOUT_DOING_RESTORE) {
    return null;
  }
  IRestoreSession binder=null;
  checkServiceBinder();
  if (sService != null) {
    try {
      binder=sService.beginRestoreSession(transport);
    }
 catch (    RemoteException e) {
      Log.d(TAG,"beginRestoreSession() couldn't connect");
    }
  }
  return binder;
}
