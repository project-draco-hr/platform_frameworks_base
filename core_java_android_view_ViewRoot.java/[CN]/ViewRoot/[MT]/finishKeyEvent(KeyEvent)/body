{
  if (LOCAL_LOGV)   Log.v(TAG,"Telling window manager key is finished");
  if (mFinishedCallback != null) {
    mFinishedCallback.run();
    mFinishedCallback=null;
  }
 else {
    Slog.w(TAG,"Attempted to tell the input queue that the current key event " + "is finished but there is no key event actually in progress.");
  }
}
