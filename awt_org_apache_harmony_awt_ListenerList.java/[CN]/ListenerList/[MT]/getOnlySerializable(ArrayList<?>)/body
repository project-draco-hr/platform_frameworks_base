{
  if (list == null) {
    return null;
  }
  ArrayList<Object> result=new ArrayList<Object>();
  for (Iterator<?> it=list.iterator(); it.hasNext(); ) {
    Object obj=it.next();
    if (obj instanceof Serializable) {
      result.add(obj);
    }
  }
  return (result.size() != 0) ? result : null;
}
