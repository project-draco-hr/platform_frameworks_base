{
  animate().translationY(ctx.offscreenTranslationY).setStartDelay(0).setUpdateListener(null).setInterpolator(mConfig.fastOutLinearInInterpolator).setDuration(mConfig.taskViewExitToHomeDuration).withLayer().withEndAction(ctx.postAnimationTrigger.decrementAsRunnable()).start();
  ctx.postAnimationTrigger.increment();
}
