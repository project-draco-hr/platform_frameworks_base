{
  for (  Overlay overlay : mOverlays) {
    if (overlay.getId().equals(overlayId)) {
      mOverlays.remove(overlay);
      if (overlay instanceof OverlayFrame) {
        ((OverlayFrame)overlay).invalidate();
      }
      invalidateTransitions(overlay);
      return overlay;
    }
  }
  return null;
}
