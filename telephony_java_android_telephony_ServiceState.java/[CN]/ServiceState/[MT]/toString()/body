{
  String radioTechnology=new String("Error in radioTechnology");
switch (this.mRadioTechnology) {
case 0:
    radioTechnology="Unknown";
  break;
case 1:
radioTechnology="GPRS";
break;
case 2:
radioTechnology="EDGE";
break;
case 3:
radioTechnology="UMTS";
break;
case 4:
radioTechnology="IS95A";
break;
case 5:
radioTechnology="IS95B";
break;
case 6:
radioTechnology="1xRTT";
break;
case 7:
radioTechnology="EvDo rev. 0";
break;
case 8:
radioTechnology="EvDo rev. A";
break;
case 9:
radioTechnology="HSDPA";
break;
case 10:
radioTechnology="HSUPA";
break;
case 11:
radioTechnology="HSPA";
break;
case 12:
radioTechnology="EvDo rev. B";
break;
default :
Log.w(LOG_TAG,"mRadioTechnology variable out of range.");
break;
}
return (mState + " " + (mRoaming ? "roaming" : "home")+ " "+ mOperatorAlphaLong+ " "+ mOperatorAlphaShort+ " "+ mOperatorNumeric+ " "+ (mIsManualNetworkSelection ? "(manual)" : "")+ " "+ radioTechnology+ " "+ (mCssIndicator ? "CSS supported" : "CSS not supported")+ " "+ mNetworkId+ " "+ mSystemId+ " RoamInd="+ mCdmaRoamingIndicator+ " DefRoamInd="+ mCdmaDefaultRoamingIndicator+ " EmergOnly="+ mIsEmergencyOnly);
}
