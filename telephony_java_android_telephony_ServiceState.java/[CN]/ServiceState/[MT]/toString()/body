{
  String radioTechnology=rilRadioTechnologyToString(mRilVoiceRadioTechnology);
  String dataRadioTechnology=rilRadioTechnologyToString(mRilDataRadioTechnology);
  return (mVoiceRegState + " " + mDataRegState+ " "+ (mRoaming ? "roaming" : "home")+ " "+ mOperatorAlphaLong+ " "+ mOperatorAlphaShort+ " "+ mOperatorNumeric+ " "+ (mIsManualNetworkSelection ? "(manual)" : "")+ " "+ radioTechnology+ " "+ dataRadioTechnology+ " "+ (mCssIndicator ? "CSS supported" : "CSS not supported")+ " "+ mNetworkId+ " "+ mSystemId+ " RoamInd="+ mCdmaRoamingIndicator+ " DefRoamInd="+ mCdmaDefaultRoamingIndicator+ " EmergOnly="+ mIsEmergencyOnly);
}
