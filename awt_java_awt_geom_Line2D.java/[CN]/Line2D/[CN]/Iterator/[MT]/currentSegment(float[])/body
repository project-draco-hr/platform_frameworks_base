{
  if (isDone()) {
    throw new NoSuchElementException(Messages.getString("awt.4B"));
  }
  int type;
  if (index == 0) {
    type=SEG_MOVETO;
    coords[0]=(float)x1;
    coords[1]=(float)y1;
  }
 else {
    type=SEG_LINETO;
    coords[0]=(float)x2;
    coords[1]=(float)y2;
  }
  if (t != null) {
    t.transform(coords,0,coords,0,1);
  }
  return type;
}
