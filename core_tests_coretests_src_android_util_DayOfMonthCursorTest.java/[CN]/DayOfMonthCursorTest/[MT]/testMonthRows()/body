{
  DayOfMonthCursor mc=new DayOfMonthCursor(2007,Calendar.SEPTEMBER,11,Calendar.SUNDAY);
  assertArraysEqual(new int[]{26,27,28,29,30,31,1},mc.getDigitsForRow(0));
  assertArraysEqual(new int[]{2,3,4,5,6,7,8},mc.getDigitsForRow(1));
  assertArraysEqual(new int[]{30,1,2,3,4,5,6},mc.getDigitsForRow(5));
}
