{
  if (query != null && query.length() > 0) {
    int start=0;
    int end=getCount() - 1;
    int index=binarySearch(query,start,end);
    for (index=index - 1; index >= 0; index--) {
      String str=getItem(index).toString().toLowerCase();
      if (!str.startsWith(query)) {
        return index + 1;
      }
      if (index == 0) {
        return 0;
      }
    }
    return -1;
  }
 else {
    return -1;
  }
}
