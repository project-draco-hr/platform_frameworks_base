{
  if (start > end) {
    return -1;
  }
  int mid=(start + end) / 2;
  String str=getItem(mid).toString().toLowerCase();
  if (prefix.compareTo(str) <= 0) {
    if (str.startsWith(prefix)) {
      return mid;
    }
 else {
      return binarySearch(prefix,start,mid - 1);
    }
  }
 else {
    return binarySearch(prefix,mid + 1,end);
  }
}
