{
  if (!mCanceled) {
    if (mIsForcedVisibility) {
      mView.setTransitionAlpha(0);
    }
 else     if (!mFinalVisibilitySet) {
      mView.setTransitionVisibility(mFinalVisibility);
      if (mParent != null) {
        mParent.invalidate();
      }
      mFinalVisibilitySet=true;
    }
  }
  suppressLayout(false);
}
