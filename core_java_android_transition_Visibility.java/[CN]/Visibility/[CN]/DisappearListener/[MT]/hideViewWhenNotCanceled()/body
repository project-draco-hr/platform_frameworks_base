{
  if (!mEnded) {
    if (!mCanceled) {
      if (mIsForcedVisibility) {
        mView.setTransitionAlpha(0);
      }
 else {
        mView.setTransitionVisibility(mFinalVisibility);
        if (mParent != null) {
          mParent.invalidate();
        }
      }
    }
    if (!mIsForcedVisibility && mParent != null) {
      mParent.suppressLayout(false);
    }
    mEnded=true;
  }
}
