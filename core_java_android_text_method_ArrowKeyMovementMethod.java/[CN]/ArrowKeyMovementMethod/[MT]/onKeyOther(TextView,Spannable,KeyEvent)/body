{
  int code=event.getKeyCode();
  if (code != KeyEvent.KEYCODE_UNKNOWN && event.getAction() == KeyEvent.ACTION_MULTIPLE) {
    int repeat=event.getRepeatCount();
    boolean handled=false;
    while ((--repeat) > 0) {
      handled|=executeDown(view,text,code);
    }
    return handled;
  }
  return false;
}
