{
  if (obj == this) {
    return true;
  }
  if (obj != null) {
    try {
      Font font=(Font)obj;
      return ((this.style == font.style) && (this.size == font.size) && this.name.equals(font.name)&& (this.pointSize == font.pointSize)&& (this.getTransform()).equals(font.getTransform()));
    }
 catch (    ClassCastException e) {
    }
  }
  return false;
}
