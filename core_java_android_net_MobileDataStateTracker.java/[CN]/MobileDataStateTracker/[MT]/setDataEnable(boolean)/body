{
  try {
    if (DBG)     log("setDataEnable: E enabled=" + enabled);
    mDataConnectionTrackerAc.sendMessage(DataConnectionTracker.CMD_SET_DATA_ENABLE,enabled ? DataConnectionTracker.ENABLED : DataConnectionTracker.DISABLED);
    if (VDBG)     log("setDataEnable: X enabled=" + enabled);
  }
 catch (  Exception e) {
    loge("setDataEnable: X mAc was null" + e);
  }
}
