{
  if (DBG) {
    Log.d(TAG,"Requested host route to " + Integer.toHexString(hostAddress) + " for "+ mApnType+ "("+ mInterfaceName+ ")");
  }
  if (mInterfaceName != null && hostAddress != -1) {
    return NetworkUtils.addHostRoute(mInterfaceName,hostAddress) == 0;
  }
 else {
    return false;
  }
}
