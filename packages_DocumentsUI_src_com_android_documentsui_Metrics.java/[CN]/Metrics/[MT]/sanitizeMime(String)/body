{
  if (mimeType == null) {
    return MIME_NONE;
  }
 else   if ("*/*".equals(mimeType)) {
    return MIME_ANY;
  }
 else {
    String type=mimeType.substring(0,mimeType.indexOf('/'));
switch (type) {
case "application":
      return MIME_APPLICATION;
case "audio":
    return MIME_AUDIO;
case "image":
  return MIME_IMAGE;
case "message":
return MIME_MESSAGE;
case "multipart":
return MIME_MULTIPART;
case "text":
return MIME_TEXT;
case "video":
return MIME_VIDEO;
}
}
return MIME_OTHER;
}
