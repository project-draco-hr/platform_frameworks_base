{
  if (timer != null) {
    long totalTimeMillis=computeWakeLock(timer,elapsedRealtimeUs,which);
    int count=timer.getCountLocked(which);
    if (totalTimeMillis != 0) {
      sb.append(linePrefix);
      formatTimeMs(sb,totalTimeMillis);
      if (name != null) {
        sb.append(name);
        sb.append(' ');
      }
      sb.append('(');
      sb.append(count);
      sb.append(" times)");
      return ", ";
    }
  }
  return linePrefix;
}
