{
  if (timer != null) {
    long totalTimeMillis=(timer.getTotalTime(now,which) + 500) / 1000;
    int count=timer.getCount(which);
    if (totalTimeMillis != 0) {
      sb.append(linePrefix);
      sb.append(formatTimeMs(totalTimeMillis));
      sb.append(name);
      sb.append(' ');
      sb.append('(');
      sb.append(count);
      sb.append(" times)");
      return ", ";
    }
  }
  return linePrefix;
}
