{
  if (steps == null) {
    return;
  }
  long timeRemaining=steps.computeTimeEstimate(0,0,tmpOutInt);
  if (timeRemaining >= 0) {
    pw.print(prefix);
    pw.print(label);
    pw.print(" total time: ");
    tmpSb.setLength(0);
    formatTimeMs(tmpSb,timeRemaining);
    pw.print(tmpSb);
    pw.print(" (from ");
    pw.print(tmpOutInt[0]);
    pw.println(" steps)");
  }
  for (int i=0; i < STEP_LEVEL_MODES_OF_INTEREST.length; i++) {
    long estimatedTime=steps.computeTimeEstimate(STEP_LEVEL_MODES_OF_INTEREST[i],STEP_LEVEL_MODE_VALUES[i],tmpOutInt);
    if (estimatedTime > 0) {
      pw.print(prefix);
      pw.print(label);
      pw.print(" ");
      pw.print(STEP_LEVEL_MODE_LABELS[i]);
      pw.print(" time: ");
      tmpSb.setLength(0);
      formatTimeMs(tmpSb,estimatedTime);
      pw.print(tmpSb);
      pw.print(" (from ");
      pw.print(tmpOutInt[0]);
      pw.println(" steps)");
    }
  }
}
