{
  long totalTimeMicros=0;
  int count=0;
  if (timer != null) {
    totalTimeMicros=timer.getTotalTime(now,which);
    count=timer.getCount(which);
  }
  sb.append(linePrefix);
  sb.append((totalTimeMicros + 500) / 1000);
  sb.append(',');
  sb.append(name);
  sb.append(',');
  sb.append(count);
  return ",";
}
