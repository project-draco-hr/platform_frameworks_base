{
  if (changes == null) {
    return;
  }
  pw.print(prefix);
  pw.println("Package changes:");
  for (int i=0; i < changes.size(); i++) {
    PackageChange pc=changes.get(i);
    if (pc.mUpdate) {
      pw.print(prefix);
      pw.print("  Update ");
      pw.print(pc.mPackageName);
      pw.print(" vers=");
      pw.println(pc.mVersionCode);
    }
 else {
      pw.print(prefix);
      pw.print("  Uninstall ");
      pw.println(pc.mPackageName);
    }
  }
}
