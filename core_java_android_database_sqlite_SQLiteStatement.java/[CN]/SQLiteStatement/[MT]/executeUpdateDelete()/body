{
  try {
    saveSqlAsLastSqlStatement();
    acquireAndLock(WRITE);
    int numChanges=0;
    if ((mStatementType & STATEMENT_DONT_PREPARE) > 0) {
      native_executeSql(mSql);
    }
 else {
      numChanges=native_execute();
    }
    return numChanges;
  }
  finally {
    releaseAndUnlock();
  }
}
