{
  mDatabase.verifyDbIsOpen();
  if (rwFlag == WRITE) {
    BlockGuard.getThreadPolicy().onWriteToDisk();
  }
 else {
    BlockGuard.getThreadPolicy().onReadFromDisk();
  }
  long startTime=SystemClock.uptimeMillis();
  mDatabase.lock();
  acquireReference();
  mDatabase.closePendingStatements();
  return startTime;
}
