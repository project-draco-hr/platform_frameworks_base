{
  long timeStart=Debug.threadCpuTimeNanos();
  mDatabase.lock();
  acquireReference();
  try {
    if (SQLiteDebug.DEBUG_SQL_STATEMENTS) {
      Log.v(TAG,"execute() for [" + mSql + "]");
    }
    native_execute();
    mDatabase.logTimeStat(mSql,timeStart);
  }
  finally {
    releaseReference();
    mDatabase.unlock();
  }
}
