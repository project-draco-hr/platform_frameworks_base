{
  mDatabase.lock();
  boolean logStats=mDatabase.mLogStats;
  long startTime=logStats ? SystemClock.elapsedRealtime() : 0;
  acquireReference();
  try {
    native_execute();
    if (logStats) {
      mDatabase.logTimeStat(false,startTime,SystemClock.elapsedRealtime());
    }
  }
  finally {
    releaseReference();
    mDatabase.unlock();
  }
}
