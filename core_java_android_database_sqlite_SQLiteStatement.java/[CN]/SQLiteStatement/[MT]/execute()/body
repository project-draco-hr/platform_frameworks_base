{
  mDatabase.verifyDbIsOpen();
  long timeStart=SystemClock.uptimeMillis();
  mDatabase.lock();
  acquireReference();
  try {
    mDatabase.closePendingStatements();
    native_execute();
    mDatabase.logTimeStat(mSql,timeStart);
  }
  finally {
    releaseReference();
    mDatabase.unlock();
  }
}
