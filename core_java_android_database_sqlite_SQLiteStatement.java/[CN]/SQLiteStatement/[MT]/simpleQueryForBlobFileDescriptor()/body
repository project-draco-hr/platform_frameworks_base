{
synchronized (this) {
    try {
      long timeStart=acquireAndLock(READ);
      ParcelFileDescriptor retValue=native_1x1_blob_ashmem();
      mDatabase.logTimeStat(mSql,timeStart);
      return retValue;
    }
 catch (    IOException ex) {
      Log.e(TAG,"simpleQueryForBlobFileDescriptor() failed",ex);
      return null;
    }
 finally {
      releaseAndUnlock();
    }
  }
}
