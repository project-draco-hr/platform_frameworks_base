{
  if (!checkInitialized()) {
    return false;
  }
  Cursor cursor=null;
  boolean ret=false;
  try {
    cursor=mCacheDatabase.query("cache",ID_PROJECTION,null,null,null,null,null);
    ret=cursor.moveToFirst() == true;
  }
 catch (  IllegalStateException e) {
    Log.e(LOGTAG,"hasCache",e);
  }
 finally {
    if (cursor != null)     cursor.close();
  }
  return ret;
}
