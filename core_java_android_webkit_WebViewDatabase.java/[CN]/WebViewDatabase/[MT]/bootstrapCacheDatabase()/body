{
  if (mCacheDatabase != null) {
    mCacheDatabase.execSQL("CREATE TABLE cache" + " (" + ID_COL + " INTEGER PRIMARY KEY, "+ CACHE_URL_COL+ " TEXT, "+ CACHE_FILE_PATH_COL+ " TEXT, "+ CACHE_LAST_MODIFY_COL+ " TEXT, "+ CACHE_ETAG_COL+ " TEXT, "+ CACHE_EXPIRES_COL+ " INTEGER, "+ CACHE_EXPIRES_STRING_COL+ " TEXT, "+ CACHE_MIMETYPE_COL+ " TEXT, "+ CACHE_ENCODING_COL+ " TEXT,"+ CACHE_HTTP_STATUS_COL+ " INTEGER, "+ CACHE_LOCATION_COL+ " TEXT, "+ CACHE_CONTENTLENGTH_COL+ " INTEGER, "+ CACHE_CONTENTDISPOSITION_COL+ " TEXT, "+ " UNIQUE ("+ CACHE_URL_COL+ ") ON CONFLICT REPLACE);");
    mCacheDatabase.execSQL("CREATE INDEX cacheUrlIndex ON cache (" + CACHE_URL_COL + ")");
  }
}
