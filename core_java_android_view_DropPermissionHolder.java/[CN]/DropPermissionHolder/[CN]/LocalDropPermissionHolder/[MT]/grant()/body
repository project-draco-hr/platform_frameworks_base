{
  if (mPermissionOwner != null) {
    return;
  }
  mPermissionOwner=mActivityManager.newUriPermissionOwner("drop");
  long origId=Binder.clearCallingIdentity();
  try {
    for (    Uri mUri : mUris) {
      mActivityManager.grantUriPermissionFromOwner(mPermissionOwner,mSourceUid,mTargetPackage,mUri,mMode,mSourceUserId,mTargetUserId);
    }
  }
  finally {
    Binder.restoreCallingIdentity(origId);
  }
}
