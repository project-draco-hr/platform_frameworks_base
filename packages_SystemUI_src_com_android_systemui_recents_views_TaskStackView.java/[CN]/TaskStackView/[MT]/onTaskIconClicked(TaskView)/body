{
  Console.log(Constants.DebugFlags.UI.ClickEvents,"[TaskStack|Clicked|Icon]",tv.getTask() + " is currently filtered: " + mStack.hasFilteredTasks(),Console.AnsiCyan);
  if (Constants.DebugFlags.App.EnableTaskFiltering) {
    if (mStack.hasFilteredTasks()) {
      mStack.unfilterTasks();
    }
 else {
      mStack.filterTasks(tv.getTask());
    }
  }
 else {
    Console.logError(getContext(),"Task Filtering TBD");
  }
}
