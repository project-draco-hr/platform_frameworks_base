{
  boolean isInitialized=mLayoutAlgorithm.isInitialized();
  mStack.setTasks(getContext(),stack.computeAllTasksList(),notifyStackChanges && isInitialized);
  if (isInitialized) {
    updateLayoutAlgorithm(false,EMPTY_TASK_SET);
    updateToInitialState();
    relayoutTaskViewsOnNextFrame(AnimationProps.IMMEDIATE);
  }
}
