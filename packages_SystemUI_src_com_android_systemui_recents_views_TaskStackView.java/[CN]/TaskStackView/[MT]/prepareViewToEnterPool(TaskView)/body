{
  Task task=tv.getTask();
  if (Console.Enabled) {
    Console.log(Constants.Log.ViewPool.PoolCallbacks,"[TaskStackView|returnToPool]",tv.getTask() + " tv: " + tv);
  }
  RecentsTaskLoader.getInstance().unloadTaskData(task);
  detachViewFromParent(tv);
  tv.disableHwLayers();
  tv.resetViewProperties();
}
