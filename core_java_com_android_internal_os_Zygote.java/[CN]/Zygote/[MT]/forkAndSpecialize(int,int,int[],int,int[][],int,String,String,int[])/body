{
  long startTime=SystemClock.elapsedRealtime();
  VM_HOOKS.preFork();
  checkTime(startTime,"Zygote.preFork");
  int pid=nativeForkAndSpecialize(uid,gid,gids,debugFlags,rlimits,mountExternal,seInfo,niceName,fdsToClose);
  checkTime(startTime,"Zygote.nativeForkAndSpecialize");
  VM_HOOKS.postForkCommon();
  checkTime(startTime,"Zygote.postForkCommon");
  return pid;
}
