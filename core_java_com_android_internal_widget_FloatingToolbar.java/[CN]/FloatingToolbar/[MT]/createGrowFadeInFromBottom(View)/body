{
  AnimatorSet growFadeInFromBottomAnimation=new AnimatorSet();
  growFadeInFromBottomAnimation.playTogether(ObjectAnimator.ofFloat(view,View.SCALE_X,0.5f,1).setDuration(125),ObjectAnimator.ofFloat(view,View.SCALE_Y,0.5f,1).setDuration(125),ObjectAnimator.ofFloat(view,View.ALPHA,0,1).setDuration(75),ObjectAnimator.ofFloat(view,View.X,view.getX(),view.getX()));
  growFadeInFromBottomAnimation.setStartDelay(50);
  return growFadeInFromBottomAnimation;
}
