{
  if (name == null) {
    throw new NullPointerException(Messages.getString("awt.38"));
  }
  if (properties == null) {
    addObserver(observer);
    startProduction();
    if (properties == null) {
      return null;
    }
  }
  Object prop=properties.get(name);
  if (prop == null) {
    prop=UndefinedProperty;
  }
  return prop;
}
