{
  ascent=ga.getAscent();
  descent=ga.getDescent();
  leading=2;
  baseLineIndex=ga.getAlignment();
  italicAngle=0;
  superScriptOffset=0;
  underlineOffset=Math.max(descent / 2,1);
  underlineThickness=Math.max(ascent / 13,1);
  strikethroughOffset=-ascent / 2;
  strikethroughThickness=underlineThickness;
}
