{
  try {
    if (event == null) {
      event=mSession.getPendingTrackballMove(this);
      onDispatchTrackball(event,eventTime,false);
    }
 else     if (callWhenDone) {
      if (!onDispatchTrackball(event,eventTime,true)) {
        mSession.finishKey(this);
      }
    }
 else {
      onDispatchTrackball(event,eventTime,false);
    }
  }
 catch (  RemoteException ex) {
  }
}
