{
  if (!iface.matches(sIfaceMatch))   return;
  Log.d(TAG,"Adding " + iface);
synchronized (this) {
    if (!mIface.isEmpty())     return;
    mIface=iface;
  }
  interfaceUpdated();
  mNetworkInfo.setIsAvailable(true);
  Message msg=mCsHandler.obtainMessage(EVENT_CONFIGURATION_CHANGED,mNetworkInfo);
  msg.sendToTarget();
}
