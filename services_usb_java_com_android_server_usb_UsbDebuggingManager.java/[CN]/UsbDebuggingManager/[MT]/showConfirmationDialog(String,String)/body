{
  Intent intent=new Intent();
  ComponentName componentName=ComponentName.unflattenFromString(Resources.getSystem().getString(com.android.internal.R.string.config_customAdbPublicKeyActivity));
  intent.setClassName(componentName.getPackageName(),componentName.getClassName());
  intent.addFlags(Intent.FLAG_ACTIVITY_NEW_TASK);
  intent.putExtra("key",key);
  intent.putExtra("fingerprints",fingerprints);
  try {
    mContext.startActivity(intent);
  }
 catch (  ActivityNotFoundException e) {
    Slog.e(TAG,"unable to start UsbDebuggingActivity");
  }
}
