{
  if (values == null || values.size() == 0) {
    return;
  }
  final Collection<String> charsetCollection=mCurrentProperty.getParameters("CHARSET");
  String charset=((charsetCollection != null) ? charsetCollection.iterator().next() : null);
  String targetCharset=CharsetUtils.nameForDefaultVendor(charset);
  final Collection<String> encodingCollection=mCurrentProperty.getParameters("ENCODING");
  String encoding=((encodingCollection != null) ? encodingCollection.iterator().next() : null);
  if (targetCharset == null || targetCharset.length() == 0) {
    targetCharset=mTargetCharset;
  }
  for (  String value : values) {
    mCurrentProperty.addToPropertyValueList(handleOneValue(value,targetCharset,encoding));
  }
}
