{
  pw.print("    ");
  if (adj >= 0) {
    pw.print(' ');
    if (adj < 10)     pw.print(' ');
  }
 else {
    if (adj > -10)     pw.print(' ');
  }
  pw.print(adj);
  pw.print(": ");
  pw.print(name);
  pw.print(" (");
  pw.print(stringifySize(mProcessList.getMemLevel(adj),1024));
  pw.println(")");
}
