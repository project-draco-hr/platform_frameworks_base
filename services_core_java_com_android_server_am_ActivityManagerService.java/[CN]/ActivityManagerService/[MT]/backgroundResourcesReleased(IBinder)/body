{
  final long origId=Binder.clearCallingIdentity();
  try {
synchronized (this) {
      ActivityStack stack=ActivityRecord.getStackLocked(token);
      if (stack != null) {
        stack.backgroundResourcesReleased(token);
      }
    }
  }
  finally {
    Binder.restoreCallingIdentity(origId);
  }
}
