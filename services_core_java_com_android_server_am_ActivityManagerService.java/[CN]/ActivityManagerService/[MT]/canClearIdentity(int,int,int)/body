{
  if (UserHandle.getUserId(callingUid) == userId) {
    return true;
  }
  if (checkComponentPermission(INTERACT_ACROSS_USERS,callingPid,callingUid,-1,true) == PackageManager.PERMISSION_GRANTED || checkComponentPermission(INTERACT_ACROSS_USERS_FULL,callingPid,callingUid,-1,true) == PackageManager.PERMISSION_GRANTED) {
    return true;
  }
  return false;
}
