{
  if ((perm.modeFlags & (Intent.FLAG_GRANT_READ_URI_PERMISSION | Intent.FLAG_GRANT_WRITE_URI_PERMISSION)) == 0) {
    ArrayMap<Uri,UriPermission> perms=mGrantedUriPermissions.get(perm.targetUid);
    if (perms != null) {
      if (DEBUG_URI_PERMISSION)       Slog.v(TAG,"Removing " + perm.targetUid + " permission to "+ perm.uri);
      perms.remove(perm.uri);
      if (perms.size() == 0) {
        mGrantedUriPermissions.remove(perm.targetUid);
      }
    }
  }
}
