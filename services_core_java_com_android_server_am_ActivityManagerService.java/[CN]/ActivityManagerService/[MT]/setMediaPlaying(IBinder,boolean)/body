{
  final long origId=Binder.clearCallingIdentity();
  try {
synchronized (this) {
      final ActivityRecord r=ActivityRecord.isInStackLocked(token);
      if (r != null) {
        return mStackSupervisor.setMediaPlayingLocked(r,playing);
      }
    }
    return false;
  }
  finally {
    Binder.restoreCallingIdentity(origId);
  }
}
