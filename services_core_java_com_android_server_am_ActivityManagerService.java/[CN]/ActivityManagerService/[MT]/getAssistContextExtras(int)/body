{
  PendingAssistExtras pae=enqueueAssistContext(requestType,null,null,null,UserHandle.getCallingUserId(),PENDING_ASSIST_EXTRAS_TIMEOUT);
  if (pae == null) {
    return null;
  }
synchronized (pae) {
    while (!pae.haveResult) {
      try {
        pae.wait();
      }
 catch (      InterruptedException e) {
      }
    }
  }
synchronized (this) {
    buildAssistBundleLocked(pae,pae.result);
    mPendingAssistExtras.remove(pae);
    mHandler.removeCallbacks(pae);
  }
  return pae.extras;
}
