{
  PendingAssistExtras pae=enqueueAssistContext(requestType,null,null,UserHandle.getCallingUserId());
  if (pae == null) {
    return null;
  }
synchronized (pae) {
    while (!pae.haveResult) {
      try {
        pae.wait();
      }
 catch (      InterruptedException e) {
      }
    }
    if (pae.result != null) {
      pae.extras.putBundle(Intent.EXTRA_ASSIST_CONTEXT,pae.result);
    }
  }
synchronized (this) {
    mPendingAssistExtras.remove(pae);
    mHandler.removeCallbacks(pae);
  }
  return pae.extras;
}
