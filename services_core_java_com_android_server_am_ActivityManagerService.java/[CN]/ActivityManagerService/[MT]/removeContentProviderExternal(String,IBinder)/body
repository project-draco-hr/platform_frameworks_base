{
  enforceCallingPermission(android.Manifest.permission.ACCESS_CONTENT_PROVIDERS_EXTERNALLY,"Do not have permission in call removeContentProviderExternal()");
  int userId=UserHandle.getCallingUserId();
  long ident=Binder.clearCallingIdentity();
  try {
    removeContentProviderExternalUnchecked(name,token,userId);
  }
  finally {
    Binder.restoreCallingIdentity(ident);
  }
}
