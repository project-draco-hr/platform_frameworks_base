{
  final ArrayMap<GrantUri,UriPermission> perms=mGrantedUriPermissions.get(callingUid);
  if (perms != null) {
    for (    GrantUri grantUri : perms.keySet()) {
      if (grantUri.sourceUserId == userId || !checkUser) {
        if (matchesProvider(grantUri.uri,cpi)) {
          return true;
        }
      }
    }
  }
  return false;
}
