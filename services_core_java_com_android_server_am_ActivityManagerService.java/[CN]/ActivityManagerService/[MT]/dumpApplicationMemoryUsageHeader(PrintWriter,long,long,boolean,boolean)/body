{
  if (isCompact) {
    pw.print("version,");
    pw.println(MEMINFO_COMPACT_VERSION);
  }
  if (isCheckinRequest || isCompact) {
    pw.print("time,");
    pw.print(uptime);
    pw.print(",");
    pw.println(realtime);
  }
 else {
    pw.println("Applications Memory Usage (in Kilobytes):");
    pw.println("Uptime: " + uptime + " Realtime: "+ realtime);
  }
}
