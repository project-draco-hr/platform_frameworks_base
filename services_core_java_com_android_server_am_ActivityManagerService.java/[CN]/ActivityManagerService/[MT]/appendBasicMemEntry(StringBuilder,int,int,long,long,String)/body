{
  sb.append("  ");
  sb.append(ProcessList.makeOomAdjString(oomAdj));
  sb.append(' ');
  sb.append(ProcessList.makeProcStateString(procState));
  sb.append(' ');
  ProcessList.appendRamKb(sb,pss);
  sb.append(" kB: ");
  sb.append(name);
  if (memtrack > 0) {
    sb.append(" (");
    sb.append(memtrack);
    sb.append(" kB memtrack)");
  }
}
