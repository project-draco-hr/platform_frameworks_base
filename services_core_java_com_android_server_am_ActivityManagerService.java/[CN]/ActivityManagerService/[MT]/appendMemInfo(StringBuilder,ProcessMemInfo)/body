{
  appendBasicMemEntry(sb,mi.oomAdj,mi.procState,mi.pss,mi.memtrack,mi.name);
  sb.append(" (pid ");
  sb.append(mi.pid);
  sb.append(") ");
  sb.append(mi.adjType);
  sb.append('\n');
  if (mi.adjReason != null) {
    sb.append("                      ");
    sb.append(mi.adjReason);
    sb.append('\n');
  }
}
