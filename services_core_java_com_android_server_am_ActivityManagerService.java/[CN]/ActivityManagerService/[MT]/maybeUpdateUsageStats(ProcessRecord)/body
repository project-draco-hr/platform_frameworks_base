{
  if (DEBUG_USAGE_STATS) {
    Slog.d(TAG,"Checking proc [" + Arrays.toString(app.getPackageList()) + "] state changes: old = "+ app.setProcState+ ", new = "+ app.curProcState);
  }
  if (mUsageStatsService == null) {
    return;
  }
  if (app.curProcState <= ActivityManager.PROCESS_STATE_IMPORTANT_FOREGROUND && (app.setProcState > ActivityManager.PROCESS_STATE_IMPORTANT_FOREGROUND || app.setProcState < 0)) {
    String[] packages=app.getPackageList();
    if (packages != null) {
      for (int i=0; i < packages.length; i++) {
        mUsageStatsService.reportEvent(packages[i],app.userId,UsageEvents.Event.INTERACTION);
      }
    }
  }
}
