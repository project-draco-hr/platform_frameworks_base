{
  StringBuffer buffer=new StringBuffer();
  if (dimensions != null) {
    int n=dimensions.length;
    for (int i=0; i < n; ++i) {
      if (dimensions[i] == SIZE_UNSPECIFIED) {
        buffer.append("[]");
      }
 else {
        buffer.append("[" + String.valueOf(dimensions[i]) + "]");
      }
    }
  }
  return buffer.toString();
}
