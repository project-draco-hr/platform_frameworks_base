{
  if (pkgName == null) {
    return;
  }
  Log.i(TAG,"Deleting package : " + pkgName);
  try {
    ApplicationInfo info=getPm().getApplicationInfo(pkgName,PackageManager.GET_UNINSTALLED_PACKAGES);
    if (info != null) {
      getPm().deletePackage(pkgName,null,0);
    }
  }
 catch (  NameNotFoundException e) {
  }
}
