{
  if (Environment.isExternalStorageEmulated()) {
    return;
  }
  installFromRawResource("install.apk",R.raw.install_loc_unspecified,PackageManager.INSTALL_FORWARD_LOCK | PackageManager.INSTALL_EXTERNAL,true,true,PackageManager.INSTALL_FAILED_INVALID_INSTALL_LOCATION,PackageInfo.INSTALL_LOCATION_UNSPECIFIED);
}
