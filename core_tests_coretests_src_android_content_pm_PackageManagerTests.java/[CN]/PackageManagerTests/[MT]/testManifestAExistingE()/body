{
  if (Environment.isExternalStorageEmulated()) {
    return;
  }
  int iFlags=PackageManager.INSTALL_EXTERNAL;
  int rFlags=PackageManager.INSTALL_REPLACE_EXISTING;
  installFromRawResource("install.apk",R.raw.install,iFlags,false,false,-1,-1);
  installFromRawResource("install.apk",R.raw.install_loc_auto,rFlags,true,false,-1,PackageInfo.INSTALL_LOCATION_PREFER_EXTERNAL);
}
