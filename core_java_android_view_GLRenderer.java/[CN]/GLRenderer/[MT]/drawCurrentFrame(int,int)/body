{
  if (index >= 0) {
    mDebugDataProvider.setupCurrentFramePaint(mProfilePaint);
switch (graphType) {
case GraphDataProvider.GRAPH_TYPE_BARS:
      mGlCanvas.drawRect(mProfileShapes[2][index],mProfileShapes[2][index + 1],mProfileShapes[2][index + 2],mProfileShapes[0][index + 3],mProfilePaint);
    break;
case GraphDataProvider.GRAPH_TYPE_LINES:
  mGlCanvas.drawLine(mProfileShapes[2][index],mProfileShapes[2][index + 1],mProfileShapes[2][index],mHeight,mProfilePaint);
break;
}
}
}
