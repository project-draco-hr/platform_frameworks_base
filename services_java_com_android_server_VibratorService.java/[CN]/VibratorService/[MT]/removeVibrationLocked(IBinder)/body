{
  ListIterator<Vibration> iter=mVibrations.listIterator(0);
  while (iter.hasNext()) {
    Vibration vib=iter.next();
    if (vib.mToken == token) {
      iter.remove();
      return vib;
    }
  }
  if (mCurrentVibration != null && mCurrentVibration.mToken == token) {
    return mCurrentVibration;
  }
  return null;
}
