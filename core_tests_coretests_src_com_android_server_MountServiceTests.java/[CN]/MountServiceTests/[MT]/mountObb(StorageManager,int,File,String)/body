{
  copyRawToFile(resource,file);
  ObbObserver observer=new ObbObserver();
  assertTrue("mountObb call on " + file.getPath() + " should succeed",sm.mountObb(file.getPath(),null,observer));
  assertTrue("Mount should have completed",waitForCompletion(observer));
  assertEquals("Actual file and resolved file should be the same",file.getPath(),observer.path);
  assertEquals(expectedState,observer.state);
}
