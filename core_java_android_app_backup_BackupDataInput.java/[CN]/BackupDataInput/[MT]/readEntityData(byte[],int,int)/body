{
  if (mHeaderReady) {
    int result=readEntityData_native(mBackupReader,data,offset,size);
    if (result >= 0) {
      return result;
    }
 else {
      throw new IOException("result=0x" + Integer.toHexString(result));
    }
  }
 else {
    throw new IllegalStateException("mHeaderReady=false");
  }
}
