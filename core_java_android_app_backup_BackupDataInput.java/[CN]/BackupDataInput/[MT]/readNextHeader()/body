{
  int result=readNextHeader_native(mBackupReader,mHeader);
  if (result == 0) {
    mHeaderReady=true;
    return true;
  }
 else   if (result > 0) {
    mHeaderReady=false;
    return false;
  }
 else {
    mHeaderReady=false;
    throw new IOException("result=0x" + Integer.toHexString(result));
  }
}
