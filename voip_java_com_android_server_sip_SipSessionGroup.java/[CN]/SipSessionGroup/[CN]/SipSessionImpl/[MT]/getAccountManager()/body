{
  return new AccountManager(){
    @Override public UserCredentials getCredentials(    ClientTransaction challengedTransaction,    String realm){
      return new UserCredentials(){
        @Override public String getUserName(){
          String username=mLocalProfile.getAuthUserName();
          return (!TextUtils.isEmpty(username) ? username : mLocalProfile.getUserName());
        }
        @Override public String getPassword(){
          return mPassword;
        }
        @Override public String getSipDomain(){
          return mLocalProfile.getSipDomain();
        }
      }
;
    }
  }
;
}
