{
  new Thread(new Runnable(){
    @Override public void run(){
      try {
        processCommand(command);
      }
 catch (      Throwable e) {
        loge("command error: " + command + ": "+ mLocalProfile.getUriString(),getRootCause(e));
        onError(e);
      }
    }
  }
,"SipSessionAsyncCmdThread").start();
}
