{
  int expires=EXPIRY_TIME;
  ExpiresHeader expiresHeader=(ExpiresHeader)response.getHeader(ExpiresHeader.NAME);
  if (expiresHeader != null)   expires=expiresHeader.getExpires();
  expiresHeader=(ExpiresHeader)response.getHeader(MinExpiresHeader.NAME);
  if (expiresHeader != null) {
    expires=Math.max(expires,expiresHeader.getExpires());
  }
  return expires;
}
