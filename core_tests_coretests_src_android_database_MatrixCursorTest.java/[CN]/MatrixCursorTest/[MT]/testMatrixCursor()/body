{
  MatrixCursor cursor=newMatrixCursor();
  cursor.newRow().add("a").add(1).add(2).add(3).add(4).add(5).add(new byte[]{(byte)0xaa,(byte)0x55});
  cursor.moveToNext();
  checkValues(cursor);
  cursor.newRow().add("a").add("1").add("2").add("3").add("4").add("5").add(new byte[]{(byte)0xaa,(byte)0x55});
  cursor.moveToNext();
  checkValues(cursor);
  cursor.moveToPrevious();
  checkValues(cursor);
}
