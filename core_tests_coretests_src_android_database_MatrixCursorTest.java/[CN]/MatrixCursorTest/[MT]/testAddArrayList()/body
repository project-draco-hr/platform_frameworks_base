{
  MatrixCursor cursor=newMatrixCursor();
  cursor.addRow(new NonIterableArrayList<Object>(Arrays.asList("a",1,2,3,4,5)));
  cursor.moveToNext();
  checkValues(cursor);
  try {
    cursor.addRow(new NonIterableArrayList<Object>());
    fail();
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    cursor.addRow(new NonIterableArrayList<Object>(Arrays.asList("a",1,2,3,4,5,"Too many!")));
    fail();
  }
 catch (  IllegalArgumentException e) {
  }
}
