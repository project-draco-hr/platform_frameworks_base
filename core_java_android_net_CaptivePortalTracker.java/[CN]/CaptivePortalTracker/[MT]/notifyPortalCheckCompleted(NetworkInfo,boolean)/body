{
  if (info == null) {
    loge("notifyPortalCheckComplete on null");
    return;
  }
  try {
    if (DBG)     log("notifyPortalCheckCompleted: captive=" + isCaptivePortal + " ni="+ info);
    mConnService.captivePortalCheckCompleted(info,isCaptivePortal);
  }
 catch (  RemoteException e) {
    e.printStackTrace();
  }
}
