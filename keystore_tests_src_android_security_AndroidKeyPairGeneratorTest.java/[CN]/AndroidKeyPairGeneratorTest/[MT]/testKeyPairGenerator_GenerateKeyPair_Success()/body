{
  mGenerator.initialize(new AndroidKeyPairGeneratorSpec(getContext(),TEST_ALIAS_1,TEST_DN_1,TEST_SERIAL_1,NOW,NOW_PLUS_10_YEARS));
  final KeyPair pair=mGenerator.generateKeyPair();
  assertNotNull("The KeyPair returned should not be null",pair);
  assertKeyPairCorrect(pair,TEST_ALIAS_1,TEST_DN_1,TEST_SERIAL_1,NOW,NOW_PLUS_10_YEARS);
}
