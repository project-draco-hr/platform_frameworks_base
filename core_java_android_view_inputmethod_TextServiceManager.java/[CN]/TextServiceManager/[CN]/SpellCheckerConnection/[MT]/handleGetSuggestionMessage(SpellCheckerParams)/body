{
  if (!checkOpenConnection()) {
    return;
  }
  try {
    scp.mCallback.getSuggestionsResult(scp.mText,scp.mStart,scp.mEnd,scp.mLocale,mSpellCheckerService.getSuggestions(scp.mText,scp.mStart,scp.mEnd,scp.mLocale.toString()));
  }
 catch (  RemoteException e) {
    Log.e(TAG,"Remote exception in getSuggestion.");
  }
}
