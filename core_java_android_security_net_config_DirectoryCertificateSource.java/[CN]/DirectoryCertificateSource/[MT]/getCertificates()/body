{
synchronized (mLock) {
    if (mCertificates != null) {
      return mCertificates;
    }
    Set<X509Certificate> certs=new ArraySet<X509Certificate>();
    if (mDir.isDirectory()) {
      for (      String caFile : mDir.list()) {
        if (isCertMarkedAsRemoved(caFile)) {
          continue;
        }
        X509Certificate cert=readCertificate(caFile);
        if (cert != null) {
          certs.add(cert);
        }
      }
    }
    mCertificates=certs;
    return mCertificates;
  }
}
