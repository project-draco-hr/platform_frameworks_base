{
  int id=event.getEventId();
  if ((id == NativeEvent.ID_INSETS_CHANGED) || (id == NativeEvent.ID_THEME_CHANGED)) {
    return dispatchInsets(event,src);
  }
 else   if ((id >= WindowEvent.WINDOW_FIRST) && (id <= WindowEvent.WINDOW_LAST)) {
    return dispatchWindow(event,src);
  }
 else {
    return dispatchPureComponent(event,src);
  }
}
