{
  if (b1 == b2) {
    return true;
  }
  if (includeSyncSettings && b1.size() != b2.size()) {
    return false;
  }
  for (  String key : b1.keySet()) {
    if (!includeSyncSettings && isSyncSetting(key)) {
      continue;
    }
    if (!b2.containsKey(key)) {
      return false;
    }
    if (!b1.get(key).equals(b2.get(key))) {
      return false;
    }
  }
  return true;
}
