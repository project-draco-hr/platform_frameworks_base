{
  mIsTemporary=false;
  setContainsDiffs(false);
  sURIMatcher.addURI(CONTENT_URI.getAuthority(),"items",MATCHER_ITEMS);
  sURIMatcher.addURI(CONTENT_URI.getAuthority(),"deleted_items",MATCHER_DELETED_ITEMS);
  mDb=SQLiteDatabase.create(null);
  mDb.execSQL("CREATE TABLE items (" + "_id INTEGER PRIMARY KEY AUTOINCREMENT, " + "data TEXT, "+ "_sync_time TEXT, "+ "_sync_version TEXT, "+ "_sync_id TEXT, "+ "_sync_local_id INTEGER, "+ "_sync_dirty INTEGER NOT NULL DEFAULT 0, "+ "_sync_account TEXT, "+ "_sync_mark INTEGER)");
  mDb.execSQL("CREATE TABLE deleted_items (" + "_id INTEGER PRIMARY KEY AUTOINCREMENT, " + "_sync_version TEXT, "+ "_sync_id TEXT, "+ "_sync_local_id INTEGER, "+ "_sync_account TEXT, "+ "_sync_mark INTEGER)");
}
