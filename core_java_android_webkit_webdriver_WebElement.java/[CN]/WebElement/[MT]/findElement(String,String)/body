{
  String findElement=mDriver.getResourceAsString(R.raw.find_element_android);
  WebElement el;
  if (mId.equals("")) {
    el=(WebElement)executeAtom(findElement,strategy,locator);
  }
 else {
    el=(WebElement)executeAtom(findElement,strategy,locator,this);
  }
  if (el == null) {
    throw new WebElementNotFoundException("Could not find element " + "with " + strategy + ": "+ locator);
  }
  return el;
}
