{
  if (target instanceof View) {
switch (property) {
case "translationX":
      return View.TRANSLATION_X;
case "translationY":
    return View.TRANSLATION_Y;
case "translationZ":
  return View.TRANSLATION_Z;
case "alpha":
return View.ALPHA;
case "rotation":
return View.ROTATION;
case "x":
return View.X;
case "y":
return View.Y;
case "scaleX":
return View.SCALE_X;
case "scaleY":
return View.SCALE_Y;
}
}
return Property.of(target.getClass(),cls,property);
}
