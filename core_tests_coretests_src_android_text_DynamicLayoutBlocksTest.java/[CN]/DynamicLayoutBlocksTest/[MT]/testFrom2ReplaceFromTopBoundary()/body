{
  defineInitialState(new int[]{10,20},new int[]{123,456});
  update(11,11,50);
  assertState(new int[]{11,50,20 - 11},new int[]{123,___,___});
  update(11,14,50);
  assertState(new int[]{11,50,20 - 14},new int[]{123,___,___});
  update(11,20,50);
  assertState(new int[]{11,50},new int[]{123,___});
}
