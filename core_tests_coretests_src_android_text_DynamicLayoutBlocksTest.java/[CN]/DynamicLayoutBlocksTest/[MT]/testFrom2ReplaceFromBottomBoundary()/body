{
  defineInitialState(new int[]{10,20},new int[]{123,456});
  update(10,10,50);
  assertState(new int[]{10,50,20 - 10},new int[]{___,___,456});
  update(10,14,50);
  assertState(new int[]{10,50,20 - 14},new int[]{___,___,___});
  update(10,20,50);
  assertState(new int[]{10,50},new int[]{___,___});
}
