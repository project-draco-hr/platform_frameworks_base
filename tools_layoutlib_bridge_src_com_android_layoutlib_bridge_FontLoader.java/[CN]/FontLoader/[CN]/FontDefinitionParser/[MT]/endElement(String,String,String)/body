{
  if (localName.equals(NODE_LEVEL[mDepth - 1])) {
    mDepth--;
    if (mDepth == 2) {
      if (mFontInfo != null) {
        String family=trimXmlWhitespaces(mBuilder.toString());
        mFontInfo.families.add(family);
        mBuilder.setLength(0);
      }
    }
 else     if (mDepth == 1) {
      mFontInfo=null;
    }
  }
}
