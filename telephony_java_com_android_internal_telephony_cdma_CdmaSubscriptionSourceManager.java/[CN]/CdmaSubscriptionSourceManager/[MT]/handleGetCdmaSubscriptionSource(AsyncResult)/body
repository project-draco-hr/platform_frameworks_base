{
  if ((ar.exception == null) && (ar.result != null)) {
    int newSubscriptionSource=((int[])ar.result)[0];
    if (newSubscriptionSource != mCdmaSubscriptionSource.get()) {
      log("Subscription Source Changed : " + mCdmaSubscriptionSource + " >> "+ newSubscriptionSource);
      mCdmaSubscriptionSource.set(newSubscriptionSource);
      mCdmaSubscriptionSourceChangedRegistrants.notifyRegistrants(new AsyncResult(null,null,null));
    }
  }
 else {
    logw("Unable to get CDMA Subscription Source, Exception: " + ar.exception + ", result: "+ ar.result);
  }
}
