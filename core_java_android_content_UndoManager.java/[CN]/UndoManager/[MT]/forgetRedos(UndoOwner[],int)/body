{
  if (count < 0) {
    count=mRedos.size();
  }
  int removed=0;
  int i=0;
  while (i < mRedos.size() && removed < count) {
    UndoState state=mRedos.get(i);
    if (count > 0 && matchOwners(state,owners)) {
      state.destroy();
      mRedos.remove(i);
      removed++;
    }
 else {
      i++;
    }
  }
  return removed;
}
