{
  if (count < 0) {
    count=mRedos.size();
  }
  int removed=0;
  for (int i=0; i < mRedos.size() && removed < count; i++) {
    UndoState state=mRedos.get(i);
    if (count > 0 && matchOwners(state,owners)) {
      state.destroy();
      mRedos.remove(i);
      removed++;
    }
  }
  return removed;
}
