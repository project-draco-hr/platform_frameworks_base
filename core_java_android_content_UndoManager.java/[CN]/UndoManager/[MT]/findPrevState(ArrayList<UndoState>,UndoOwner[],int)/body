{
  final int N=states.size();
  if (from == -1) {
    from=N - 1;
  }
  if (from >= N) {
    return -1;
  }
  if (owners == null) {
    return from;
  }
  while (from >= 0) {
    UndoState state=states.get(from);
    if (matchOwners(state,owners)) {
      return from;
    }
    from--;
  }
  return -1;
}
