{
  final int N=mOperations.size();
  if (clazz == null && owner == null) {
    return N > 0 ? (T)mOperations.get(N - 1) : null;
  }
  for (int i=N - 1; i >= 0; i--) {
    UndoOperation<?> op=mOperations.get(i);
    if (owner != null && op.getOwner() != owner) {
      continue;
    }
    if (clazz != null && op.getClass() != clazz) {
      return null;
    }
    return (T)op;
  }
  return null;
}
