{
  for (int i=mOperations.size() - 1; i >= 0; i--) {
    UndoOwner owner=mOperations.get(i).mOwner;
    owner.mOpCount--;
    if (owner.mOpCount <= 0) {
      if (owner.mOpCount < 0) {
        throw new IllegalStateException("Underflow of op count on owner " + owner + " in op "+ mOperations.get(i));
      }
      mManager.removeOwner(owner);
    }
  }
}
