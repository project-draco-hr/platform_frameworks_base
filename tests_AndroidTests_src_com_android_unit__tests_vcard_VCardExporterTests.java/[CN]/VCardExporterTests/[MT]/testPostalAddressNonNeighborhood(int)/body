{
  VCardVerifier verifier=new VCardVerifier(vcardType);
  ContactEntry entry=verifier.addInputEntry();
  entry.buildData(StructuredPostal.CONTENT_ITEM_TYPE).put(StructuredPostal.CITY,"City");
  verifier.addPropertyNodesVerifierElemWithEmptyName().addNodeWithoutOrder("ADR",Arrays.asList("","","","City","","",""),new TypeSet("HOME"));
  verifier.verify();
}
