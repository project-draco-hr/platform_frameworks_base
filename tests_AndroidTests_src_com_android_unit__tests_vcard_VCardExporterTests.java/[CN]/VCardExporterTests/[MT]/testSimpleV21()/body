{
  VCardVerifier verifier=new VCardVerifier(V21);
  ContactEntry entry=verifier.addInputEntry();
  entry.buildData(StructuredName.CONTENT_ITEM_TYPE).put(StructuredName.FAMILY_NAME,"Ando").put(StructuredName.GIVEN_NAME,"Roid");
  verifier.addPropertyNodesVerifierElem().addNodeWithoutOrder("FN","Roid Ando").addNodeWithoutOrder("N","Ando;Roid;;;",Arrays.asList("Ando","Roid","","",""));
  verifier.verify();
}
