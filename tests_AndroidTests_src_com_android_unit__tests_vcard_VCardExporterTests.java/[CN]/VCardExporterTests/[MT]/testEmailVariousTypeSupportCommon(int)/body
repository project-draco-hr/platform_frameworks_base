{
  ExportTestResolver resolver=new ExportTestResolver();
  ContactEntry entry=resolver.buildContactEntry();
  entry.buildData(Email.CONTENT_ITEM_TYPE).put(Email.DATA,"type_home@example.com").put(Email.TYPE,Email.TYPE_HOME);
  entry.buildData(Email.CONTENT_ITEM_TYPE).put(Email.DATA,"type_work@example.com").put(Email.TYPE,Email.TYPE_WORK);
  entry.buildData(Email.CONTENT_ITEM_TYPE).put(Email.DATA,"type_mobile@example.com").put(Email.TYPE,Email.TYPE_MOBILE);
  entry.buildData(Email.CONTENT_ITEM_TYPE).put(Email.DATA,"type_other@example.com").put(Email.TYPE,Email.TYPE_OTHER);
  VCardVerifier verifier=new VCardVerifier(resolver,version);
  verifier.addPropertyNodesVerifierElemWithEmptyName().addNodeWithoutOrder("EMAIL","type_home@example.com",new TypeSet("HOME")).addNodeWithoutOrder("EMAIL","type_work@example.com",new TypeSet("WORK")).addNodeWithoutOrder("EMAIL","type_mobile@example.com",new TypeSet("CELL")).addNodeWithoutOrder("EMAIL","type_other@example.com");
  verifier.verify();
}
