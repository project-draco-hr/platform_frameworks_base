{
  VCardVerifier verifier=new VCardVerifier(vcardType);
  ContactEntry entry=verifier.addInputEntry();
  entry.buildData(StructuredPostal.CONTENT_ITEM_TYPE).put(StructuredPostal.NEIGHBORHOOD,"Neighborhood");
  verifier.addPropertyNodesVerifierElemWithEmptyName().addNodeWithoutOrder("ADR",Arrays.asList("","","","Neighborhood","","",""),new TypeSet("HOME"));
  verifier.verify();
}
