{
  ExportTestResolver resolver=new ExportTestResolver();
  resolver.buildInput(StructuredPostal.CONTENT_ITEM_TYPE).put(StructuredPostal.FORMATTED_ADDRESS,"Formatted address CA 123-334 United Statue");
  VCardVerificationHandler handler=new VCardVerificationHandler(this,version);
  handler.addPropertyVerifierWithEmptyName().addNodeWithOrder("ADR",";Formatted address CA 123-334 United Statue;;;;;",Arrays.asList("","Formatted address CA 123-334 United Statue","","","","",""),new TypeSet("HOME"));
  verifyOneComposition(resolver,handler,version);
}
