{
  ExportTestResolver resolver=new ExportTestResolver();
  resolver.buildContactEntry().buildData(StructuredPostal.CONTENT_ITEM_TYPE).put(StructuredPostal.FORMATTED_ADDRESS,"Formatted address CA 123-334 United Statue");
  VCardVerifier verifier=new VCardVerifier(resolver,version);
  verifier.addPropertyNodesVerifierWithEmptyName().addNodeWithOrder("ADR",";Formatted address CA 123-334 United Statue;;;;;",Arrays.asList("","Formatted address CA 123-334 United Statue","","","","",""),new TypeSet("HOME"));
  verifier.verify();
}
