{
  ExportTestResolver resolver=new ExportTestResolver();
  ContentValues contentValues=resolver.buildData(StructuredPostal.CONTENT_ITEM_TYPE);
  contentValues.put(StructuredPostal.FORMATTED_ADDRESS,"Formatted address CA 123-334 United Statue");
  VCardVerificationHandler handler=new VCardVerificationHandler(this,version);
  handler.addNewVerifierWithEmptyName().addNodeWithOrder("ADR",";Formatted address CA 123-334 United Statue;;;;;",Arrays.asList("","Formatted address CA 123-334 United Statue","","","","",""),new TypeSet("HOME"));
  verifyOneComposition(resolver,handler,version);
}
