{
  VCardVerifier verifier=new VCardVerifier(vcardType);
  ContactEntry entry=verifier.addInputEntry();
  entry.buildData(Im.CONTENT_ITEM_TYPE).put(Im.PROTOCOL,Im.PROTOCOL_AIM).put(Im.DATA,"aim1");
  entry.buildData(Im.CONTENT_ITEM_TYPE).put(Im.PROTOCOL,Im.PROTOCOL_AIM).put(Im.DATA,"aim2").put(Im.TYPE,Im.TYPE_HOME).put(Im.IS_PRIMARY,1);
  verifier.addPropertyNodesVerifierElemWithEmptyName().addNodeWithoutOrder("X-AIM","aim1").addNodeWithoutOrder("X-AIM","aim2",new TypeSet("HOME","PREF"));
  verifier.verify();
}
