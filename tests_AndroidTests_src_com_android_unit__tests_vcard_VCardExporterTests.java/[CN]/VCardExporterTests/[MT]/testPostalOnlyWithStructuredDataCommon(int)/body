{
  ExportTestResolver resolver=new ExportTestResolver();
  resolver.buildInput(StructuredPostal.CONTENT_ITEM_TYPE).put(StructuredPostal.POBOX,"Pobox").put(StructuredPostal.NEIGHBORHOOD,"Neighborhood").put(StructuredPostal.STREET,"Street").put(StructuredPostal.CITY,"City").put(StructuredPostal.REGION,"Region").put(StructuredPostal.POSTCODE,"100").put(StructuredPostal.COUNTRY,"Country");
  VCardVerificationHandler handler=new VCardVerificationHandler(this,version);
  handler.addPropertyVerifierWithEmptyName().addNodeWithoutOrder("ADR","Pobox;Neighborhood;Street;City;Region;100;Country",Arrays.asList("Pobox","Neighborhood","Street","City","Region","100","Country"),new TypeSet("HOME"));
  verifyOneComposition(resolver,handler,version);
}
