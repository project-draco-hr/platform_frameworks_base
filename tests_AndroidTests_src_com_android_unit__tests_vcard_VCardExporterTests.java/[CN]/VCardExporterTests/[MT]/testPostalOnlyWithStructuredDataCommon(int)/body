{
  VCardVerifier verifier=new VCardVerifier(vcardType);
  ContactEntry entry=verifier.addInputEntry();
  entry.buildData(StructuredPostal.CONTENT_ITEM_TYPE).put(StructuredPostal.POBOX,"Pobox").put(StructuredPostal.NEIGHBORHOOD,"Neighborhood").put(StructuredPostal.STREET,"Street").put(StructuredPostal.CITY,"City").put(StructuredPostal.REGION,"Region").put(StructuredPostal.POSTCODE,"100").put(StructuredPostal.COUNTRY,"Country");
  verifier.addPropertyNodesVerifierElemWithEmptyName().addNodeWithoutOrder("ADR","Pobox;Neighborhood;Street;City;Region;100;Country",Arrays.asList("Pobox","Neighborhood","Street","City","Region","100","Country"),new TypeSet("HOME"));
  verifier.verify();
}
