{
  VCardVerifier verifier=new VCardVerifier(vcardType);
  ContactEntry entry=verifier.addInputEntry();
  entry.buildData(Relation.CONTENT_ITEM_TYPE).put(Relation.TYPE,Relation.TYPE_MOTHER).put(Relation.NAME,"Ms. Mother");
  ImportVerifierElem elem=verifier.addImportVerifier();
  elem.addExpected(Relation.CONTENT_ITEM_TYPE).put(Relation.TYPE,Relation.TYPE_MOTHER).put(Relation.NAME,"Ms. Mother");
  verifier.verify();
}
