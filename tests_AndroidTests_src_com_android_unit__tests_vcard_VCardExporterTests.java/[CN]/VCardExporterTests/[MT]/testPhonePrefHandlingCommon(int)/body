{
  VCardVerifier verifier=new VCardVerifier(vcardType);
  ContactEntry entry=verifier.addInputEntry();
  entry.buildData(Phone.CONTENT_ITEM_TYPE).put(Phone.NUMBER,"1").put(Phone.TYPE,Phone.TYPE_HOME);
  entry.buildData(Phone.CONTENT_ITEM_TYPE).put(Phone.NUMBER,"2").put(Phone.TYPE,Phone.TYPE_WORK).put(Phone.IS_PRIMARY,1);
  entry.buildData(Phone.CONTENT_ITEM_TYPE).put(Phone.NUMBER,"3").put(Phone.TYPE,Phone.TYPE_FAX_HOME).put(Phone.IS_PRIMARY,1);
  entry.buildData(Phone.CONTENT_ITEM_TYPE).put(Phone.NUMBER,"4").put(Phone.TYPE,Phone.TYPE_FAX_WORK);
  verifier.addPropertyNodesVerifierElemWithEmptyName().addNodeWithoutOrder("TEL","4",new TypeSet("WORK","FAX")).addNodeWithoutOrder("TEL","3",new TypeSet("HOME","FAX","PREF")).addNodeWithoutOrder("TEL","2",new TypeSet("WORK","PREF")).addNodeWithoutOrder("TEL","1",new TypeSet("HOME"));
  verifier.verify();
}
