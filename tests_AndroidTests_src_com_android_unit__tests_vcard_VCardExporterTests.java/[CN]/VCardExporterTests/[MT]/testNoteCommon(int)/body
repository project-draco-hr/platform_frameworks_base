{
  VCardVerifier verifier=new VCardVerifier(vcardType);
  ContactEntry entry=verifier.addInputEntry();
  entry.buildData(Note.CONTENT_ITEM_TYPE).put(Note.NOTE,"note1");
  entry.buildData(Note.CONTENT_ITEM_TYPE).put(Note.NOTE,"note2").put(Note.IS_PRIMARY,1);
  verifier.addPropertyNodesVerifierElemWithEmptyName().addNodeWithOrder("NOTE","note1").addNodeWithOrder("NOTE","note2");
  verifier.verify();
}
