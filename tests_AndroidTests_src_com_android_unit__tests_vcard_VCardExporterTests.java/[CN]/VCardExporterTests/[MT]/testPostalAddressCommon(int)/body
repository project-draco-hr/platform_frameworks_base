{
  VCardVerifier verifier=new VCardVerifier(vcardType);
  ContactEntry entry=verifier.addInputEntry();
  entry.buildData(StructuredPostal.CONTENT_ITEM_TYPE).put(StructuredPostal.POBOX,"Pobox").put(StructuredPostal.NEIGHBORHOOD,"Neighborhood").put(StructuredPostal.STREET,"Street").put(StructuredPostal.CITY,"City").put(StructuredPostal.REGION,"Region").put(StructuredPostal.POSTCODE,"100").put(StructuredPostal.COUNTRY,"Country").put(StructuredPostal.FORMATTED_ADDRESS,"Formatted Address").put(StructuredPostal.TYPE,StructuredPostal.TYPE_WORK);
  verifier.addPropertyNodesVerifierElemWithEmptyName().addNodeWithoutOrder("ADR",Arrays.asList("Pobox","","Street","City Neighborhood","Region","100","Country"),new TypeSet("WORK"));
  verifier.verify();
}
