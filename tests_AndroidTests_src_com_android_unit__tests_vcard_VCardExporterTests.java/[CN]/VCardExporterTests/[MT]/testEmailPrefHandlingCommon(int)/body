{
  mVerifier.initForExportTest(vcardType);
  ContactEntry entry=mVerifier.addInputEntry();
  entry.addContentValues(Email.CONTENT_ITEM_TYPE).put(Email.DATA,"type_home@example.com").put(Email.TYPE,Email.TYPE_HOME).put(Email.IS_PRIMARY,1);
  entry.addContentValues(Email.CONTENT_ITEM_TYPE).put(Email.DATA,"type_notype@example.com").put(Email.IS_PRIMARY,1);
  mVerifier.addPropertyNodesVerifierElemWithEmptyName().addExpectedNode("EMAIL","type_notype@example.com",new TypeSet("PREF")).addExpectedNode("EMAIL","type_home@example.com",new TypeSet("HOME","PREF"));
}
