{
  try {
    Bundle accountManagerResult=accountManagerFuture.getResult();
    Bundle bundle=new Bundle();
    bundle.putString(AccountManager.KEY_ACCOUNT_NAME,accountManagerResult.getString(AccountManager.KEY_ACCOUNT_NAME));
    bundle.putString(AccountManager.KEY_ACCOUNT_TYPE,accountManagerResult.getString(AccountManager.KEY_ACCOUNT_TYPE));
    setResult(Activity.RESULT_OK,new Intent().putExtras(bundle));
    finish();
    return;
  }
 catch (  OperationCanceledException e) {
    setResult(Activity.RESULT_CANCELED);
    finish();
    return;
  }
catch (  IOException e) {
  }
catch (  AuthenticatorException e) {
  }
  Bundle bundle=new Bundle();
  bundle.putString(AccountManager.KEY_ERROR_MESSAGE,"error communicating with server");
  setResult(Activity.RESULT_OK,new Intent().putExtras(bundle));
  finish();
}
