{
  if (!onTop) {
    return RUBBER_BAND_FACTOR_NORMAL;
  }
  if (mExpandedInThisMotion) {
    return RUBBER_BAND_FACTOR_AFTER_EXPAND;
  }
 else   if (mIsExpansionChanging) {
    return RUBBER_BAND_FACTOR_ON_PANEL_EXPAND;
  }
 else   if (mScrolledToTopOnFirstDown) {
    return 1.0f;
  }
  return RUBBER_BAND_FACTOR_NORMAL;
}
