{
  for (  Pair<ExpandableNotificationRow,Boolean> eventPair : mHeadsUpChangeAnimations) {
    int type=eventPair.second ? AnimationEvent.ANIMATION_TYPE_HEADS_UP_APPEAR : AnimationEvent.ANIMATION_TYPE_HEADS_UP_DISAPPEAR;
    mAnimationEvents.add(new AnimationEvent(eventPair.first,type));
  }
  mHeadsUpChangeAnimations.clear();
}
