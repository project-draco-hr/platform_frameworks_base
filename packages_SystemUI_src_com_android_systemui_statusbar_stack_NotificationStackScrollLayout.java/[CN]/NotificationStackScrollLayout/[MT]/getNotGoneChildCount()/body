{
  int childCount=getChildCount();
  int count=0;
  for (int i=0; i < childCount; i++) {
    View child=getChildAt(i);
    if (child.getVisibility() != View.GONE) {
      count++;
    }
  }
  if (mDismissView.willBeGone()) {
    count--;
  }
  if (mEmptyShadeView.willBeGone()) {
    count--;
  }
  return count;
}
