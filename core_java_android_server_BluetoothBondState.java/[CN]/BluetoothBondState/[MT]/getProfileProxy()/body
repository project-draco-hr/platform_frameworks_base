{
  BluetoothAdapter bluetoothAdapter=BluetoothAdapter.getDefaultAdapter();
  if (mA2dpProxy == null) {
    bluetoothAdapter.getProfileProxy(mContext,mProfileServiceListener,BluetoothProfile.A2DP);
  }
  if (mHeadsetProxy == null) {
    bluetoothAdapter.getProfileProxy(mContext,mProfileServiceListener,BluetoothProfile.HEADSET);
  }
}
