{
  Log.v(TAG,"getDuration - " + filePath);
  MediaPlayer mp=new MediaPlayer();
  try {
    mp.setDataSource(filePath);
    mp.prepare();
  }
 catch (  Exception e) {
  }
  int duration=mp.getDuration();
  Log.v(TAG,"Duration " + duration);
  mp.release();
  Log.v(TAG,"release");
  return duration;
}
