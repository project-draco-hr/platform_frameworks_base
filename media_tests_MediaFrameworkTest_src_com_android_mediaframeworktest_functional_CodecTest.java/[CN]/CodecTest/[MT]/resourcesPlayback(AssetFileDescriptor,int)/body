{
  int duration=0;
  try {
    MediaPlayer mp=new MediaPlayer();
    mp.setDataSource(afd.getFileDescriptor(),afd.getStartOffset(),afd.getLength());
    mp.prepare();
    mp.start();
    duration=mp.getDuration();
    Thread.sleep(5000);
    mp.release();
  }
 catch (  Exception e) {
    Log.v(TAG,e.getMessage());
  }
  if (duration > expectedDuration)   return true;
 else   return false;
}
