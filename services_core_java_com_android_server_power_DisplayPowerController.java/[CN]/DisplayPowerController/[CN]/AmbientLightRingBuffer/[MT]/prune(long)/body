{
  int removalCount=0;
  while (removalCount < size() && getTime(removalCount) <= horizon) {
    removalCount++;
  }
  if (removalCount > 0) {
    removalCount--;
    mCount-=removalCount;
    mStart+=removalCount;
    if (mStart >= mCapacity) {
      mStart-=mCapacity;
    }
    if (getTime(0) < horizon) {
      mRingTime[mStart]=horizon;
    }
  }
}
