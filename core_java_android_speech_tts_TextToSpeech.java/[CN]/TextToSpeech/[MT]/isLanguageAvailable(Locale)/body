{
synchronized (mStartLock) {
    if (!mStarted) {
      return TTS_LANG_NOT_SUPPORTED;
    }
    try {
      return mITts.isLanguageAvailable(loc.getISO3Language(),loc.getISO3Country(),loc.getVariant());
    }
 catch (    RemoteException e) {
      mStarted=false;
      initTts();
    }
    return TTS_LANG_NOT_SUPPORTED;
  }
}
