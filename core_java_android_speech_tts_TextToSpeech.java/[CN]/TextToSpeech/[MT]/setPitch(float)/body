{
synchronized (mStartLock) {
    int result=ERROR;
    if (!mStarted) {
      return result;
    }
    try {
      if (pitch > 0) {
        int p=(int)(pitch * 100);
        mCachedParams[Engine.PARAM_POSITION_PITCH + 1]=String.valueOf(p);
        result=SUCCESS;
      }
    }
 catch (    NullPointerException e) {
      restart("setPitch",e);
    }
catch (    IllegalStateException e) {
      restart("setPitch",e);
    }
    return result;
  }
}
