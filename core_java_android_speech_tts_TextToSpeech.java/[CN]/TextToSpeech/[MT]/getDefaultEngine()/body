{
synchronized (mStartLock) {
    String engineName="";
    if (!mStarted) {
      return engineName;
    }
    try {
      engineName=mITts.getDefaultEngine();
    }
 catch (    RemoteException e) {
      restart("getDefaultEngine",e);
    }
catch (    NullPointerException e) {
      restart("getDefaultEngine",e);
    }
catch (    IllegalStateException e) {
      restart("getDefaultEngine",e);
    }
    return engineName;
  }
}
