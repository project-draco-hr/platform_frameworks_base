{
  return runAction(new Action<Integer>(){
    @Override public Integer run(    ITextToSpeechService service) throws RemoteException {
      return service.playSilence(getCallerIdentity(),durationInMs,queueMode,params == null ? null : params.get(Engine.KEY_PARAM_UTTERANCE_ID));
    }
  }
,ERROR,"playSilence");
}
