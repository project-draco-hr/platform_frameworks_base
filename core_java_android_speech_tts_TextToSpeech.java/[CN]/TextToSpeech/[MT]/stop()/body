{
synchronized (mStartLock) {
    int result=ERROR;
    if (!mStarted) {
      return result;
    }
    try {
      result=mITts.stop(mPackageName);
    }
 catch (    RemoteException e) {
      restart("stop",e);
    }
catch (    NullPointerException e) {
      restart("stop",e);
    }
catch (    IllegalStateException e) {
      restart("stop",e);
    }
    return result;
  }
}
