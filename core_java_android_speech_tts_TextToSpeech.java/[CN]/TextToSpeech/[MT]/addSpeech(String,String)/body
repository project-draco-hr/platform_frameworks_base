{
synchronized (mStartLock) {
    if (!mStarted) {
      return ERROR;
    }
    try {
      mITts.addSpeechFile(mPackageName,text,filename);
      return SUCCESS;
    }
 catch (    RemoteException e) {
      restart("addSpeech",e);
    }
catch (    NullPointerException e) {
      restart("addSpeech",e);
    }
catch (    IllegalStateException e) {
      restart("addSpeech",e);
    }
    return ERROR;
  }
}
