{
synchronized (mStartLock) {
    if (!mStarted) {
      return;
    }
    try {
      if (speechRate > 0) {
        mCachedRate=(int)(speechRate * 100);
        updateCachedParamArray();
        mITts.setSpeechRate(mCachedRate);
      }
    }
 catch (    RemoteException e) {
      mStarted=false;
      initTts();
    }
  }
}
