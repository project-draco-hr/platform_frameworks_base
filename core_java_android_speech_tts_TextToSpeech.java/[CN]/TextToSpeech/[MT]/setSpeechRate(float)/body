{
synchronized (mStartLock) {
    if (!mStarted) {
      return TTS_SUCCESS;
    }
    try {
      if (speechRate > 0) {
        mCachedRate=(int)(speechRate * 100);
        updateCachedParamArray();
        mITts.setSpeechRate(mCachedRate);
        return TTS_SUCCESS;
      }
    }
 catch (    RemoteException e) {
      mStarted=false;
      initTts();
    }
    return TTS_ERROR;
  }
}
