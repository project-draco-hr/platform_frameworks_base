{
synchronized (mStartLock) {
    int result=ERROR;
    if (!mStarted) {
      return result;
    }
    try {
      if (speechRate > 0) {
        int rate=(int)(speechRate * 100);
        mCachedParams[Engine.PARAM_POSITION_RATE + 1]=String.valueOf(rate);
        if (speechRate > 0.0f) {
          result=SUCCESS;
        }
 else {
          result=ERROR;
        }
      }
    }
 catch (    NullPointerException e) {
      restart("setSpeechRate",e);
    }
catch (    IllegalStateException e) {
      restart("setSpeechRate",e);
    }
    return result;
  }
}
