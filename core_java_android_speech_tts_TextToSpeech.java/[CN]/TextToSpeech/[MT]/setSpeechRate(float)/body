{
synchronized (mStartLock) {
    int result=TTS_ERROR;
    if (!mStarted) {
      return result;
    }
    try {
      if (speechRate > 0) {
        mCachedRate=(int)(speechRate * 100);
        updateCachedParamArray();
        result=mITts.setSpeechRate(mCachedRate);
      }
    }
 catch (    RemoteException e) {
      mStarted=false;
      initTts();
    }
 finally {
      return result;
    }
  }
}
