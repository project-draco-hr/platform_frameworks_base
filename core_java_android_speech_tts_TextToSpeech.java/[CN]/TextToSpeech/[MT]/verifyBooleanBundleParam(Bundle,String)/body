{
  if (bundle.containsKey(key)) {
    if (!(bundle.get(key) instanceof Boolean || bundle.get(key) instanceof String)) {
      bundle.remove(key);
      Log.w(TAG,"Synthesis request paramter " + key + " containst value "+ " with invalid type. Should be a Boolean or String");
      return false;
    }
  }
  return true;
}
