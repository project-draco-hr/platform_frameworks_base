{
synchronized (mStartLock) {
    int result=TTS_ERROR;
    Log.i("TTS received: ",text);
    if (!mStarted) {
      return result;
    }
    try {
      result=mITts.speak(text,queueMode,mCachedParams);
    }
 catch (    RemoteException e) {
      mStarted=false;
      initTts();
    }
catch (    NullPointerException e) {
      mStarted=false;
      initTts();
    }
catch (    IllegalStateException e) {
      mStarted=false;
      initTts();
    }
 finally {
      return result;
    }
  }
}
