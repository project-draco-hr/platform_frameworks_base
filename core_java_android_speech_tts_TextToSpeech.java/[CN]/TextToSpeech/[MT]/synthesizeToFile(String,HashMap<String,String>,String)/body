{
synchronized (mStartLock) {
    if (!mStarted) {
      return TTS_ERROR;
    }
    try {
      if (mITts.synthesizeToFile(text,null,filename)) {
        return TTS_SUCCESS;
      }
    }
 catch (    RemoteException e) {
      mStarted=false;
      initTts();
    }
catch (    NullPointerException e) {
      mStarted=false;
      initTts();
    }
catch (    IllegalStateException e) {
      mStarted=false;
      initTts();
    }
    return TTS_ERROR;
  }
}
