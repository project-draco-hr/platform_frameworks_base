{
synchronized (mStartLock) {
    int result=TTS_ERROR;
    if (!mStarted) {
      return result;
    }
    try {
      if (mITts.synthesizeToFile(mPackageName,text,null,filename)) {
        result=TTS_SUCCESS;
      }
    }
 catch (    RemoteException e) {
      mStarted=false;
      initTts();
    }
catch (    NullPointerException e) {
      mStarted=false;
      initTts();
    }
catch (    IllegalStateException e) {
      mStarted=false;
      initTts();
    }
 finally {
      return result;
    }
  }
}
