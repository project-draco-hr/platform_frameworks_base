{
synchronized (mStartLock) {
    if (!mStarted) {
      return ERROR;
    }
    try {
      mITts.addEarcon(mPackageName,earcon,packagename,resourceId);
      return SUCCESS;
    }
 catch (    RemoteException e) {
      restart("addEarcon",e);
    }
catch (    NullPointerException e) {
      restart("addEarcon",e);
    }
catch (    IllegalStateException e) {
      restart("addEarcon",e);
    }
    return ERROR;
  }
}
