{
synchronized (mStartLock) {
    if (isCancelled()) {
      return null;
    }
    try {
      mConnectedService.setCallback(getCallerIdentity(),mCallback);
      Log.i(TAG,"Setuped connection to " + mName);
      return SUCCESS;
    }
 catch (    RemoteException re) {
      Log.e(TAG,"Error connecting to service, setCallback() failed");
      return ERROR;
    }
  }
}
