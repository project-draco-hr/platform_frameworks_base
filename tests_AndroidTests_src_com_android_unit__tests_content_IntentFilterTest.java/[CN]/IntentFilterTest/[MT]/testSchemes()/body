{
  IntentFilter filter=new Match(null,null,null,new String[]{"scheme1"},null,null);
  checkMatches(filter,new MatchCondition[]{new MatchCondition(IntentFilter.NO_MATCH_DATA,null,null,null,null),new MatchCondition(IntentFilter.MATCH_CATEGORY_SCHEME,null,null,null,"scheme1:foo"),new MatchCondition(IntentFilter.NO_MATCH_DATA,null,null,null,"scheme2:foo")});
  filter=new Match(null,null,null,new String[]{"scheme1","scheme2"},null,null);
  checkMatches(filter,new MatchCondition[]{new MatchCondition(IntentFilter.NO_MATCH_DATA,null,null,null,null),new MatchCondition(IntentFilter.MATCH_CATEGORY_SCHEME,null,null,null,"scheme1:foo"),new MatchCondition(IntentFilter.MATCH_CATEGORY_SCHEME,null,null,null,"scheme2:foo"),new MatchCondition(IntentFilter.NO_MATCH_DATA,null,null,null,"scheme3:foo")});
}
