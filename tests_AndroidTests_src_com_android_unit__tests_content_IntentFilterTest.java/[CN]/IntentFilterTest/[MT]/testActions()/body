{
  IntentFilter filter=new Match(new String[]{"action1"},null,null,null,null,null);
  checkMatches(filter,new MatchCondition[]{new MatchCondition(IntentFilter.MATCH_CATEGORY_EMPTY,null,null,null,null),new MatchCondition(IntentFilter.MATCH_CATEGORY_EMPTY,"action1",null,null,null),new MatchCondition(IntentFilter.NO_MATCH_ACTION,"action2",null,null,null)});
  filter=new Match(new String[]{"action1","action2"},null,null,null,null,null);
  checkMatches(filter,new MatchCondition[]{new MatchCondition(IntentFilter.MATCH_CATEGORY_EMPTY,null,null,null,null),new MatchCondition(IntentFilter.MATCH_CATEGORY_EMPTY,"action1",null,null,null),new MatchCondition(IntentFilter.MATCH_CATEGORY_EMPTY,"action2",null,null,null),new MatchCondition(IntentFilter.NO_MATCH_ACTION,"action3",null,null,null)});
}
