{
  TouchPoint tp=mTouchPoints.get(0);
  if (tp == null) {
    return;
  }
  if (!tp.hasMoved()) {
    return;
  }
  MotionEvent event=MotionEvent.obtain(tp.downTime(),SystemClock.uptimeMillis(),MotionEvent.ACTION_MOVE,tp.getX(),tp.getY(),mTouchMetaState);
  mWebView.onTouchEvent(event);
  tp.setMoved(false);
}
