{
  if (DBG)   log("onVoiceCallStarted");
  if (isConnected() && !mPhone.getServiceStateTracker().isConcurrentVoiceAndDataAllowed()) {
    if (DBG)     log("onVoiceCallStarted stop polling");
    stopNetStatPoll();
    stopDataStallAlarm();
    notifyDataConnection(Phone.REASON_VOICE_CALL_STARTED);
  }
}
