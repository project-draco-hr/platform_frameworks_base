{
  int cid=-1;
  CellLocation loc=mPhone.getCellLocation();
  if (loc != null) {
    if (loc instanceof GsmCellLocation) {
      cid=((GsmCellLocation)loc).getCid();
    }
 else     if (loc instanceof CdmaCellLocation) {
      cid=((CdmaCellLocation)loc).getBaseStationId();
    }
  }
  return cid;
}
