{
  if (Build.VERSION.SDK_INT >= Build.VERSION_CODES.ICE_CREAM_SANDWICH) {
    assertEquals(mBindingObject.getPopupBackground(),((ColorDrawable)mView.getDropDownBackground()).getColor());
    changeValues();
    assertEquals(mBindingObject.getPopupBackground(),((ColorDrawable)mView.getDropDownBackground()).getColor());
  }
}
