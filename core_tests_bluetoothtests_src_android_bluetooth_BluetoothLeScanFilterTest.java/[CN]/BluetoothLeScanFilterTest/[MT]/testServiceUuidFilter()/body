{
  BluetoothLeScanFilter filter=mFilterBuilder.serviceUuid(ParcelUuid.fromString("0000110A-0000-1000-8000-00805F9B34FB")).build();
  assertTrue("uuid filter fails",filter.matches(mScanResult));
  filter=mFilterBuilder.serviceUuid(ParcelUuid.fromString("0000110C-0000-1000-8000-00805F9B34FB")).build();
  assertFalse("uuid filter fails",filter.matches(mScanResult));
  filter=mFilterBuilder.serviceUuid(ParcelUuid.fromString("0000110C-0000-1000-8000-00805F9B34FB")).serviceUuidMask(ParcelUuid.fromString("FFFFFFF0-FFFF-FFFF-FFFF-FFFFFFFFFFFF")).build();
  assertTrue("uuid filter fails",filter.matches(mScanResult));
}
