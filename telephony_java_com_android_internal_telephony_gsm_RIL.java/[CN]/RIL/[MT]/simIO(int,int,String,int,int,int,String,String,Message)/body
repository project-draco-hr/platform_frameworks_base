{
  RILRequest rr=RILRequest.obtain(RIL_REQUEST_SIM_IO,result);
  rr.mp.writeInt(command);
  rr.mp.writeInt(fileid);
  rr.mp.writeString(path);
  rr.mp.writeInt(p1);
  rr.mp.writeInt(p2);
  rr.mp.writeInt(p3);
  rr.mp.writeString(data);
  rr.mp.writeString(pin2);
  if (RILJ_LOGD)   riljLog(rr.serialString() + "> simIO: " + requestToString(rr.mRequest)+ " 0x"+ Integer.toHexString(command)+ " 0x"+ Integer.toHexString(fileid)+ " "+ p1+ ","+ p2+ ","+ p3);
  send(rr);
}
