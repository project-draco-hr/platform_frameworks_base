{
  String strings[]=(String[])responseStrings(p);
  ArrayList<NetworkInfo> ret;
  if (strings.length % 4 != 0) {
    throw new RuntimeException("RIL_REQUEST_QUERY_AVAILABLE_NETWORKS: invalid response. Got " + strings.length + " strings, expected multible of 4");
  }
  ret=new ArrayList<NetworkInfo>(strings.length / 4);
  for (int i=0; i < strings.length; i+=4) {
    ret.add(new NetworkInfo(strings[i + 0],strings[i + 1],strings[i + 2],strings[i + 3]));
  }
  return ret;
}
