{
  CommandException ex;
  ex=CommandException.fromRilErrno(error);
  if (RIL.RILJ_LOGD)   Log.d(LOG_TAG,serialString() + "< " + RIL.requestToString(mRequest)+ " error: "+ ex);
  if (mResult != null) {
    AsyncResult.forMessage(mResult,null,ex);
    mResult.sendToTarget();
  }
  if (mp != null) {
    mp.recycle();
    mp=null;
  }
}
