{
  final int N=mSize;
  if (N == 0) {
    return ~0;
  }
  int index=ContainerHelpers.binarySearch(mHashes,N,0);
  if (index < 0) {
    return index;
  }
  if (null == mArray[index]) {
    return index;
  }
  int end;
  for (end=index + 1; end < N && mHashes[end] == 0; end++) {
    if (null == mArray[end])     return end;
  }
  for (int i=index - 1; i >= 0 && mHashes[i] == 0; i--) {
    if (null == mArray[i])     return i;
  }
  return ~end;
}
