{
  super(context,attrs,defStyle);
  TypedArray a=context.obtainStyledAttributes(attrs,com.android.internal.R.styleable.CheckBoxPreference,defStyle,0);
  mSummaryOn=a.getString(com.android.internal.R.styleable.CheckBoxPreference_summaryOn);
  mSummaryOff=a.getString(com.android.internal.R.styleable.CheckBoxPreference_summaryOff);
  mDisableDependentsState=a.getBoolean(com.android.internal.R.styleable.CheckBoxPreference_disableDependentsState,false);
  a.recycle();
}
