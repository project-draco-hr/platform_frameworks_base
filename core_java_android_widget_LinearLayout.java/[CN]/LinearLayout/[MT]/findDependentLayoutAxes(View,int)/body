{
  if (!checkPartialLayoutParams(child,LayoutParams.class))   return axisFilter;
  final LayoutParams lp=(LayoutParams)child.getLayoutParams();
  if (child.didLayoutParamsChange()) {
    return axisFilter;
  }
  final int wrapAxisFilter=((lp.width == WRAP_CONTENT ? FLAG_LAYOUT_AXIS_HORIZONTAL : 0) | (lp.height == WRAP_CONTENT ? FLAG_LAYOUT_AXIS_VERTICAL : 0)) & axisFilter;
  if (wrapAxisFilter == axisFilter) {
    return wrapAxisFilter;
  }
  int matchAxisFilter=((lp.width == MATCH_PARENT ? FLAG_LAYOUT_AXIS_HORIZONTAL : 0) | (lp.height == MATCH_PARENT ? FLAG_LAYOUT_AXIS_VERTICAL : 0)) & axisFilter;
  if (lp.weight > 0) {
    if (mOrientation == HORIZONTAL) {
      matchAxisFilter|=FLAG_LAYOUT_AXIS_HORIZONTAL & axisFilter;
    }
 else {
      matchAxisFilter|=FLAG_LAYOUT_AXIS_VERTICAL & axisFilter;
    }
  }
  if (matchAxisFilter != 0) {
    final ViewParent parent=getParent();
    if (parent != null) {
      return getParent().findDependentLayoutAxes(this,matchAxisFilter) | wrapAxisFilter;
    }
    return matchAxisFilter | wrapAxisFilter;
  }
  return 0;
}
