{
  if (mEncoding != null && mEncoding.equalsIgnoreCase("B")) {
    String result=getBase64(propertyValue);
    if (mBuilder != null) {
      ArrayList<String> v=new ArrayList<String>();
      v.add(result);
      mBuilder.propertyValues(v);
    }
  }
  super.handlePropertyValue(propertyName,propertyValue);
}
