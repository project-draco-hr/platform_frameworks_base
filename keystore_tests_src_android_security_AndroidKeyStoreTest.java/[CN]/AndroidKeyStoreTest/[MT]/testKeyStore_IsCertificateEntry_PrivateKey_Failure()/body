{
  mKeyStore.load(null,null);
  assertTrue(mAndroidKeyStore.importKey(Credentials.USER_PRIVATE_KEY + TEST_ALIAS_1,FAKE_KEY_1));
  assertTrue(mAndroidKeyStore.put(Credentials.USER_CERTIFICATE + TEST_ALIAS_1,FAKE_USER_1));
  assertTrue(mAndroidKeyStore.put(Credentials.CA_CERTIFICATE + TEST_ALIAS_1,FAKE_CA_1));
  assertFalse("Should return false for PrivateKeyEntry",mKeyStore.isCertificateEntry(TEST_ALIAS_1));
}
