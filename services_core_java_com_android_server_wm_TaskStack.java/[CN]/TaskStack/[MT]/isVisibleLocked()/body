{
  final boolean keyguardOn=mService.mPolicy.isKeyguardShowingOrOccluded() && !mService.mAnimator.mKeyguardGoingAway;
  if (keyguardOn && !StackId.isAllowedOverLockscreen(mStackId)) {
    return false;
  }
  for (int i=mTasks.size() - 1; i >= 0; i--) {
    final Task task=mTasks.get(i);
    for (int j=task.mAppTokens.size() - 1; j >= 0; j--) {
      if (!task.mAppTokens.get(j).hidden) {
        return true;
      }
    }
  }
  return false;
}
