{
  assertTrue("failed to connect to " + config.SSID,connectToWifiWithConfiguration(config));
  assertTrue(waitForNetworkState(ConnectivityManager.TYPE_WIFI,State.CONNECTED,WIFI_CONNECTION_TIMEOUT));
  assertNotNull("Wifi connection returns null",mWifiManager.getConnectionInfo());
  if (DEBUG) {
    log("config.SSID = " + config.SSID);
    log("mWifiManager.getConnectionInfo.getSSID()" + mWifiManager.getConnectionInfo().getSSID());
  }
  assertTrue(config.SSID.contains(mWifiManager.getConnectionInfo().getSSID()));
}
