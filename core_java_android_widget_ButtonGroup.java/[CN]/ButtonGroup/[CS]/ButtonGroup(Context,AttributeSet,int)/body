{
  super(context,attrs,defStyleRes);
  TypedArray a=context.obtainStyledAttributes(attrs,com.android.internal.R.styleable.ButtonGroup);
  mDivider=a.getDrawable(com.android.internal.R.styleable.ButtonGroup_divider);
  mButtonBackground=a.getDrawable(com.android.internal.R.styleable.ButtonGroup_buttonBackground);
  a.recycle();
}
