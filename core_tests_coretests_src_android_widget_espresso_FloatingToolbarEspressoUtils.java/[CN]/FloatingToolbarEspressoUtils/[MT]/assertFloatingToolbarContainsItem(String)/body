{
  try {
    onFloatingToolBar().check(matches(hasDescendant(ViewMatchers.withText(itemLabel))));
  }
 catch (  AssertionError e) {
    try {
      toggleOverflow();
    }
 catch (    NoMatchingViewException|NoMatchingRootException e2) {
      throw e;
    }
    try {
      onFloatingToolBar().check(matches(hasDescendant(ViewMatchers.withText(itemLabel))));
    }
  finally {
      toggleOverflow();
    }
  }
}
