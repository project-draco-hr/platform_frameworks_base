{
  WifiInfo wifiInfo=getConnectionInfo();
  if (wifiInfo == null) {
    return null;
  }
  for (  PasspointConfig passpointConfig : mPasspointConfigs.values()) {
    if (passpointConfig.getWifiConfiguration().networkId == wifiInfo.getNetworkId()) {
      return passpointConfig;
    }
  }
  return null;
}
