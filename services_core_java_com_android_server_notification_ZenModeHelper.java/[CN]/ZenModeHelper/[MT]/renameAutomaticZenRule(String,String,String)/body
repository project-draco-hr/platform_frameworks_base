{
  if (mConfig == null)   return false;
  if (DEBUG) {
    Log.d(TAG,"renameAutomaticZenRule oldName=" + oldName + "  newName="+ newName+ " reason="+ reason);
  }
  final ZenModeConfig newConfig=mConfig.copy();
  String ruleId=findMatchingRuleId(newConfig,oldName);
  if (ruleId == null) {
    return false;
  }
 else {
    ZenRule rule=newConfig.automaticRules.get(ruleId);
    if (!canManageAutomaticZenRule(rule)) {
      throw new SecurityException("Cannot update rules not owned by your condition provider");
    }
    rule.name=newName;
    return setConfig(newConfig,reason,true);
  }
}
