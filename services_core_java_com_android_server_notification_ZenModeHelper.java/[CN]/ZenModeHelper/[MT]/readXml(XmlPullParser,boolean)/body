{
  final ZenModeConfig config=ZenModeConfig.readXml(parser,mConfigMigration);
  if (config != null) {
    if (forRestore) {
      if (config.user != UserHandle.USER_OWNER) {
        return;
      }
      config.manualRule=null;
    }
    if (DEBUG)     Log.d(TAG,"readXml");
    setConfig(config,"readXml");
  }
}
