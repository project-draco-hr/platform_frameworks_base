{
  if (denominator < 0) {
    numerator=-numerator;
    denominator=-denominator;
  }
  if (denominator == 0 && numerator > 0) {
    mNumerator=1;
    mDenominator=0;
  }
 else   if (denominator == 0 && numerator < 0) {
    mNumerator=-1;
    mDenominator=0;
  }
 else   if (denominator == 0 && numerator == 0) {
    mNumerator=0;
    mDenominator=0;
  }
 else   if (numerator == 0) {
    mNumerator=0;
    mDenominator=1;
  }
 else {
    int gcd=gcd(numerator,denominator);
    mNumerator=numerator / gcd;
    mDenominator=denominator / gcd;
  }
}
