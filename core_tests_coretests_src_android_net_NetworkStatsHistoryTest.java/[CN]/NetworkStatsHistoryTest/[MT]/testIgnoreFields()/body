{
  final NetworkStatsHistory history=new NetworkStatsHistory(MINUTE_IN_MILLIS,0,FIELD_RX_BYTES | FIELD_TX_BYTES);
  history.recordData(0,MINUTE_IN_MILLIS,new NetworkStats.Entry(1024L,10L,2048L,20L,4L));
  history.recordData(0,2 * MINUTE_IN_MILLIS,new NetworkStats.Entry(2L,2L,2L,2L,2L));
  assertFullValues(history,UNKNOWN,1026L,UNKNOWN,2050L,UNKNOWN,UNKNOWN);
}
