{
  if (lhs.right < rhs.left || lhs.left > rhs.right || lhs.bottom < rhs.top || lhs.top > rhs.bottom) {
    return false;
  }
  if (lhs.left < rhs.left) {
    lhs.right=rhs.left;
  }
  if (lhs.top < rhs.top) {
    lhs.bottom=rhs.top;
  }
  if (lhs.right > rhs.right) {
    lhs.left=rhs.right;
  }
  if (lhs.bottom > rhs.bottom) {
    lhs.top=rhs.bottom;
  }
  return true;
}
