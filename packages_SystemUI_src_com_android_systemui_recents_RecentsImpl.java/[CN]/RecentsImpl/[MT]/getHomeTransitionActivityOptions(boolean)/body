{
  if (fromSearchHome) {
    return ActivityOptions.makeCustomAnimation(mContext,R.anim.recents_from_search_launcher_enter,R.anim.recents_from_search_launcher_exit,mHandler,null);
  }
  return ActivityOptions.makeCustomAnimation(mContext,R.anim.recents_from_launcher_enter,R.anim.recents_from_launcher_exit,mHandler,null);
}
