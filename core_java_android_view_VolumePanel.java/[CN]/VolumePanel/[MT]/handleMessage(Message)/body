{
switch (msg.what) {
case MSG_VOLUME_CHANGED:
{
      onVolumeChanged(msg.arg1,msg.arg2);
      break;
    }
case MSG_FREE_RESOURCES:
{
    onFreeResources();
    break;
  }
case MSG_STOP_SOUNDS:
{
  onStopSounds();
  break;
}
case MSG_PLAY_SOUND:
{
onPlaySound(msg.arg1,msg.arg2);
break;
}
case MSG_VIBRATE:
{
onVibrate();
break;
}
}
}
