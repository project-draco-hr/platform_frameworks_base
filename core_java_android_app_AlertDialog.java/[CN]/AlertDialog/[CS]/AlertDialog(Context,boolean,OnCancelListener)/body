{
  super(context,context.getApplicationInfo().targetSdkVersion >= Build.VERSION_CODES.HONEYCOMB ? com.android.internal.R.style.Theme_Holo_Dialog_Alert : com.android.internal.R.style.Theme_Dialog_Alert);
  setCancelable(cancelable);
  setOnCancelListener(cancelListener);
  mAlert=new AlertController(context,this,getWindow());
}
