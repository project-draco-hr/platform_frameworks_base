{
  if (v == mRemoveButton) {
    mHost.remove(mQsPanel.getSpec(data));
  }
 else   if (v == mInfoButton) {
    mHost.unstashTiles();
    SystemUIDialog dialog=new SystemUIDialog(mContext);
    dialog.setTitle(mQsPanel.getSpec(data));
    dialog.setPositiveButton(R.string.ok,null);
    dialog.show();
  }
}
