{
  if (DBG)   Log.d(TAG,"beginReliableWrite() - device: " + device.getAddress());
  if (mService == null || mClientIf == 0)   return false;
  try {
    mService.beginReliableWrite(mClientIf,device.getAddress());
  }
 catch (  RemoteException e) {
    Log.e(TAG,"",e);
    return false;
  }
  return true;
}
