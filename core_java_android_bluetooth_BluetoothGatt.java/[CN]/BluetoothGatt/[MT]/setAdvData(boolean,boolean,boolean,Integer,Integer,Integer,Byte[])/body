{
  if (DBG)   Log.d(TAG,"setAdvData()");
  if (mService == null || mClientIf == 0)   return;
  byte[] data=new byte[0];
  if (manufacturerData != null) {
    data=new byte[manufacturerData.length];
    for (int i=0; i != manufacturerData.length; ++i) {
      data[i]=manufacturerData[i];
    }
  }
  try {
    mService.setAdvData(mClientIf,!advData,includeName,includeTxPower,minInterval != null ? minInterval : 0,maxInterval != null ? maxInterval : 0,appearance != null ? appearance : 0,data);
  }
 catch (  RemoteException e) {
    Log.e(TAG,"",e);
  }
}
