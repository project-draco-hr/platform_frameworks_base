{
  if (!mState.stackTouched) {
    super.onBackPressed();
    return;
  }
  final int size=mState.stack.size();
  if (size > 1) {
    mState.stack.pop();
    onCurrentDirectoryChanged();
  }
 else   if (size == 1 && !isRootsDrawerOpen()) {
    super.onBackPressed();
  }
 else {
    super.onBackPressed();
  }
}
