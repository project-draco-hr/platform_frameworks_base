{
  if (mSearchManager.cancelSearch()) {
    return;
  }
  if (!mState.stackTouched) {
    super.onBackPressed();
    return;
  }
  final int size=mState.stack.size();
  if (size > 1) {
    mState.stack.pop();
    onCurrentDirectoryChanged(ANIM_UP);
  }
 else   if (size == 1 && !mDrawer.isOpen()) {
    super.onBackPressed();
  }
 else {
    super.onBackPressed();
  }
}
