{
  final FragmentManager fm=getFragmentManager();
  if (doc.isDirectory()) {
    mStack.push(doc);
    updateDirectoryFragment();
  }
 else   if (mAction == ACTION_OPEN) {
    onFinished(doc.uri);
  }
 else   if (mAction == ACTION_CREATE) {
    SaveFragment.get(fm).setDisplayName(doc.displayName);
  }
}
