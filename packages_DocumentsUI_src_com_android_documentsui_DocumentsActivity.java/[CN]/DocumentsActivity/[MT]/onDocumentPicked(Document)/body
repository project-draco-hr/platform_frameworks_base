{
  final FragmentManager fm=getFragmentManager();
  if (doc.isDirectory()) {
    mStack.push(doc);
    onCurrentDirectoryChanged();
  }
 else   if (mAction == ACTION_OPEN || mAction == ACTION_GET_CONTENT) {
    onFinished(doc.uri);
  }
 else   if (mAction == ACTION_CREATE) {
    SaveFragment.get(fm).setReplaceTarget(doc);
  }
}
