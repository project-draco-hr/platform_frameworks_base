{
  this(args,(PackageItemInfo)outInfo);
  if (args.outError[0] != null) {
    return;
  }
  if (args.processRes != 0) {
    outInfo.processName=buildProcessName(owner.applicationInfo.packageName,owner.applicationInfo.processName,args.sa.getNonResourceString(args.processRes),args.flags,args.sepProcesses,args.outError);
  }
  outInfo.enabled=args.sa.getBoolean(args.enabledRes,true);
}
