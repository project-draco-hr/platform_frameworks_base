{
  if (!parsePackageItemInfo(owner,outInfo,outError,tag,sa,nameRes,labelRes,iconRes)) {
    return false;
  }
  if (processRes != 0) {
    outInfo.processName=buildProcessName(owner.applicationInfo.packageName,owner.applicationInfo.processName,sa.getNonResourceString(processRes),flags,mSeparateProcesses,outError);
  }
  outInfo.enabled=sa.getBoolean(enabledRes,true);
  return outError[0] == null;
}
