{
  try {
    final JarFile jarFile=new JarFile(mArchiveSourcePath);
    try {
      final ZipEntry je=jarFile.getEntry(ANDROID_MANIFEST_FILENAME);
      if (je != null) {
        pkg.manifestDigest=ManifestDigest.fromInputStream(jarFile.getInputStream(je));
      }
    }
  finally {
      jarFile.close();
    }
    return true;
  }
 catch (  IOException e) {
    return false;
  }
}
