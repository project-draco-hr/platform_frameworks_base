{
  StringBuffer sb=new StringBuffer();
  sb.append("CellNetworkScanResult: {");
  sb.append(" status:").append(mStatus);
  if (mOperators != null) {
    for (    OperatorInfo network : mOperators) {
      sb.append(" network:").append(network);
    }
  }
  sb.append("}");
  return sb.toString();
}
