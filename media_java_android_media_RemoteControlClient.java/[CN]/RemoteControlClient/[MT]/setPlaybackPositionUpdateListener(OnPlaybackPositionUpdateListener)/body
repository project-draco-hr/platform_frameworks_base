{
synchronized (mCacheLock) {
    if ((mPositionUpdateListener == null) && (l != null)) {
      mPlaybackPositionCapabilities|=MEDIA_POSITION_WRITABLE;
    }
 else     if ((mPositionUpdateListener != null) && (l == null)) {
      mPlaybackPositionCapabilities&=~MEDIA_POSITION_WRITABLE;
    }
    mPositionUpdateListener=l;
  }
}
