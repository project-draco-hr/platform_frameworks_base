{
synchronized (mCacheLock) {
    int oldCapa=mPlaybackPositionCapabilities;
    if (l != null) {
      mPlaybackPositionCapabilities|=MEDIA_POSITION_READABLE;
    }
 else {
      mPlaybackPositionCapabilities&=~MEDIA_POSITION_READABLE;
    }
    mPositionProvider=l;
    if (oldCapa != mPlaybackPositionCapabilities) {
      sendTransportControlInfo_syncCacheLock();
    }
  }
}
