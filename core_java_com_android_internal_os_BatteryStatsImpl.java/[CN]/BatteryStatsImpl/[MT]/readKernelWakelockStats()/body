{
  byte[] buffer=new byte[4096];
  int len;
  try {
    FileInputStream is=new FileInputStream("/proc/wakelocks");
    len=is.read(buffer);
    is.close();
    if (len > 0) {
      int i;
      for (i=0; i < len; i++) {
        if (buffer[i] == '\0') {
          len=i;
          break;
        }
      }
    }
  }
 catch (  java.io.FileNotFoundException e) {
    return null;
  }
catch (  java.io.IOException e) {
    return null;
  }
  return parseProcWakelocks(buffer,len);
}
