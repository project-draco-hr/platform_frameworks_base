{
  if (mCharging != charging) {
    mCharging=charging;
    if (charging) {
      mHistoryCur.states|=HistoryItem.STATE_CHARGING_FLAG;
    }
 else {
      mHistoryCur.states&=~HistoryItem.STATE_CHARGING_FLAG;
    }
    mHandler.sendEmptyMessage(MSG_REPORT_CHARGING);
    return true;
  }
  return false;
}
