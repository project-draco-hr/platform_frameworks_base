{
  if (DEBUG) {
    Log.i(TAG,"*** Screen timer:");
    mScreenOnTimer.logState();
    Log.i(TAG,"*** Phone timer:");
    mPhoneOnTimer.logState();
    Log.i(TAG,"*** Wifi timer:");
    mWifiOnTimer.logState();
    Log.i(TAG,"*** Bluetooth timer:");
    mBluetoothOnTimer.logState();
  }
  super.dumpLocked(pw);
}
