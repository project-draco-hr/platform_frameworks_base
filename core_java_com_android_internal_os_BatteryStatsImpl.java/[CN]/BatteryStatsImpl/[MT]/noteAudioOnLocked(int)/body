{
  if (!mAudioOn) {
    mHistoryCur.states|=BatteryHistoryRecord.STATE_AUDIO_ON_FLAG;
    if (DEBUG_HISTORY)     Slog.v(TAG,"Audio on to: " + Integer.toHexString(mHistoryCur.states));
    addHistoryRecord(SystemClock.elapsedRealtime());
    mAudioOn=true;
    mAudioOnTimer.startRunningLocked(this);
  }
  getUidStatsLocked(uid).noteAudioTurnedOnLocked();
}
