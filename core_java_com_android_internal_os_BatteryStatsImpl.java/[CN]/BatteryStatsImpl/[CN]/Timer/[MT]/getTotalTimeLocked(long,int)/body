{
  long val;
  if (which == STATS_LAST) {
    val=mLastTime;
  }
 else {
    val=computeRunTimeLocked(mTimeBase.getRealtime(elapsedRealtimeUs));
    if (which == STATS_SINCE_UNPLUGGED) {
      val-=mUnpluggedTime;
    }
 else     if (which != STATS_SINCE_CHARGED) {
      val-=mLoadedTime;
    }
  }
  return val;
}
