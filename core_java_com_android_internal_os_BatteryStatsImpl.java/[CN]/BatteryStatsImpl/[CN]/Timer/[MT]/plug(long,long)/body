{
  if (DEBUG && mType < 0) {
    Log.v(TAG,"plug #" + mType + ": realtime="+ batteryRealtime+ " old mTotalTime="+ mTotalTime);
  }
  mTotalTime=computeRunTimeLocked(batteryRealtime);
  mCount=computeCurrentCountLocked();
  if (DEBUG && mType < 0) {
    Log.v(TAG,"plug #" + mType + ": new mTotalTime="+ mTotalTime);
  }
}
