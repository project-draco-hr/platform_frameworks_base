{
  if (mExcessivePower == null) {
    out.writeInt(0);
    return;
  }
  final int N=mExcessivePower.size();
  out.writeInt(N);
  for (int i=0; i < N; i++) {
    ExcessivePower ew=mExcessivePower.get(i);
    out.writeInt(ew.type);
    out.writeLong(ew.overTime);
    out.writeLong(ew.usedTime);
  }
}
