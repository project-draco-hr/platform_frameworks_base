{
  final int N=in.readInt();
  if (N == 0) {
    mExcessiveWake=null;
    return;
  }
  mExcessiveWake=new ArrayList<ExcessiveWake>();
  for (int i=0; i < N; i++) {
    ExcessiveWake ew=new ExcessiveWake();
    ew.overTime=in.readLong();
    ew.usedTime=in.readLong();
    mExcessiveWake.add(ew);
  }
}
