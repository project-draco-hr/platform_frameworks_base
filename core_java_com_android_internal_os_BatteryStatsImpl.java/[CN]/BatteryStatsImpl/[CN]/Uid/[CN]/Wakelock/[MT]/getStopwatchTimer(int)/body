{
  StopwatchTimer t;
switch (type) {
case WAKE_TYPE_PARTIAL:
    t=mTimerPartial;
  if (t == null) {
    t=new StopwatchTimer(Uid.this,WAKE_TYPE_PARTIAL,mPartialTimers,mOnBatteryScreenOffTimeBase);
    mTimerPartial=t;
  }
return t;
case WAKE_TYPE_FULL:
t=mTimerFull;
if (t == null) {
t=new StopwatchTimer(Uid.this,WAKE_TYPE_FULL,mFullTimers,mOnBatteryTimeBase);
mTimerFull=t;
}
return t;
case WAKE_TYPE_WINDOW:
t=mTimerWindow;
if (t == null) {
t=new StopwatchTimer(Uid.this,WAKE_TYPE_WINDOW,mWindowTimers,mOnBatteryTimeBase);
mTimerWindow=t;
}
return t;
default :
throw new IllegalArgumentException("type=" + type);
}
}
