{
  if (!mFullWifiLockOut) {
    mFullWifiLockOut=true;
    if (mFullWifiLockTimer == null) {
      mFullWifiLockTimer=new StopwatchTimer(mBsi.mClocks,Uid.this,FULL_WIFI_LOCK,mBsi.mFullWifiLockTimers,mBsi.mOnBatteryTimeBase);
    }
    mFullWifiLockTimer.startRunningLocked(elapsedRealtimeMs);
  }
}
