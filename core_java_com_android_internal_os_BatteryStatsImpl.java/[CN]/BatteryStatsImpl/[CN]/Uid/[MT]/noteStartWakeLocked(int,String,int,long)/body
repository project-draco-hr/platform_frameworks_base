{
  Wakelock wl=mWakelockStats.startObject(name);
  if (wl != null) {
    wl.getStopwatchTimer(type).startRunningLocked(elapsedRealtimeMs);
  }
  if (pid >= 0 && type == WAKE_TYPE_PARTIAL) {
    Pid p=getPidStatsLocked(pid);
    if (p.mWakeNesting++ == 0) {
      p.mWakeStartMs=elapsedRealtimeMs;
    }
  }
}
