{
  if (i < 0 || i >= NUM_WIFI_BATCHED_SCAN_BINS)   return;
  ArrayList<StopwatchTimer> collected=mWifiBatchedScanTimers.get(i);
  if (collected == null) {
    collected=new ArrayList<StopwatchTimer>();
    mWifiBatchedScanTimers.put(i,collected);
  }
  if (in == null) {
    mWifiBatchedScanTimer[i]=new StopwatchTimer(this,WIFI_BATCHED_SCAN,collected,mOnBatteryTimeBase);
  }
 else {
    mWifiBatchedScanTimer[i]=new StopwatchTimer(this,WIFI_BATCHED_SCAN,collected,mOnBatteryTimeBase,in);
  }
}
