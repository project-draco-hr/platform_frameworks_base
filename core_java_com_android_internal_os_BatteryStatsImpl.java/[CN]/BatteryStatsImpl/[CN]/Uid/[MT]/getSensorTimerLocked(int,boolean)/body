{
  Integer sId=Integer.valueOf(sensor);
  Sensor se=mSensorStats.get(sId);
  if (se == null) {
    if (!create) {
      return null;
    }
    se=new Sensor();
    mSensorStats.put(sId,se);
  }
  Timer t=se.sensorTime;
  if (t == null) {
    t=new Timer(0,mSensorTimers);
    se.sensorTime=t;
  }
  return t;
}
