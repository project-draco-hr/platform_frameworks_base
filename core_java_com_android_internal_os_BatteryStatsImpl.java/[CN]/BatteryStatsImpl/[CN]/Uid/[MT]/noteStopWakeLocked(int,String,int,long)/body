{
  StopwatchTimer t=getWakeTimerLocked(name,type);
  if (t != null) {
    t.stopRunningLocked(BatteryStatsImpl.this,elapsedRealtime);
  }
  if (pid >= 0 && type == WAKE_TYPE_PARTIAL) {
    Pid p=mPids.get(pid);
    if (p != null && p.mWakeStart != 0) {
      p.mWakeSum+=elapsedRealtime - p.mWakeStart;
      p.mWakeStart=0;
    }
  }
}
