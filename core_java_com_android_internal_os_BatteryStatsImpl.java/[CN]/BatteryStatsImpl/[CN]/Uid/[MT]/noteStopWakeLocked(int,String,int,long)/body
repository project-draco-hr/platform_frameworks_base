{
  StopwatchTimer t=getWakeTimerLocked(name,type);
  if (t != null) {
    t.stopRunningLocked(elapsedRealtimeMs);
  }
  if (pid >= 0 && type == WAKE_TYPE_PARTIAL) {
    Pid p=mPids.get(pid);
    if (p != null && p.mWakeNesting > 0) {
      if (p.mWakeNesting-- == 1) {
        p.mWakeSumMs+=elapsedRealtimeMs - p.mWakeStartMs;
        p.mWakeStartMs=0;
      }
    }
  }
}
