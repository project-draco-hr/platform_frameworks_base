{
  if (which == STATS_LAST) {
    return mLoadedTcpBytesReceived;
  }
 else {
    long current=computeCurrentTcpBytesReceived();
    if (which == STATS_UNPLUGGED) {
      current-=mTcpBytesReceivedAtLastUnplug;
    }
 else     if (which == STATS_TOTAL) {
      current+=mLoadedTcpBytesReceived;
    }
    return current;
  }
}
