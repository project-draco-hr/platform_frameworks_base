  mAudioTurnedOnTimer=new StopwatchTimer(Uid.this,AUDIO_TURNED_ON,null,mUnpluggables);
  mVideoTurnedOnTimer=new StopwatchTimer(Uid.this,VIDEO_TURNED_ON,null,mUnpluggables);
}
