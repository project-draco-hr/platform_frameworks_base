{
  mUid=uid;
  mWifiRunningTimer=new StopwatchTimer(Uid.this,WIFI_RUNNING,mWifiRunningTimers,mUnpluggables);
  mFullWifiLockTimer=new StopwatchTimer(Uid.this,FULL_WIFI_LOCK,mFullWifiLockTimers,mUnpluggables);
  mWifiScanTimer=new StopwatchTimer(Uid.this,WIFI_SCAN,mWifiScanTimers,mUnpluggables);
  mWifiBatchedScanTimer=new StopwatchTimer[NUM_WIFI_BATCHED_SCAN_BINS];
  mWifiMulticastTimer=new StopwatchTimer(Uid.this,WIFI_MULTICAST_ENABLED,mWifiMulticastTimers,mUnpluggables);
}
