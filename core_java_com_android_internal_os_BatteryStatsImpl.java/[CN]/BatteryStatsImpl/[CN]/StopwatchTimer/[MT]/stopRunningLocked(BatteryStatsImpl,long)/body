{
  if (mNesting == 0) {
    return;
  }
  if (--mNesting == 0) {
    final long batteryRealtime=stats.getBatteryRealtimeLocked(elapsedRealtime * 1000);
    if (mTimerPool != null) {
      refreshTimersLocked(stats,batteryRealtime,mTimerPool,null);
      mTimerPool.remove(this);
    }
 else {
      mNesting=1;
      mTotalTime=computeRunTimeLocked(batteryRealtime);
      mNesting=0;
    }
    if (DEBUG && mType < 0) {
      Log.v(TAG,"stop #" + mType + ": mUpdateTime="+ mUpdateTime+ " mTotalTime="+ mTotalTime+ " mCount="+ mCount+ " mAcquireTime="+ mAcquireTime);
    }
    if (mTotalTime == mAcquireTime) {
      mCount--;
    }
  }
}
