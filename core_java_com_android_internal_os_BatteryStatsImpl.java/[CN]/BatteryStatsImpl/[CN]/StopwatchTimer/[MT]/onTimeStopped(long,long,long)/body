{
  if (mNesting > 0) {
    if (DEBUG && mType < 0) {
      Log.v(TAG,"old mUpdateTime=" + mUpdateTime);
    }
    super.onTimeStopped(elapsedRealtime,baseUptime,baseRealtime);
    mUpdateTime=baseRealtime;
    if (DEBUG && mType < 0) {
      Log.v(TAG,"new mUpdateTime=" + mUpdateTime);
    }
  }
}
