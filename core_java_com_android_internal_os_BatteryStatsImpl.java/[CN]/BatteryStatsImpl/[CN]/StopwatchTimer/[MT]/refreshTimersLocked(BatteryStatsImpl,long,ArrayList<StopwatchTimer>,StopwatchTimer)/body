{
  long selfTime=0;
  final int N=pool.size();
  for (int i=N - 1; i >= 0; i--) {
    final StopwatchTimer t=pool.get(i);
    long heldTime=batteryRealtime - t.mUpdateTime;
    if (heldTime > 0) {
      final long myTime=heldTime / N;
      if (t == self) {
        selfTime=myTime;
      }
      t.mTotalTime+=myTime;
    }
    t.mUpdateTime=batteryRealtime;
  }
  return selfTime;
}
