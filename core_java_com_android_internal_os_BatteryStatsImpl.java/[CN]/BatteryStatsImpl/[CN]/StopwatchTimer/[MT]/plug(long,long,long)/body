{
  if (mNesting > 0) {
    if (DEBUG && mType < 0) {
      Log.v(TAG,"old mUpdateTime=" + mUpdateTime);
    }
    super.plug(elapsedRealtime,batteryUptime,batteryRealtime);
    mUpdateTime=batteryRealtime;
    if (DEBUG && mType < 0) {
      Log.v(TAG,"new mUpdateTime=" + mUpdateTime);
    }
  }
}
