{
  if (LOCAL_LOGV) {
    Log.v(TAG,"Purge cache in message box: " + msgBoxId);
  }
  if (msgBoxId != null) {
    HashSet<Uri> msgBox=mMessageBoxes.remove(msgBoxId);
    if (msgBox != null) {
      for (      Uri key : msgBox) {
        mUpdating.remove(key);
        PduCacheEntry entry=super.purge(key);
        if (entry != null) {
          removeFromThreads(key,entry);
        }
      }
    }
  }
}
