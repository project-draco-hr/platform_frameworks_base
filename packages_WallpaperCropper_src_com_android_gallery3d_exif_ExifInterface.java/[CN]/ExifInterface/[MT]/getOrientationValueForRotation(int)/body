{
  degrees%=360;
  if (degrees < 0) {
    degrees+=360;
  }
  if (degrees < 90) {
    return Orientation.TOP_LEFT;
  }
 else   if (degrees < 180) {
    return Orientation.RIGHT_TOP;
  }
 else   if (degrees < 270) {
    return Orientation.BOTTOM_LEFT;
  }
 else {
    return Orientation.RIGHT_BOTTOM;
  }
}
