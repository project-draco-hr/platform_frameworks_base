{
  File keyFile=getKeyFile(keyName);
  if (!keyFile.exists()) {
    return null;
  }
  try {
    FileInputStream fis=new FileInputStream(keyFile);
    int keyLen=fis.available();
    byte[] retKey=new byte[keyLen];
    fis.read(retKey);
    fis.close();
    return retKey;
  }
 catch (  IOException ioe) {
  }
  throw new IllegalArgumentException();
}
