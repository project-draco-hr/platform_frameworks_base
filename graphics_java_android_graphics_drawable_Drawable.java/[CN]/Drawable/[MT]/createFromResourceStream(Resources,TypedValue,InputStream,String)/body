{
  if (is == null) {
    return null;
  }
  Rect pad=new Rect();
  Bitmap bm=BitmapFactory.decodeStream(res,value,is,pad,null);
  if (bm != null) {
    byte[] np=bm.getNinePatchChunk();
    if (np == null || !NinePatch.isNinePatchChunk(np)) {
      np=null;
      pad=null;
    }
    return drawableFromBitmap(res,bm,np,pad,srcName);
  }
  return null;
}
