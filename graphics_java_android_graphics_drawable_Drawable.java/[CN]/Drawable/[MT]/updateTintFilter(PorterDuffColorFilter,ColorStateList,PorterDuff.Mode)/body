{
  if (tint == null || tintMode == null) {
    return null;
  }
  final int color=tint.getColorForState(getState(),Color.TRANSPARENT);
  if (tintFilter == null) {
    return new PorterDuffColorFilter(color,tintMode);
  }
  tintFilter.setColor(color);
  tintFilter.setMode(tintMode);
  return tintFilter;
}
