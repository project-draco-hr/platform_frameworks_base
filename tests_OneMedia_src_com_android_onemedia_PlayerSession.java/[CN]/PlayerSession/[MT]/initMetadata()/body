{
  mMetadataBuilder=new MediaMetadata.Builder();
  mMetadataBuilder.putString(MediaMetadata.METADATA_KEY_DISPLAY_TITLE,"OneMedia display title");
  mMetadataBuilder.putString(MediaMetadata.METADATA_KEY_DISPLAY_SUBTITLE,"OneMedia display subtitle");
  mQueue.add(new QueueItem(mMetadataBuilder.build().getDescription(),11));
}
