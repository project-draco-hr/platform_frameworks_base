{
  String classString="import android.binding.BindingAppInfo;\n\n" + "@BindingAppInfo(buildId=\"" + mBuildId + "\", applicationPackage=\""+ mResourceBundle.getAppPackage()+ "\")\n"+ "public class "+ APPLICATION_INFO_CLASS+ " {}\n";
  mFileWriter.writeToFile(RESOURCE_BUNDLE_PACKAGE + APPLICATION_INFO_CLASS,classString);
}
