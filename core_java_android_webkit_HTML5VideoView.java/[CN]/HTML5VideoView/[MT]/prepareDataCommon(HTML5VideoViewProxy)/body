{
  if (!mSkipPrepare) {
    try {
      mPlayer.reset();
      mPlayer.setDataSource(proxy.getContext(),mUri,mHeaders);
      mPlayer.prepareAsync();
    }
 catch (    IllegalArgumentException e) {
      e.printStackTrace();
    }
catch (    IllegalStateException e) {
      e.printStackTrace();
    }
catch (    IOException e) {
      e.printStackTrace();
    }
    mCurrentState=STATE_PREPARING;
  }
 else {
    if (mCurrentState >= STATE_PREPARED) {
      onPrepared(mPlayer);
    }
    mSkipPrepare=false;
  }
}
