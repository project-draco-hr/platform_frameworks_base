{
  StringBuilder sb=new StringBuilder(128);
  sb.append("UidRecord{");
  sb.append(Integer.toHexString(System.identityHashCode(this)));
  sb.append(' ');
  UserHandle.formatUid(sb,uid);
  sb.append(' ');
  sb.append(ProcessList.makeProcStateString(curProcState));
  sb.append(" / ");
  sb.append(numProcs);
  sb.append(" procs}");
  return sb.toString();
}
