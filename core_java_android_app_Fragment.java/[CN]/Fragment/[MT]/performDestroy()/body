{
  if (mChildFragmentManager != null) {
    mChildFragmentManager.dispatchDestroy();
  }
  mState=INITIALIZING;
  mCalled=false;
  onDestroy();
  if (!mCalled) {
    throw new SuperNotCalledException("Fragment " + this + " did not call through to super.onDestroy()");
  }
  mChildFragmentManager=null;
}
