{
  mCalled=true;
  if (!mLoadersStarted) {
    mLoadersStarted=true;
    if (!mCheckedForLoaderManager) {
      mCheckedForLoaderManager=true;
      mLoaderManager=mActivity.getLoaderManager(mWho,mLoadersStarted,false);
    }
    if (mLoaderManager != null) {
      mLoaderManager.doStart();
    }
  }
}
