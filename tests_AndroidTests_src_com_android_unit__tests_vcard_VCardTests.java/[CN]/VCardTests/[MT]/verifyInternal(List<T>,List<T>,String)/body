{
  if (verifySize((expected == null ? 0 : expected.size()),(actual == null ? 0 : actual.size()),name) > 0) {
    int size=expected.size();
    for (int i=0; i < size; i++) {
      final T expectedObj=expected.get(i);
      final T actualObj=actual.get(i);
      if (!expected.equals(actual)) {
        fail(String.format("The %i %s are different: %s != %s",i,name,expectedObj,actualObj));
      }
    }
  }
}
