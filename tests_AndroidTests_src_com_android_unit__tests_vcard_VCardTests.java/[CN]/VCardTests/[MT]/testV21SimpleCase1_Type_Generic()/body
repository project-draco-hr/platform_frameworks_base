{
  ContactStructVerifier verifier=new ContactStructVerifier(R.raw.v21_simple_1,VCardConfig.VCARD_TYPE_V21_GENERIC);
  ContentValues contentValues=verifier.createExpected(StructuredName.CONTENT_ITEM_TYPE);
  contentValues.put(StructuredName.FAMILY_NAME,"Ando");
  contentValues.put(StructuredName.GIVEN_NAME,"Roid");
  contentValues.put(StructuredName.DISPLAY_NAME,"Roid Ando");
  verifier.verify();
}
