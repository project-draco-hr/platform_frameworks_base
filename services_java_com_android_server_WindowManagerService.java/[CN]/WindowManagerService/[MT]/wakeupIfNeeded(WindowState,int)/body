{
  long curTime=SystemClock.uptimeMillis();
  if (eventType == LONG_TOUCH_EVENT || eventType == CHEEK_EVENT) {
    if (mLastTouchEventType == eventType && (curTime - mLastUserActivityCallTime) < MIN_TIME_BETWEEN_USERACTIVITIES) {
      return;
    }
    mLastUserActivityCallTime=curTime;
    mLastTouchEventType=eventType;
  }
  if (targetWin == null || targetWin.mAttrs.type != WindowManager.LayoutParams.TYPE_KEYGUARD) {
    mPowerManager.userActivity(curTime,false,eventType,false);
  }
}
