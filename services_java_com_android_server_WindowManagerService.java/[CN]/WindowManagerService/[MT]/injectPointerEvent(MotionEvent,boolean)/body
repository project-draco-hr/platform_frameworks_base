{
  final int pid=Binder.getCallingPid();
  final int uid=Binder.getCallingUid();
  final long ident=Binder.clearCallingIdentity();
  final int result;
  if (ENABLE_NATIVE_INPUT_DISPATCH) {
    result=mInputManager.injectMotionEvent(ev,InputQueue.INPUT_EVENT_NATURE_TOUCH,pid,uid,sync,INJECTION_TIMEOUT_MILLIS);
  }
 else {
    result=dispatchPointer(null,ev,pid,uid);
    if (sync) {
      mKeyWaiter.waitForNextEventTarget(null,null,null,false,true,pid,uid);
    }
  }
  Binder.restoreCallingIdentity(ident);
  return reportInjectionResult(result);
}
