{
  int N=tokens.size();
  for (int i=0; i < N; i++) {
    IBinder token=tokens.get(i);
    final AppWindowToken wtoken=findAppWindowToken(token);
    if (!mAppTokens.remove(wtoken)) {
      Log.w(TAG,"Attempting to reorder token that doesn't exist: " + token + " ("+ wtoken+ ")");
      i--;
      N--;
    }
  }
}
