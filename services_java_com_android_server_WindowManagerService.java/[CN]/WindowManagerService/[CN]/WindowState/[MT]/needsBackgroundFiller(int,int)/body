{
  return (mAttrs.flags & FLAG_COMPATIBLE_WINDOW) != 0 && mHasDrawn && mViewVisibility == View.VISIBLE && mAttrs.width == mAttrs.FILL_PARENT && mAttrs.height == mAttrs.FILL_PARENT && mFrame.left == mCompatibleScreenFrame.left && mAttrs.type != mAttrs.TYPE_APPLICATION_STARTING && ((mFrame.right - mFrame.right) < screenWidth || (mFrame.bottom - mFrame.top) < screenHeight);
}
