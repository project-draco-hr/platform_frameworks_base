{
  return (mAttrs.flags & FLAG_COMPATIBLE_WINDOW) != 0 && mHasDrawn && mViewVisibility == View.VISIBLE && mFrame.left <= mCompatibleScreenFrame.left && mFrame.top <= mCompatibleScreenFrame.top && mFrame.right >= mCompatibleScreenFrame.right && mFrame.bottom >= mCompatibleScreenFrame.bottom && mAttrs.type != mAttrs.TYPE_APPLICATION_STARTING;
}
