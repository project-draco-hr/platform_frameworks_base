{
synchronized (this) {
    if (token.paused) {
      if (DEBUG_INPUT)       Slog.v(TAG,"Resuming WindowToken " + token + ", last="+ mLastBinder+ " (token="+ (mLastWin != null ? mLastWin.mToken : null)+ "), finished="+ mFinished+ ", paused="+ token.paused);
      token.paused=false;
      if (mLastWin != null && mLastWin.mToken == token && mFinished) {
        doFinishedKeyLocked(false);
      }
 else {
        notifyAll();
      }
    }
  }
}
