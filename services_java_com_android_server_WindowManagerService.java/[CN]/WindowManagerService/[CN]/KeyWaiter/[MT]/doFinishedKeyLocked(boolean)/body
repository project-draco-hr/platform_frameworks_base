{
  if (mLastWin != null) {
    releasePendingPointerLocked(mLastWin.mSession);
    releasePendingTrackballLocked(mLastWin.mSession);
  }
  if (force || mLastWin == null || !mLastWin.mToken.paused || !mLastWin.isVisibleLw()) {
    mLastWin=null;
    mLastBinder=null;
  }
  mFinished=true;
  notifyAll();
}
