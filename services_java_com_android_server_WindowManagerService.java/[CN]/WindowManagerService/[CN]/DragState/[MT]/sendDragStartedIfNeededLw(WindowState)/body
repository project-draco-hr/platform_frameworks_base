{
  if (mDragInProgress) {
    for (    WindowState ws : mNotifiedWindows) {
      if (ws == newWin) {
        return;
      }
    }
    if (DEBUG_DRAG) {
      Slog.d(TAG,"sending DRAG_STARTED to new window " + newWin);
    }
    DragEvent event=DragEvent.obtain(DragEvent.ACTION_DRAG_STARTED,0,0,mDataDescription,null);
    sendDragStartedLw(newWin,event);
    event.recycle();
  }
}
