{
switch (ev.classType) {
case RawInputEvent.CLASS_KEYBOARD:
    KeyEvent ke=(KeyEvent)ev.event;
  if (mPolicy.isMovementKeyTi(ke.getKeyCode())) {
    Log.w(TAG,"Dropping movement key during app switch: " + ke.getKeyCode() + ", action="+ ke.getAction());
    return FILTER_REMOVE;
  }
return FILTER_ABORT;
default :
return FILTER_KEEP;
}
}
