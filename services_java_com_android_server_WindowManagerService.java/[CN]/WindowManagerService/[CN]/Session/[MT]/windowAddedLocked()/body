{
  if (mSurfaceSession == null) {
    if (localLOGV)     Slog.v(TAG,"First window added to " + this + ", creating SurfaceSession");
    mSurfaceSession=new SurfaceSession();
    if (SHOW_TRANSACTIONS)     Slog.i(TAG,"  NEW SURFACE SESSION " + mSurfaceSession);
    mSessions.add(this);
  }
  mNumWindow++;
}
