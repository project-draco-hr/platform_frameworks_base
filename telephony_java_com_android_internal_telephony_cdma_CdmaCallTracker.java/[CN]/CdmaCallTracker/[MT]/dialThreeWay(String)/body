{
  if (!foregroundCall.isIdle()) {
    disableDataCallInEmergencyCall(dialString);
    pendingMO=new CdmaConnection(phone.getContext(),checkForTestEmergencyNumber(dialString),this,foregroundCall);
    cm.sendCDMAFeatureCode(pendingMO.address,obtainMessage(EVENT_THREE_WAY_DIAL_L2_RESULT_CDMA));
    return pendingMO;
  }
  return null;
}
