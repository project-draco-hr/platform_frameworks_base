{
  super.draw(c);
  if (DEBUG) {
    final Paint debugPaint=new Paint();
    debugPaint.setColor(0x40FF00CC);
    c.drawRect(mDragHandleEdgeSlop,mChallengeView.getTop() - getDragHandleSizeAbove(),getWidth() - mDragHandleEdgeSlop,mChallengeView.getTop() + getDragHandleSizeBelow(),debugPaint);
  }
}
