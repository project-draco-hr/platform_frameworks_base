{
  if (mLogger == null) {
    mLogger=new ILogger(){
      @Override public void error(      Throwable t,      String msgFormat,      Object... args){
        if (t != null) {
          t.printStackTrace();
        }
        failWithMsg(msgFormat,args);
      }
      @Override public void warning(      String msgFormat,      Object... args){
        failWithMsg(msgFormat,args);
      }
      @Override public void info(      String msgFormat,      Object... args){
      }
      @Override public void verbose(      String msgFormat,      Object... args){
      }
    }
;
  }
  return mLogger;
}
