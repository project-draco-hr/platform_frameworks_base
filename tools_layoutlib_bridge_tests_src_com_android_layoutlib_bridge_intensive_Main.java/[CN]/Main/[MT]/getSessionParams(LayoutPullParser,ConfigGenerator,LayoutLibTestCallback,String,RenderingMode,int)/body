{
  FolderConfiguration config=configGenerator.getFolderConfig();
  ResourceResolver resourceResolver=ResourceResolver.create(mProjectResources.getConfiguredResources(config),mFrameworkRepo.getConfiguredResources(config),themeName,false);
  return new SessionParams(layoutParser,renderingMode,null,configGenerator.getHardwareConfig(),resourceResolver,layoutLibCallback,0,targetSdk,getLayoutLog());
}
