{
  if (mText != null) {
    out.writeInt(1);
    TextUtils.writeToParcel(mText,out,flags);
  }
 else {
    out.writeInt(0);
  }
  out.writeLong(mTimestamp);
  if (mSender != null) {
    out.writeInt(1);
    TextUtils.writeToParcel(mSender,out,flags);
  }
 else {
    out.writeInt(0);
  }
  if (mDataMimeType != null) {
    out.writeInt(1);
    out.writeString(mDataMimeType);
  }
 else {
    out.writeInt(0);
  }
  if (mDataUri != null) {
    out.writeInt(1);
    out.writeParcelable(mDataUri,flags);
  }
 else {
    out.writeInt(0);
  }
}
