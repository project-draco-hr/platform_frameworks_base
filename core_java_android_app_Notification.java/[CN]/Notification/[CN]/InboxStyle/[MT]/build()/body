{
  checkBuilder();
  Notification wip=mBuilder.buildUnstyled();
  wip.bigContentView=makeBigContentView();
  StringBuilder builder=new StringBuilder();
  for (  CharSequence str : mTexts) {
    builder.append(str);
    builder.append("\n");
  }
  wip.extras.putCharSequence(EXTRA_TEXT,builder);
  return wip;
}
