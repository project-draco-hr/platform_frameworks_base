{
  PackageManager packageManager=mContext.getPackageManager();
  ApplicationInfo info=null;
  try {
    info=packageManager.getApplicationInfo(mContext.getApplicationInfo().packageName,0);
  }
 catch (  final NameNotFoundException e) {
    return;
  }
  CharSequence appName=info != null ? packageManager.getApplicationLabel(info) : null;
  if (TextUtils.isEmpty(appName)) {
    return;
  }
  contentView.setTextViewText(R.id.app_name_text,appName);
}
