{
  if (mTickerView != null) {
    return mTickerView;
  }
 else {
    if (mContentView == null) {
      return applyStandardTemplate(mLargeIcon == null ? R.layout.status_bar_latest_event_ticker : R.layout.status_bar_latest_event_ticker_large_icon,true);
    }
 else {
      return null;
    }
  }
}
