{
  Class<? extends Style>[] classes=new Class[]{BigTextStyle.class,BigPictureStyle.class,InboxStyle.class,MediaStyle.class,DecoratedCustomViewStyle.class,DecoratedMediaCustomViewStyle.class};
  for (  Class<? extends Style> innerClass : classes) {
    if (templateClass.equals(innerClass.getName())) {
      return innerClass;
    }
  }
  return null;
}
