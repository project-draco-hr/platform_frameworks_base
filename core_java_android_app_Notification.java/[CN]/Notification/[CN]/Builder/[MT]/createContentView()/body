{
  if (mN.contentView != null && (mStyle == null || !mStyle.displayCustomViewInline())) {
    return mN.contentView;
  }
 else   if (mStyle != null) {
    final RemoteViews styleView=mStyle.makeContentView();
    if (styleView != null) {
      return styleView;
    }
  }
  return applyStandardTemplate(getBaseLayoutResource());
}
