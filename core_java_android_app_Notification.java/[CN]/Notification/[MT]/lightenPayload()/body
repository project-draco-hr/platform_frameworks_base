{
  tickerView=null;
  contentView=null;
  bigContentView=null;
  headsUpContentView=null;
  mLargeIcon=null;
  if (extras != null) {
    extras.remove(Notification.EXTRA_LARGE_ICON);
    extras.remove(Notification.EXTRA_LARGE_ICON_BIG);
    extras.remove(Notification.EXTRA_PICTURE);
    extras.remove(Notification.EXTRA_BIG_TEXT);
  }
}
