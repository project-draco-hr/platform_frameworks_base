{
  reset(mServiceContext);
  assertTrue(mLauncherApps.startShortcut(getShortcutInfoAsLauncher(packageName,shortcutId,userId),null,null));
  final ArgumentCaptor<Intent> intentCaptor=ArgumentCaptor.forClass(Intent.class);
  verify(mServiceContext).startActivityAsUser(intentCaptor.capture(),any(Bundle.class),eq(UserHandle.of(userId)));
  return intentCaptor.getValue();
}
