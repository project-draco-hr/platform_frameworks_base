{
  final ShortcutInfo si1=makeShortcut("shortcut1");
  final ShortcutInfo si2=makeShortcut("shortcut2");
  final ShortcutInfo si3=makeShortcut("shortcut3");
  assertTrue(mManager.setDynamicShortcuts(list(si1,si2,si3)));
  assertShortcutIds(assertAllNotKeyFieldsOnly(mManager.getDynamicShortcuts()),"shortcut1","shortcut2","shortcut3");
  assertEquals(2,mManager.getRemainingCallCount());
  mManager.deleteDynamicShortcut("shortcut1");
  assertShortcutIds(assertAllNotKeyFieldsOnly(mManager.getDynamicShortcuts()),"shortcut2","shortcut3");
  mManager.deleteDynamicShortcut("shortcut1");
  assertShortcutIds(assertAllNotKeyFieldsOnly(mManager.getDynamicShortcuts()),"shortcut2","shortcut3");
  mManager.deleteDynamicShortcut("shortcutXXX");
  assertShortcutIds(assertAllNotKeyFieldsOnly(mManager.getDynamicShortcuts()),"shortcut2","shortcut3");
  mManager.deleteDynamicShortcut("shortcut2");
  assertShortcutIds(assertAllNotKeyFieldsOnly(mManager.getDynamicShortcuts()),"shortcut3");
  mManager.deleteDynamicShortcut("shortcut3");
  assertShortcutIds(assertAllNotKeyFieldsOnly(mManager.getDynamicShortcuts()));
  assertEquals(2,mManager.getRemainingCallCount());
}
