{
switch (msgBox) {
case MESSAGE_BOX_ALL:
    return "all";
case MESSAGE_BOX_INBOX:
  return "inbox";
case MESSAGE_BOX_SENT:
return "sent";
case MESSAGE_BOX_DRAFTS:
return "drafts";
case MESSAGE_BOX_OUTBOX:
return "outbox";
default :
throw new IllegalArgumentException("Invalid message box: " + msgBox);
}
}
