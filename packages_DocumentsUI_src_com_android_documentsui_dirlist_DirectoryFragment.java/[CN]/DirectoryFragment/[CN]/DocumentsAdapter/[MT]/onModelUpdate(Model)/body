{
  mModelIds=Lists.newArrayList(model.getModelIds());
  mDividerPosition=0;
  for (int i=0; i < mModelIds.size(); ++i) {
    final String mimeType=getCursorString(model.getItem(mModelIds.get(i)),Document.COLUMN_MIME_TYPE);
    if (!Document.MIME_TYPE_DIR.equals(mimeType)) {
      mDividerPosition=i;
      break;
    }
  }
  mModelIds.add(mDividerPosition,null);
}
