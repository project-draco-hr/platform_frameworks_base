{
  if (event.getAction() != KeyEvent.ACTION_DOWN) {
    return false;
  }
  if (mFocusManager.handleKey(doc,keyCode,event)) {
    return true;
  }
  if (keyCode == KeyEvent.KEYCODE_ENTER) {
    if (event.isShiftPressed()) {
      return onSelect(doc);
    }
 else {
      return onActivate(doc);
    }
  }
  return false;
}
