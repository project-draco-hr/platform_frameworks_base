{
  assert(clipData != null);
  new AsyncTask<Void,Void,List<DocumentInfo>>(){
    @Override protected List<DocumentInfo> doInBackground(    Void... params){
      return mClipper.getDocumentsFromClipData(clipData);
    }
    @Override protected void onPostExecute(    List<DocumentInfo> docs){
      copyDocuments(docs,destination);
    }
  }
.execute();
}
