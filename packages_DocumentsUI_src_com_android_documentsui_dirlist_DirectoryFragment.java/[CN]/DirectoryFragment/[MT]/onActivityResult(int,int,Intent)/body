{
  if (requestCode != REQUEST_COPY_DESTINATION) {
    return;
  }
  if (resultCode == Activity.RESULT_CANCELED || data == null) {
    return;
  }
  int operationType=data.getIntExtra(FileOperationService.EXTRA_OPERATION,FileOperationService.OPERATION_COPY);
  FileOperations.start(getActivity(),getDisplayState().selectedDocumentsForCopy,(DocumentStack)data.getParcelableExtra(Shared.EXTRA_STACK),operationType);
}
