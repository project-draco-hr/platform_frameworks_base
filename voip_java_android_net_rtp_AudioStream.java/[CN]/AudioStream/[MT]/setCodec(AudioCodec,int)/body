{
  if (isBusy()) {
    throw new IllegalStateException("Busy");
  }
  if (type < 0 || type > 127 || (type != codec.defaultType && type < 96)) {
    throw new IllegalArgumentException("Invalid type");
  }
  if (type == mDtmfType) {
    throw new IllegalArgumentException("The type is used by DTMF");
  }
  mCodec=codec;
  mCodecType=type;
}
