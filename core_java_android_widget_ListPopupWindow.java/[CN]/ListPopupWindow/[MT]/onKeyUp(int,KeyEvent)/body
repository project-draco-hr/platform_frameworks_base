{
  if (isShowing() && mDropDownList.getSelectedItemPosition() >= 0) {
    boolean consumed=mDropDownList.onKeyUp(keyCode,event);
    if (consumed) {
switch (keyCode) {
case KeyEvent.KEYCODE_ENTER:
case KeyEvent.KEYCODE_DPAD_CENTER:
        dismiss();
      break;
  }
}
return consumed;
}
return false;
}
