{
  mDrawsInPressedState=false;
  setPressed(false);
  updateSelectorState();
  final View motionView=getChildAt(mMotionPosition - mFirstPosition);
  if (motionView != null) {
    motionView.setPressed(false);
  }
  if (mClickAnimation != null) {
    mClickAnimation.cancel();
    mClickAnimation=null;
  }
}
