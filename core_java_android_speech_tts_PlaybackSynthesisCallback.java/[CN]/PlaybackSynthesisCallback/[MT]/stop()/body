{
  if (DBG)   Log.d(TAG,"stop()");
  mLogger.onStopped();
  SynthesisMessageParams token=null;
synchronized (mStateLock) {
    if (mStopped) {
      Log.w(TAG,"stop() called twice");
      return;
    }
    if (mToken == null) {
      mLogger.onWriteData();
    }
 else {
      token=mToken;
    }
    mStopped=true;
  }
  if (token != null) {
    token.clearBuffers();
  }
}
