{
  if (DBG)   Log.d(TAG,"done()");
synchronized (mStateLock) {
    if (mDone) {
      Log.w(TAG,"Duplicate call to done()");
      return TextToSpeech.ERROR;
    }
    mDone=true;
    if (mToken == null) {
      return TextToSpeech.ERROR;
    }
    mAudioTrackHandler.enqueueSynthesisDone(mToken);
  }
  return TextToSpeech.SUCCESS;
}
