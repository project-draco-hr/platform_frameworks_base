{
  Arrays.sort(rates);
  ArrayList<Range<Integer>> ranges=new ArrayList<Range<Integer>>();
  for (  int rate : rates) {
    if (supports(rate,null)) {
      ranges.add(Range.create(rate,rate));
    }
  }
  mSampleRateRanges=ranges.toArray(new Range[ranges.size()]);
  createDiscreteSampleRates();
}
