{
synchronized (mToken) {
    if (!mRefCounted || --mCount == 0) {
      mHandler.removeCallbacks(mReleaser);
      if (mHeld) {
        try {
          mService.releaseWakeLock(mToken,flags);
        }
 catch (        RemoteException e) {
        }
        mHeld=false;
      }
    }
    if (mCount < 0) {
      throw new RuntimeException("WakeLock under-locked " + mTag);
    }
  }
}
