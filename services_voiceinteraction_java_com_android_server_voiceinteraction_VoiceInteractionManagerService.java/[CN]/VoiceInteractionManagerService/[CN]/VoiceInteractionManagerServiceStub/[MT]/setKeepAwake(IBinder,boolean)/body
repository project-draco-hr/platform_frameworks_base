{
synchronized (this) {
    if (mImpl == null) {
      Slog.w(TAG,"setKeepAwake without running voice interaction service");
      return;
    }
    final long caller=Binder.clearCallingIdentity();
    try {
      mImpl.setKeepAwakeLocked(token,keepAwake);
    }
  finally {
      Binder.restoreCallingIdentity(caller);
    }
  }
}
