{
synchronized (this) {
    if (mImpl == null) {
      Slog.w(TAG,"hideSessionFromSession without running voice interaction service");
      return false;
    }
    final long caller=Binder.clearCallingIdentity();
    try {
      return mImpl.hideSessionLocked();
    }
  finally {
      Binder.restoreCallingIdentity(caller);
    }
  }
}
