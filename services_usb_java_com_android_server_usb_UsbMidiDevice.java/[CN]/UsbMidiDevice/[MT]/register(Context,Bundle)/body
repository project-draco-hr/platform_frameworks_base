{
  MidiManager midiManager=(MidiManager)context.getSystemService(Context.MIDI_SERVICE);
  if (midiManager == null) {
    Log.e(TAG,"No MidiManager in UsbMidiDevice.create()");
    return false;
  }
  mServer=midiManager.createDeviceServer(mInputPortReceivers,mSubdeviceCount,null,null,properties,MidiDeviceInfo.TYPE_USB,mCallback);
  if (mServer == null) {
    return false;
  }
  return true;
}
