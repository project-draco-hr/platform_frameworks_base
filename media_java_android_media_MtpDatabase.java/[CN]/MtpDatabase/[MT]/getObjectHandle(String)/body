{
  Log.d(TAG,"getObjectHandle " + path);
  Cursor c=null;
  try {
    c=mMediaProvider.query(mObjectsUri,ID_PROJECTION,PATH_WHERE,new String[]{path},null);
    if (c != null && c.moveToNext()) {
      return c.getInt(0);
    }
  }
 catch (  RemoteException e) {
    Log.e(TAG,"RemoteException in getObjectHandle",e);
  }
 finally {
    if (c != null) {
      c.close();
    }
  }
  return 0;
}
