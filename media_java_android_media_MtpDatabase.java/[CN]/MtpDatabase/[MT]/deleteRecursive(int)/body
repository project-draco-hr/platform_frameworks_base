{
  int[] children=getObjectList(0,0,handle);
  Uri uri=Files.getMtpObjectsUri(mVolumeName,handle);
  int count=mMediaProvider.delete(uri,null,null);
  if (count == 1) {
    if (children != null) {
      for (int i=0; i < children.length; i++) {
        count+=deleteRecursive(children[i]);
      }
    }
  }
  return count;
}
