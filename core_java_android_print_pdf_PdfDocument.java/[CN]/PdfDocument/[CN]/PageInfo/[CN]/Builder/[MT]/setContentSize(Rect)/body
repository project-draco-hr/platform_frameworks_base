{
  Rect pageSize=mPageInfo.mPageSize;
  if (contentSize != null && (pageSize.left > contentSize.left || pageSize.top > contentSize.top || pageSize.right < contentSize.right || pageSize.bottom < contentSize.bottom)) {
    throw new IllegalArgumentException("contentSize does not fit the pageSize!");
  }
  mPageInfo.mContentSize=contentSize;
  return this;
}
