{
  throwIfClosed();
  if (pageInfo == null) {
    throw new IllegalArgumentException("page cannot be null!");
  }
  if (mCurrentPage != null) {
    throw new IllegalStateException("Previous page not finished!");
  }
  Canvas canvas=new PdfCanvas(nativeCreatePage(pageInfo.mPageSize,pageInfo.mContentSize,pageInfo.mInitialTransform.native_instance));
  mCurrentPage=new Page(canvas,pageInfo);
  return mCurrentPage;
}
