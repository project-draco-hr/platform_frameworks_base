{
  if (DEBUG) {
    int nameResId=newSubtype.getNameResId();
    int modeResId=newSubtype.getModeResId();
    String output="changeInputMethodSubtype:" + (nameResId == 0 ? "<none>" : getString(nameResId)) + ","+ (modeResId == 0 ? "<none>" : getString(modeResId))+ ","+ newSubtype.getLocale()+ ","+ newSubtype.getExtraValue();
    Log.v(TAG,"--- " + output);
  }
}
