{
  ArrayList<ArrayList> results=new ArrayList<ArrayList>();
  List<AdnRecord> adnRecords=null;
  if (DBG)   log("loadFromEf: efType=" + efType);
  try {
    ISimPhoneBook simIpb=ISimPhoneBook.Stub.asInterface(ServiceManager.getService("simphonebook"));
    if (simIpb != null) {
      adnRecords=simIpb.getAdnRecordsInEf(efType);
    }
  }
 catch (  RemoteException ex) {
  }
catch (  SecurityException ex) {
    if (DBG)     log(ex.toString());
  }
  if (adnRecords != null) {
    int N=adnRecords.size();
    if (DBG)     log("adnRecords.size=" + N);
    for (int i=0; i < N; i++) {
      loadRecord(adnRecords.get(i),results);
    }
  }
 else {
    Log.w(TAG,"Cannot load ADN records");
    results.clear();
  }
  if (DBG)   log("loadFromEf: return results");
  return results;
}
