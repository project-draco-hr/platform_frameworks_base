{
  int[] buf=new int[b - ycount + 1];
  int d=a * a + 2 * b * b - 2 * a * a* b;
  int x=0;
  int y=b;
  while (y >= ycount) {
    if (d < 0) {
      d=d + b * b * (4 * x + 6);
    }
 else {
      buf[b - y]=x;
      d=d + b * b * (4 * x + 6) + 4 * a * a* (1 - y);
      y--;
    }
    x++;
  }
  return buf;
}
