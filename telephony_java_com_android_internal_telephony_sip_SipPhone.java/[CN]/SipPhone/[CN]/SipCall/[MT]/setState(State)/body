{
  if (state != newState) {
    Log.v(LOG_TAG,"++******++ call state changed: " + state + " --> "+ newState+ ": "+ this+ ": on phone "+ getPhone()+ " "+ connections.size());
    if (newState == Call.State.ALERTING) {
      state=newState;
      SipPhone.this.startRingbackTone();
    }
 else     if (state == Call.State.ALERTING) {
      SipPhone.this.stopRingbackTone();
    }
    state=newState;
    updatePhoneState();
    notifyPreciseCallStateChanged();
  }
}
