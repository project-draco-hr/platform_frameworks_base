{
synchronized (mMutex) {
    mLocationTracking=enable;
    if (!enable) {
      mMinTime=-1;
      mMinTimeSource.clear();
    }
    ILocationProvider provider=mServiceConnection.getProvider();
    if (provider != null) {
      try {
        provider.enableLocationTracking(enable);
      }
 catch (      RemoteException e) {
      }
    }
  }
}
