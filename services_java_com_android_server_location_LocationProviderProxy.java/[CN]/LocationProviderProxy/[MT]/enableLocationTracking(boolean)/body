{
  mLocationTracking=enable;
  if (!enable) {
    mMinTime=-1;
    mMinTimeSource.clear();
  }
  ILocationProvider provider;
synchronized (mServiceConnection) {
    provider=mProvider;
  }
  if (provider != null) {
    try {
      provider.enableLocationTracking(enable);
    }
 catch (    RemoteException e) {
    }
  }
}
