{
  ILocationProvider service=getService();
  if (service == null)   return 0;
  try {
    return service.getStatusUpdateTime();
  }
 catch (  RemoteException e) {
    Log.w(TAG,e);
  }
catch (  Exception e) {
    Log.e(TAG,"Exception from " + mServiceWatcher.getBestPackageName(),e);
  }
  return 0;
}
