{
  if (mPasswordOwner >= 0) {
    boolean haveOwner=false;
    for (int i=mAdminList.size() - 1; i >= 0; i--) {
      if (mAdminList.get(i).getUid() == mPasswordOwner) {
        haveOwner=true;
        break;
      }
    }
    if (!haveOwner) {
      Slog.w(TAG,"Previous password owner " + mPasswordOwner + " no longer active; disabling");
      mPasswordOwner=-1;
    }
  }
}
