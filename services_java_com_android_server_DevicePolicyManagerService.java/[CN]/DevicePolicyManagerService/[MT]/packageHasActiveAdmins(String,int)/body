{
  enforceCrossUserPermission(userHandle);
synchronized (this) {
    DevicePolicyData policy=getUserData(userHandle);
    final int N=policy.mAdminList.size();
    for (int i=0; i < N; i++) {
      if (policy.mAdminList.get(i).info.getPackageName().equals(packageName)) {
        return true;
      }
    }
    return false;
  }
}
