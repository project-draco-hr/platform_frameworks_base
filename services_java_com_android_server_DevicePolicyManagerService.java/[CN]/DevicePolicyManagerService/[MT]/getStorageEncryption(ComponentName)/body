{
synchronized (this) {
    if (who != null) {
      ActiveAdmin ap=getActiveAdminUncheckedLocked(who);
      return ap != null ? ap.encryptionRequested : false;
    }
    final int N=mAdminList.size();
    for (int i=0; i < N; i++) {
      if (mAdminList.get(i).encryptionRequested) {
        return true;
      }
    }
    return false;
  }
}
