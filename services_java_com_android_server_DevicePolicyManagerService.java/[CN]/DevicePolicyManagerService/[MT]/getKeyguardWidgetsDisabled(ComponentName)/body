{
synchronized (this) {
    if (who != null) {
      ActiveAdmin admin=getActiveAdminUncheckedLocked(who);
      return (admin != null) ? admin.disableKeyguardWidgets : 0;
    }
    final int N=mAdminList.size();
    int which=0;
    for (int i=0; i < N; i++) {
      ActiveAdmin admin=mAdminList.get(i);
      which|=admin.disableKeyguardWidgets;
    }
    return which;
  }
}
