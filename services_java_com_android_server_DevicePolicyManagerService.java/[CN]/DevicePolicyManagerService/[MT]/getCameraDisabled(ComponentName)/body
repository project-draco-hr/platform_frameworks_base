{
synchronized (this) {
    if (who != null) {
      ActiveAdmin admin=getActiveAdminUncheckedLocked(who);
      return (admin != null) ? admin.disableCamera : false;
    }
    final int N=mAdminList.size();
    for (int i=0; i < N; i++) {
      ActiveAdmin admin=mAdminList.get(i);
      if (admin.disableCamera) {
        return true;
      }
    }
    return false;
  }
}
