{
synchronized (this) {
    final int N=mAdminList.size();
    long time=0;
    for (int i=0; i < N; i++) {
      ActiveAdmin admin=mAdminList.get(i);
      if (time == 0) {
        time=admin.maximumTimeToUnlock;
      }
 else       if (admin.maximumTimeToUnlock != 0 && time > admin.maximumTimeToUnlock) {
        time=admin.maximumTimeToUnlock;
      }
    }
    return time;
  }
}
