{
synchronized (this) {
    final int N=mAdminList.size();
    int count=0;
    for (int i=0; i < N; i++) {
      ActiveAdmin admin=mAdminList.get(i);
      if (count == 0) {
        count=admin.maximumFailedPasswordsForWipe;
      }
 else       if (admin.maximumFailedPasswordsForWipe != 0 && count > admin.maximumFailedPasswordsForWipe) {
        count=admin.maximumFailedPasswordsForWipe;
      }
    }
    return count;
  }
}
