{
synchronized (this) {
    final int N=mAdminList.size();
    int mode=DevicePolicyManager.PASSWORD_MODE_UNSPECIFIED;
    for (int i=0; i < N; i++) {
      ActiveAdmin admin=mAdminList.get(i);
      if (mode < admin.passwordMode) {
        mode=admin.passwordMode;
      }
    }
    return mode;
  }
}
