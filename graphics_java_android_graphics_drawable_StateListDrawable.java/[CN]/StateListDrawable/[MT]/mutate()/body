{
  if (!mMutated && super.mutate() == this) {
    final int[][] sets=mStateListState.mStateSets;
    final int count=sets.length;
    mStateListState.mStateSets=new int[count][];
    for (int i=0; i < count; i++) {
      mStateListState.mStateSets[i]=sets[i].clone();
    }
    mMutated=true;
  }
  return this;
}
