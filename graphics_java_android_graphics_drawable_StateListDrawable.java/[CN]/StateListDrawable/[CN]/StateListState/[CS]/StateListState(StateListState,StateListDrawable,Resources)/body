{
  super(orig,owner,res);
  if (orig != null) {
    final int[][] sets=orig.mStateSets;
    final int count=sets.length;
    mStateSets=new int[count][];
    for (int i=0; i < count; i++) {
      final int[] set=sets[i];
      if (set != null) {
        mStateSets[i]=set.clone();
      }
    }
  }
 else {
    mStateSets=new int[getCapacity()][];
  }
}
