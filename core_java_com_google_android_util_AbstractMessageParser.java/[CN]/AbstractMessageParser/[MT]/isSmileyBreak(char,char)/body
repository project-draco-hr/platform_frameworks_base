{
switch (c1) {
case '$':
case '&':
case '*':
case '+':
case '-':
case '/':
case '<':
case '=':
case '>':
case '@':
case '[':
case '\\':
case ']':
case '^':
case '|':
case '}':
case '~':
switch (c2) {
case '#':
case '$':
case '%':
case '*':
case '/':
case '<':
case '=':
case '>':
case '@':
case '[':
case '\\':
case '^':
case '~':
      return true;
  }
}
return false;
}
