{
  int index=start;
  TrieNode bestMatch=null;
  while (index < p.getRawText().length()) {
    root=root.getChild(p.getRawText().charAt(index++));
    if (root == null) {
      break;
    }
 else     if (root.exists()) {
      if (p.isWordBreak(index)) {
        bestMatch=root;
      }
 else       if (smiley && p.isSmileyBreak(index)) {
        bestMatch=root;
      }
    }
  }
  return bestMatch;
}
