{
synchronized (mLock) {
    Iterator<GeofenceState> iter=mFences.iterator();
    while (iter.hasNext()) {
      GeofenceState state=iter.next();
      if (state.mIntent.equals(intent)) {
        if (fence == null) {
          iter.remove();
        }
 else {
          if (fence.equals(state.mFence)) {
            iter.remove();
          }
        }
      }
    }
    updateProviderRequirementsLocked();
  }
}
