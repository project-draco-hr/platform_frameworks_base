{
synchronized (mProfiles) {
    for (    UserInfo ui : mProfiles) {
      if (enableWorkMode) {
        if (!mUserManager.trySetQuietModeDisabled(ui.id,null)) {
          StatusBarManager statusBarManager=(StatusBarManager)mContext.getSystemService(android.app.Service.STATUS_BAR_SERVICE);
          statusBarManager.collapsePanels();
        }
      }
 else {
        mUserManager.setQuietModeEnabled(ui.id,true);
      }
    }
  }
}
