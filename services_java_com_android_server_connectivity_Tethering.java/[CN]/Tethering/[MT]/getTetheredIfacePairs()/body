{
  final ArrayList<String> list=Lists.newArrayList();
synchronized (mIfaces) {
    for (    TetherInterfaceSM sm : mIfaces.values()) {
      if (sm.isTethered()) {
        list.add(sm.mMyUpstreamIfaceName);
        list.add(sm.mIfaceName);
      }
    }
  }
  return list.toArray(new String[list.size()]);
}
