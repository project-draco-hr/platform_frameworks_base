{
  mTetherableUsbRegexs=mContext.getResources().getStringArray(com.android.internal.R.array.config_tether_usb_regexs);
  mTetherableWifiRegexs=mContext.getResources().getStringArray(com.android.internal.R.array.config_tether_wifi_regexs);
  mTetherableBluetoothRegexs=mContext.getResources().getStringArray(com.android.internal.R.array.config_tether_bluetooth_regexs);
  int ifaceTypes[]=mContext.getResources().getIntArray(com.android.internal.R.array.config_tether_upstream_types);
  mUpstreamIfaceTypes=new ArrayList();
  for (  int i : ifaceTypes) {
    mUpstreamIfaceTypes.add(new Integer(i));
  }
  checkDunRequired();
}
