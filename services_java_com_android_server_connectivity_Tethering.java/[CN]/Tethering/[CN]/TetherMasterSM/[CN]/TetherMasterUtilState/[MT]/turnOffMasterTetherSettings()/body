{
  try {
    mNMService.stopTethering();
  }
 catch (  Exception e) {
    transitionTo(mStopTetheringErrorState);
    return false;
  }
  try {
    mNMService.setIpForwardingEnabled(false);
  }
 catch (  Exception e) {
    transitionTo(mSetIpForwardingDisabledErrorState);
    return false;
  }
  transitionTo(mInitialState);
  return true;
}
