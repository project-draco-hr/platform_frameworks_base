{
  if (mMobileReserved) {
    IBinder b=ServiceManager.getService(Context.CONNECTIVITY_SERVICE);
    IConnectivityManager cm=IConnectivityManager.Stub.asInterface(b);
    try {
      cm.stopUsingNetworkFeature(ConnectivityManager.TYPE_MOBILE,(mDunRequired ? Phone.FEATURE_ENABLE_DUN_ALWAYS : Phone.FEATURE_ENABLE_HIPRI));
    }
 catch (    Exception e) {
      return false;
    }
    mMobileReserved=false;
  }
  return true;
}
