{
  IBinder b=ServiceManager.getService(Context.NETWORKMANAGEMENT_SERVICE);
  INetworkManagementService service=INetworkManagementService.Stub.asInterface(b);
  try {
    service.setIpForwardingEnabled(true);
  }
 catch (  Exception e) {
    sendMessageAtFrontOfQueue(CMD_TRANSITION_TO_STATE,mSetIpForwardingEnabledErrorState);
    return false;
  }
  try {
    service.startTethering(mDhcpRange[0],mDhcpRange[1]);
  }
 catch (  Exception e) {
    sendMessageAtFrontOfQueue(CMD_TRANSITION_TO_STATE,mStartTetheringErrorState);
    return false;
  }
  try {
    service.setDnsForwarders(mDnsServers);
  }
 catch (  Exception e) {
    sendMessageAtFrontOfQueue(CMD_TRANSITION_TO_STATE,mSetDnsForwardersErrorState);
    return false;
  }
  return true;
}
