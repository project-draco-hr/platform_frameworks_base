{
  IndentingPrintWriter ipw=new IndentingPrintWriter(pw,"  ");
synchronized (mSearchables) {
    for (int i=0; i < mSearchables.size(); i++) {
      ipw.print("\nUser: ");
      ipw.println(mSearchables.keyAt(i));
      ipw.increaseIndent();
      mSearchables.valueAt(i).dump(fd,ipw,args);
      ipw.decreaseIndent();
    }
  }
}
