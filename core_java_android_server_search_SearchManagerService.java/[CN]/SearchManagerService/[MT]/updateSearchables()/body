{
  mSearchables.buildSearchableList();
  mSearchablesDirty=false;
  ComponentName defaultSearch=new ComponentName("com.android.globalsearch","com.android.globalsearch.GlobalSearch");
  try {
    mContext.getPackageManager().getActivityInfo(defaultSearch,0);
  }
 catch (  NameNotFoundException e) {
    defaultSearch=new ComponentName("com.android.googlesearch","com.android.googlesearch.GoogleSearch");
  }
  mSearchables.setDefaultSearchable(defaultSearch);
}
