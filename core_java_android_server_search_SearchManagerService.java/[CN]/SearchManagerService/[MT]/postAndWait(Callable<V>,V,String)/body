{
  FutureTask<V> task=new FutureTask<V>(callable);
  mHandler.post(task);
  try {
    return task.get();
  }
 catch (  InterruptedException ex) {
    Log.e(TAG,"Error calling " + name + ": "+ ex);
    return errorResult;
  }
catch (  ExecutionException ex) {
    Log.e(TAG,"Error calling " + name + ": "+ ex);
    return errorResult;
  }
}
