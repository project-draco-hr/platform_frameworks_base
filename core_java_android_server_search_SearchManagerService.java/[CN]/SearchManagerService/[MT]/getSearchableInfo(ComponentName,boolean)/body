{
  if (mSearchables == null)   return null;
  if (globalSearch) {
    return mSearchables.getDefaultSearchable();
  }
 else {
    if (launchActivity == null) {
      Log.e(TAG,"getSearchableInfo(), activity == null");
      return null;
    }
    return mSearchables.getSearchableInfo(launchActivity);
  }
}
