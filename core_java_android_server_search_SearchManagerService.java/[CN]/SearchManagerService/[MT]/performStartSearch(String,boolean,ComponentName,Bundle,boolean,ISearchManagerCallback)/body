{
  if (DBG)   debug("performStartSearch()");
  if (mDisabledOnBoot) {
    Log.d(TAG,"ignoring start search request because " + DISABLE_SEARCH_PROPERTY + " system property is set.");
    return;
  }
  mSearchDialog.show(initialQuery,selectInitialQuery,launchActivity,appSearchData,globalSearch);
  if (searchManagerCallback != null) {
    mCallback=searchManagerCallback;
  }
}
