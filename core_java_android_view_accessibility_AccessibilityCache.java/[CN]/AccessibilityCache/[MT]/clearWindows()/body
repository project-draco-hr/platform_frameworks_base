{
synchronized (mLock) {
    final int windowCount=mWindowCache.size();
    for (int i=windowCount - 1; i >= 0; i--) {
      AccessibilityWindowInfo window=mWindowCache.valueAt(i);
      if (window != null) {
        if (DEBUG) {
          Log.i(LOG_TAG,"Removing window: " + window.getId());
        }
        window.recycle();
        mWindowCache.removeAt(i);
      }
    }
  }
}
