{
synchronized (mLock) {
    if (DEBUG) {
      Log.i(LOG_TAG,"Caching window: " + window.getId());
    }
    final int windowId=window.getId();
    AccessibilityWindowInfo oldWindow=mWindowCache.get(windowId);
    if (oldWindow != null) {
      oldWindow.recycle();
    }
    mWindowCache.put(windowId,AccessibilityWindowInfo.obtain(window));
  }
}
