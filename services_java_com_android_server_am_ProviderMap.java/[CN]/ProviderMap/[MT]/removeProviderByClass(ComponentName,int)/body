{
  if (mSingletonByClass.containsKey(name)) {
    if (DBG)     Slog.i(TAG,"Removing from globalByClass name=" + name);
    mSingletonByClass.remove(name);
  }
 else {
    if (DBG)     Slog.i(TAG,"Removing from providersByClass name=" + name + " user="+ (optionalUserId == -1 ? Binder.getOrigCallingUser() : optionalUserId));
    getProvidersByClass(optionalUserId).remove(name);
  }
}
