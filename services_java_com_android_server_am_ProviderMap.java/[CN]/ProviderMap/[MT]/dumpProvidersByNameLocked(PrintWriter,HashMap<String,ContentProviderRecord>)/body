{
  Iterator<Map.Entry<String,ContentProviderRecord>> it=map.entrySet().iterator();
  while (it.hasNext()) {
    Map.Entry<String,ContentProviderRecord> e=it.next();
    ContentProviderRecord r=e.getValue();
    pw.print("  ");
    pw.print(e.getKey());
    pw.print(": ");
    pw.println(r.toShortString());
  }
}
