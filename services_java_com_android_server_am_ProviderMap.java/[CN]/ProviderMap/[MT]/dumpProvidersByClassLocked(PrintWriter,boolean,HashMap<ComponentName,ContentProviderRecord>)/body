{
  Iterator<Map.Entry<ComponentName,ContentProviderRecord>> it=map.entrySet().iterator();
  while (it.hasNext()) {
    Map.Entry<ComponentName,ContentProviderRecord> e=it.next();
    ContentProviderRecord r=e.getValue();
    pw.print("  * ");
    pw.println(r);
    r.dump(pw,"    ",dumpAll);
  }
}
