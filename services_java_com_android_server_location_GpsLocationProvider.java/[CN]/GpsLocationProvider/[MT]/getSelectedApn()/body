{
  Uri uri=Uri.parse("content://telephony/carriers/preferapn");
  String apn=null;
  Cursor cursor=mContext.getContentResolver().query(uri,new String[]{"apn"},null,null,Carriers.DEFAULT_SORT_ORDER);
  if (null != cursor) {
    try {
      if (cursor.moveToFirst()) {
        apn=cursor.getString(0);
      }
    }
  finally {
      cursor.close();
    }
  }
  return apn;
}
