{
  Intent intent=null;
  int shortcut=kcm.get(keyCode,metaState);
  if (shortcut != 0) {
    intent=mShortcutIntents.get(shortcut);
  }
  if (intent == null) {
    shortcut=Character.toLowerCase(kcm.getDisplayLabel(keyCode));
    if (shortcut != 0) {
      intent=mShortcutIntents.get(shortcut);
    }
  }
  return intent;
}
