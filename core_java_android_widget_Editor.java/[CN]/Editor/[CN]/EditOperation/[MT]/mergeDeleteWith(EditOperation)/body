{
  if (DEBUG_UNDO)   Log.d(TAG,"mergeDeleteWith " + edit);
  if (edit.mType != TYPE_DELETE) {
    return false;
  }
  if (mOldTextStart != edit.getOldTextEnd()) {
    return false;
  }
  mOldTextStart=edit.mOldTextStart;
  mOldText=edit.mOldText + mOldText;
  mNewCursorPos=edit.mNewCursorPos;
  return true;
}
