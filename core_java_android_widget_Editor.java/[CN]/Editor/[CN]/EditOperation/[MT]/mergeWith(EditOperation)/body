{
  if (DEBUG_UNDO) {
    Log.d(TAG,"mergeWith old " + this);
    Log.d(TAG,"mergeWith new " + edit);
  }
  if (edit.mForceMerge) {
    forceMergeWith(edit);
    return true;
  }
switch (mType) {
case TYPE_INSERT:
    return mergeInsertWith(edit);
case TYPE_DELETE:
  return mergeDeleteWith(edit);
case TYPE_REPLACE:
return mergeReplaceWith(edit);
default :
return false;
}
}
