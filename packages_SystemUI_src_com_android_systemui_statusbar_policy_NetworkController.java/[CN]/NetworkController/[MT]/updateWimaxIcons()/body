{
  Slog.d(TAG,"in ....  updateWimaxIcons function    :  " + mIsWimaxEnabled);
  if (mIsWimaxEnabled) {
    if (mWimaxConnected) {
      Slog.d(TAG,"in ....  updateWimaxIcons function WiMAX COnnected");
      if (mWimaxIdle)       mWimaxIconId=WimaxIcons.WIMAX_IDLE;
 else       mWimaxIconId=WimaxIcons.WIMAX_SIGNAL_STRENGTH[mInetCondition][mWimaxSignal];
      mContentDescriptionWimax=mContext.getString(AccessibilityContentDescriptions.WIMAX_CONNECTION_STRENGTH[mWimaxSignal]);
    }
 else {
      Slog.d(TAG,"in ....  updateWimaxIcons function WiMAX Disconnected");
      mWimaxIconId=WimaxIcons.WIMAX_DISCONNECTED;
      mContentDescriptionWimax=mContext.getString(R.string.accessibility_no_wimax);
    }
  }
 else {
    Slog.d(TAG,"in ....  updateWimaxIcons function wimax icon id 0");
    mWimaxIconId=0;
  }
}
