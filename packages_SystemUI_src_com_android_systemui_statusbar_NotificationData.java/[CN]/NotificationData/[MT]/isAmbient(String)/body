{
  if (mRankingMap == null) {
    for (    Entry entry : mEntries) {
      if (key.equals(entry.key)) {
        return entry.notification.getNotification().priority == Notification.PRIORITY_MIN;
      }
    }
  }
 else {
    mRankingMap.getRanking(key,mTmpRanking);
    return mTmpRanking.isAmbient();
  }
  return false;
}
