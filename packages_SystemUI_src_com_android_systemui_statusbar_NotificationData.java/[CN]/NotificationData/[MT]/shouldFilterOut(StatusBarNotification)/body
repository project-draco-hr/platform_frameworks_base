{
  if (!(mEnvironment.isDeviceProvisioned() || showNotificationEvenIfUnprovisioned(sbn))) {
    return true;
  }
  if (!mEnvironment.isNotificationForCurrentProfiles(sbn)) {
    return true;
  }
  if (sbn.getNotification().visibility == Notification.VISIBILITY_SECRET && mEnvironment.shouldHideSensitiveContents(sbn.getUserId())) {
    return true;
  }
  if (mGroupManager.isChildInGroupWithSummary(sbn)) {
    return true;
  }
  return false;
}
