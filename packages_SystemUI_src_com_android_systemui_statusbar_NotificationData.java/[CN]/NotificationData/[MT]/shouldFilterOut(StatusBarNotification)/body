{
  if (!(mEnvironment.isDeviceProvisioned() || showNotificationEvenIfUnprovisioned(sbn))) {
    return true;
  }
  if (!mEnvironment.isNotificationForCurrentProfiles(sbn)) {
    return true;
  }
  if (mEnvironment.onSecureLockScreen() && (sbn.getNotification().visibility == Notification.VISIBILITY_SECRET || mEnvironment.shouldHideNotifications(sbn.getUserId()) || mEnvironment.shouldHideNotifications(sbn.getKey()))) {
    return true;
  }
  if (!BaseStatusBar.ENABLE_CHILD_NOTIFICATIONS && mGroupManager.isChildInGroupWithSummary(sbn)) {
    return true;
  }
  return false;
}
