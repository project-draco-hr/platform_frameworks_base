{
  if (Looper.getMainLooper() == Looper.myLooper()) {
    String databasePath=mDatabase.getPath();
    if (!sAlreadyWarned.containsKey(databasePath)) {
      sAlreadyWarned.put(databasePath,true);
      String packageName=ActivityThread.currentPackageName();
      Log.w(TAG,"should not attempt requery on main (UI) thread: app = " + packageName == null ? "'unknown'" : packageName,new RequeryOnUiThreadException(packageName));
    }
  }
}
