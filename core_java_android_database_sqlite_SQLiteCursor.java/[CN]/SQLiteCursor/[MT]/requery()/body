{
  if (isClosed()) {
    return false;
  }
synchronized (this) {
    if (!mQuery.getDatabase().isOpen()) {
      return false;
    }
    if (mWindow != null) {
      mWindow.clear();
    }
    mPos=-1;
    mCount=NO_COUNT;
    mDriver.cursorRequeried(this);
  }
  try {
    return super.requery();
  }
 catch (  IllegalStateException e) {
    Log.w(TAG,"requery() failed " + e.getMessage(),e);
    return false;
  }
}
