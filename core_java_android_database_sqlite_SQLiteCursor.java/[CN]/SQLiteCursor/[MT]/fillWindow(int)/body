{
  if (mWindow == null) {
    mWindow=new CursorWindow(true);
  }
 else {
    mCursorState++;
    queryThreadLock();
    try {
      mWindow.clear();
    }
  finally {
      queryThreadUnlock();
    }
  }
  mWindow.setStartPosition(startPos);
  mCount=mQuery.fillWindow(mWindow,mInitialRead,0);
  if (mCount == NO_COUNT) {
    mCount=startPos + mInitialRead;
    Thread t=new Thread(new QueryThread(mCursorState),"query thread");
    t.start();
  }
}
