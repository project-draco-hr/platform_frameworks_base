{
  checkPosition();
  if (mRowIdColumnIndex == -1 || mCurrentRowID == null) {
    Log.e(TAG,"Could not delete row because either the row ID column is not available or it" + "has not been read.");
    return false;
  }
  boolean success;
  mDatabase.lock();
  try {
    try {
      mDatabase.delete(mEditTable,mColumns[mRowIdColumnIndex] + "=?",new String[]{mCurrentRowID.toString()});
      success=true;
    }
 catch (    SQLException e) {
      success=false;
    }
    int pos=mPos;
    requery();
    moveToPosition(pos);
  }
  finally {
    mDatabase.unlock();
  }
  if (success) {
    onChange(true);
    return true;
  }
 else {
    return false;
  }
}
