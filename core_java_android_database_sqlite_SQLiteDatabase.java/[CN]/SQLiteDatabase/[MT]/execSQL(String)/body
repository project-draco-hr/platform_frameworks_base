{
  boolean logStats=mLogStats;
  long timeStart=logStats ? SystemClock.elapsedRealtime() : 0;
  lock();
  try {
    native_execSQL(sql);
  }
 catch (  SQLiteDatabaseCorruptException e) {
    onCorruption();
    throw e;
  }
 finally {
    unlock();
  }
  if (logStats) {
    logTimeStat(false,timeStart,SystemClock.elapsedRealtime());
  }
}
