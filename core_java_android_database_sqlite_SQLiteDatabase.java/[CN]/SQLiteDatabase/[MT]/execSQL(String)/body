{
  long timeStart=Debug.threadCpuTimeNanos();
  if (SQLiteDebug.DEBUG_CAPTURE_SQL) {
    Log.v(TAG,SQLiteDebug.captureSql(this.getPath(),sql,null));
  }
  lock();
  try {
    native_execSQL(sql);
  }
 catch (  SQLiteDatabaseCorruptException e) {
    onCorruption();
    throw e;
  }
 finally {
    unlock();
  }
  logTimeStat(sql,timeStart);
}
