{
  if (isOpen()) {
    if (mPrograms.isEmpty()) {
      Log.e(TAG,"Leak found",mLeakedException);
    }
 else {
      IllegalStateException leakProgram=new IllegalStateException("mPrograms size " + mPrograms.size(),mLeakedException);
      Log.e(TAG,"Leak found",leakProgram);
    }
    closeClosable();
    onAllReferencesReleased();
  }
}
