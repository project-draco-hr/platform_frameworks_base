{
  long timeStart=SystemClock.uptimeMillis();
  SQLiteStatement statement=new SQLiteStatement(this,sql,bindArgs);
  try {
    statement.executeUpdateDelete();
  }
 catch (  SQLiteDatabaseCorruptException e) {
    onCorruption();
    throw e;
  }
 finally {
    statement.close();
  }
  logTimeStat(sql,timeStart);
}
