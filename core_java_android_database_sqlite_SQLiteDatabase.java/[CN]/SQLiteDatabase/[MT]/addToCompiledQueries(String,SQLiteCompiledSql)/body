{
  if (mMaxSqlCacheSize == 0) {
    if (SQLiteDebug.DEBUG_SQL_CACHE) {
      Log.v(TAG,"|NOT adding_sql_to_cache|" + getPath() + "|"+ sql);
    }
    return;
  }
  String prefixSql=sql.substring(0,6);
  if (prefixSql.toLowerCase().startsWith("pragma")) {
    return;
  }
  SQLiteCompiledSql compiledSql=null;
synchronized (mCompiledQueries) {
    compiledSql=mCompiledQueries.get(sql);
    if (compiledSql != null) {
      return;
    }
    if (mCompiledQueries.size() == mMaxSqlCacheSize) {
      Log.w(TAG,"Reached MAX size for compiled-sql statement cache for database " + getPath() + "; i.e., NO space for this sql statement in cache: "+ sql+ ". Make sure your sql "+ "statements are using prepared-sql-statement syntax with '?' for "+ "bindargs, instead of using actual values");
      if (++mCacheFullWarnings > MAX_WARNINGS_ON_CACHESIZE_CONDITION) {
        mCacheFullWarnings=0;
        mCompiledQueries.clear();
        Log.w(TAG,"compiled-sql statement cache cleared for the database " + getPath());
      }
 else {
        Set<String> keySet=mCompiledQueries.keySet();
        for (        String s : keySet) {
          mCompiledQueries.remove(s);
          break;
        }
      }
    }
    mCompiledQueries.put(sql,compiledStatement);
  }
  if (SQLiteDebug.DEBUG_SQL_CACHE) {
    Log.v(TAG,"|adding_sql_to_cache|" + getPath() + "|"+ mCompiledQueries.size()+ "|"+ sql);
  }
  return;
}
