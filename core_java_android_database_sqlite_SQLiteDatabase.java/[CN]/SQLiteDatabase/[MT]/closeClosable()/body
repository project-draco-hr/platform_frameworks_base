{
  Iterator<Map.Entry<SQLiteClosable,Object>> iter=mPrograms.entrySet().iterator();
  while (iter.hasNext()) {
    Map.Entry<SQLiteClosable,Object> entry=iter.next();
    SQLiteClosable program=entry.getKey();
    if (program != null) {
      program.onAllReferencesReleasedFromContainer();
    }
  }
}
