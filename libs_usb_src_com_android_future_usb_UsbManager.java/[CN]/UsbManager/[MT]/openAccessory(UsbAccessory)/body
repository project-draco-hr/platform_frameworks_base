{
  try {
    return mService.openAccessory(new android.hardware.usb.UsbAccessory(accessory.getManufacturer(),accessory.getModel(),accessory.getDescription(),accessory.getVersion(),accessory.getUri()));
  }
 catch (  RemoteException e) {
    Log.e(TAG,"RemoteException in openAccessory",e);
    return null;
  }
}
