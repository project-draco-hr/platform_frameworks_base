{
  if (DEBUG) {
    Log.i(LOG_TAG,"[CALLED] finish()");
  }
  if (mState != STATE_STARTED && mState != STATE_UPDATED && mState != STATE_FAILED && mState != STATE_CANCELING && mState != STATE_CANCELED) {
    throw new IllegalStateException("Cannot finish in state:" + stateToString(mState));
  }
  try {
    mPrintDocumentAdapter.finish();
    mState=STATE_FINISHED;
  }
 catch (  RemoteException re) {
    Log.e(LOG_TAG,"Error calling finish()",re);
    mState=STATE_FAILED;
  }
}
