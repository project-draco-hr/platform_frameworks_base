{
  MoreAsserts.assertEquals(new long[]{1},ArrayUtils.appendLong(null,1));
  MoreAsserts.assertEquals(new long[]{1},ArrayUtils.appendLong(new long[]{},1));
  MoreAsserts.assertEquals(new long[]{1,2},ArrayUtils.appendLong(new long[]{1},2));
  MoreAsserts.assertEquals(new long[]{1,2},ArrayUtils.appendLong(new long[]{1,2},1));
}
