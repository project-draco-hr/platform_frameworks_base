{
  if (!mPattern.isEmpty() && mOnPatternListener != null) {
    mPatternInProgress=false;
    mOnPatternListener.onPatternDetected(mPattern);
    invalidate();
  }
  if (PROFILE_DRAWING) {
    if (mDrawingProfilingStarted) {
      Debug.stopMethodTracing();
      mDrawingProfilingStarted=false;
    }
  }
}
