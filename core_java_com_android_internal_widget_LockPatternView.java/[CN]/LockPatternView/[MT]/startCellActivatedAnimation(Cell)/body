{
  final CellState cellState=mCellStates[cell.row][cell.column];
  startRadiusAnimation(mDotSize / 2,mDotSizeActivated / 2,96,mLinearOutSlowInInterpolator,cellState,new Runnable(){
    @Override public void run(){
      startRadiusAnimation(mDotSizeActivated / 2,mDotSize / 2,192,mFastOutSlowInInterpolator,cellState,null);
    }
  }
);
  startLineEndAnimation(cellState,mInProgressX,mInProgressY,getCenterXForColumn(cell.column),getCenterYForRow(cell.row));
}
