{
  Cursor cursor=db.rawQuery(sql,selectionArgs);
  if (cursor == null) {
    return null;
  }
  try {
    if (!cursor.moveToFirst()) {
      return null;
    }
    byte[] bytes=cursor.getBlob(0);
    if (bytes == null) {
      return null;
    }
    MemoryFile file=new MemoryFile(null,bytes.length);
    file.writeBytes(bytes,0,0,bytes.length);
    file.deactivate();
    return file;
  }
  finally {
    cursor.close();
  }
}
