{
  if (sInstance == null) {
    return;
  }
  if (!event.getText().isEmpty()) {
    CharSequence text=event.getText().get(0);
    if (text != null) {
      sReceivedSelectionString=stripAppleSpanFromMarkup(text.toString());
    }
 else {
      sReceivedSelectionString=null;
    }
  }
synchronized (sTestLock) {
    sTestLock.notifyAll();
  }
}
