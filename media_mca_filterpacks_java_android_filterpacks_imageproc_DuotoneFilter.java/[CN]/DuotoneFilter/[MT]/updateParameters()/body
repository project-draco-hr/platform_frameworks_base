{
  float first[]={Color.red(mFirstColor) / 255f,Color.green(mFirstColor) / 255f,Color.blue(mFirstColor) / 255f};
  float second[]={Color.red(mSecondColor) / 255f,Color.green(mSecondColor) / 255f,Color.blue(mSecondColor) / 255f};
  mProgram.setHostValue("first",first);
  mProgram.setHostValue("second",second);
}
