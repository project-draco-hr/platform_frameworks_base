{
  if (args.length < 1) {
    onShowUsage(System.out);
    return;
  }
  mArgs=args;
  mNextArg=0;
  mCurArgData=null;
  try {
    onRun();
  }
 catch (  IllegalArgumentException e) {
    onShowUsage(System.err);
    System.err.println();
    System.err.println("Error: " + e.getMessage());
  }
catch (  Exception e) {
    e.printStackTrace(System.err);
    System.exit(1);
  }
}
