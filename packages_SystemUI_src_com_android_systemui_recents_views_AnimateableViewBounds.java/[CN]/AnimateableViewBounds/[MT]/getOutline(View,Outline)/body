{
  outline.setAlpha(mMinAlpha + mAlpha / (1f - mMinAlpha));
  outline.setRoundRect(Math.max(mClipRect.left,mOutlineClipRect.left),Math.max(mClipRect.top,mOutlineClipRect.top),mSourceView.getWidth() - Math.max(mClipRect.right,mOutlineClipRect.right),mSourceView.getHeight() - Math.max(mClipRect.bottom,mOutlineClipRect.bottom),mCornerRadius);
}
