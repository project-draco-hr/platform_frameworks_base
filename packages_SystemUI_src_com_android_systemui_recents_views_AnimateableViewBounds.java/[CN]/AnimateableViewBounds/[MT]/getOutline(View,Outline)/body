{
  outline.setRoundRect(Math.max(mClipRect.left,mOutlineClipRect.left),Math.max(mClipRect.top,mOutlineClipRect.top),mSourceView.getMeasuredWidth() - Math.max(mClipRect.right,mOutlineClipRect.right),mSourceView.getMeasuredHeight() - Math.max(mClipRect.bottom,mOutlineClipRect.bottom),mCornerRadius);
}
