{
switch (msg.what) {
case CMD_AIRPLANE_TOGGLED:
    if (mSettingsStore.isAirplaneModeOn()) {
      mWifiStateMachine.setHostApRunning(null,false);
      transitionTo(mApStaDisabledState);
    }
  break;
case CMD_SET_AP:
if (msg.arg1 == 0) {
  mWifiStateMachine.setHostApRunning(null,false);
  transitionTo(mApStaDisabledState);
}
break;
default :
return NOT_HANDLED;
}
return HANDLED;
}
