{
  int ret=0, sum=0;
  ret=parseCalentity(offset);
  if (PARSE_ERROR == ret) {
    return PARSE_ERROR;
  }
  offset+=ret;
  sum+=ret;
  for (; ; ) {
    while (PARSE_ERROR != (ret=parseCrlf(offset))) {
      offset+=ret;
      sum+=ret;
    }
    ret=parseCalentity(offset);
    if (PARSE_ERROR == ret) {
      break;
    }
    offset+=ret;
    sum+=ret;
  }
  return sum;
}
