{
  int ret=0, sum=0;
  ret=parseString(offset,";",true);
  if (PARSE_ERROR == ret) {
    return PARSE_ERROR;
  }
  offset+=ret;
  sum+=ret;
  ret=removeWs(offset);
  offset+=ret;
  sum+=ret;
  ret=parseParamlist(offset);
  if (PARSE_ERROR == ret) {
    return PARSE_ERROR;
  }
  sum+=ret;
  return sum;
}
