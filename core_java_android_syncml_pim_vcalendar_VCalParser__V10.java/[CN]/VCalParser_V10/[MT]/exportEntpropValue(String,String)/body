{
  if (null == propName || null == str)   return null;
  if ("".equals(propName) || "".equals(str))   return "";
  if (!mEscAllowedProps.contains(propName))   return str;
  String tmp=str.replace("\\\\","\n\r\n");
  tmp=tmp.replace("\\;",";");
  tmp=tmp.replace("\\:",":");
  tmp=tmp.replace("\\,",",");
  tmp=tmp.replace("\n\r\n","\\");
  return tmp;
}
