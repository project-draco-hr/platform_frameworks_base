{
  int ret=0, sum=0;
  while (PARSE_ERROR != (ret=parseWsls(offset))) {
    offset+=ret;
    sum+=ret;
  }
  ret=parseVCal(offset);
  if (PARSE_ERROR != ret) {
    offset+=ret;
    sum+=ret;
  }
 else {
    return PARSE_ERROR;
  }
  while (PARSE_ERROR != (ret=parseWsls(offset))) {
    offset+=ret;
    sum+=ret;
  }
  return sum;
}
