{
  int ret=0, sum=0, start=offset;
  boolean flag=false;
  ret=parseString(offset,"ENCODING",true);
  if (PARSE_ERROR != ret) {
    offset+=ret;
    sum+=ret;
    flag=true;
  }
  if (flag == true && mBuilder != null) {
    mBuilder.propertyParamType(mBuffer.substring(start,offset));
  }
  ret=removeWs(offset);
  offset+=ret;
  sum+=ret;
  ret=parseString(offset,"=",true);
  if (PARSE_ERROR != ret) {
    if (flag == false) {
      return PARSE_ERROR;
    }
    offset+=ret;
    sum+=ret;
  }
 else {
    if (flag == true) {
      return PARSE_ERROR;
    }
  }
  ret=removeWs(offset);
  offset+=ret;
  sum+=ret;
  start=offset;
  ret=parsePEncodingVal(offset);
  if (PARSE_ERROR == ret) {
    return PARSE_ERROR;
  }
  offset+=ret;
  sum+=ret;
  if (mBuilder != null) {
    mBuilder.propertyParamValue(mBuffer.substring(start,offset));
  }
  return sum;
}
