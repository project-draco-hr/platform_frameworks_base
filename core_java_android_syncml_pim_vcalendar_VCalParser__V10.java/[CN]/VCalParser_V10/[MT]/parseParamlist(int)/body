{
  int ret=0, sum=0;
  ret=parseParam(offset);
  if (PARSE_ERROR == ret) {
    return PARSE_ERROR;
  }
  offset+=ret;
  sum+=ret;
  int offsetTemp=offset;
  int sumTemp=sum;
  for (; ; ) {
    ret=removeWs(offsetTemp);
    offsetTemp+=ret;
    sumTemp+=ret;
    ret=parseString(offsetTemp,";",false);
    if (PARSE_ERROR == ret) {
      return sum;
    }
    offsetTemp+=ret;
    sumTemp+=ret;
    ret=removeWs(offsetTemp);
    offsetTemp+=ret;
    sumTemp+=ret;
    ret=parseParam(offsetTemp);
    if (PARSE_ERROR == ret) {
      break;
    }
    offsetTemp+=ret;
    sumTemp+=ret;
    sum=sumTemp;
  }
  return sum;
}
