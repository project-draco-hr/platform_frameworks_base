{
  int ret=0, sum=0, start=0;
  ret=parseString(offset,propName,true);
  if (PARSE_ERROR == ret) {
    return PARSE_ERROR;
  }
  offset+=ret;
  sum+=ret;
  if (mBuilder != null) {
    mBuilder.propertyName(propName);
  }
  ret=parseParams(offset);
  if (PARSE_ERROR != ret) {
    offset+=ret;
    sum+=ret;
  }
  ret=parseString(offset,":",true);
  if (PARSE_ERROR == ret) {
    return PARSE_ERROR;
  }
  offset+=ret;
  sum+=ret;
  start=offset;
  ret=parseValue(offset);
  if (PARSE_ERROR == ret) {
    return PARSE_ERROR;
  }
  offset+=ret;
  sum+=ret;
  if (mBuilder != null) {
    ArrayList<String> v=new ArrayList<String>();
    v.add(mBuffer.substring(start,offset));
    mBuilder.propertyValues(v);
  }
  ret=parseCrlf(offset);
  if (PARSE_ERROR == ret) {
    return PARSE_ERROR;
  }
  sum+=ret;
  return sum;
}
