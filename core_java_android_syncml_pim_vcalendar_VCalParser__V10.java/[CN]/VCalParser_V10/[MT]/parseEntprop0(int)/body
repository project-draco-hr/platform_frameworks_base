{
  int ret=0, sum=0, start=0;
  ret=removeWs(offset);
  offset+=ret;
  sum+=ret;
  String propName=getWord(offset).toUpperCase();
  if (!mEvtPropNameGroup1.contains(propName)) {
    if (PARSE_ERROR == parseXWord(offset))     return PARSE_ERROR;
  }
  ret=propName.length();
  offset+=ret;
  sum+=ret;
  if (mBuilder != null) {
    mBuilder.propertyName(propName);
  }
  ret=parseParams(offset);
  if (PARSE_ERROR != ret) {
    offset+=ret;
    sum+=ret;
  }
  ret=parseString(offset,":",false);
  if (PARSE_ERROR == ret) {
    return PARSE_ERROR;
  }
  offset+=ret;
  sum+=ret;
  start=offset;
  ret=parseValue(offset);
  if (PARSE_ERROR == ret) {
    return PARSE_ERROR;
  }
  offset+=ret;
  sum+=ret;
  if (mBuilder != null) {
    ArrayList<String> v=new ArrayList<String>();
    v.add(exportEntpropValue(propName,mBuffer.substring(start,offset)));
    mBuilder.propertyValues(v);
    if (PARSE_ERROR == valueFilter(propName,v))     return PARSE_ERROR;
  }
  ret=parseCrlf(offset);
  if (PARSE_ERROR == ret) {
    return PARSE_ERROR;
  }
  sum+=ret;
  return sum;
}
