{
  try (final ParcelFileDescriptor.AutoCloseOutputStream stream=new ParcelFileDescriptor.AutoCloseOutputStream(mDescriptors[1])){
    try {
      final byte[] bytes=getBytes();
      stream.write(bytes);
    }
 catch (    IOException error) {
      mDescriptors[1].closeWithError("Failed to load bytes.");
      return;
    }
  }
 catch (  IOException closeError) {
    Log.d(MtpDocumentsProvider.TAG,closeError.getMessage());
  }
}
