{
  if (pointer != null) {
synchronized (sPool) {
      int count=sRecycledPointerCount;
      if (count >= POINTER_POOL_CAPACITY) {
        return;
      }
      Pointer tail=pointer;
      for (; ; ) {
        count+=1;
        if (count >= POINTER_POOL_CAPACITY) {
          break;
        }
        Pointer next=tail.next;
        if (next == null) {
          break;
        }
        tail=next;
      }
      tail.next=sRecycledPointerListHead;
      sRecycledPointerCount=count;
      sRecycledPointerListHead=pointer;
    }
  }
}
