{
synchronized (mAssist) {
    if (!mAsync) {
      throw new IllegalStateException("Child " + this + " was not created with ViewAssistStructure.asyncNewChild");
    }
    if (!mAssist.mPendingAsyncChildren.remove(this)) {
      throw new IllegalStateException("Child " + this + " already committed");
    }
    mAssist.notifyAll();
  }
}
