{
  mAlpha*=t.getAlpha();
  mMatrix.postConcat(t.getMatrix());
  if (t.mHasClipRect) {
    Rect bounds=t.getClipRect();
    if (mHasClipRect) {
      setClipRect(mClipRect.left + bounds.left,mClipRect.top + bounds.top,mClipRect.right + bounds.right,mClipRect.bottom + bounds.bottom);
    }
 else {
      setClipRect(bounds);
    }
  }
}
