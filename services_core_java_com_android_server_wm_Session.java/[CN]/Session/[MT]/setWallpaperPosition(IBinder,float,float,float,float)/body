{
synchronized (mService.mWindowMap) {
    long ident=Binder.clearCallingIdentity();
    try {
      mService.mWallpaperControllerLocked.setWindowWallpaperPosition(mService.windowForClientLocked(this,window,true),x,y,xStep,yStep);
    }
  finally {
      Binder.restoreCallingIdentity(ident);
    }
  }
}
