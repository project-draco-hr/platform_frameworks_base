{
  if (mHeaderViewInfos.size() > 0) {
    boolean result=false;
    if (((HeaderViewListAdapter)mAdapter).removeHeader(v)) {
      if (mDataSetObserver != null) {
        mDataSetObserver.onChanged();
      }
      result=true;
    }
    removeFixedViewInfo(v,mHeaderViewInfos);
    return result;
  }
  return false;
}
