{
  if (mFooterViewInfos.size() > 0) {
    boolean result=false;
    if (((HeaderViewListAdapter)mAdapter).removeFooter(v)) {
      if (mDataSetObserver != null) {
        mDataSetObserver.onChanged();
      }
      result=true;
    }
    removeFixedViewInfo(v,mFooterViewInfos);
    return result;
  }
  return false;
}
