{
  if (resId == 0) {
    return null;
  }
  Context packageContext;
  try {
    packageContext=context.createPackageContext(mAccountHandle.getComponentName().getPackageName(),0);
  }
 catch (  PackageManager.NameNotFoundException e) {
    Log.w(this,"Cannot find package %s",mAccountHandle.getComponentName().getPackageName());
    return null;
  }
  try {
    return packageContext.getDrawable(resId);
  }
 catch (  NotFoundException|MissingResourceException e) {
    Log.e(this,e,"Cannot find icon %d in package %s",resId,mAccountHandle.getComponentName().getPackageName());
    return null;
  }
}
