{
  UserSwitcherController.UserRecord tag=(UserSwitcherController.UserRecord)view.getTag();
  if (tag.isDisabledByAdmin) {
    final Intent intent=RestrictedLockUtils.getShowAdminSupportDetailsIntent(mContext,tag.enforcedAdmin);
    mController.startActivity(intent);
  }
 else {
    MetricsLogger.action(mContext,MetricsLogger.QS_SWITCH_USER);
    switchTo(tag);
  }
}
