{
  assertSame("All focus change callback should be invoked.",3,mInvocations.size());
  assertInvioked("Callback for View clearing focus explected.",0,"OnFocusChangeListener#onFocusChange",new Object[]{mClearingFocusView,false});
  assertInvioked("Callback for View global focus change explected.",1,"OnFocusChangeListener#onFocusChange",new Object[]{mClearingFocusView,mGainingFocusView});
  assertInvioked("Callback for View gaining focus explected.",2,"OnFocusChangeListener#onFocusChange",new Object[]{mGainingFocusView,true});
}
