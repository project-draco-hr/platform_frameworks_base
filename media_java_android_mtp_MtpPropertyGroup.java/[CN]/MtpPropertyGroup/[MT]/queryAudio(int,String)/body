{
  Cursor c=null;
  try {
    c=mProvider.query(Audio.Media.getContentUri(mVolumeName),new String[]{Files.FileColumns._ID,column},ID_WHERE,new String[]{Integer.toString(id)},null,null);
    if (c != null && c.moveToNext()) {
      return c.getString(1);
    }
 else {
      return "";
    }
  }
 catch (  Exception e) {
    return null;
  }
 finally {
    if (c != null) {
      c.close();
    }
  }
}
