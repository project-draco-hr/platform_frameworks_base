{
  ResolveInfo ri=new ResolveInfo();
  ServiceInfo si=new ServiceInfo();
  ApplicationInfo ai=new ApplicationInfo();
  ai.packageName=packageName;
  ai.enabled=true;
  si.applicationInfo=ai;
  si.enabled=true;
  si.packageName=packageName;
  si.name=className;
  si.exported=true;
  si.nonLocalizedLabel=label;
  ri.serviceInfo=si;
  return ri;
}
