{
  if (mGestureType == MULTIPLE_STROKE) {
    overlay.cancelFadingOut();
  }
  mX=event.getX();
  mY=event.getY();
  mTotalLength=0;
  mIsGesturing=false;
  if (mGestureType == SINGLE_STROKE || overlay.getCurrentGesture() == null || overlay.getCurrentGesture().getStrokesCount() == 0) {
    overlay.setGestureColor(mUncertainGestureColor);
  }
  mModel.dispatchTouchEvent(event);
}
