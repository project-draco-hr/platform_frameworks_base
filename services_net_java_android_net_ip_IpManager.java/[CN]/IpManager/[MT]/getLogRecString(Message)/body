{
  final String logLine=String.format("iface{%s/%d} arg1{%d} arg2{%d} obj{%s}",mInterfaceName,mNetworkInterface == null ? -1 : mNetworkInterface.getIndex(),msg.arg1,msg.arg2,Objects.toString(msg.obj));
  if (VDBG) {
    Log.d(mTag,getWhatToString(msg.what) + " " + logLine);
  }
  return logLine;
}
