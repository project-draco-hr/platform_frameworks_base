{
  FontFamily family=sFontFamilyMap.get(familyName);
  if (family != null) {
switch (style) {
case NORMAL:
      return family.mNormalFileName;
case BOLD:
    return family.mBoldFileName;
case ITALIC:
  return family.mItalicFileName;
case BOLD_ITALIC:
return family.mBoldItalicFileName;
}
}
return "DroidSans.ttf";
}
