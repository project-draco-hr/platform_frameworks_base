{
  String homeRoamIndicators=SystemProperties.get("ro.cdma.homesystem");
  if (!TextUtils.isEmpty(homeRoamIndicators)) {
    for (    String homeRoamInd : homeRoamIndicators.split(",")) {
      if (homeRoamInd.equals(roamInd)) {
        return true;
      }
    }
    return false;
  }
  return false;
}
