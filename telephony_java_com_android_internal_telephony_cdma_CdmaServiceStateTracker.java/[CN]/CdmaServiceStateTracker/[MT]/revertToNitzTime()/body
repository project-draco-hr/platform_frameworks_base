{
  if (Settings.System.getInt(cr,Settings.System.AUTO_TIME,0) == 0) {
    return;
  }
  Log.d(LOG_TAG,"Reverting to NITZ Time: mSavedTime=" + mSavedTime + " mSavedAtTime="+ mSavedAtTime);
  if (mSavedTime != 0 && mSavedAtTime != 0) {
    setAndBroadcastNetworkSetTime(mSavedTime + (SystemClock.elapsedRealtime() - mSavedAtTime));
  }
}
