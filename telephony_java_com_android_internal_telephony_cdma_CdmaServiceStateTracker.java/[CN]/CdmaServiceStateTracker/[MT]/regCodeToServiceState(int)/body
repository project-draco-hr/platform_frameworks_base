{
switch (code) {
case 0:
    return ServiceState.STATE_OUT_OF_SERVICE;
case 1:
  return ServiceState.STATE_IN_SERVICE;
case 2:
case 3:
case 4:
return ServiceState.STATE_OUT_OF_SERVICE;
case 5:
case 6:
return ServiceState.STATE_IN_SERVICE;
default :
Log.w(LOG_TAG,"unexpected service state " + code);
return ServiceState.STATE_OUT_OF_SERVICE;
}
}
