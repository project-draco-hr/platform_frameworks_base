{
  int oldRSSI=rssi;
  if (ar.exception != null) {
    rssi=99;
  }
 else {
    int[] ints=(int[])ar.result;
    if (ints.length != 0) {
      rssi=ints[0];
    }
 else {
      Log.e(LOG_TAG,"Bogus signal strength response");
      rssi=99;
    }
  }
  if (rssi != oldRSSI) {
    try {
      phone.notifySignalStrength();
    }
 catch (    NullPointerException ex) {
      log("onSignalStrengthResult() Phone already destroyed: " + ex + "Signal Stranth not notified");
    }
  }
}
