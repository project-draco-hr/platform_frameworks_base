{
switch (view.getId()) {
case R.id.star:
{
      if (mContactUri != null) {
        final ContentValues values=new ContentValues(1);
        values.put(Contacts.STARRED,mStarredView.isChecked());
        mContentResolver.update(mContactUri,values,null,null);
      }
      break;
    }
case R.id.photo:
{
    performPhotoClick();
    break;
  }
case R.id.name:
{
  performDisplayNameClick();
  break;
}
}
}
