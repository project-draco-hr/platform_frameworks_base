{
  final int N=mHosts.size();
  for (int i=0; i < N; i++) {
    Host h=mHosts.get(i);
    if (h.hostId == hostId && h.packageName.equals(packageName)) {
      return h;
    }
  }
  Host host=new Host();
  host.packageName=packageName;
  host.uid=uid;
  host.hostId=hostId;
  mHosts.add(host);
  return host;
}
