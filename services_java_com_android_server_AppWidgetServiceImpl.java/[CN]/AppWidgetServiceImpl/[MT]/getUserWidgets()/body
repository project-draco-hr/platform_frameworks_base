{
  StringBuffer sb=new StringBuffer();
  for (  AppWidgetId widget : mAppWidgetIds) {
    sb.append("    id=");
    sb.append(widget.appWidgetId);
    sb.append(", hostUid=");
    sb.append(widget.host.uid);
    sb.append(", provider=");
    sb.append(widget.provider.info.provider.toString());
    sb.append("\n");
  }
  sb.append("\n");
  return sb.toString();
}
