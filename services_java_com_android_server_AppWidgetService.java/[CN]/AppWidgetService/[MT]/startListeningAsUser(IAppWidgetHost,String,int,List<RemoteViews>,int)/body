{
  if (Binder.getCallingPid() != android.os.Process.myPid() && userId != UserHandle.getCallingUserId()) {
    throw new SecurityException("Call from non-system process. Calling uid = " + Binder.getCallingUid());
  }
  return getImplForUser(userId).startListening(host,packageName,hostId,updatedViews);
}
