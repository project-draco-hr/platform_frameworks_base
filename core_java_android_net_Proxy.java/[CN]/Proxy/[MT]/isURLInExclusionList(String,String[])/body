{
  if (url == null) {
    return false;
  }
  Uri u=Uri.parse(url);
  String urlDomain=u.getHost();
  for (  String excludedDomain : exclusionList) {
    String dotDomain="." + excludedDomain;
    if (urlDomain.equals(excludedDomain)) {
      return true;
    }
    if (urlDomain.endsWith(dotDomain)) {
      return true;
    }
  }
  return false;
}
