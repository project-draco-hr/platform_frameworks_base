{
  if (host == null) {
    return false;
  }
  try {
    if (host != null) {
      if (host.equalsIgnoreCase("localhost")) {
        return true;
      }
      if (NetworkUtils.numericToInetAddress(host).isLoopbackAddress()) {
        return true;
      }
    }
  }
 catch (  IllegalArgumentException iex) {
  }
  return false;
}
