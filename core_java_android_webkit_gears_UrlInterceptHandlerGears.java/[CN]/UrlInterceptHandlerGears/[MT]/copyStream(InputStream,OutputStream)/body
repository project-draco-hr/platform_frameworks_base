{
  try {
    byte[] buf=new byte[BUFFER_SIZE];
    for (; ; ) {
      int bytes=inputStream.read(buf);
      if (bytes < 0) {
        break;
      }
      outputStream.write(buf,0,bytes);
    }
  }
 catch (  IOException ex) {
    log("Got IOException copying stream: " + ex);
    return false;
  }
  return true;
}
