{
  boolean changed=expanded != mExpanded;
  mExpanded=expanded;
  if (changed) {
    updateHeights();
    updateVisibilities();
    updateSystemIconsLayoutParams();
    updateBrightnessControllerState();
    updateZTranslation();
    if (mQSPanel != null) {
      mQSPanel.setExpanded(expanded);
    }
  }
}
