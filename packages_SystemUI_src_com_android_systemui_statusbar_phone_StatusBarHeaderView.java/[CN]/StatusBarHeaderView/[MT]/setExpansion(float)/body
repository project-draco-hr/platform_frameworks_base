{
  if (!mExpanded) {
    t=0f;
  }
  mCurrentT=t;
  float height=mCollapsedHeight + t * (mExpandedHeight - mCollapsedHeight);
  if (height < mCollapsedHeight) {
    height=mCollapsedHeight;
  }
  if (height > mExpandedHeight) {
    height=mExpandedHeight;
  }
  setClipping(height);
  updateLayoutValues(t);
}
