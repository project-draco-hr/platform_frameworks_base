{
  checkSystemUid("setDeviceOwner");
  try {
    mDeviceOwnerUid=mContext.getPackageManager().getPackageUid(packageName,UserHandle.USER_OWNER);
  }
 catch (  NameNotFoundException e) {
    Log.e(TAG,"Could not find Device Owner UID");
    mDeviceOwnerUid=-1;
    throw new IllegalArgumentException("Could not find device owner package " + packageName);
  }
}
