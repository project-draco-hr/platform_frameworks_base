{
  if (op.time == 0 && op.rejectTime == 0) {
    Ops ops=getOpsLocked(uid,packageName,false);
    if (ops != null) {
      ops.remove(op.op);
      if (ops.size() <= 0) {
        HashMap<String,Ops> pkgOps=mUidOps.get(uid);
        if (pkgOps != null) {
          pkgOps.remove(ops.packageName);
          if (pkgOps.size() <= 0) {
            mUidOps.remove(uid);
          }
        }
      }
    }
  }
}
