{
  verifyIncomingOp(code);
  final int proxyMode=noteOperationUnchecked(code,Binder.getCallingUid(),proxyPackageName,-1,null);
  if (proxyMode != AppOpsManager.MODE_ALLOWED || Binder.getCallingUid() == proxiedUid) {
    return proxyMode;
  }
  return noteOperationUnchecked(code,proxiedUid,proxiedPackageName,Binder.getCallingUid(),proxyPackageName);
}
