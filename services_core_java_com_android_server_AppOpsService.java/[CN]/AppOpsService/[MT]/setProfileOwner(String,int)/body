{
  checkSystemUid("setProfileOwner");
  try {
    int uid=mContext.getPackageManager().getPackageUid(packageName,userHandle);
    mProfileOwnerUids.put(userHandle,uid);
  }
 catch (  NameNotFoundException e) {
    Log.e(TAG,"Could not find Profile Owner UID");
    mProfileOwnerUids.put(userHandle,-1);
    throw new IllegalArgumentException("Could not find profile owner package " + packageName);
  }
}
