{
  AndroidFont peer;
  int familyIndex=getFamilyIndex(name);
  if (familyIndex != -1) {
    peer=new AndroidFont(getFamily(familyIndex),style,size);
    peer.setFamily(getFamily(familyIndex));
    return peer;
  }
  int faceIndex=getFaceIndex(name);
  if (faceIndex != -1) {
    peer=new AndroidFont(name,style,size);
    return peer;
  }
  return null;
}
