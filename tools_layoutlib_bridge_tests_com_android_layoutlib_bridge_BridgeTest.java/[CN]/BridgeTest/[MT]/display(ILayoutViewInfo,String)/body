{
  String msg=String.format("%s%s L:%d T:%d R:%d B:%d",offset,result.getName(),result.getLeft(),result.getTop(),result.getRight(),result.getBottom());
  System.out.println(msg);
  ILayoutViewInfo[] children=result.getChildren();
  if (children != null) {
    offset+="+-";
    for (    ILayoutViewInfo child : children) {
      display(child,offset);
    }
  }
}
