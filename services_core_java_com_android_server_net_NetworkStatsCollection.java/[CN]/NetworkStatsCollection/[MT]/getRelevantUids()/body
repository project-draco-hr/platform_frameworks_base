{
  final int callerUid=Binder.getCallingUid();
  IntArray uids=new IntArray();
  for (int i=0; i < mStats.size(); i++) {
    final Key key=mStats.keyAt(i);
    if (isAccessibleToUser(key.uid,callerUid)) {
      int j=uids.binarySearch(key.uid);
      if (j < 0) {
        j=~j;
        uids.add(j,key.uid);
      }
    }
  }
  return uids.toArray();
}
