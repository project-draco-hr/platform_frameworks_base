{
  if (mService == null) {
    return Collections.emptyList();
  }
  if (profile == null) {
    profile=Process.myUserHandle();
  }
  try {
    ParceledListSlice<AppWidgetProviderInfo> providers=mService.getInstalledProvidersForProfile(categoryFilter,profile.getIdentifier());
    if (providers == null) {
      return Collections.emptyList();
    }
    for (    AppWidgetProviderInfo info : providers.getList()) {
      convertSizesToPixels(info);
    }
    return providers.getList();
  }
 catch (  RemoteException e) {
    throw e.rethrowFromSystemServer();
  }
}
