{
  tv().setAudioStatus(false,Constants.UNKNOWN_VOLUME);
  int uiCommand=tv().isSystemAudioActivated() ? HdmiCecKeycode.CEC_KEYCODE_RESTORE_VOLUME_FUNCTION : HdmiCecKeycode.CEC_KEYCODE_MUTE_FUNCTION;
  sendUserControlPressedAndReleased(mAvrAddress,uiCommand);
  finishWithCallback(HdmiControlManager.RESULT_SUCCESS);
}
