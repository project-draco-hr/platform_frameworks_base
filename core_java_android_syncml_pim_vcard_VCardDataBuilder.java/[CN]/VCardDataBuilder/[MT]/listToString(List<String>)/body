{
  int size=list.size();
  if (size > 1) {
    StringBuilder builder=new StringBuilder();
    int i=0;
    for (    String type : list) {
      builder.append(type);
      if (i < size - 1) {
        builder.append(";");
      }
    }
    return builder.toString();
  }
 else   if (size == 1) {
    return list.get(0);
  }
 else {
    return "";
  }
}
