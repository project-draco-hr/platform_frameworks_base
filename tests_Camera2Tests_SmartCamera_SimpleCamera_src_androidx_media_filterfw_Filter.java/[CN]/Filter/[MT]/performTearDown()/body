{
synchronized (mState) {
    if (mState.current == State.STATE_OPEN) {
      throw new RuntimeException("Attempting to tear-down filter " + this + " which is "+ "in an open state!");
    }
 else     if (mState.current != State.STATE_DESTROYED && mState.current != State.STATE_UNPREPARED) {
      onTearDown();
      mState.current=State.STATE_DESTROYED;
    }
  }
}
