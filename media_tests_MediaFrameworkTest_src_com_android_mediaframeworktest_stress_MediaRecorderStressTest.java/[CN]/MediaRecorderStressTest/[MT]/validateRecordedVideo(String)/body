{
  try {
    MediaPlayer mp=new MediaPlayer();
    mp.setDataSource(recorded_file);
    mp.prepare();
    int duration=mp.getDuration();
    if (duration <= 0) {
      assertTrue("stressRecordAndPlayback",false);
    }
    mp.release();
  }
 catch (  Exception e) {
    assertTrue("stressRecordAndPlayback",false);
  }
}
