{
  if (gDefault != null) {
    return gDefault;
  }
  IBinder b=ServiceManager.getService("activity");
  if (Config.LOGV)   Log.v("ActivityManager","default service binder = " + b);
  gDefault=asInterface(b);
  if (Config.LOGV)   Log.v("ActivityManager","default service = " + gDefault);
  return gDefault;
}
