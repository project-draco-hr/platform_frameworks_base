{
  Parcel data=Parcel.obtain();
  Parcel reply=Parcel.obtain();
  data.writeInterfaceToken(IActivityManager.descriptor);
  data.writeInt(pid);
  mRemote.transact(GET_PROCESS_MEMORY_INFO_TRANSACTION,data,reply,0);
  reply.readException();
  outInfo.readFromParcel(reply);
  data.recycle();
  reply.recycle();
}
