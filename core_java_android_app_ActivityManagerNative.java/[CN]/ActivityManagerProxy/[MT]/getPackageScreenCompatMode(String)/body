{
  Parcel data=Parcel.obtain();
  Parcel reply=Parcel.obtain();
  data.writeInterfaceToken(IActivityManager.descriptor);
  mRemote.transact(SET_PACKAGE_SCREEN_COMPAT_MODE_TRANSACTION,data,reply,0);
  reply.readException();
  int mode=reply.readInt();
  reply.recycle();
  data.recycle();
  return mode;
}
