{
  final List<ImeSubtypeListItem> disabledItems=createDisabledImeSubtypes();
  final ImeSubtypeListItem disabledIme_en_US=disabledItems.get(0);
  final ImeSubtypeListItem disabledIme_hi=disabledItems.get(1);
  final ImeSubtypeListItem disabledSwitchingUnawareIme=disabledItems.get(2);
  final ImeSubtypeListItem disabledSubtypeUnawareIme=disabledItems.get(3);
  final List<ImeSubtypeListItem> enabledItems=createEnabledImeSubtypes();
  final ImeSubtypeListItem latinIme_en_US=enabledItems.get(0);
  final ImeSubtypeListItem latinIme_fr=enabledItems.get(1);
  final ImeSubtypeListItem switchingUnawarelatinIme_en_UK=enabledItems.get(2);
  final ImeSubtypeListItem switchingUnawarelatinIme_hi=enabledItems.get(3);
  final ImeSubtypeListItem subtypeUnawareIme=enabledItems.get(4);
  final ImeSubtypeListItem japaneseIme_ja_JP=enabledItems.get(5);
  final ImeSubtypeListItem switchUnawareJapaneseIme_ja_JP=enabledItems.get(6);
  final ControllerImpl controller=new ControllerImpl(enabledItems);
  assertRotationOrder(controller,false,latinIme_en_US,latinIme_fr,japaneseIme_ja_JP);
  assertRotationOrder(controller,false,switchingUnawarelatinIme_en_UK,switchingUnawarelatinIme_hi,subtypeUnawareIme,switchUnawareJapaneseIme_ja_JP);
  assertRotationOrder(controller,true,latinIme_en_US,latinIme_fr);
  assertRotationOrder(controller,true,switchingUnawarelatinIme_en_UK,switchingUnawarelatinIme_hi);
  assertNextInputMethod(controller,true,subtypeUnawareIme,null);
  assertNextInputMethod(controller,true,japaneseIme_ja_JP,null);
  assertNextInputMethod(controller,true,switchUnawareJapaneseIme_ja_JP,null);
  assertNextInputMethod(controller,false,disabledIme_en_US,null);
  assertNextInputMethod(controller,false,disabledIme_hi,null);
  assertNextInputMethod(controller,false,disabledSwitchingUnawareIme,null);
  assertNextInputMethod(controller,false,disabledSubtypeUnawareIme,null);
  assertNextInputMethod(controller,true,disabledIme_en_US,null);
  assertNextInputMethod(controller,true,disabledIme_hi,null);
  assertNextInputMethod(controller,true,disabledSwitchingUnawareIme,null);
  assertNextInputMethod(controller,true,disabledSubtypeUnawareIme,null);
}
