{
  if (!SipManager.isVoipSupported(mContext)) {
    throw new SipException("VOIP API is not supported");
  }
synchronized (this) {
    mSipSession=session;
    mPeerSd=sessionDescription;
    if (DBG)     log("attachCall(): " + mPeerSd);
    try {
      session.setListener(createListener());
    }
 catch (    Throwable e) {
      loge("attachCall()",e);
      throwSipException(e);
    }
  }
}
