{
  IFingerprintServiceReceiver rx=receiver.get();
  if (rx != null) {
    try {
      rx.onRemoved(mHalDeviceId,fingerId,groupId);
      return true;
    }
 catch (    RemoteException e) {
      if (DEBUG)       Slog.v(TAG,"Failed to invoke sendRemoved:",e);
    }
  }
  removeClient(this);
  return false;
}
