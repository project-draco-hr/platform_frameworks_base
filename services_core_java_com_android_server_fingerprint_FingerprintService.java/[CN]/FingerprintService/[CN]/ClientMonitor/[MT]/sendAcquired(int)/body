{
  IFingerprintServiceReceiver rx=receiver.get();
  if (rx != null) {
    try {
      rx.onAcquired(mHalDeviceId,acquiredInfo);
      return true;
    }
 catch (    RemoteException e) {
      if (DEBUG)       Slog.v(TAG,"Failed to invoke sendAcquired:",e);
    }
  }
  removeClient(this);
  return false;
}
