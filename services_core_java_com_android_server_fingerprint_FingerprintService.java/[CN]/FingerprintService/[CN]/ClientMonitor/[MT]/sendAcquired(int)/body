{
  if (receiver == null)   return true;
  try {
    receiver.onAcquired(mHalDeviceId,acquiredInfo);
    return false;
  }
 catch (  RemoteException e) {
    Slog.w(TAG,"Failed to invoke sendAcquired:",e);
    return true;
  }
 finally {
    if (acquiredInfo == FINGERPRINT_ACQUIRED_GOOD) {
      userActivity();
    }
  }
}
