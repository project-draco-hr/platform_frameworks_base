{
  ClientData clientData=mClients.get(token);
  if (clientData != null) {
    if (clientData.userId != userId)     throw new IllegalStateException("Bad user");
    clientData.state=STATE_LISTENING;
    nativeEnrollCancel();
  }
 else {
    Slog.w(TAG,"enrollCancel(): No listener registered");
  }
}
