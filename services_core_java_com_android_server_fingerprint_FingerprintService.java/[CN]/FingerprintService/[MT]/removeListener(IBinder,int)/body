{
  if (DEBUG)   Slog.v(TAG,"stopListening(" + token + ")");
  ClientData clientData=mClients.get(token);
  if (clientData != null) {
    token.unlinkToDeath(clientData.tokenWatcher,0);
    mClients.remove(token);
  }
 else {
    if (DEBUG)     Slog.v(TAG,"listener not registered: " + token);
  }
  mClients.remove(token);
}
