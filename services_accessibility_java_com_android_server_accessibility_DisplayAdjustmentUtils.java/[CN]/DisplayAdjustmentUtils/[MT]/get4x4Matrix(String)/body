{
  String[] strValues=matrix.split(",");
  if (strValues.length != 16) {
    return null;
  }
  float[] values=new float[strValues.length];
  try {
    for (int i=0; i < values.length; i++) {
      values[i]=Float.parseFloat(strValues[i]);
    }
  }
 catch (  java.lang.NumberFormatException ex) {
    return null;
  }
  return values;
}
