{
  final PrintWriter pw=getOutPrintWriter();
  final int[] uids=mInterface.getRestrictBackgroundWhitelistedUids();
  pw.print("Restrict background whitelisted UIDs: ");
  if (uids.length == 0) {
    pw.println("none");
  }
 else {
    for (int i=0; i < uids.length; i++) {
      int uid=uids[i];
      pw.print(uid);
      pw.print(' ');
    }
  }
  pw.println();
  return 0;
}
