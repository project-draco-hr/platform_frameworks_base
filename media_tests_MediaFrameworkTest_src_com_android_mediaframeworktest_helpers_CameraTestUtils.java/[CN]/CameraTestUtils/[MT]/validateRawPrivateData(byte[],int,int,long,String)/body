{
  if (VERBOSE)   Log.v(TAG,"Validating private raw data");
  int expectedSizeMin=width * height;
  int expectedSizeMax=width * height * 5 / 2;
  assertTrue("Opaque RAW size " + rawData.length + "out of normal bound ["+ expectedSizeMin+ ","+ expectedSizeMax+ "]",expectedSizeMin <= rawData.length && rawData.length <= expectedSizeMax);
  if (DEBUG && filePath != null) {
    String fileName=filePath + "/" + width+ "x"+ height+ "_"+ ts / 1e6 + ".rawPriv";
    dumpFile(fileName,rawData);
  }
  return;
}
