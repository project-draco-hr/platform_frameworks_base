{
  float[] apertures=staticInfo.getAvailableAperturesChecked();
  Float resultAperture=result.get(CaptureResult.LENS_APERTURE);
  if (collector.expectTrue("Capture result aperture is invalid",resultAperture != null && resultAperture > 0)) {
    List<Float> apertureList=Arrays.asList(CameraTestUtils.toObject(apertures));
    collector.expectTrue("Aperture should be one of the available apertures",apertureList.contains(resultAperture));
    return resultAperture;
  }
  return -1;
}
