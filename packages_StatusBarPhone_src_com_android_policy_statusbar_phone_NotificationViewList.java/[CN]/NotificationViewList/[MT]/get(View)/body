{
  int N=mOngoing.size();
  for (int i=0; i < N; i++) {
    StatusBarNotification notification=mOngoing.get(i);
    View v=notification.view;
    if (v == view) {
      return notification;
    }
  }
  N=mLatest.size();
  for (int i=0; i < N; i++) {
    StatusBarNotification notification=mLatest.get(i);
    View v=notification.view;
    if (v == view) {
      return notification;
    }
  }
  return null;
}
