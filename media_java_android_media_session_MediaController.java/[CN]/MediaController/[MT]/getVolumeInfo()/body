{
  try {
    ParcelableVolumeInfo result=mSessionBinder.getVolumeAttributes();
    return new VolumeInfo(result.volumeType,result.audioStream,result.controlType,result.maxVolume,result.currentVolume);
  }
 catch (  RemoteException e) {
    Log.wtf(TAG,"Error calling getVolumeInfo.",e);
  }
  return null;
}
