  builder.append(String.format(formatWithUncertainty,"ElevationInDeg",mHasElevationInDeg ? mElevationInDeg : null,"ElevationUncertaintyInDeg",mHasElevationUncertaintyInDeg ? mElevationUncertaintyInDeg : null));
