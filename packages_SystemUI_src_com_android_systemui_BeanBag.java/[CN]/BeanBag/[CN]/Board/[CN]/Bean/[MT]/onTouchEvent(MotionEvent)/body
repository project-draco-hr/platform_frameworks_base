{
switch (e.getAction()) {
case MotionEvent.ACTION_DOWN:
    grabbed=true;
  va=0;
case MotionEvent.ACTION_MOVE:
grabx=e.getRawX();
graby=e.getRawY();
grabtime=e.getEventTime();
break;
case MotionEvent.ACTION_CANCEL:
case MotionEvent.ACTION_UP:
grabbed=false;
va=randfrange(-5,5);
break;
}
return true;
}
