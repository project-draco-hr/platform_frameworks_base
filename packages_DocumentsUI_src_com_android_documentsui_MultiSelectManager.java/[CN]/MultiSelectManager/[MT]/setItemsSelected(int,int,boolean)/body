{
  boolean changed=false;
  for (int i=position; i < position + length; i++) {
    boolean itemChanged=selected ? mSelection.add(i) : mSelection.remove(i);
    if (itemChanged) {
      notifyItemStateChanged(i,selected);
    }
    changed|=itemChanged;
  }
  notifySelectionChanged();
  return changed;
}
