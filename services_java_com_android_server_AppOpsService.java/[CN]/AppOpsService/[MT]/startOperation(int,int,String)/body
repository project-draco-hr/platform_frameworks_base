{
  uid=handleIncomingUid(uid);
synchronized (this) {
    Op op=getOpLocked(code,uid,packageName,true);
    if (op == null) {
      return AppOpsManager.MODE_IGNORED;
    }
    if (op.nesting == 0) {
      op.time=System.currentTimeMillis();
      op.duration=-1;
    }
    op.nesting++;
  }
  return AppOpsManager.MODE_ALLOWED;
}
