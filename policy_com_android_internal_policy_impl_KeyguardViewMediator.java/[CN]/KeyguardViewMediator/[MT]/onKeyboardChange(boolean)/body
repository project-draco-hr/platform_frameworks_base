{
  mKeyboardOpen=isKeyboardOpen;
  if (mUpdateMonitor.isKeyguardBypassEnabled() && mKeyboardOpen && !mKeyguardViewProperties.isSecure()&& mKeyguardViewManager.isShowing()) {
    if (DEBUG)     Log.d(TAG,"bypassing keyguard on sliding open of keyboard with non-secure keyguard");
    keyguardDone(true);
  }
}
