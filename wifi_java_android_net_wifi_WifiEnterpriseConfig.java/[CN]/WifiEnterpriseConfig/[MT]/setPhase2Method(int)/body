{
switch (phase2Method) {
case Phase2.NONE:
    mFields.put(PHASE2_KEY,EMPTY_VALUE);
  break;
case Phase2.PAP:
case Phase2.MSCHAP:
case Phase2.MSCHAPV2:
case Phase2.GTC:
mFields.put(PHASE2_KEY,convertToQuotedString(Phase2.PREFIX + Phase2.strings[phase2Method]));
break;
default :
throw new IllegalArgumentException("Unknown Phase 2 method");
}
}
