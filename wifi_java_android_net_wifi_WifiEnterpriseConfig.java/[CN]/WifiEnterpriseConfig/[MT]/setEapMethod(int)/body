{
switch (eapMethod) {
case Eap.TLS:
    setPhase2Method(Phase2.NONE);
case Eap.PEAP:
case Eap.PWD:
case Eap.TTLS:
case Eap.SIM:
case Eap.AKA:
case Eap.AKA_PRIME:
  mFields.put(EAP_KEY,Eap.strings[eapMethod]);
mFields.put(OPP_KEY_CACHING,"1");
break;
default :
throw new IllegalArgumentException("Unknown EAP method");
}
}
