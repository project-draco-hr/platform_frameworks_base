{
  mFields.put(EAP_KEY,Eap.strings[Eap.PEAP]);
  mFields.put(ENGINE_KEY,ENGINE_DISABLE);
  for (  String key : new String[]{PHASE2_KEY,IDENTITY_KEY,ANON_IDENTITY_KEY,PASSWORD_KEY,CLIENT_CERT_KEY,ENGINE_ID_KEY,PRIVATE_KEY_ID_KEY,CA_CERT_KEY,SUBJECT_MATCH_KEY}) {
    mFields.put(key,EMPTY_VALUE);
  }
}
