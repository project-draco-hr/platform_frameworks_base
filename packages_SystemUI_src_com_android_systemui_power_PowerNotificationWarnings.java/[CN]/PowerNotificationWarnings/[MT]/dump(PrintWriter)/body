{
  pw.print("mSaver=");
  pw.println(mSaver);
  pw.print("mWarning=");
  pw.println(mWarning);
  pw.print("mPlaySound=");
  pw.println(mPlaySound);
  pw.print("mInvalidCharger=");
  pw.println(mInvalidCharger);
  pw.print("mShowing=");
  pw.println(SHOWING_STRINGS[mShowing]);
  pw.print("mSaverConfirmation=");
  pw.println(mSaverConfirmation != null ? "not null" : null);
}
