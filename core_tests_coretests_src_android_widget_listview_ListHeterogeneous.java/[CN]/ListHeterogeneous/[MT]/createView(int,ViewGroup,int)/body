{
switch (position % 3) {
case 0:
    return ListItemFactory.text(position,parent.getContext(),getValueAtPosition(position),desiredHeight);
case 1:
  return ListItemFactory.button(position,parent.getContext(),getValueAtPosition(position),desiredHeight);
case 2:
return ListItemFactory.doubleText(position,parent.getContext(),getValueAtPosition(position),desiredHeight);
}
return null;
}
