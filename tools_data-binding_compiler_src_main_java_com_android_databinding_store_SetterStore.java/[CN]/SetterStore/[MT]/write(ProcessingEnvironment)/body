{
  Filer filer=processingEnvironment.getFiler();
  FileObject resource=filer.createResource(StandardLocation.CLASS_OUTPUT,SetterStore.class.getPackage().getName(),"setter_store.bin");
  processingEnvironment.getMessager().printMessage(Diagnostic.Kind.NOTE,"============= Writing intermediate file: " + resource.getName());
  ObjectOutputStream out=null;
  try {
    out=new ObjectOutputStream(resource.openOutputStream());
    processingEnvironment.getMessager().printMessage(Diagnostic.Kind.NOTE,"============= adapters: " + mAdapters);
    out.writeObject(mAdapters);
    out.writeObject(mRenamed);
    out.writeObject(mConversions);
  }
  finally {
    if (out != null) {
      out.close();
    }
  }
}
