{
  int colonIndex=attribute.indexOf(':');
  String propertyName;
  if (colonIndex >= 0 && colonIndex + 1 < attribute.length()) {
    propertyName=Character.toUpperCase(attribute.charAt(colonIndex + 1)) + attribute.substring(colonIndex + 2);
  }
 else {
    propertyName="";
  }
  return "set" + propertyName;
}
