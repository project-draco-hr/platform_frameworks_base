{
  if (sStore == null) {
    InputStream in=null;
    try {
      Filer filer=processingEnvironment.getFiler();
      FileObject resource=filer.getResource(StandardLocation.CLASS_OUTPUT,SetterStore.class.getPackage().getName(),SETTER_STORE_FILE_NAME);
      if (resource != null && new File(resource.getName()).exists()) {
        in=resource.openInputStream();
        if (in != null) {
          sStore=load(in);
        }
      }
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
catch (    ClassNotFoundException e) {
      e.printStackTrace();
    }
 finally {
      if (in != null) {
        try {
          in.close();
        }
 catch (        IOException e) {
          e.printStackTrace();
        }
      }
    }
    if (sStore == null) {
      sStore=new SetterStore(null,new IntermediateV1());
    }
  }
  return sStore;
}
