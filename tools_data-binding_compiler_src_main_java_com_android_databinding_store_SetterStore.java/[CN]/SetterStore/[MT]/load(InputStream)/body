{
  ObjectInputStream in=new ObjectInputStream(inputStream);
  HashMap<String,HashMap<AccessorKey,MethodDescription>> adapters=(HashMap<String,HashMap<AccessorKey,MethodDescription>>)in.readObject();
  HashMap<String,HashMap<String,MethodDescription>> renamedMethods=(HashMap<String,HashMap<String,MethodDescription>>)in.readObject();
  HashMap<String,HashMap<String,MethodDescription>> conversionMethods=(HashMap<String,HashMap<String,MethodDescription>>)in.readObject();
  return new SetterStore(null,adapters,renamedMethods,conversionMethods);
}
