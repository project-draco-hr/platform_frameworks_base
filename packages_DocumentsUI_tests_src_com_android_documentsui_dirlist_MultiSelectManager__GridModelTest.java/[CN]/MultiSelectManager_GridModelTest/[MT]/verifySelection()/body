{
  Rect selectionArea=getSelectionArea();
  for (  TestEnvironment.Item item : env.items) {
    if (Rect.intersects(selectionArea,item.rect)) {
      assertTrue("Expected item " + item + " was not in selection "+ selectionArea,lastSelection.contains(item.name));
    }
 else {
      assertFalse("Unexpected item " + item + " in selection"+ selectionArea,lastSelection.contains(item.name));
    }
  }
}
