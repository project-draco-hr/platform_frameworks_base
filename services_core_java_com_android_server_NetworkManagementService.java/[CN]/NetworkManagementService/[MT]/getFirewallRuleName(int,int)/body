{
  String ruleName;
  if (getFirewallType(chain) == FIREWALL_TYPE_WHITELIST) {
    if (rule == NetworkPolicyManager.FIREWALL_RULE_ALLOW) {
      ruleName="allow";
    }
 else {
      ruleName="deny";
    }
  }
 else {
    if (rule == NetworkPolicyManager.FIREWALL_RULE_DENY) {
      ruleName="deny";
    }
 else {
      ruleName="allow";
    }
  }
  return ruleName;
}
