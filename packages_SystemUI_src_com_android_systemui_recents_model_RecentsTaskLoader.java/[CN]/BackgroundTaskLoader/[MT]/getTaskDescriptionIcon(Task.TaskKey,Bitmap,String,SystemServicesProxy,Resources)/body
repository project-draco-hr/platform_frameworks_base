{
  Bitmap tdIcon=null;
  if (iconBitmap != null) {
    tdIcon=iconBitmap;
  }
 else {
    try {
      tdIcon=ActivityManager.TaskDescription.loadTaskDescriptionIcon(iconFilename,taskKey.userId);
    }
 catch (    Exception e) {
    }
  }
  if (tdIcon != null) {
    return ssp.getBadgedIcon(new BitmapDrawable(res,tdIcon),taskKey.userId);
  }
  return null;
}
