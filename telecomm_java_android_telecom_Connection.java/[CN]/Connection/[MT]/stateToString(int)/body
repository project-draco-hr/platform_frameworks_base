{
switch (state) {
case STATE_INITIALIZING:
    return "INITIALIZING";
case STATE_NEW:
  return "NEW";
case STATE_RINGING:
return "RINGING";
case STATE_DIALING:
return "DIALING";
case STATE_ACTIVE:
return "ACTIVE";
case STATE_HOLDING:
return "HOLDING";
case STATE_DISCONNECTED:
return "DISCONNECTED";
default :
Log.wtf(Connection.class,"Unknown state %d",state);
return "UNKNOWN";
}
}
