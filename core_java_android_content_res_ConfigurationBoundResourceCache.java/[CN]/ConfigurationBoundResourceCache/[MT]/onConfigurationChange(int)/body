{
synchronized (this) {
    final int size=mCache.size();
    for (int i=size - 1; i >= 0; i--) {
      final LongSparseArray<WeakReference<ConstantState<T>>> themeCache=mCache.valueAt(i);
      onConfigurationChangeInt(themeCache,configChanges);
      if (themeCache.size() == 0) {
        mCache.removeAt(i);
      }
    }
  }
}
