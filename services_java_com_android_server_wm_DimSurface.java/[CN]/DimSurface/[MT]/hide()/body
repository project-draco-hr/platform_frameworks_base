{
  if (mDimShown) {
    mDimShown=false;
    try {
      if (WindowManagerService.SHOW_TRANSACTIONS)       Slog.i(WindowManagerService.TAG,"  HIDE " + mDimSurface);
      mDimSurface.hide();
    }
 catch (    RuntimeException e) {
      Slog.w(WindowManagerService.TAG,"Illegal argument exception hiding dim surface");
    }
  }
}
