{
  if (isStarted()) {
    return;
  }
  if (mAnimatedVectorState.mPendingAnims != null) {
    if (mRes != null) {
      inflatePendingAnimators(mRes,null);
      mRes=null;
    }
 else {
      Log.e(LOGTAG,"Failed to load animators. Either the AnimatedVectorDrawable must be" + " created using a Resources object or applyTheme() must be called with" + " a non-null Theme object.");
    }
    mAnimatedVectorState.mPendingAnims=null;
  }
  mAnimatedVectorState.mAnimatorSet.start();
  invalidateSelf();
}
