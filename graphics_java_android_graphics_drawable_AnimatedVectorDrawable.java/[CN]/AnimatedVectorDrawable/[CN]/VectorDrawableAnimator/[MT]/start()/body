{
  if (!mInitialized) {
    return;
  }
  if (mStarted) {
    return;
  }
  if (!useLastSeenTarget()) {
    mAnimationPending=true;
    return;
  }
  if (DBG_ANIMATION_VECTOR_DRAWABLE) {
    Log.d(LOGTAG,"Target is set. Starting VDAnimatorSet from java");
  }
  nStart(mSetPtr,this);
  if (mListener != null) {
    mListener.onAnimationStart(null);
  }
  mStarted=true;
}
