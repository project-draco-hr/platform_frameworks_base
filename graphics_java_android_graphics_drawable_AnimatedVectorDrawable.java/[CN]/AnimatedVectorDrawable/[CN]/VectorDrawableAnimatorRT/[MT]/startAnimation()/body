{
  if (DBG_ANIMATION_VECTOR_DRAWABLE) {
    Log.w(LOGTAG,"starting animation on VD: " + ((VectorDrawable.VectorDrawableState)((AnimatedVectorDrawableState)mDrawable.getConstantState()).mVectorDrawable.getConstantState()).mRootName);
  }
  mStarted=true;
  nStart(mSetPtr,this,++mLastListenerId);
  invalidateOwningView();
  if (mListener != null) {
    mListener.onAnimationStart(null);
  }
}
