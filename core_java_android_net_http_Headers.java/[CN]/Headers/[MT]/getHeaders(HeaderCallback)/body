{
  for (int i=0; i < HEADER_COUNT; i++) {
    String h=mHeaders[i];
    if (h != null) {
      hcb.header(sHeaderNames[i],h);
    }
  }
  int extraLen=mExtraHeaderNames.size();
  for (int i=0; i < extraLen; i++) {
    if (Config.LOGV) {
      HttpLog.v("Headers.getHeaders() extra: " + i + " "+ mExtraHeaderNames.get(i)+ " "+ mExtraHeaderValues.get(i));
    }
    hcb.header(mExtraHeaderNames.get(i),mExtraHeaderValues.get(i));
  }
}
