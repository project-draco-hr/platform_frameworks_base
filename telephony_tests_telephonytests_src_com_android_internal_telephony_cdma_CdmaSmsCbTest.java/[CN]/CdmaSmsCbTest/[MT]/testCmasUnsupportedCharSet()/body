{
  SmsMessage msg=createCmasSmsMessage(SmsEnvelope.SERVICE_CATEGORY_CMAS_EXTREME_THREAT,12345,BearerData.PRIORITY_EMERGENCY,BearerData.LANGUAGE_ENGLISH,UserData.ENCODING_GSM_DCS,EXTREME_ALERT,-1,-1,-1,-1,-1);
  SmsCbMessage cbMessage=msg.parseBroadcastSms();
  assertNull("expected null for unsupported charset",cbMessage);
}
