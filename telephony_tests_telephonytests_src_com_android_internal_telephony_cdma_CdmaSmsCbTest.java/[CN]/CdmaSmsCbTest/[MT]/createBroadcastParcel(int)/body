{
  Parcel p=Parcel.obtain();
  p.writeInt(SmsEnvelope.TELESERVICE_NOT_SET);
  p.writeByte((byte)1);
  p.writeInt(serviceCategory);
  p.writeInt(CdmaSmsAddress.DIGIT_MODE_4BIT_DTMF);
  p.writeInt(CdmaSmsAddress.NUMBER_MODE_NOT_DATA_NETWORK);
  p.writeInt(CdmaSmsAddress.TON_UNKNOWN);
  p.writeInt(CdmaSmsAddress.NUMBERING_PLAN_ISDN_TELEPHONY);
  p.writeByte((byte)0);
  p.writeInt((byte)0);
  p.writeByte((byte)0);
  p.writeByte((byte)0);
  return p;
}
