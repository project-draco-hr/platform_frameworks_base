{
  T result=null;
  for (  Element element : roundEnv.getElementsAnnotatedWith(annotationClass)) {
    final T info=element.getAnnotation(annotationClass);
    if (info == null) {
      continue;
    }
    Preconditions.checkState(result == null,"Should have only one %s",annotationClass.getCanonicalName());
    result=info;
  }
  return result;
}
