{
  if (keyCode == KeyEvent.KEYCODE_BACK) {
    if (event.getAction() == KeyEvent.ACTION_DOWN) {
      return true;
    }
 else     if (event.getAction() == KeyEvent.ACTION_UP && !event.isCanceled()) {
      VideoPlayer.exitFullScreenVideo(this,mWebView);
      return true;
    }
  }
  return false;
}
