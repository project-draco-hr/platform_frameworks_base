{
  Set<String> blacklist=StatusBarIconController.getIconBlacklist(getContext());
  if (!value) {
    if (!blacklist.contains(getKey())) {
      blacklist.add(getKey());
      setList(blacklist);
    }
  }
 else {
    if (blacklist != null && blacklist.remove(getKey())) {
      setList(blacklist);
    }
  }
  return true;
}
