{
  WeakReference weakSelf=(WeakReference)selfRef;
  final ImageReader ir=(ImageReader)weakSelf.get();
  if (ir == null) {
    return;
  }
  if (ir.mHandler != null) {
    ir.mHandler.post(new Runnable(){
      @Override public void run(){
        ir.mImageListener.onImageAvailable(ir);
      }
    }
);
  }
}
