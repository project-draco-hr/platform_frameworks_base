{
  if (dataType == 1)   return "1x";
  if (dataType == 2)   return "3g";
  if (dataType == 3)   return "4g";
  if (dataType == 4)   return "e";
  if (dataType == 5)   return "g";
  if (dataType == 6)   return "h";
  if (dataType == 7)   return "lte";
  if (dataType == 8)   return "roam";
  return "";
}
