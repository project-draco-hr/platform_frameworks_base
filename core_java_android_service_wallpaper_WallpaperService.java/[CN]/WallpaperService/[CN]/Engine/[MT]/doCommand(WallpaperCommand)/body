{
  Bundle result;
  if (!mDestroyed) {
    result=onCommand(cmd.action,cmd.x,cmd.y,cmd.z,cmd.extras,cmd.sync);
  }
 else {
    result=null;
  }
  if (cmd.sync) {
    try {
      if (DEBUG)       Log.v(TAG,"Reporting command complete");
      mSession.wallpaperCommandComplete(mWindow.asBinder(),result);
    }
 catch (    RemoteException e) {
    }
  }
}
