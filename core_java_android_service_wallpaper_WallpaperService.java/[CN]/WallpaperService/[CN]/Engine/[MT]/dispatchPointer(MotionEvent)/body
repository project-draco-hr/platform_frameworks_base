{
  if (event.isTouchEvent()) {
synchronized (mLock) {
      if (event.getAction() == MotionEvent.ACTION_MOVE) {
        mPendingMove=event;
      }
 else {
        mPendingMove=null;
      }
    }
    Message msg=mCaller.obtainMessageO(MSG_TOUCH_EVENT,event);
    mCaller.sendMessage(msg);
  }
 else {
    event.recycle();
  }
}
