{
switch (message.what) {
case DO_ATTACH:
{
      Engine engine=onCreateEngine();
      mEngine=engine;
      engine.attach(this);
      return;
    }
case DO_DETACH:
{
    mEngine.detach();
    return;
  }
case MSG_UPDATE_SURFACE:
mEngine.updateSurface(false);
break;
default :
Log.w(TAG,"Unknown message type " + message.what);
}
}
