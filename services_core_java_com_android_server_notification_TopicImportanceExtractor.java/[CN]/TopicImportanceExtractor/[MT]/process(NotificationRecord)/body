{
  if (record == null || record.getNotification() == null) {
    if (DBG)     Slog.d(TAG,"skipping empty notification");
    return null;
  }
  if (mConfig == null) {
    if (DBG)     Slog.d(TAG,"missing config");
    return null;
  }
  final int topicImportance=mConfig.getTopicImportance(record.sbn.getPackageName(),record.sbn.getUid(),record.sbn.getNotification().getTopic());
  record.setTopicImportance(topicImportance);
  return null;
}
