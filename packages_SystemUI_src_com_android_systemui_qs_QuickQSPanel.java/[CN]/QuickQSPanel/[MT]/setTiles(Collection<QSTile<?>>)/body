{
  ArrayList<QSTile<?>> quickTiles=new ArrayList<>();
  for (  QSTile<?> tile : tiles) {
    if (tile.getTileType() == QSTileView.QS_TYPE_QUICK) {
      Log.d("QSPanel","Adding " + tile.getTileSpec());
      quickTiles.add(tile);
    }
    if (quickTiles.size() == 2) {
      break;
    }
  }
  super.setTiles(quickTiles);
}
