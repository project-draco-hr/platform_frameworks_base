{
  try {
    Bridge.prepareThread();
    mLastResult=mSession.acquire(timeout);
    if (mLastResult.isSuccess()) {
      if (forceMeasure) {
        mSession.invalidateRenderingSize();
      }
      mLastResult=mSession.render(false);
    }
  }
  finally {
    mSession.release();
    Bridge.cleanupThread();
  }
  return mLastResult;
}
