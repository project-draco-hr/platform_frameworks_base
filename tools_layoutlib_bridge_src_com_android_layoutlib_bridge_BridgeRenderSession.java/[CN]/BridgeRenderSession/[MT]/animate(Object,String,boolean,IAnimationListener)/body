{
  try {
    Bridge.prepareThread();
    mLastResult=mSession.acquire(RenderParams.DEFAULT_TIMEOUT);
    if (mLastResult.isSuccess()) {
      mLastResult=mSession.animate(targetObject,animationName,isFrameworkAnimation,listener);
    }
  }
  finally {
    mSession.release();
    Bridge.cleanupThread();
  }
  return mLastResult;
}
