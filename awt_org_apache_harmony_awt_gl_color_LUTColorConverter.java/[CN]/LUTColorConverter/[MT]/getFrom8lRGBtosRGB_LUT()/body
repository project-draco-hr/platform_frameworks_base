{
  if (from8lRGBtosRGB_LUT == null) {
    from8lRGBtosRGB_LUT=new byte[256];
    float v;
    for (int i=0; i < 256; i++) {
      v=(float)i / 255;
      v=(v <= 0.04045f) ? v / 12.92f : (float)Math.pow((v + 0.055) / 1.055,2.4);
      from8lRGBtosRGB_LUT[i]=(byte)Math.round(v * 255.0f);
    }
  }
  return from8lRGBtosRGB_LUT;
}
