{
  if (fromsRGBto8lRGB_LUT == null) {
    fromsRGBto8lRGB_LUT=new byte[256];
    float v;
    for (int i=0; i < 256; i++) {
      v=(float)i / 255;
      v=(v <= 0.0031308f) ? v * 12.92f : ((float)Math.pow(v,1.0 / 2.4)) * 1.055f - 0.055f;
      fromsRGBto8lRGB_LUT[i]=(byte)Math.round(v * 255.0f);
    }
  }
  return fromsRGBto8lRGB_LUT;
}
