{
switch (state) {
case STATE_NEW:
    return "NEW";
case STATE_RINGING:
  return "RINGING";
case STATE_DIALING:
return "DIALING";
case STATE_ACTIVE:
return "ACTIVE";
case STATE_HOLDING:
return "HOLDING";
case STATE_DISCONNECTED:
return "DISCONNECTED";
case STATE_CONNECTING:
return "CONNECTING";
case STATE_DISCONNECTING:
return "DISCONNECTING";
case STATE_SELECT_PHONE_ACCOUNT:
return "SELECT_PHONE_ACCOUNT";
default :
Log.w(Call.class,"Unknown state %d",state);
return "UNKNOWN";
}
}
