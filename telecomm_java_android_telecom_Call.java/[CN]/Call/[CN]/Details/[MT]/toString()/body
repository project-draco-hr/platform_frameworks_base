{
  StringBuilder sb=new StringBuilder();
  sb.append("[pa: ");
  sb.append(mAccountHandle);
  sb.append(", hdl: ");
  sb.append(Log.pii(mHandle));
  sb.append(", caps: ");
  sb.append(capabilitiesToString(mCallCapabilities));
  sb.append(", props: ");
  sb.append(propertiesToString(mCallProperties));
  sb.append("]");
  return sb.toString();
}
