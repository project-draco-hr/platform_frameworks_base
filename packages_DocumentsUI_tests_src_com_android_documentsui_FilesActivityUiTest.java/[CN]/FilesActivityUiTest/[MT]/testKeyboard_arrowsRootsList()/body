{
  bot.pressKey(KeyEvent.KEYCODE_TAB);
  for (int i=0; i < 10; i++) {
    bot.pressKey(KeyEvent.KEYCODE_DPAD_RIGHT);
    bot.assertHasFocus("com.android.documentsui:id/roots_list");
  }
  for (int i=0; i < 10; i++) {
    bot.pressKey(KeyEvent.KEYCODE_DPAD_LEFT);
    bot.assertHasFocus("com.android.documentsui:id/roots_list");
  }
}
