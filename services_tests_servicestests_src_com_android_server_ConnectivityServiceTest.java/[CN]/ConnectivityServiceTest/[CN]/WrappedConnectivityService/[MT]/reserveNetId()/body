{
  while (true) {
    final int netId=super.reserveNetId();
    final Network[] networks=ConnectivityManager.from(getContext()).getAllNetworks();
    boolean overlaps=false;
    for (    Network network : networks) {
      if (netId == network.netId) {
        overlaps=true;
        break;
      }
    }
    if (overlaps)     continue;
    return netId;
  }
}
