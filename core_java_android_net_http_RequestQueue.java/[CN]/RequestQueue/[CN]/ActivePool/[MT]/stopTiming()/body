{
  int totalTime=0;
  for (int i=0; i < mConnectionCount; i++) {
    ConnectionThread rt=mThreads[i];
    totalTime+=(rt.mCurrentThreadTime - rt.mStartThreadTime);
    rt.mStartThreadTime=-1;
  }
  Log.d("Http","Http thread used " + totalTime + " ms "+ " for "+ mTotalRequest+ " requests and "+ mTotalConnection+ " connections");
}
