{
  HttpHost host=request.mHost;
  LinkedList<Request> reqList;
  if (mPending.containsKey(host)) {
    reqList=mPending.get(host);
  }
 else {
    reqList=new LinkedList<Request>();
    mPending.put(host,reqList);
  }
  if (head) {
    reqList.addFirst(request);
  }
 else {
    reqList.add(request);
  }
}
