{
synchronized (mLock) {
    mEngine=engine;
    if (mDimensionsChanged) {
      try {
        mEngine.setDesiredSize(mWallpaper.width,mWallpaper.height);
      }
 catch (      RemoteException e) {
        Slog.w(TAG,"Failed to set wallpaper dimensions",e);
      }
      mDimensionsChanged=false;
    }
  }
}
