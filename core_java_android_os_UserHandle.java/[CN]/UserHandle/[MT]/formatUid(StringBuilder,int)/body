{
  if (uid < Process.FIRST_APPLICATION_UID) {
    sb.append(uid);
  }
 else {
    sb.append('u');
    sb.append(getUserId(uid));
    final int appId=getAppId(uid);
    if (appId >= Process.FIRST_ISOLATED_UID && appId <= Process.LAST_ISOLATED_UID) {
      sb.append('i');
      sb.append(appId - Process.FIRST_ISOLATED_UID);
    }
 else {
      sb.append('a');
      sb.append(appId);
    }
  }
}
