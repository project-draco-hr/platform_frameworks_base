{
  MenuItemImpl itemImpl=(MenuItemImpl)item;
  if (itemImpl == null || !itemImpl.isEnabled()) {
    return false;
  }
  boolean invoked=itemImpl.invoke();
  if (item.hasSubMenu()) {
    close(false);
    if (mCallback != null) {
      invoked=mCallback.onSubMenuSelected((SubMenuBuilder)item.getSubMenu()) || invoked;
    }
  }
 else {
    if ((flags & FLAG_PERFORM_NO_CLOSE) == 0) {
      close(true);
    }
  }
  return invoked;
}
