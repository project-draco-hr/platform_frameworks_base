{
  OverflowMenuAdapter adapter=mOverflowAdapterCache[menuType] == null ? null : mOverflowAdapterCache[menuType].get();
  if (adapter != null)   return adapter;
  adapter=new OverflowMenuAdapter(menuType);
  mOverflowAdapterCache[menuType]=new WeakReference<OverflowMenuAdapter>(adapter);
  return adapter;
}
