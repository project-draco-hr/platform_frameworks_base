{
  long numEntries=stats.numEntries;
  long numDeletedEntries=stats.numDeletes;
  long percentDeleted=(numDeletedEntries == 0) ? 0 : (100 * numDeletedEntries / (numEntries + numDeletedEntries));
  boolean tooManyDeletions=(numDeletedEntries > NUM_ALLOWED_SIMULTANEOUS_DELETIONS) && (percentDeleted > PERCENT_ALLOWED_SIMULTANEOUS_DELETIONS);
  return tooManyDeletions;
}
