{
  if (conn.owner != this) {
    throw new CallStateException("Connection " + conn + "does not belong to CallTracker "+ this);
  }
  try {
    cm.separateConnection(conn.getGSMIndex(),obtainCompleteMessage(EVENT_SEPARATE_RESULT));
  }
 catch (  CallStateException ex) {
    Log.w(LOG_TAG,"CallTracker WARN: separate() on absent connection " + conn);
  }
}
