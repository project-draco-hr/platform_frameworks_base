{
  try {
    int crashSize=values.getAsString(Checkin.Crashes.DATA).length();
    if (crashSize > CRASH_SIZE) {
      Checkin.updateStats(getContext().getContentResolver(),Checkin.Stats.Tag.CRASHES_TRUNCATED,1,0.0);
      throw new IllegalArgumentException("Too big: " + crashSize);
    }
    Checkin.updateStats(getContext().getContentResolver(),Checkin.Stats.Tag.CRASHES_REPORTED,1,0.0);
    SQLiteDatabase db=mOpenHelper.getWritableDatabase();
    db.execSQL("DELETE FROM " + Checkin.Crashes.TABLE_NAME + " WHERE "+ Checkin.Crashes._ID+ " IN (SELECT "+ Checkin.Crashes._ID+ " FROM "+ Checkin.Crashes.TABLE_NAME+ " ORDER BY "+ Checkin.Crashes._ID+ " DESC LIMIT -1 OFFSET "+ (CRASH_LIMIT - 1)+ ")");
    return db.insert(Checkin.Crashes.TABLE_NAME,null,values);
  }
 catch (  Throwable t) {
    Log.e("CheckinProvider","Error inserting crash: " + t);
    return -1;
  }
}
