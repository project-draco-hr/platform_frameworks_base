{
  if (uri.getPathSegments().size() < 1) {
    throw new IllegalArgumentException("Invalid query URI: " + uri);
  }
  String table=uri.getPathSegments().get(0);
  if (table.equals(Checkin.Properties.TABLE_NAME) && getContext().checkCallingOrSelfPermission(PROPERTIES_PERMISSION) != PackageManager.PERMISSION_GRANTED) {
    throw new SecurityException("Cannot access checkin properties");
  }
}
