{
  try {
    for (    ActivityManager.RunningAppProcessInfo proc : ActivityManagerNative.getDefault().getRunningAppProcesses()) {
      if (proc.pid == pid) {
        return proc.pkgList;
      }
    }
  }
 catch (  RemoteException e) {
    Slog.w(TAG,"ActivityManager.getRunningAppProcesses() failed");
  }
  return null;
}
