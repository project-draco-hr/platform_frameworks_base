{
  String hostIp=getHostIp();
  AndroidServiceProxy ipsecService=startService(IPSEC_SERVICE);
  ipsecService.sendCommand(String.format("SETKEY %s %s",hostIp,serverIp));
  ipsecService.sendCommand(String.format("SET_CERTS %s %s %s %s",serverIp,getCaCertPath(),getUserCertPath(),getUserkeyPath()));
  AndroidServiceProxy l2tpService=startService(L2TP_SERVICE);
  l2tpService.sendCommand2("l2tp",serverIp,"1701","","file",getPppOptionFilePath(),"name",username,"password",password);
}
