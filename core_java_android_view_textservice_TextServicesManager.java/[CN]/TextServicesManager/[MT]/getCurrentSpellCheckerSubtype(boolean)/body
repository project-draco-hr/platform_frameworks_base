{
  try {
    if (sService == null) {
      Log.e(TAG,"sService is null.");
      return null;
    }
    return sService.getCurrentSpellCheckerSubtype(null,allowImplicitlySelectedSubtype);
  }
 catch (  RemoteException e) {
    Log.e(TAG,"Error in getCurrentSpellCheckerSubtype: " + e);
    return null;
  }
}
