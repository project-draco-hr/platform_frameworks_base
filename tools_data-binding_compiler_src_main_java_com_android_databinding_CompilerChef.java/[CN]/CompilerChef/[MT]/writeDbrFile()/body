{
  ensureDataBinder();
  final String pkg="com.android.databinding.library";
  DataBinderWriter dbr=new DataBinderWriter(pkg,mResourceBundle.getAppPackage(),"GeneratedDataBinderRenderer",mDataBinder.getLayoutBinders());
  if (dbr.getLayoutBinders().size() > 0) {
    mFileWriter.writeToFile(dbr.getPkg() + "." + dbr.getClassName(),dbr.write());
  }
}
