{
synchronized (this) {
    if (!mIsCanceled) {
      mIsCanceled=true;
      if (mOnCancelListener != null) {
        mOnCancelListener.onCancel();
      }
      if (mRemote != null) {
        try {
          mRemote.cancel();
        }
 catch (        RemoteException ex) {
        }
      }
    }
  }
}
