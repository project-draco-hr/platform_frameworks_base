{
  xml.startTag(null,PACKAGE_TAG);
  XmlUtils.writeLongAttribute(xml,LAST_TIME_ACTIVE_ATTR,usageStats.mLastTimeUsed - stats.beginTime);
  XmlUtils.writeLongAttribute(xml,LAST_TIME_ACTIVE_SYSTEM_ATTR,usageStats.mLastTimeSystemUsed - stats.beginTime);
  XmlUtils.writeStringAttribute(xml,PACKAGE_ATTR,usageStats.mPackageName);
  XmlUtils.writeLongAttribute(xml,TOTAL_TIME_ACTIVE_ATTR,usageStats.mTotalTimeInForeground);
  XmlUtils.writeIntAttribute(xml,LAST_EVENT_ATTR,usageStats.mLastEvent);
  XmlUtils.writeLongAttribute(xml,BEGIN_IDLE_TIME_ATTR,usageStats.mBeginIdleTime);
  xml.endTag(null,PACKAGE_TAG);
}
