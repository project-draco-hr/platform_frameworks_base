{
  pw.println("    NextAlarmConditionProvider:");
  pw.print("      mConnected=");
  pw.println(mConnected);
  pw.print("      mLookaheadThreshold=");
  pw.print(mLookaheadThreshold);
  pw.print(" (");
  TimeUtils.formatDuration(mLookaheadThreshold,pw);
  pw.println(")");
  pw.print("      mCurrentSubscription=");
  pw.println(mCurrentSubscription);
}
