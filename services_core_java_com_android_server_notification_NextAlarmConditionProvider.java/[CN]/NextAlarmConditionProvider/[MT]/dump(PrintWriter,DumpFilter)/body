{
  pw.println("    NextAlarmConditionProvider:");
  pw.print("      mConnected=");
  pw.println(mConnected);
  pw.print("      mBootCompleted=");
  pw.println(mBootCompleted);
  pw.print("      mRegistered=");
  pw.println(mRegistered);
  pw.print("      mCurrentUserId=");
  pw.println(mCurrentUserId);
  pw.print("      mScheduledAlarmTime=");
  pw.println(formatAlarmDebug(mScheduledAlarmTime));
  pw.print("      mLookaheadThreshold=");
  pw.print(mLookaheadThreshold);
  pw.print(" (");
  TimeUtils.formatDuration(mLookaheadThreshold,pw);
  pw.println(")");
  pw.print("      mCurrentSubscription=");
  pw.println(mCurrentSubscription);
  pw.print("      mWakeLock=");
  pw.println(mWakeLock);
}
