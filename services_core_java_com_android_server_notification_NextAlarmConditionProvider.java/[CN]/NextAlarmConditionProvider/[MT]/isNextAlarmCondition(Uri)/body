{
  return conditionId != null && conditionId.getScheme().equals(Condition.SCHEME) && conditionId.getAuthority().equals(ZenModeConfig.SYSTEM_AUTHORITY) && conditionId.getPathSegments().size() == 2 && conditionId.getPathSegments().get(0).equals(NEXT_ALARM_PATH) && conditionId.getPathSegments().get(1).equals(Integer.toString(mTracker.getCurrentUserId()));
}
