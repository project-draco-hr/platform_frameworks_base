{
  return conditionId != null && conditionId.getScheme().equals(Condition.SCHEME) && conditionId.getAuthority().equals(ZenModeConfig.SYSTEM_AUTHORITY) && conditionId.getPathSegments().size() == 3 && conditionId.getPathSegments().get(0).equals(ZenModeConfig.NEXT_ALARM_PATH) && conditionId.getPathSegments().get(1).equals(Integer.toString(mTracker.getCurrentUserId())) ? tryParseLong(conditionId.getPathSegments().get(2),BAD_CONDITION) : BAD_CONDITION;
}
