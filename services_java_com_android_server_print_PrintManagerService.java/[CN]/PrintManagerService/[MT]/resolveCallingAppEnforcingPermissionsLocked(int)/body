{
  final int callingUid=Binder.getCallingUid();
  if (callingUid == 0 || callingUid == Process.SYSTEM_UID || callingUid == Process.SHELL_UID) {
    return appId;
  }
  final int callingAppId=UserHandle.getAppId(callingUid);
  if (appId == callingAppId) {
    return appId;
  }
  if (mContext.checkCallingPermission(Manifest.permission.ACCESS_ALL_PRINT_JOBS) != PackageManager.PERMISSION_GRANTED) {
    throw new SecurityException("Call from app " + callingAppId + " as app "+ appId+ " without permission INTERACT_ACROSS_APPS");
  }
  return appId;
}
