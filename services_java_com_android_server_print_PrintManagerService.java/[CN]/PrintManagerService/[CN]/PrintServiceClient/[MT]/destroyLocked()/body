{
  if (mServices.remove(mComponentName) == null) {
    return;
  }
  if (isConnectedLocked()) {
    try {
      mInterface.asBinder().unlinkToDeath(this,0);
    }
 catch (    NoSuchElementException nse) {
    }
    try {
      mInterface.setClient(null);
    }
 catch (    RemoteException re) {
    }
    mInterface=null;
  }
  mBinding=false;
}
