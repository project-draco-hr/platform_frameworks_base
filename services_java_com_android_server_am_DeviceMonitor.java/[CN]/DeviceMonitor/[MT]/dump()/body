{
  OutputStream out=new FileOutputStream(new File(BASE,String.valueOf(System.currentTimeMillis())));
  try {
    for (    File processDirectory : PROC.listFiles()) {
      if (isProcessDirectory(processDirectory)) {
        dump(new File(processDirectory,"stat"),out);
      }
    }
    for (    File file : PATHS) {
      dump(file,out);
    }
  }
  finally {
    closeQuietly(out);
  }
}
