{
  final List<UserInfo> users=getUsers(true);
  final int totalUserCount=users.size();
  int aliveUserCount=0;
  for (int i=0; i < totalUserCount; i++) {
    UserInfo user=users.get(i);
    if (!user.isGuest()) {
      aliveUserCount++;
    }
  }
  return aliveUserCount < getMaxSupportedUsers();
}
