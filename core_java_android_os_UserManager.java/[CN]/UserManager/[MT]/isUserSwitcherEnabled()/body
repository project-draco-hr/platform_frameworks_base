{
  List<UserInfo> users=getUsers(true);
  if (users == null) {
    return false;
  }
  int switchableUserCount=0;
  for (  UserInfo user : users) {
    if (user.supportsSwitchTo()) {
      ++switchableUserCount;
    }
  }
  final boolean guestEnabled=Settings.Global.getInt(mContext.getContentResolver(),Settings.Global.GUEST_USER_ENABLED,0) == 1;
  return switchableUserCount > 1 || guestEnabled;
}
