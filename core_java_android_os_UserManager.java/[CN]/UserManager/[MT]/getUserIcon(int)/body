{
  try {
    ParcelFileDescriptor fd=mService.getUserIcon(userHandle);
    if (fd != null) {
      try {
        return BitmapFactory.decodeFileDescriptor(fd.getFileDescriptor());
      }
  finally {
        try {
          fd.close();
        }
 catch (        IOException e) {
        }
      }
    }
  }
 catch (  RemoteException re) {
    Log.w(TAG,"Could not get the user icon ",re);
  }
  return null;
}
