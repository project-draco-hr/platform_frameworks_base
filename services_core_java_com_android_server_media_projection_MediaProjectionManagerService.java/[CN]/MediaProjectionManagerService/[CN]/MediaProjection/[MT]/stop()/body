{
synchronized (mLock) {
    if (!isValidMediaProjection(asBinder())) {
      Slog.w(TAG,"Attempted to stop inactive MediaProjection " + "(uid=" + Binder.getCallingUid() + ", "+ "pid="+ Binder.getCallingPid()+ ")");
      return;
    }
    mToken.unlinkToDeath(mDeathEater,0);
    stopProjectionLocked(this);
  }
}
