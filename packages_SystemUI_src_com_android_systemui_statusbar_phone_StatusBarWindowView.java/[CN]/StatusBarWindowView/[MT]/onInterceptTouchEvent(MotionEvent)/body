{
  boolean intercept=false;
  if (mNotificationPanel.isFullyExpanded() && mStackScrollLayout.getVisibility() == View.VISIBLE && (mService.getBarState() == StatusBarState.SHADE || (mService.getBarState() == StatusBarState.SHADE_LOCKED && !mService.isBouncerShowing()))) {
    intercept=mExpandHelper.onInterceptTouchEvent(ev);
  }
 else   if (mNotificationPanel.isFullyExpanded() && mStackScrollLayout.getVisibility() == View.VISIBLE && mService.getBarState() == StatusBarState.KEYGUARD && !mService.isBouncerShowing()) {
    intercept=mDragDownHelper.onInterceptTouchEvent(ev);
  }
  if (!intercept) {
    super.onInterceptTouchEvent(ev);
  }
  if (intercept) {
    MotionEvent cancellation=MotionEvent.obtain(ev);
    cancellation.setAction(MotionEvent.ACTION_CANCEL);
    mStackScrollLayout.onInterceptTouchEvent(cancellation);
    cancellation.recycle();
  }
  return intercept;
}
