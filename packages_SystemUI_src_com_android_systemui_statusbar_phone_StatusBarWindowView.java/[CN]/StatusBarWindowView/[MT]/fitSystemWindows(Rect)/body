{
  if (getFitsSystemWindows()) {
    boolean changed=insets.left != getPaddingLeft() || insets.top != getPaddingTop() || insets.right != getPaddingRight() || insets.bottom != getPaddingBottom();
    if (changed) {
      setPadding(insets.left,insets.top,insets.right,0);
    }
    insets.left=0;
    insets.top=0;
    insets.right=0;
  }
 else {
    boolean changed=getPaddingLeft() != 0 || getPaddingRight() != 0 || getPaddingTop() != 0 || getPaddingBottom() != 0;
    if (changed) {
      setPadding(0,0,0,0);
    }
  }
  return false;
}
