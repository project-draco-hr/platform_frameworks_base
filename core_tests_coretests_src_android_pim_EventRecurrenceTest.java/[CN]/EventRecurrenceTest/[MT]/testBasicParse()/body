{
  for (  String rule : GOOD_RRULES) {
    EventRecurrence recur=new EventRecurrence();
    recur.parse(rule);
  }
  for (  String rule : BAD_RRULES) {
    EventRecurrence recur=new EventRecurrence();
    boolean didThrow=false;
    try {
      recur.parse(rule);
    }
 catch (    InvalidFormatException ife) {
      didThrow=true;
    }
    assertTrue("Expected throw on " + rule,didThrow);
  }
}
