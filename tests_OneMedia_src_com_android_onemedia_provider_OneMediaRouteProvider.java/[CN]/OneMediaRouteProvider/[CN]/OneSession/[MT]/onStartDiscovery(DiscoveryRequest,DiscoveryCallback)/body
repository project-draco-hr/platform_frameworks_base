{
  for (  MediaRouteSelector selector : req.getSelectors()) {
    if (isMatch(selector)) {
      DestinationInfo destination=new DestinationInfo.Builder(TEST_DESTINATION_ID,getServiceMetadata(),"OneMedia").setDescription("Test route from OneMedia app.").build();
      ArrayList<RouteInfo> routes=new ArrayList<RouteInfo>();
      routes.add(new RouteInfo.Builder(TEST_ROUTE_ID,destination,selector).build());
      callback.onDestinationFound(destination,routes);
      return true;
    }
  }
  return false;
}
