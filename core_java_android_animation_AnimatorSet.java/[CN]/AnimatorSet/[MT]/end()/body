{
  mTerminated=true;
  if (isStarted()) {
    endRemainingAnimations();
  }
  if (mListeners != null) {
    ArrayList<AnimatorListener> tmpListeners=(ArrayList<AnimatorListener>)mListeners.clone();
    for (int i=0; i < tmpListeners.size(); i++) {
      tmpListeners.get(i).onAnimationEnd(this);
    }
  }
  mStarted=false;
}
