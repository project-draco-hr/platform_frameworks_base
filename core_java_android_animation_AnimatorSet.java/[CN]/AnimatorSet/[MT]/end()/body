{
  mCanceled=true;
  if (mSortedNodes.size() != mNodes.size()) {
    sortNodes();
    for (    Node node : mSortedNodes) {
      if (mSetListener == null) {
        mSetListener=new AnimatorSetListener(this);
      }
      node.animation.addListener(mSetListener);
    }
  }
  if (mSortedNodes.size() > 0) {
    for (    Node node : mSortedNodes) {
      node.animation.end();
    }
  }
}
