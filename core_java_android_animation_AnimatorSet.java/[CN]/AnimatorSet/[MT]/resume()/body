{
  boolean previouslyPaused=mPaused;
  super.resume();
  if (previouslyPaused && !mPaused) {
    if (mDelayAnim != null) {
      mDelayAnim.resume();
    }
 else {
      int size=mNodes.size();
      for (int i=0; i < size; i++) {
        Node node=mNodes.get(i);
        if (node != mRootNode) {
          node.mAnimation.resume();
        }
      }
    }
  }
}
