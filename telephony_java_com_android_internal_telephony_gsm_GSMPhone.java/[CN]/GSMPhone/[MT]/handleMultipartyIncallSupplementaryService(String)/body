{
  if (dialString.length() > 1) {
    return false;
  }
  if (LOCAL_DEBUG)   Log.d(LOG_TAG,"MmiCode 3: merge calls");
  try {
    conference();
  }
 catch (  CallStateException e) {
    if (LOCAL_DEBUG)     Log.d(LOG_TAG,"conference failed",e);
    notifySuppServiceFailed(Phone.SuppService.CONFERENCE);
  }
  return true;
}
