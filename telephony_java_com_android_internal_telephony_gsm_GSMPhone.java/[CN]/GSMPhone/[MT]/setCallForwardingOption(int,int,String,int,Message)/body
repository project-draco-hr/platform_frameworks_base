{
  if ((isValidCommandInterfaceCFAction(commandInterfaceCFAction)) && (isValidCommandInterfaceCFReason(commandInterfaceCFReason))) {
    Message resp;
    if (commandInterfaceCFReason == CF_REASON_UNCONDITIONAL) {
      resp=obtainMessage(EVENT_SET_CALL_FORWARD_DONE,isCfEnable(commandInterfaceCFAction) ? 1 : 0,0,onComplete);
    }
 else {
      resp=onComplete;
    }
    mCM.setCallForward(commandInterfaceCFAction,commandInterfaceCFReason,CommandsInterface.SERVICE_CLASS_VOICE,dialingNumber,timerSeconds,resp);
  }
}
