{
  if (DEBUG)   Log.d(TAG,"in handleNiNotification () :" + " notificationId: " + notif.notificationId + " requestorId: "+ notif.requestorId+ " text: "+ notif.text+ " mIsSuplEsEnabled"+ mIsSuplEsEnabled);
  if (mIsSuplEsEnabled == false) {
    handleNi(notif);
  }
 else {
    handleNiInEs(notif);
  }
}
