{
  long oldId=Binder.clearCallingIdentity();
  try {
    DatabaseHelper dbHelper=mOpenHelpers.get(callingUser);
    if (null == dbHelper) {
      establishDbTrackingLocked(callingUser);
      dbHelper=mOpenHelpers.get(callingUser);
    }
    return dbHelper;
  }
  finally {
    Binder.restoreCallingIdentity(oldId);
  }
}
