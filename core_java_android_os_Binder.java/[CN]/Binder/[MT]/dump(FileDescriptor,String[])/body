{
  FileOutputStream fout=new FileOutputStream(fd);
  PrintWriter pw=new FastPrintWriter(fout);
  try {
    final String disabled;
synchronized (Binder.class) {
      disabled=sDumpDisabled;
    }
    if (disabled == null) {
      dump(fd,pw,args);
    }
 else {
      pw.println(sDumpDisabled);
    }
  }
  finally {
    pw.flush();
  }
}
