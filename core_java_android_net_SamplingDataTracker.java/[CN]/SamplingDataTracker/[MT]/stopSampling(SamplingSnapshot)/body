{
synchronized (mSamplingDataLock) {
    if (mLastSample != null) {
      if (s.mTimestamp - mLastSample.mTimestamp > MINIMUM_SAMPLING_INTERVAL && getSampledPacketCount(mLastSample,s) > MINIMUM_SAMPLED_PACKETS) {
        mBeginningSample=mLastSample;
        mEndingSample=s;
        mLastSample=null;
      }
 else {
        if (DBG)         Slog.d(TAG,"Throwing current sample away because it is too small");
      }
    }
  }
}
