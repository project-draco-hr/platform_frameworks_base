{
  if ((mInterfaceName != null) && (mDefaultGatewayAddr != 0)) {
    if (DBG) {
      Log.d(TAG,"addDefaultRoute for " + mNetworkInfo.getTypeName() + " ("+ mInterfaceName+ "), GatewayAddr="+ mDefaultGatewayAddr);
    }
    NetworkUtils.addHostRoute(mInterfaceName,mDefaultGatewayAddr);
    NetworkUtils.setDefaultRoute(mInterfaceName,mDefaultGatewayAddr);
  }
}
