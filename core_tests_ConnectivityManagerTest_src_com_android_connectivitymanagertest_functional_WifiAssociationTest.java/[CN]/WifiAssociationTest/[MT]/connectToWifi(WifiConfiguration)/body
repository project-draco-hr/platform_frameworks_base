{
  assertTrue("failed to associate with " + config.SSID,connectToWifiWithConfiguration(config));
  assertTrue("failed to connect with " + config.SSID,waitForNetworkState(ConnectivityManager.TYPE_WIFI,State.CONNECTED,WIFI_CONNECTION_TIMEOUT));
  assertNotNull("Wifi connection returns null",mWifiManager.getConnectionInfo());
  assertTrue(config.SSID.contains(mWifiManager.getConnectionInfo().getSSID()));
}
