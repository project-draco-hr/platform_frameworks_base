{
  MagnificationSpec spec=mDisplayContent.mMagnificationSpec;
  if (spec != null && !spec.isNop()) {
    if (mAttachedWindow != null) {
      if (!mPolicy.canMagnifyWindowLw(mAttachedWindow.mAttrs)) {
        return null;
      }
    }
    if (!mPolicy.canMagnifyWindowLw(mAttrs)) {
      return null;
    }
  }
  return spec;
}
