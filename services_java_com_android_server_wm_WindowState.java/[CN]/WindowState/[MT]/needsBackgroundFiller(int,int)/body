{
  return (mAttrs.flags & FLAG_COMPATIBLE_WINDOW) != 0 && mHasDrawn && mViewVisibility == View.VISIBLE && mFrame.left <= mService.mCompatibleScreenFrame.left && mFrame.top <= mService.mCompatibleScreenFrame.top && mFrame.right >= mService.mCompatibleScreenFrame.right && mFrame.bottom >= mService.mCompatibleScreenFrame.bottom;
}
