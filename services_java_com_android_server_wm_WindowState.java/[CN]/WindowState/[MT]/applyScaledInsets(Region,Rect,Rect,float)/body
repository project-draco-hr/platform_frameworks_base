{
  if (scale != 1) {
    outRegion.set(frame.left + (int)(inset.left * scale),frame.top + (int)(inset.top * scale),frame.right - (int)(inset.right * scale),frame.bottom - (int)(inset.bottom * scale));
  }
 else {
    outRegion.set(frame.left + inset.left,frame.top + inset.top,frame.right - inset.right,frame.bottom - inset.bottom);
  }
}
