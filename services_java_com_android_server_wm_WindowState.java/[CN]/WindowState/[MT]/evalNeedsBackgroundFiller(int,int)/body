{
  mNeedsBackgroundFiller=mEnforceSizeCompat && mHasDrawn && mViewVisibility == View.VISIBLE && !isFullscreenIgnoringCompat(screenWidth,screenHeight) && mFrame.left <= mService.mCompatibleScreenFrame.left && mFrame.top <= mService.mCompatibleScreenFrame.top && mFrame.right >= mService.mCompatibleScreenFrame.right && mFrame.bottom >= mService.mCompatibleScreenFrame.bottom;
}
