{
  try {
synchronized (mService.mWindowMap) {
      WindowState win=mService.windowForClientLocked(mSession,mClient,false);
      Slog.i(TAG,"WIN DEATH: " + win);
      if (win != null) {
        mService.removeWindowLocked(mSession,win);
      }
    }
  }
 catch (  IllegalArgumentException ex) {
  }
}
