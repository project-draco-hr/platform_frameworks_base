{
  MonthDisplayHelper helper=new MonthDisplayHelper(2007,Calendar.SEPTEMBER);
  assertArraysEqual(new int[]{26,27,28,29,30,31,1},helper.getDigitsForRow(0));
  assertArraysEqual(new int[]{2,3,4,5,6,7,8},helper.getDigitsForRow(1));
  assertArraysEqual(new int[]{30,1,2,3,4,5,6},helper.getDigitsForRow(5));
}
