{
  MonthDisplayHelper helper=new MonthDisplayHelper(2007,Calendar.SEPTEMBER,Calendar.MONDAY);
  assertArraysEqual(new int[]{27,28,29,30,31,1,2},helper.getDigitsForRow(0));
  assertArraysEqual(new int[]{3,4,5,6,7,8,9},helper.getDigitsForRow(1));
  assertArraysEqual(new int[]{24,25,26,27,28,29,30},helper.getDigitsForRow(4));
  assertArraysEqual(new int[]{1,2,3,4,5,6,7},helper.getDigitsForRow(5));
}
