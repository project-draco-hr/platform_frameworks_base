{
  if (isDone()) {
    throw new NoSuchElementException(Messages.getString("awt.110"));
  }
  if (index == p.npoints) {
    return SEG_CLOSE;
  }
  coords[0]=p.xpoints[index];
  coords[1]=p.ypoints[index];
  if (t != null) {
    t.transform(coords,0,coords,0,1);
  }
  return index == 0 ? SEG_MOVETO : SEG_LINETO;
}
