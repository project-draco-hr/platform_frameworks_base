  mBatteryLedOn=mContext.getResources().getInteger(com.android.internal.R.integer.config_notificationsBatteryLedOn);
  mBatteryLedOff=mContext.getResources().getInteger(com.android.internal.R.integer.config_notificationsBatteryLedOff);
}
