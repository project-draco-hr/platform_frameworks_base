{
  mLightsService=lights;
  mBatteryLight=lights.getLight(LightsService.LIGHT_ID_BATTERY);
  mBatteryLowARGB=mContext.getResources().getInteger(com.android.internal.R.integer.config_notificationsBatteryLowARGB);
  mBatteryMediumARGB=mContext.getResources().getInteger(com.android.internal.R.integer.config_notificationsBatteryMediumARGB);
