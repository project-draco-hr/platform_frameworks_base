{
  Attributes a=new Attributes();
  a.put(SHA1_DIGEST,DIGEST_1_STR);
  a.put(MD5_DIGEST,DIGEST_2_STR);
  ManifestDigest fromAttributes=ManifestDigest.fromAttributes(a);
  assertNotNull("A valid ManifestDigest should be returned",fromAttributes);
  ManifestDigest created=new ManifestDigest(DIGEST_1);
  assertEquals("SHA-1 should be preferred over MD5: " + created.toString() + " vs. "+ fromAttributes.toString(),created,fromAttributes);
  assertEquals("Hash codes should be the same: " + created.toString() + " vs. "+ fromAttributes.toString(),created.hashCode(),fromAttributes.hashCode());
}
