{
  try {
    alphaTag=SimUtils.adnStringFieldToString(record,0,record.length - FOOTER_SIZE_BYTES);
    int footerOffset=record.length - FOOTER_SIZE_BYTES;
    int numberLength=0xff & record[footerOffset];
    if (numberLength > MAX_NUMBER_SIZE_BYTES) {
      number="";
      return;
    }
    number=PhoneNumberUtils.calledPartyBCDToString(record,footerOffset + 1,numberLength);
    extRecord=0xff & record[record.length - 1];
  }
 catch (  RuntimeException ex) {
    Log.w(LOG_TAG,"Error parsing AdnRecord",ex);
    number="";
    alphaTag="";
  }
}
