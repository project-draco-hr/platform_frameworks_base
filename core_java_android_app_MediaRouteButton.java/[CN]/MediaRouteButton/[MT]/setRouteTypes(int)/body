{
  if (types == mRouteTypes) {
    return;
  }
  if (mAttachedToWindow && mRouteTypes != 0) {
    mRouter.removeCallback(mRouterCallback);
  }
  mRouteTypes=types;
  if (mAttachedToWindow) {
    updateRouteInfo();
    mRouter.addCallback(types,mRouterCallback,MediaRouter.CALLBACK_FLAG_PASSIVE_DISCOVERY);
  }
}
