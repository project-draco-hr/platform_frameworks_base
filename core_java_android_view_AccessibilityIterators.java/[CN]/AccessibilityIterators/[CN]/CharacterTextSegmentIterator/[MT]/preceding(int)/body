{
  final int textLegth=mText.length();
  if (textLegth <= 0) {
    return null;
  }
  if (offset <= 0) {
    return null;
  }
  int end=offset;
  if (end > textLegth) {
    end=textLegth;
  }
  while (!mImpl.isBoundary(end)) {
    end=mImpl.preceding(end);
    if (end == BreakIterator.DONE) {
      return null;
    }
  }
  final int start=mImpl.preceding(end);
  if (start == BreakIterator.DONE) {
    return null;
  }
  return getRange(start,end);
}
