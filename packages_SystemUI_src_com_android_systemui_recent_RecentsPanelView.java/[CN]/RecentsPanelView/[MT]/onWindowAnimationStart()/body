{
  if (mItemToAnimateInWhenWindowAnimationIsFinished != null) {
    final int startDelay=100;
    final int duration=250;
    final ViewHolder holder=mItemToAnimateInWhenWindowAnimationIsFinished;
    final TimeInterpolator cubic=new DecelerateInterpolator(1.5f);
    for (    View v : new View[]{holder.iconView,holder.labelView,holder.calloutLine}) {
      if (v != null) {
        v.animate().translationX(0).translationY(0).alpha(1f).setStartDelay(startDelay).setDuration(duration).setInterpolator(cubic);
      }
    }
    mItemToAnimateInWhenWindowAnimationIsFinished=null;
  }
}
