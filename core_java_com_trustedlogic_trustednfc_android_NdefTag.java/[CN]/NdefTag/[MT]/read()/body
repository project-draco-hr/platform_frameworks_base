{
  checkState();
  if (isNdef != true) {
    isNdef=isNdef();
    if (isNdef != true) {
      throw new NfcException("Tag is not NDEF compliant");
    }
  }
  try {
    NdefMessage msg=mService.read(mHandle);
    if (msg == null) {
      throw new IOException("NDEF read failed");
    }
    return msg;
  }
 catch (  RemoteException e) {
    Log.e(TAG,"RemoteException in read(): ",e);
    return null;
  }
}
