{
  if (gDefault != null) {
    return gDefault;
  }
  IBinder b=ServiceManager.getService("content");
  if (Config.LOGV)   Log.v("ContentService","default service binder = " + b);
  gDefault=asInterface(b);
  if (Config.LOGV)   Log.v("ContentService","default service = " + gDefault);
  return gDefault;
}
