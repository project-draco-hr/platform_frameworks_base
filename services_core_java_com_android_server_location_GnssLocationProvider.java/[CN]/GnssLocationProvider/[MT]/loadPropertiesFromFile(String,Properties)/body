{
  try {
    File file=new File(filename);
    FileInputStream stream=null;
    try {
      stream=new FileInputStream(file);
      properties.load(stream);
    }
  finally {
      IoUtils.closeQuietly(stream);
    }
  }
 catch (  IOException e) {
    Log.w(TAG,"Could not open GPS configuration file " + filename);
    return false;
  }
  return true;
}
