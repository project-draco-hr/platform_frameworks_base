{
synchronized (mLock) {
    if (mCallback == null) {
      return;
    }
switch (msg.what) {
case MSG_MEDIA_BUTTON:
      mCallback.onMediaButtonEvent((Intent)msg.obj);
    break;
case MSG_COMMAND:
  Command cmd=(Command)msg.obj;
mCallback.onControlCommand(cmd.command,cmd.extras,cmd.stub);
break;
}
}
}
