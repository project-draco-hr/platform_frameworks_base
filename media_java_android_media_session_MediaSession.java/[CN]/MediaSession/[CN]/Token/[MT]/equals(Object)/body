{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  Token other=(Token)obj;
  if (mBinder == null) {
    if (other.mBinder != null)     return false;
  }
 else   if (!mBinder.asBinder().equals(other.mBinder.asBinder()))   return false;
  return true;
}
