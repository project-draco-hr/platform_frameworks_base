{
  if (fullscreen == toOpaque) {
    return false;
  }
  AttributeCache.Entry ent=AttributeCache.instance().get(packageName,realTheme,styleable.Window,userId);
  if (ent == null || !ent.array.getBoolean(styleable.Window_windowIsTranslucent,false) || ent.array.getBoolean(styleable.Window_windowIsFloating,false)) {
    return false;
  }
  task.numFullscreen+=toOpaque ? +1 : -1;
  fullscreen=toOpaque;
  return true;
}
