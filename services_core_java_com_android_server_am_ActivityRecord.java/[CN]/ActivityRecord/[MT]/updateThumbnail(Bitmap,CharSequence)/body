{
  if (newThumbnail != null) {
    if (ActivityManagerService.DEBUG_THUMBNAILS)     Slog.i(ActivityManagerService.TAG,"Setting thumbnail of " + this + " to "+ newThumbnail);
    task.setLastThumbnail(newThumbnail);
    if (isPersistable()) {
      mStackSupervisor.mService.notifyTaskPersisterLocked(task,false);
    }
  }
  task.lastDescription=description;
}
