{
  super(0,rs);
  mName=name;
  mClosures=closures;
  mInputs=inputs;
  mOutputs=outputs;
  long[] closureIDs=new long[closures.size()];
  for (int i=0; i < closureIDs.length; i++) {
    closureIDs[i]=closures.get(i).getID(rs);
  }
  long id=rs.nScriptGroup2Create(name,ScriptC.mCachePath,closureIDs);
  setID(id);
}
