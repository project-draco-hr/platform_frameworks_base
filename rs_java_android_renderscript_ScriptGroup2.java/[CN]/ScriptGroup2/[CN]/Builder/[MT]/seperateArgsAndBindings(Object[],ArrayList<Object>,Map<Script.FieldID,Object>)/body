{
  int i;
  for (i=0; i < argsAndBindings.length; i++) {
    if (argsAndBindings[i] instanceof Binding) {
      break;
    }
    args.add(argsAndBindings[i]);
  }
  for (; i < argsAndBindings.length; i++) {
    if (!(argsAndBindings[i] instanceof Binding)) {
      return false;
    }
    Binding b=(Binding)argsAndBindings[i];
    bindingMap.put(b.mField,b.mValue);
  }
  return true;
}
