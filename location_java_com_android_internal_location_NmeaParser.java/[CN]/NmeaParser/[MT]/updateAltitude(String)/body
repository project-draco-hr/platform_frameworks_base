{
  if (altitude.length() == 0) {
    return false;
  }
  double alt;
  try {
    alt=Double.parseDouble(altitude);
  }
 catch (  NumberFormatException nfe) {
    Log.e(TAG,"Exception parsing altitude " + altitude + ": "+ nfe,nfe);
    return false;
  }
  mHasAltitude=true;
  mAltitude=alt;
  return true;
}
