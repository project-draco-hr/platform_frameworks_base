{
  if (date.length() != 6) {
    return false;
  }
  int month, day, year;
  try {
    day=Integer.parseInt(date.substring(0,2));
    month=Integer.parseInt(date.substring(2,4));
    year=2000 + Integer.parseInt(date.substring(4,6));
  }
 catch (  NumberFormatException nfe) {
    Log.e(TAG,"Error parsing date " + date);
    return false;
  }
  mYear=year;
  mMonth=month;
  mDay=day;
  return true;
}
