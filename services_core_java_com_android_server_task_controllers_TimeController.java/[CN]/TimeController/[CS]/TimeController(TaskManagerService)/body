{
  super(service);
  mTaskExpiredAlarmIntent=PendingIntent.getBroadcast(mContext,0,new Intent(ACTION_TASK_EXPIRED),0);
  mNextDelayExpiredAlarmIntent=PendingIntent.getBroadcast(mContext,0,new Intent(ACTION_TASK_DELAY_EXPIRED),0);
  IntentFilter intentFilter=new IntentFilter(ACTION_TASK_EXPIRED);
  intentFilter.addAction(ACTION_TASK_DELAY_EXPIRED);
  mContext.registerReceiver(mAlarmExpiredReceiver,intentFilter);
}
