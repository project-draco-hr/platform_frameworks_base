{
  final long nowElapsed=SystemClock.elapsedRealtime();
  pw.println("Alarms (" + SystemClock.elapsedRealtime() + ")");
  pw.println("Next delay alarm in " + (mNextDelayExpiredElapsedMillis - nowElapsed) / 1000 + "s");
  pw.println("Next deadline alarm in " + (mNextTaskExpiredElapsedMillis - nowElapsed) / 1000 + "s");
  pw.println("Tracking:");
  for (  TaskStatus ts : mTrackedTasks) {
    pw.println(String.valueOf(ts.hashCode()).substring(0,3) + ".." + ": ("+ (ts.hasTimingDelayConstraint() ? ts.getEarliestRunTime() : "N/A")+ ", "+ (ts.hasDeadlineConstraint() ? ts.getLatestRunTimeElapsed() : "N/A")+ ")");
  }
}
