{
  if (httpProxy == null || httpProxy.length() == 0) {
    phone.setSystemProperty("net.gprs.http-proxy",null);
    return;
  }
  if (httpPort == null || httpPort.length() == 0) {
    httpPort="8080";
  }
  phone.setSystemProperty("net.gprs.http-proxy","http://" + httpProxy + ":"+ httpPort+ "/");
}
