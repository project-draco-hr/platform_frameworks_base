{
  if (NULL_IP.equals(domainNameServers[0]) && NULL_IP.equals(domainNameServers[1]) && !((GSMPhone)phone).isDnsCheckDisabled()) {
    if (!apn.types[0].equals(Phone.APN_TYPE_MMS) || !isIpAddress(apn.mmsProxy)) {
      log(String.format("isDnsOk: return false apn.types[0]=%s APN_TYPE_MMS=%s isIpAddress(%s)=%s",apn.types[0],Phone.APN_TYPE_MMS,apn.mmsProxy,isIpAddress(apn.mmsProxy)));
      return false;
    }
  }
  return true;
}
