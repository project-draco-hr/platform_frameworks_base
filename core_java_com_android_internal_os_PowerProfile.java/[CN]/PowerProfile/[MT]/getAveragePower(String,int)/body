{
  if (sPowerMap.containsKey(type)) {
    Object data=sPowerMap.get(type);
    if (data instanceof Double[]) {
      final Double[] values=(Double[])data;
      if (values.length > level && level >= 0) {
        return values[level];
      }
 else       if (level < 0) {
        return 0;
      }
 else {
        return values[values.length - 1];
      }
    }
 else {
      return (Double)data;
    }
  }
 else {
    return 0;
  }
}
