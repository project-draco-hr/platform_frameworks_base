{
  if ((dimX < 1) || (dimY < 1) || (dimZ < 1)) {
    throw new RSInvalidStateException("Dimension must be >= 1.");
  }
  int id=rs.nTypeCreate(e.getID(rs),dimX,dimY,dimZ,false,false,0);
  Type t=new Type(id,rs);
  t.mElement=e;
  t.mDimX=dimX;
  t.mDimY=dimY;
  t.mDimZ=dimZ;
  t.calcElementCount();
  return t;
}
