{
  if (tag == null)   throw new NullPointerException();
  if (((flags & IS_EMPTY) != 0) != (data == null))   throw new IllegalArgumentException();
  mTag=tag;
  mTimeMillis=millis;
  mText=null;
  mFlags=flags;
  mFileDescriptor=data == null ? null : ParcelFileDescriptor.open(data,ParcelFileDescriptor.MODE_READ_ONLY);
}
