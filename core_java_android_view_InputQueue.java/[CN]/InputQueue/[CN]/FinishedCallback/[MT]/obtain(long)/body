{
synchronized (sLock) {
    FinishedCallback callback=sRecycleHead;
    if (callback != null) {
      callback.mRecycleNext=null;
      sRecycleHead=callback.mRecycleNext;
      sRecycleCount-=1;
    }
 else {
      callback=new FinishedCallback();
    }
    callback.mFinishedToken=finishedToken;
    return callback;
  }
}
