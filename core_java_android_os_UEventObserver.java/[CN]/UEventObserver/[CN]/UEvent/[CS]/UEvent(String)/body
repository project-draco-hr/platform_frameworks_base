{
  int offset=0;
  int length=message.length();
  while (offset < length) {
    int equals=message.indexOf('=',offset);
    int at=message.indexOf('\0',offset);
    if (at < 0)     break;
    if (equals > offset && equals < at) {
      mMap.put(message.substring(offset,equals),message.substring(equals + 1,at));
    }
    offset=at + 1;
  }
}
