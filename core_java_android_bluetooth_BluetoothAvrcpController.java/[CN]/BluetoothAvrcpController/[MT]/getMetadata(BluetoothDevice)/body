{
  if (DBG)   Log.d(TAG,"getMetadata");
  MediaMetadata metadata=null;
  if (mService != null && isEnabled()) {
    try {
      metadata=mService.getMetadata(device);
    }
 catch (    RemoteException e) {
      Log.e(TAG,"Error talking to BT service in getMetadata() " + e);
      return null;
    }
  }
  return metadata;
}
