{
  Class cl;
  Object args[]=new Object[1];
  args[0]=new String[0];
  try {
    cl=Class.forName(className);
    cl.getMethod("main",new Class[]{String[].class}).invoke(null,args);
  }
 catch (  Exception ex) {
    Log.e(LOG_TAG,"Exception invoking static main on " + className,ex);
    throw new RuntimeException(ex);
  }
}
