{
  super();
  try {
    File resultsPassedFile=new File("/sdcard/layout_test_presults.txt");
    File resultsFailedFile=new File("/sdcard/layout_test_fresults.txt");
    mBufferedOutputPassedStream=new BufferedOutputStream(new FileOutputStream(resultsPassedFile,true));
    mBufferedOutputFailedStream=new BufferedOutputStream(new FileOutputStream(resultsFailedFile,true));
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
