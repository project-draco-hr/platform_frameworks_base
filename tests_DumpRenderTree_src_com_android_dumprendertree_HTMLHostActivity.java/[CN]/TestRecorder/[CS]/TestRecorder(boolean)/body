{
  try {
    File resultsPassedFile=new File("/sdcard/layout_tests_passed.txt");
    File resultsFailedFile=new File("/sdcard/layout_tests_failed.txt");
    File resultsNontextFile=new File("/sdcard/layout_tests_nontext.txt");
    mBufferedOutputPassedStream=new BufferedOutputStream(new FileOutputStream(resultsPassedFile,resume));
    mBufferedOutputFailedStream=new BufferedOutputStream(new FileOutputStream(resultsFailedFile,resume));
    mBufferedOutputNontextStream=new BufferedOutputStream(new FileOutputStream(resultsNontextFile,resume));
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
