{
  try {
    mBufferedOutputNontextStream.write(layout_file.getBytes());
    if (has_results) {
      mBufferedOutputNontextStream.write(" : has expected results".getBytes());
    }
    mBufferedOutputNontextStream.write('\n');
    mBufferedOutputNontextStream.flush();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
