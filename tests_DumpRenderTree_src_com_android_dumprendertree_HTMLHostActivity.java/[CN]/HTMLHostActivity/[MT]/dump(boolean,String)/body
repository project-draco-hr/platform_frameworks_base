{
  mDumpRequested=false;
  System.out.print('.');
  String resultFile=mCurrentFile.substring(0,mCurrentFile.lastIndexOf('.'));
  mFinishedStack.push(resultFile);
  if (dumpAsText) {
    resultFile+="-results.txt";
  }
 else {
    resultFile+="-android-results.txt";
  }
  try {
    FileOutputStream os=new FileOutputStream(resultFile);
    if (timeout) {
      Log.i("Layout test: Timeout",resultFile);
      os.write("**Test timeout\n".getBytes());
    }
    if (dumpTitleChanges)     os.write(mTitleChanges.toString().getBytes());
    if (mDialogStrings != null)     os.write(mDialogStrings.toString().getBytes());
    mDialogStrings=null;
    os.write(webkitData.getBytes());
    os.flush();
    os.close();
  }
 catch (  FileNotFoundException ex) {
    ex.printStackTrace();
  }
catch (  IOException ex) {
    ex.printStackTrace();
  }
  if (mProcessStack != null)   loadNextPage();
 else   finished();
}
