{
  if (mActionMode != this) {
    return;
  }
  mCallback.onDestroyActionMode(this);
  mCallback=null;
  animateTo(NORMAL_VIEW);
  mUpperContextView.closeMode();
  if (mLowerContextView != null && mLowerContextView.getVisibility() != View.GONE) {
    mLowerContextView.setVisibility(View.GONE);
  }
  mActionMode=null;
}
