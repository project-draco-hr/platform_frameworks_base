{
  if (timezone == null) {
    throw new NullPointerException("timezone is null!");
  }
  this.timezone=timezone;
  this.allDay=false;
  this.second=0;
  this.minute=0;
  this.hour=0;
  this.monthDay=0;
  this.month=0;
  this.year=0;
  this.weekDay=0;
  this.yearDay=0;
  this.gmtoff=0;
  this.isDst=-1;
}
