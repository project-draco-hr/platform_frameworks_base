      sDateTimeFormat=r.getString(com.android.internal.R.string.date_and_time);
      LocaleData localeData=LocaleData.get(locale);
      sAm=localeData.amPm[0];
      sPm=localeData.amPm[1];
      sLocale=locale;
    }
    return format1(format);
  }
