{
  try {
    boolean result=mBulkCursor.deleteRow(mPos);
    if (result != false) {
      mWindow=null;
      mCount=mBulkCursor.count();
      if (mPos < mCount) {
        int oldPos=mPos;
        mPos=-1;
        moveToPosition(oldPos);
      }
 else {
        mPos=mCount;
      }
      onChange(true);
    }
    return result;
  }
 catch (  RemoteException ex) {
    Log.e(TAG,"Unable to delete row because the remote process is dead");
    return false;
  }
}
