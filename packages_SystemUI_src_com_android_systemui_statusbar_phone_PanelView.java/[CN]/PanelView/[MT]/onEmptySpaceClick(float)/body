{
  if (mHintAnimationRunning) {
    return true;
  }
  if (x < mEdgeTapAreaWidth && mStatusBar.getBarState() == StatusBarState.KEYGUARD) {
    onEdgeClicked(false);
    return true;
  }
 else   if (x > getWidth() - mEdgeTapAreaWidth && mStatusBar.getBarState() == StatusBarState.KEYGUARD) {
    onEdgeClicked(true);
    return true;
  }
 else {
    return onMiddleClicked();
  }
}
