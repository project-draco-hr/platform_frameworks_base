{
switch (mStatusBar.getBarState()) {
case StatusBarState.KEYGUARD:
    if (!mDozingOnDown) {
      startUnlockHintAnimation();
    }
  return true;
case StatusBarState.SHADE_LOCKED:
mStatusBar.goToKeyguard();
return true;
case StatusBarState.SHADE:
post(mPostCollapseRunnable);
return false;
default :
return true;
}
}
