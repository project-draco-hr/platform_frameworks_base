{
  if (!mStatusBar.isFalsingThresholdNeeded()) {
    return false;
  }
  if (mFalsingManager.isFalseTouch(Classifier.UNLOCK)) {
    return true;
  }
  if (!mTouchAboveFalsingThreshold) {
    return true;
  }
  if (mUpwardsWhenTresholdReached) {
    return false;
  }
  return !isDirectionUpwards(x,y);
}
