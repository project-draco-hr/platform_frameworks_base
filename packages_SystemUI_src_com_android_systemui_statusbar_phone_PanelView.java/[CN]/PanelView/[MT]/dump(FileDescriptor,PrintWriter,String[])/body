{
  pw.println(String.format("[PanelView(%s): expandedHeight=%f maxPanelHeight=%d closing=%s" + " tracking=%s justPeeked=%s peekAnim=%s%s timeAnim=%s%s" + "]",this.getClass().getSimpleName(),getExpandedHeight(),getMaxPanelHeight(),mClosing ? "T" : "f",mTracking ? "T" : "f",mJustPeeked ? "T" : "f",mPeekAnimator,((mPeekAnimator != null && mPeekAnimator.isStarted()) ? " (started)" : ""),mHeightAnimator,((mHeightAnimator != null && mHeightAnimator.isStarted()) ? " (started)" : "")));
}
