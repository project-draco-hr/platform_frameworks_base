{
  float currentMaxPanelHeight=getMaxPanelHeight();
  if ((!mTracking || isTrackingBlocked()) && mHeightAnimator == null && !isFullyCollapsed() && currentMaxPanelHeight != mExpandedHeight && !mPeekPending && mPeekAnimator == null && !mPeekTouching) {
    setExpandedHeight(currentMaxPanelHeight);
  }
}
