{
  if (mMaxPanelHeight <= 0) {
    if (DEBUG)     logf("Forcing measure() since mMaxPanelHeight=" + mMaxPanelHeight);
    measure(MeasureSpec.makeMeasureSpec(android.view.ViewGroup.LayoutParams.WRAP_CONTENT,MeasureSpec.EXACTLY),MeasureSpec.makeMeasureSpec(android.view.ViewGroup.LayoutParams.WRAP_CONTENT,MeasureSpec.EXACTLY));
  }
  return mMaxPanelHeight;
}
