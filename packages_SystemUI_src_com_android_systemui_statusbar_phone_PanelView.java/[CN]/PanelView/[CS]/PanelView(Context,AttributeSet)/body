{
  super(context,attrs);
  mFlingAnimationUtils=new FlingAnimationUtils(context,0.6f);
  mLinearOutSlowInInterpolator=AnimationUtils.loadInterpolator(context,android.R.interpolator.fast_out_slow_in);
