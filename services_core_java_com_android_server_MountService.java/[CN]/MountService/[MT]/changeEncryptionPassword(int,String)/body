{
  mContext.enforceCallingOrSelfPermission(Manifest.permission.CRYPT_KEEPER,"no permission to access the crypt keeper");
  waitForReady();
  if (DEBUG_EVENTS) {
    Slog.i(TAG,"changing encryption password...");
  }
  final NativeDaemonEvent event;
  try {
    event=mConnector.execute("cryptfs","changepw",CRYPTO_TYPES[type],new SensitiveArg(toHex(password)));
    return Integer.parseInt(event.getMessage());
  }
 catch (  NativeDaemonConnectorException e) {
    return e.getCode();
  }
}
