{
  final int size=mVolumes.size();
  for (int i=0; i < size; i++) {
    final VolumeInfo vol=mVolumes.valueAt(i);
    final VolumeMetadata meta=mMetadata.get(vol.fsUuid);
    if (meta != null) {
      vol.nickname=meta.nickname;
      vol.userFlags=meta.userFlags;
    }
 else {
      vol.nickname=null;
      vol.userFlags=0;
    }
  }
}
