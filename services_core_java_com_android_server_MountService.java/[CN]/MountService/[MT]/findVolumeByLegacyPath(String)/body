{
synchronized (mLock) {
    for (int i=0; i < mVolumes.size(); i++) {
      final VolumeInfo vol=mVolumes.valueAt(i);
      if (vol.path != null && legacyPath.startsWith(vol.path)) {
        return vol;
      }
    }
  }
  Slog.w(TAG,"Failed to find volume for path " + legacyPath);
  return null;
}
