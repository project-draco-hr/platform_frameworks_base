{
  assertRunOnServiceThread();
  Iterator<HdmiCecFeatureAction> iter=mActions.iterator();
  while (iter.hasNext()) {
    HdmiCecFeatureAction action=iter.next();
    action.finish(false);
    iter.remove();
  }
  if (mPendingActionClearedCallback != null) {
    mPendingActionClearedCallback.onCleared(this);
  }
}
