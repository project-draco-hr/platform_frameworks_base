{
  assertRunOnServiceThread();
  Iterator<FeatureAction> iter=mActions.iterator();
  while (iter.hasNext()) {
    FeatureAction action=iter.next();
    if (action != exception && action.getClass().equals(clazz)) {
      action.finish(false);
      iter.remove();
    }
  }
  checkIfPendingActionsCleared();
}
