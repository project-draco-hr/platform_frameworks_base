{
  assertRunOnServiceThread();
  List<T> actions=Collections.<T>emptyList();
  for (  HdmiCecFeatureAction action : mActions) {
    if (action.getClass().equals(clazz)) {
      if (actions.isEmpty()) {
        actions=new ArrayList<T>();
      }
      actions.add((T)action);
    }
  }
  return actions;
}
