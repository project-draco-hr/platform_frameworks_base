{
synchronized (mLock) {
    super.readFromParcel(in);
    mSurfaceSet.clear();
    Parcelable[] parcelableArray=in.readParcelableArray(Surface.class.getClassLoader());
    if (parcelableArray == null) {
      return;
    }
    for (    Parcelable p : parcelableArray) {
      Surface s=(Surface)p;
      mSurfaceSet.add(s);
    }
  }
}
