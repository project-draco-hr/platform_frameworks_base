{
  if (position == 0) {
    if (convertView == null) {
      convertView=getLayoutInflater().inflate(R.layout.add_printer_list_header,parent,false);
    }
    ((TextView)convertView.findViewById(R.id.text)).setText(R.string.enabled_services_title);
    return convertView;
  }
  if (convertView == null) {
    convertView=getLayoutInflater().inflate(R.layout.enabled_print_services_list_item,parent,false);
  }
  PrintServiceInfo service=(PrintServiceInfo)getItem(position);
  TextView title=(TextView)convertView.findViewById(R.id.title);
  ImageView icon=(ImageView)convertView.findViewById(R.id.icon);
  TextView subtitle=(TextView)convertView.findViewById(R.id.subtitle);
  title.setText(service.getResolveInfo().loadLabel(getPackageManager()));
  icon.setImageDrawable(service.getResolveInfo().loadIcon(getPackageManager()));
  if (getAddPrinterIntent(service) == null) {
    subtitle.setText(getString(R.string.cannot_add_printer));
  }
 else {
    subtitle.setText(getString(R.string.select_to_add_printers));
  }
  return convertView;
}
