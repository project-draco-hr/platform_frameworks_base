{
  String addPrinterActivityName=service.getAddPrintersActivityName();
  if (!TextUtils.isEmpty(addPrinterActivityName)) {
    Intent intent=new Intent(Intent.ACTION_MAIN);
    intent.setComponent(new ComponentName(service.getComponentName().getPackageName(),addPrinterActivityName));
    List<ResolveInfo> resolvedActivities=getPackageManager().queryIntentActivities(intent,0);
    if (!resolvedActivities.isEmpty()) {
      if (resolvedActivities.get(0).activityInfo.exported) {
        return intent;
      }
    }
  }
  return null;
}
