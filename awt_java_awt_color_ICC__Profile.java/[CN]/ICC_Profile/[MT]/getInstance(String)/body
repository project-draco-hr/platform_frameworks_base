{
  final String fName=fileName;
  FileInputStream fiStream=(FileInputStream)AccessController.doPrivileged(new PrivilegedAction<FileInputStream>(){
    public FileInputStream run(){
      FileInputStream fiStream=null;
      try {
        fiStream=new FileInputStream(fName);
        if (fiStream != null) {
          return fiStream;
        }
      }
 catch (      FileNotFoundException e) {
      }
      fiStream=tryPath(System.getProperty("java.iccprofile.path"),fName);
      if (fiStream != null) {
        return fiStream;
      }
      fiStream=tryPath(System.getProperty("java.class.path"),fName);
      if (fiStream != null) {
        return fiStream;
      }
      String home=System.getProperty("java.home");
      if (home != null) {
        fiStream=tryPath(home + File.separatorChar + "lib"+ File.separatorChar+ "cmm",fName);
      }
      return fiStream;
    }
  }
);
  if (fiStream == null) {
    throw new IOException(Messages.getString("awt.161",fileName));
  }
  ICC_Profile pf=getInstance(fiStream);
  fiStream.close();
  return pf;
}
