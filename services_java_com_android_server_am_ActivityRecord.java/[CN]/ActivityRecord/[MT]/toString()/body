{
  if (stringName != null) {
    return stringName;
  }
  StringBuilder sb=new StringBuilder(128);
  sb.append("ActivityRecord{");
  sb.append(Integer.toHexString(System.identityHashCode(this)));
  sb.append(" u");
  sb.append(userId);
  sb.append(' ');
  sb.append(intent.getComponent().flattenToShortString());
  sb.append('}');
  return stringName=sb.toString();
}
