{
  if (thumbHolder != null) {
    if (newThumbnail != null) {
      if (ActivityManagerService.DEBUG_THUMBNAILS)       Slog.i(ActivityManagerService.TAG,"Setting thumbnail of " + this + " holder "+ thumbHolder+ " to "+ newThumbnail);
      thumbHolder.lastThumbnail=newThumbnail;
    }
    thumbHolder.lastDescription=description;
  }
}
