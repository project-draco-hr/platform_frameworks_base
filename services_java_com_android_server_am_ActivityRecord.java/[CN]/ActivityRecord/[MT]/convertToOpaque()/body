{
  if (fullscreen) {
    return false;
  }
  AttributeCache.Entry ent=AttributeCache.instance().get(packageName,realTheme,com.android.internal.R.styleable.Window);
  if (ent != null && !ent.array.getBoolean(com.android.internal.R.styleable.Window_windowIsFloating,false)) {
    fullscreen=true;
    ++task.numFullscreen;
  }
  return fullscreen;
}
