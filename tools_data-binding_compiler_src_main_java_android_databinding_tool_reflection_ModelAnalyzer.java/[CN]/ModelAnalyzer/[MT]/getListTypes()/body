{
  if (mListTypes == null) {
    mListTypes=new ModelClass[LIST_CLASS_NAMES.length];
    for (int i=0; i < mListTypes.length; i++) {
      final ModelClass modelClass=findClass(LIST_CLASS_NAMES[i],null);
      if (modelClass != null) {
        mListTypes[i]=modelClass.erasure();
      }
    }
  }
  return mListTypes;
}
