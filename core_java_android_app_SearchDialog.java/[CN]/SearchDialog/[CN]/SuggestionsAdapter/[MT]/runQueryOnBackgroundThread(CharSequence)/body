{
  String query=(constraint == null) ? "" : constraint.toString();
  Cursor c=null;
synchronized (this) {
    if (mNonUserQuery) {
      c=mRecentCursor.get();
      mNonUserQuery=false;
    }
  }
  if (c == null) {
    c=getSuggestions(mSearchable,query);
synchronized (this) {
      mRecentCursor=new WeakReference<Cursor>(c);
    }
  }
  return c;
}
