{
  mLastKeyDown=keyCode;
  if (mSearchDialog.mSearchable == null) {
    return false;
  }
  if (keyCode == KeyEvent.KEYCODE_BACK) {
    if (event.getAction() == KeyEvent.ACTION_DOWN && event.getRepeatCount() == 0) {
      if (mSearchDialog.backToPreviousComponent(false)) {
        return true;
      }
      if (isInputMethodNotNeeded() || (isEmpty() && getDropDownChildCount() >= getAdapterCount())) {
        return true;
      }
      mLastKeyDown=0;
      return false;
    }
 else     if (event.getAction() == KeyEvent.ACTION_UP && mLastKeyDown == keyCode && !event.isCanceled()) {
      if (mSearchDialog.backToPreviousComponent(true)) {
        return true;
      }
      if (isInputMethodNotNeeded() || (isEmpty() && getDropDownChildCount() >= getAdapterCount())) {
        mSearchDialog.cancel();
        return true;
      }
      return false;
    }
  }
  return false;
}
