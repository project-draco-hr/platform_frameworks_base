{
  String textLabel=null;
  Drawable iconLabel=null;
  int textId=mSearchable.getSearchButtonText();
  if (isBrowserSearch()) {
    iconLabel=getContext().getResources().getDrawable(com.android.internal.R.drawable.ic_btn_search_play);
  }
 else   if (textId != 0) {
    textLabel=mActivityContext.getResources().getString(textId);
  }
 else {
    iconLabel=getContext().getResources().getDrawable(com.android.internal.R.drawable.ic_btn_search);
  }
  mGoButton.setText(textLabel);
  mGoButton.setCompoundDrawablesWithIntrinsicBounds(iconLabel,null,null,null);
}
