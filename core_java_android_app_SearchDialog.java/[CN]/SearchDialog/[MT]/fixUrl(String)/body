{
  if (inUrl.startsWith("http://") || inUrl.startsWith("https://"))   return inUrl;
  if (inUrl.startsWith("http:") || inUrl.startsWith("https:")) {
    if (inUrl.startsWith("http:/") || inUrl.startsWith("https:/")) {
      inUrl=inUrl.replaceFirst("/","//");
    }
 else {
      inUrl=inUrl.replaceFirst(":","://");
    }
  }
  if (inUrl.indexOf("://") == -1) {
    inUrl="http://" + inUrl;
  }
  return inUrl;
}
