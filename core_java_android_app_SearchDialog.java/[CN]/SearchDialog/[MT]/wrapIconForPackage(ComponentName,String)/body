{
  if (icon == null || icon.length() == 0 || "0".equals(icon)) {
    return null;
  }
 else   if (!Character.isDigit(icon.charAt(0))) {
    return icon;
  }
 else {
    String packageName=source.getPackageName();
    return new Uri.Builder().scheme(ContentResolver.SCHEME_ANDROID_RESOURCE).authority(packageName).encodedPath(icon).toString();
  }
}
