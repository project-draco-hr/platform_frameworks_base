{
  if (keyCode == KeyEvent.KEYCODE_BACK) {
    if (event.getAction() == KeyEvent.ACTION_DOWN && event.getRepeatCount() == 0) {
      event.startTracking();
      return true;
    }
    if (event.getAction() == KeyEvent.ACTION_UP && event.isTracking() && !event.isCanceled()) {
      if (backToPreviousComponent(true)) {
        return true;
      }
      cancel();
    }
    return true;
  }
  return false;
}
