{
  if (DBG)   Log.d(LOG_TAG,"onKeyUp(" + keyCode + ","+ event+ ")");
  if (mSearchable == null) {
    return false;
  }
  if (handleBackKey(keyCode,event)) {
    return true;
  }
  if (keyCode == KeyEvent.KEYCODE_SEARCH && event.isTracking() && !event.isCanceled()) {
    return toggleGlobalSearch();
  }
  return false;
}
