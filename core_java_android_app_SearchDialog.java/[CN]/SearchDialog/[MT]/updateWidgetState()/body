{
  boolean enabled=!mSearchAutoComplete.isEmpty();
  if (isBrowserSearch()) {
    if (enabled) {
      mSearchAutoComplete.setBackgroundResource(com.android.internal.R.drawable.textfield_search);
      mGoButton.setVisibility(View.VISIBLE);
      mGoButton.setEnabled(true);
      mGoButton.setFocusable(true);
    }
 else {
      mSearchAutoComplete.setBackgroundResource(com.android.internal.R.drawable.textfield_search_empty);
      mGoButton.setVisibility(View.GONE);
    }
  }
 else {
    mGoButton.setEnabled(enabled);
    mGoButton.setFocusable(enabled);
  }
}
