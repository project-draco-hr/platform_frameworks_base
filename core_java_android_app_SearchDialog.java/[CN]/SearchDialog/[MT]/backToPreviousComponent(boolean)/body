{
  ComponentName previous=popPreviousComponent();
  if (previous == null) {
    return false;
  }
  if (doIt) {
    if (!show(previous,mAppSearchData,false)) {
      Log.w(LOG_TAG,"Failed to switch to source " + previous);
      return false;
    }
    String query=mSearchAutoComplete.getText().toString();
    setUserQuery(query);
  }
  return true;
}
