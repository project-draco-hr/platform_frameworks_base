{
  String result=null;
  final String column=actionKey.mSuggestActionMsgColumn;
  if (column != null) {
    try {
      int colId=c.getColumnIndexOrThrow(column);
      result=c.getString(colId);
    }
 catch (    RuntimeException e) {
    }
  }
  if (result == null) {
    result=actionKey.mSuggestActionMsg;
  }
  return result;
}
