{
  int code=0;
  if (width <= 0.0f) {
    code|=OUT_LEFT | OUT_RIGHT;
  }
 else   if (px < x) {
    code|=OUT_LEFT;
  }
 else   if (px > x + width) {
    code|=OUT_RIGHT;
  }
  if (height <= 0.0f) {
    code|=OUT_TOP | OUT_BOTTOM;
  }
 else   if (py < y) {
    code|=OUT_TOP;
  }
 else   if (py > y + height) {
    code|=OUT_BOTTOM;
  }
  return code;
}
