{
  List<String> apps=mAppsQueryHelper.queryApps(AppsQueryHelper.GET_NON_LAUNCHABLE_APPS,true,UserHandle.SYSTEM);
  assertEqualsIgnoreOrder(Arrays.asList("sys_app1","sys_app3"),apps);
  apps=mAppsQueryHelper.queryApps(AppsQueryHelper.GET_NON_LAUNCHABLE_APPS,false,UserHandle.SYSTEM);
  assertEqualsIgnoreOrder(Arrays.asList("sys_app1","sys_app3"),apps);
}
