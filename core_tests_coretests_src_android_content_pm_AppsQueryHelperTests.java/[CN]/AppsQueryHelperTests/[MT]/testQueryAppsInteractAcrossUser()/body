{
  List<String> apps=mAppsQueryHelper.queryApps(AppsQueryHelper.GET_APPS_WITH_INTERACT_ACROSS_USERS_PERM,true,UserHandle.SYSTEM);
  assertEqualsIgnoreOrder(Arrays.asList("sys_app1"),apps);
  apps=mAppsQueryHelper.queryApps(AppsQueryHelper.GET_APPS_WITH_INTERACT_ACROSS_USERS_PERM,false,UserHandle.SYSTEM);
  assertEqualsIgnoreOrder(Arrays.asList("sys_app1"),apps);
}
