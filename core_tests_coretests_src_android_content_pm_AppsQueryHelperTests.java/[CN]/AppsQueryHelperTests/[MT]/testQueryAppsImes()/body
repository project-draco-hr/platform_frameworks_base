{
  List<String> apps=mAppsQueryHelper.queryApps(AppsQueryHelper.GET_IMES,true,UserHandle.of(UserHandle.myUserId()));
  assertEqualsIgnoreOrder(Arrays.asList("sys_app1"),apps);
  apps=mAppsQueryHelper.queryApps(AppsQueryHelper.GET_IMES,false,UserHandle.of(UserHandle.myUserId()));
  assertEqualsIgnoreOrder(Arrays.asList("sys_app1","app4"),apps);
}
