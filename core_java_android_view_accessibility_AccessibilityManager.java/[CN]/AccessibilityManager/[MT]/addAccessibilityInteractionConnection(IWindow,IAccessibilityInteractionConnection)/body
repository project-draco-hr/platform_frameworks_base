{
  if (mService == null) {
    return View.NO_ID;
  }
  try {
    return mService.addAccessibilityInteractionConnection(windowToken,connection,mUserId);
  }
 catch (  RemoteException re) {
    Log.e(LOG_TAG,"Error while adding an accessibility interaction connection. ",re);
  }
  return View.NO_ID;
}
