{
  final boolean isTouchExplorationEnabled;
synchronized (mLock) {
    isTouchExplorationEnabled=mIsTouchExplorationEnabled;
  }
  final int listenerCount=mTouchExplorationStateChangeListeners.size();
  for (int i=0; i < listenerCount; i++) {
    mTouchExplorationStateChangeListeners.get(i).onTouchExplorationStateChanged(isTouchExplorationEnabled);
  }
}
