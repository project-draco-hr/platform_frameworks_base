{
  Component current=component;
  ParserState state=new ParserState();
  state.index=0;
  String[] lines=text.split("\n");
  for (  String line : lines) {
    try {
      current=parseLine(line,state,current);
      if (component == null) {
        component=current;
      }
    }
 catch (    FormatException fe) {
      if (false) {
        Log.v(TAG,"Cannot parse " + line,fe);
      }
    }
    continue;
  }
  return component;
}
