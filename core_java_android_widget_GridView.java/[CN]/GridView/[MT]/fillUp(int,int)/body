{
  View selectedView=null;
  final int end=mListPadding.top;
  while (nextBottom > end && pos >= 0) {
    View temp=makeRow(pos,nextBottom,false);
    if (temp != null) {
      selectedView=temp;
    }
    nextBottom=mReferenceView.getTop() - mVerticalSpacing;
    mFirstPosition=pos;
    pos-=mNumColumns;
  }
  if (mStackFromBottom) {
    mFirstPosition=Math.max(0,pos + 1);
  }
  return selectedView;
}
