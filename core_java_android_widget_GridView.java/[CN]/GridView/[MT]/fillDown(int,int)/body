{
  View selectedView=null;
  final int end=(mBottom - mTop) - mListPadding.bottom;
  while (nextTop < end && pos < mItemCount) {
    View temp=makeRow(pos,nextTop,true);
    if (temp != null) {
      selectedView=temp;
    }
    nextTop=mReferenceView.getBottom() + mVerticalSpacing;
    pos+=mNumColumns;
  }
  return selectedView;
}
