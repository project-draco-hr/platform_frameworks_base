{
  final int count=mListView.getAdapter().getCount();
  final String text;
  if (count <= 0) {
    text=getString(R.string.print_no_printers);
  }
 else {
    text=getActivity().getResources().getQuantityString(R.plurals.print_search_result_count_utterance,count,count);
  }
  mListView.announceForAccessibility(text);
}
