{
  int size=0;
  if (m.getType() != null && m.getType().equals("text") && m.getParameter("text") != null) {
    size+=m.getParameter("text").length();
  }
 else   if (m.getType() == null || !m.getType().equals("utterance")) {
    size+=1;
  }
  for (  Markup nested : m.getNestedMarkups()) {
    size+=estimateSynthesisLengthFromMarkup(nested);
  }
  return size;
}
