{
  try {
    int rc=getMountService().unmountSecureContainer(cid,false);
    if (rc != StorageResultCode.OperationSucceeded) {
      Log.e(TAG,"Failed to unmount " + cid + " with rc "+ rc);
      return false;
    }
    return true;
  }
 catch (  RemoteException e) {
    Log.e(TAG,"MountService running?");
  }
  return false;
}
