{
  super.onDraw(c);
  if (antenna != null) {
    antenna.setBounds(0,0,c.getWidth(),c.getHeight());
    antenna.draw(c);
  }
  if (eyes != null) {
    eyes.setBounds(0,0,c.getWidth(),c.getHeight());
    eyes.draw(c);
  }
  if (mouth != null) {
    mouth.setBounds(0,0,c.getWidth(),c.getHeight());
    mouth.draw(c);
  }
}
