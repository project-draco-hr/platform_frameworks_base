{
  mService=service;
  int[] types=null;
  if (mService != null) {
    try {
      types=mService.getSupportedTypes();
    }
 catch (    RemoteException e) {
    }
  }
  mHasTvDevice=hasDeviceType(types,HdmiCecDeviceInfo.DEVICE_TV);
  mHasPlaybackDevice=hasDeviceType(types,HdmiCecDeviceInfo.DEVICE_PLAYBACK);
}
