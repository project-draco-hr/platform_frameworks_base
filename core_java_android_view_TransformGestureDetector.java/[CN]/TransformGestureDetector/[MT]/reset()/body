{
  if (mInitialEvent != null) {
    mInitialEvent.recycle();
    mInitialEvent=null;
  }
  if (mPrevEvent != null) {
    mPrevEvent.recycle();
    mPrevEvent=null;
  }
  if (mCurrEvent != null) {
    mCurrEvent.recycle();
    mCurrEvent=null;
  }
  if (mVelocityTracker != null) {
    mVelocityTracker.recycle();
    mVelocityTracker=null;
  }
  mEventIgnoreCount=INITIAL_EVENT_IGNORES;
}
