{
  final String key=pack(deviceId,CREATED_DOCUMENT_HANDLE);
  if (!mDocuments.containsKey(key)) {
    mDocuments.put(key,new MtpDocument(CREATED_DOCUMENT_HANDLE,objectInfo.getFormat(),objectInfo.getName(),new Date(objectInfo.getDateModified()),objectInfo.getCompressedSize(),objectInfo.getThumbCompressedSize(),false));
  }
 else {
    throw new IOException();
  }
  return CREATED_DOCUMENT_HANDLE;
}
