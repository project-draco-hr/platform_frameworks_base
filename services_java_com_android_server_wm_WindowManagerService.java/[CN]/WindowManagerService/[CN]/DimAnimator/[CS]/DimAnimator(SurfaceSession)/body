{
  if (mDimSurface == null) {
    if (SHOW_TRANSACTIONS)     Slog.i(TAG,"  DIM " + mDimSurface + ": CREATE");
    try {
      mDimSurface=new Surface(session,0,"DimSurface",-1,16,16,PixelFormat.OPAQUE,Surface.FX_SURFACE_DIM);
      mDimSurface.setAlpha(0.0f);
    }
 catch (    Exception e) {
      Slog.e(TAG,"Exception creating Dim surface",e);
    }
  }
}
