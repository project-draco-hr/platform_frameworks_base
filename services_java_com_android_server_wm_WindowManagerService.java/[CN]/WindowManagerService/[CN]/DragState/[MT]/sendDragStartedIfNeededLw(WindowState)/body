{
  if (mDragInProgress) {
    for (    WindowState ws : mNotifiedWindows) {
      if (ws == newWin) {
        return;
      }
    }
    if (DEBUG_DRAG) {
      Slog.d(TAG,"need to send DRAG_STARTED to new window " + newWin);
    }
    sendDragStartedLw(newWin,mCurrentX,mCurrentY,mDataDescription);
  }
}
