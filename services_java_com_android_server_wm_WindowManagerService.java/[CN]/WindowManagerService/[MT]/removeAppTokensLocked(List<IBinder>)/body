{
  int N=tokens.size();
  for (int i=0; i < N; i++) {
    IBinder token=tokens.get(i);
    final AppWindowToken wtoken=findAppWindowToken(token);
    if (wtoken != null) {
      final DisplayContent displayContent=mTaskIdToDisplayContents.get(wtoken.groupId);
      if (DEBUG_REORDER || DEBUG_TOKEN_MOVEMENT)       Slog.v(TAG,"Temporarily removing " + wtoken + " from "+ displayContent.mAppTokens.indexOf(wtoken));
      if (!displayContent.mAppTokens.remove(wtoken)) {
        Slog.w(TAG,"Attempting to reorder token that doesn't exist: " + token + " ("+ wtoken+ ")");
        i--;
        N--;
      }
    }
  }
}
