{
  if (hashCode == -1) {
    return getFocusedWindow();
  }
synchronized (mWindowMap) {
    final AllWindowsIterator iterator=new AllWindowsIterator();
    while (iterator.hasNext()) {
      final WindowState w=iterator.next();
      if (System.identityHashCode(w) == hashCode) {
        return w;
      }
    }
  }
  return null;
}
