{
  int anim=0;
  Context context=mContext;
  if (resId >= 0) {
    AttributeCache.Entry ent=getCachedAnimations(userId,packageName,resId);
    if (ent != null) {
      context=ent.context;
      anim=resId;
    }
  }
  if (anim != 0) {
    return AnimationUtils.loadAnimation(context,anim);
  }
  return null;
}
