{
  final WindowList windows=pos.getWindowList();
  final int i=windows.indexOf(pos);
  if (DEBUG_FOCUS || DEBUG_WINDOW_MOVEMENT || DEBUG_ADD_REMOVE)   Slog.v(TAG,"Adding window " + window + " at "+ i+ " of "+ windows.size()+ " (before "+ pos+ ")");
  windows.add(i,window);
  mWindowsChanged=true;
}
