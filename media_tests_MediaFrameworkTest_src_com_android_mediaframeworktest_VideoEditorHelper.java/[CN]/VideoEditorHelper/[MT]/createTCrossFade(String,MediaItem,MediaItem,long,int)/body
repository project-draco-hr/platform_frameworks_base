{
  Log.i("TransitionCrossfade Details === ","Transid ID = " + transitionId + " Duration= "+ durationMs+ " Behaviour "+ behavior);
  TransitionCrossfade transitionCF=null;
  transitionCF=new TransitionCrossfade(transitionId,afterMediaItem,beforeMediaItem,durationMs,behavior);
  return transitionCF;
}
