{
  String msg="";
  String currentVectorString=(currentVector == null) ? "null" : currentVector.toString();
  String previousVectorString=(previousVector == null) ? "null" : previousVector.toString();
  msg+="previousVector = " + previousVectorString;
  msg+=", currentVector = " + currentVectorString;
  msg+=", sampleCount = " + sampleCount;
  msg+=", energy = " + energy;
  return msg;
}
