{
switch (className) {
case "long":
    return long.class;
case "int":
  return int.class;
case "short":
return short.class;
case "byte":
return byte.class;
case "char":
return char.class;
case "float":
return float.class;
case "double":
return double.class;
case "boolean":
return boolean.class;
default :
return Class.forName(className,false,classLoader);
}
}
