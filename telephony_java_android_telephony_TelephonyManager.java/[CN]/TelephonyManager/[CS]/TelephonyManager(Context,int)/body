{
  mSubId=subId;
  Context appContext=context.getApplicationContext();
  if (appContext != null) {
    mContext=appContext;
  }
 else {
    mContext=context;
  }
  mSubscriptionManager=SubscriptionManager.from(mContext);
  if (sRegistry == null) {
    sRegistry=ITelephonyRegistry.Stub.asInterface(ServiceManager.getService("telephony.registry"));
