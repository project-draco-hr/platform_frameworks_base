{
  long[] subId=SubscriptionManager.getSubId(slotId);
  if (subId == null || subId.length == 0) {
    return null;
  }
  try {
    return getSubscriberInfo().getDeviceIdUsingSubId(subId[0]);
  }
 catch (  RemoteException ex) {
    return null;
  }
catch (  NullPointerException ex) {
    return null;
  }
}
