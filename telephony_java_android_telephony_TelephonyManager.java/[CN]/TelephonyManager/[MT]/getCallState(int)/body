{
  try {
    ITelephony telephony=getITelephony();
    if (telephony == null)     return CALL_STATE_IDLE;
    return telephony.getCallStateForSubscriber(subId);
  }
 catch (  RemoteException ex) {
    return CALL_STATE_IDLE;
  }
catch (  NullPointerException ex) {
    return CALL_STATE_IDLE;
  }
}
