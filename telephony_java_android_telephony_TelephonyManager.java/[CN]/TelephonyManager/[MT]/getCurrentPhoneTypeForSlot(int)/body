{
  try {
    ITelephony telephony=getITelephony();
    if (telephony != null) {
      return telephony.getActivePhoneTypeForSlot(slotId);
    }
 else {
      return getPhoneTypeFromProperty(slotId);
    }
  }
 catch (  RemoteException ex) {
    return getPhoneTypeFromProperty(slotId);
  }
catch (  NullPointerException ex) {
    return getPhoneTypeFromProperty(slotId);
  }
}
