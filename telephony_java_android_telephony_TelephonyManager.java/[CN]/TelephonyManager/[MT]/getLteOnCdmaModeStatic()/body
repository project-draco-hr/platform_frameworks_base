{
  int retVal;
  int curVal;
  String productType="";
  curVal=SystemProperties.getInt(TelephonyProperties.PROPERTY_LTE_ON_CDMA_DEVICE,PhoneConstants.LTE_ON_CDMA_UNKNOWN);
  retVal=curVal;
  if (retVal == PhoneConstants.LTE_ON_CDMA_UNKNOWN) {
    Matcher matcher=sProductTypePattern.matcher(sKernelCmdLine);
    if (matcher.find()) {
      productType=matcher.group(1);
      if (sLteOnCdmaProductType.equals(productType)) {
        retVal=PhoneConstants.LTE_ON_CDMA_TRUE;
      }
 else {
        retVal=PhoneConstants.LTE_ON_CDMA_FALSE;
      }
    }
 else {
      retVal=PhoneConstants.LTE_ON_CDMA_FALSE;
    }
  }
  Log.d(TAG,"getLteOnCdmaMode=" + retVal + " curVal="+ curVal+ " product_type='"+ productType+ "' lteOnCdmaProductType='"+ sLteOnCdmaProductType+ "'");
  return retVal;
}
