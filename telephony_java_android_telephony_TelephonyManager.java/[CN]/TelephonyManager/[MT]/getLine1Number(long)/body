{
  String number=null;
  try {
    number=getITelephony().getLine1NumberForDisplay(subId);
  }
 catch (  RemoteException ex) {
  }
catch (  NullPointerException ex) {
  }
  if (number != null) {
    return number;
  }
  try {
    return getSubscriberInfo().getLine1NumberUsingSubId(subId);
  }
 catch (  RemoteException ex) {
    return null;
  }
catch (  NullPointerException ex) {
    return null;
  }
}
