{
  try {
    return getITelephony().getCarrierPackageNamesForBroadcastIntent(intent);
  }
 catch (  RemoteException ex) {
    Rlog.e(TAG,"getCarrierPackageNamesForBroadcastIntent RemoteException",ex);
  }
catch (  NullPointerException ex) {
