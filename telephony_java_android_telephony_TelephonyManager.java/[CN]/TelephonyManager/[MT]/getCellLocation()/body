{
  try {
    Bundle bundle=getITelephony().getCellLocation();
    if (bundle.isEmpty())     return null;
    CellLocation cl=CellLocation.newFromBundle(bundle);
    if (cl.isEmpty())     return null;
    return cl;
  }
 catch (  RemoteException ex) {
    return null;
  }
catch (  NullPointerException ex) {
    return null;
  }
}
