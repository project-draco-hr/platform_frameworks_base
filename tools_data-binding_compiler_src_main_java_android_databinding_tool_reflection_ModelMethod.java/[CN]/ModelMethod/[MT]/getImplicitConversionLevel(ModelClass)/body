{
  if (primitive == null) {
    return -1;
  }
 else   if (primitive.isByte()) {
    return 0;
  }
 else   if (primitive.isChar()) {
    return 1;
  }
 else   if (primitive.isShort()) {
    return 2;
  }
 else   if (primitive.isInt()) {
    return 3;
  }
 else   if (primitive.isLong()) {
    return 4;
  }
 else   if (primitive.isFloat()) {
    return 5;
  }
 else   if (primitive.isDouble()) {
    return 6;
  }
 else {
    return -1;
  }
}
