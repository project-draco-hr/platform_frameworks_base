{
  List<URL> urlList=new ArrayList<URL>();
  Enumeration<URL> urls=null;
  try {
    urls=mClassLoader.getResources(name);
    if (urls != null) {
      while (urls.hasMoreElements()) {
        urlList.add(urls.nextElement());
      }
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  return urlList;
}
