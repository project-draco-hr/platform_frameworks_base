{
  Log.d(this,"call %s",originalRequest);
  onCreateConnections(originalRequest,new OutgoingCallResponse<Connection>(){
    @Override public void onSuccess(    ConnectionRequest request,    Connection connection){
      Log.d(this,"adapter handleSuccessfulOutgoingCall %s",request.getCallId());
      mAdapter.handleSuccessfulOutgoingCall(request);
      addConnection(request.getCallId(),connection);
    }
    @Override public void onFailure(    ConnectionRequest request,    int code,    String msg){
      mAdapter.handleFailedOutgoingCall(request,code,msg);
    }
    @Override public void onCancel(    ConnectionRequest request){
      mAdapter.cancelOutgoingCall(request);
    }
  }
);
}
