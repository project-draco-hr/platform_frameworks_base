{
  if (number == null) {
    return "";
  }
  if (PII_DEBUG) {
    return number;
  }
  StringBuilder builder=new StringBuilder();
  for (int i=0; i < number.length(); i++) {
    char c=number.charAt(i);
    if (c == '-' || c == '@' || c == '.') {
      builder.append(c);
    }
 else {
      builder.append('x');
    }
  }
  return builder.toString();
}
