{
  Log.d(this,"isCompatibleWith %s",callInfo);
  onFindSubscriptions(callInfo.getHandle(),new Response<Uri,Subscription>(){
    @Override public void onResult(    Uri handle,    Subscription... result){
      boolean isCompatible=result.length > 0;
      Log.d(this,"adapter setIsCompatibleWith " + callInfo.getId() + " "+ isCompatible);
      getAdapter().setIsCompatibleWith(callInfo.getId(),isCompatible);
    }
    @Override public void onError(    Uri handle,    int code,    String msg){
      Log.w(this,"Error in onFindSubscriptions %s %d %s",handle,code,msg);
      getAdapter().setIsCompatibleWith(callInfo.getId(),false);
    }
  }
);
}
