{
  Log.d(this,"call %s",callInfo);
  onCreateConnections(new ConnectionRequest(callInfo.getId(),callInfo.getHandle(),callInfo.getExtras()),new OutgoingCallResponse<Connection>(){
    @Override public void onSuccess(    ConnectionRequest request,    Connection connection){
      Log.d(this,"adapter handleSuccessfulOutgoingCall %s",callInfo.getId());
      getAdapter().handleSuccessfulOutgoingCall(callInfo.getId());
      addConnection(callInfo.getId(),connection);
    }
    @Override public void onFailure(    ConnectionRequest request,    int code,    String msg){
      getAdapter().handleFailedOutgoingCall(request,code,msg);
    }
    @Override public void onCancel(    ConnectionRequest request){
      getAdapter().cancelOutgoingCall(callInfo.getId());
    }
  }
);
}
