{
  mHandler.post(new Runnable(){
    @Override public void run(){
      try {
        if (DEBUG)         Log.d(TAG,"notifyVideoAvailable");
        mSessionCallback.onVideoAvailable();
      }
 catch (      RemoteException e) {
        Log.w(TAG,"error in notifyVideoAvailable");
      }
    }
  }
);
}
