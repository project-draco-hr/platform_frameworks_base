{
  mHandler.post(new Runnable(){
    @Override public void run(){
      try {
        if (DEBUG)         Log.d(TAG,"dispatchContentAllowed");
        mSessionCallback.onContentAllowed();
      }
 catch (      RemoteException e) {
        Log.w(TAG,"error in dispatchContentAllowed");
      }
    }
  }
);
}
