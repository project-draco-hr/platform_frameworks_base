{
  mHandler.post(new Runnable(){
    @Override public void run(){
      try {
        if (DEBUG)         Log.d(TAG,"dispatchChannelRetuned");
        mSessionCallback.onChannelRetuned(channelUri);
      }
 catch (      RemoteException e) {
        Log.w(TAG,"error in dispatchChannelRetuned");
      }
    }
  }
);
}
