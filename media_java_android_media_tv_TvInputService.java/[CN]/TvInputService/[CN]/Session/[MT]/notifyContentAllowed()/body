{
  runOnMainThread(new Runnable(){
    @Override public void run(){
      try {
        if (DEBUG)         Log.d(TAG,"notifyContentAllowed");
        mSessionCallback.onContentAllowed();
      }
 catch (      RemoteException e) {
        Log.w(TAG,"error in notifyContentAllowed");
      }
    }
  }
);
}
