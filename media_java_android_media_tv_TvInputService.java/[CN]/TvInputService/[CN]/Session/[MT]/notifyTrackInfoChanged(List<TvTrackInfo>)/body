{
  mHandler.post(new Runnable(){
    @Override public void run(){
      try {
        if (DEBUG)         Log.d(TAG,"notifyTrackInfoChanged");
        mSessionCallback.onTrackInfoChanged(tracks);
      }
 catch (      RemoteException e) {
        Log.w(TAG,"error in notifyTrackInfoChanged");
      }
    }
  }
);
}
