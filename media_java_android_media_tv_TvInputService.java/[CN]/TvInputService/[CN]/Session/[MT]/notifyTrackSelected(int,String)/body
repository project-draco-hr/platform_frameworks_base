{
  mHandler.post(new Runnable(){
    @Override public void run(){
      try {
        if (DEBUG)         Log.d(TAG,"notifyTrackSelected");
        mSessionCallback.onTrackSelected(type,trackId);
      }
 catch (      RemoteException e) {
        Log.w(TAG,"error in notifyTrackSelected");
      }
    }
  }
);
}
