{
  View overlayViewParent=views[0];
  try {
    Thread.sleep(DETACH_OVERLAY_VIEW_TIMEOUT);
  }
 catch (  InterruptedException e) {
    return null;
  }
  if (isCancelled()) {
    return null;
  }
  if (overlayViewParent.isAttachedToWindow()) {
    Log.e(TAG,"Time out on releasing overlay view. Killing " + overlayViewParent.getContext().getPackageName());
    Process.killProcess(Process.myPid());
  }
  return null;
}
