{
  assertRunOnServiceThread();
  byte params[]=message.getParams();
  if (params.length < 1) {
    Slog.w(TAG,"Invalide <Report Audio Status> message:" + message);
    return true;
  }
  int mute=params[0] & 0x80;
  int volume=params[0] & 0x7F;
  setAudioStatus(mute == 0x80,volume);
  return true;
}
