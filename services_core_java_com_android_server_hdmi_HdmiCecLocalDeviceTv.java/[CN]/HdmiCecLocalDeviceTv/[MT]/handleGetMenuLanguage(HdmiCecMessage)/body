{
  assertRunOnServiceThread();
  HdmiCecMessage command=HdmiCecMessageBuilder.buildSetMenuLanguageCommand(mAddress,Locale.getDefault().getISO3Language());
  if (command != null) {
    mService.sendCecCommand(command);
  }
 else {
    Slog.w(TAG,"Failed to respond to <Get Menu Language>: " + message.toString());
  }
  return true;
}
