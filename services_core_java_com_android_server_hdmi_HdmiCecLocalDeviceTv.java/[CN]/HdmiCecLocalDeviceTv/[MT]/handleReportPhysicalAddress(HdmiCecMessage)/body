{
  if (mService.hasAction(DeviceDiscoveryAction.class)) {
    Slog.i(TAG,"Ignore unrecognizable <Report Physical Address> " + "because Device Discovery Action is on-going:" + message);
    return true;
  }
  int physicalAddress=HdmiUtils.twoBytesToInt(message.getParams());
  mService.addAndStartAction(new NewDeviceAction(mService,mAddress,message.getSource(),physicalAddress));
  return true;
}
