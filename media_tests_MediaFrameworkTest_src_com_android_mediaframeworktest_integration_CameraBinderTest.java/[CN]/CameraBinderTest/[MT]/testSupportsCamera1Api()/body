{
  for (int cameraId=0; cameraId < mUtils.getGuessedNumCameras(); ++cameraId) {
    int res=mUtils.getCameraService().supportsCameraApi(cameraId,API_VERSION_1);
    assertEquals("Camera service returned bad value when queried if it supports camera1 api: " + res + " for camera ID "+ cameraId,CameraBinderTestUtils.NO_ERROR,res);
  }
}
