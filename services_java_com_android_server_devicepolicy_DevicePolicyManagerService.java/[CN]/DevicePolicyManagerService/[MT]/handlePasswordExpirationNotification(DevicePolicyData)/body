{
synchronized (this) {
    final long now=System.currentTimeMillis();
    final int N=policy.mAdminList.size();
    if (N <= 0) {
      return;
    }
    for (int i=0; i < N; i++) {
      ActiveAdmin admin=policy.mAdminList.get(i);
      if (admin.info.usesPolicy(DeviceAdminInfo.USES_POLICY_EXPIRE_PASSWORD) && admin.passwordExpirationTimeout > 0L && admin.passwordExpirationDate > 0L && now >= admin.passwordExpirationDate - EXPIRATION_GRACE_PERIOD_MS) {
        sendAdminCommandLocked(admin,DeviceAdminReceiver.ACTION_PASSWORD_EXPIRING);
      }
    }
    setExpirationAlarmCheckLocked(mContext,policy);
  }
}
