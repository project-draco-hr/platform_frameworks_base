{
  assertEquals(ViewGroup.FOCUS_BEFORE_DESCENDANTS,a.beforeDescendants.getDescendantFocusability());
  assertEquals(ViewGroup.FOCUS_AFTER_DESCENDANTS,a.afterDescendants.getDescendantFocusability());
  assertEquals(ViewGroup.FOCUS_BLOCK_DESCENDANTS,a.blocksDescendants.getDescendantFocusability());
  assertTrue(a.beforeDescendantsChild.isFocusable());
  assertTrue(a.afterDescendantsChild.isFocusable());
  assertTrue(a.blocksDescendantsChild.isFocusable());
}
