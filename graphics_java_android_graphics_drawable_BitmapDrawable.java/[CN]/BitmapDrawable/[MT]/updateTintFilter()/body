{
  final ColorStateList tint=mBitmapState.mTint;
  final Mode tintMode=mBitmapState.mTintMode;
  if (tint != null && tintMode != null) {
    if (mTintFilter == null) {
      mTintFilter=new PorterDuffColorFilter(0,tintMode);
    }
 else {
      mTintFilter.setMode(tintMode);
    }
  }
 else {
    mTintFilter=null;
  }
}
