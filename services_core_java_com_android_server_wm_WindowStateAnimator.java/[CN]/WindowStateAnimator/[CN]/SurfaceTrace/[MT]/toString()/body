{
  return "Surface " + Integer.toHexString(System.identityHashCode(this)) + " "+ mName+ " ("+ mLayerStack+ "): shown="+ mShown+ " layer="+ mLayer+ " alpha="+ mSurfaceTraceAlpha+ " "+ mPosition.x+ ","+ mPosition.y+ " "+ mSize.x+ "x"+ mSize.y+ " crop="+ mWindowCrop.toShortString()+ " opaque="+ mIsOpaque+ " ("+ mDsdx+ ","+ mDtdx+ ","+ mDsdy+ ","+ mDtdy+ ")";
}
