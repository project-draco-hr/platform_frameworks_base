{
  Map<String,ResourceValue> typeMap;
  if (frameworkOnly == false) {
    typeMap=mProjectResources.get(resType);
    if (typeMap != null) {
      ResourceValue item=typeMap.get(resName);
      if (item != null) {
        return item;
      }
    }
  }
  typeMap=mFrameworkResources.get(resType);
  if (typeMap != null) {
    ResourceValue item=typeMap.get(resName);
    if (item != null) {
      return item;
    }
    if (BridgeConstants.RES_ID.equals(resType)) {
      if (Bridge.getResourceValue(resType,resName) != null) {
        return new ResourceValue(resType,resName,true);
      }
    }
  }
  if ("+id".equals(resType) == false && "+android:id".equals(resType) == false) {
    Bridge.getLog().warning(LayoutLog.TAG_RESOURCES_RESOLVE,"Couldn't resolve resource @" + (frameworkOnly ? "android:" : "") + resType+ "/"+ resName,new ResourceValue(resType,resName,frameworkOnly));
  }
  return null;
}
