{
  boolean first=true;
  for (int i=0; i < mMimeTypes.length; i++) {
    if (!first) {
      b.append(' ');
    }
    first=false;
    b.append(mMimeTypes[i]);
  }
  if (mLabel != null) {
    if (!first) {
      b.append(' ');
    }
    first=false;
    b.append('"');
    b.append(mLabel);
    b.append('"');
  }
  return !first;
}
