{
  if (builder.mCount == 0) {
    mKeys=null;
    mCoordinates=null;
  }
 else {
    mKeys=new int[builder.mCount];
    mCoordinates=new float[builder.mCount * 4];
    System.arraycopy(builder.mKeys,0,mKeys,0,builder.mCount);
    System.arraycopy(builder.mCoordinates,0,mCoordinates,0,builder.mCount * 4);
  }
}
