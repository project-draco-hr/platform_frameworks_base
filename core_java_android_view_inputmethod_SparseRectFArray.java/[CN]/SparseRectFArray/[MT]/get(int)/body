{
  if (mKeys == null) {
    return null;
  }
  if (index < 0) {
    return null;
  }
  final int arrayIndex=Arrays.binarySearch(mKeys,index);
  if (arrayIndex < 0) {
    return null;
  }
  final int baseCoordIndex=arrayIndex * 4;
  return new RectF(mCoordinates[baseCoordIndex],mCoordinates[baseCoordIndex + 1],mCoordinates[baseCoordIndex + 2],mCoordinates[baseCoordIndex + 3]);
}
