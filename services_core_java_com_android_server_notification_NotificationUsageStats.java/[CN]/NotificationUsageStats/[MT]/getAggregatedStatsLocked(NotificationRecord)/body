{
  if (!ENABLE_AGGREGATED_IN_MEMORY_STATS) {
    return EMPTY_AGGREGATED_STATS;
  }
  StatusBarNotification n=record.sbn;
  String user=String.valueOf(n.getUserId());
  String userPackage=user + ":" + n.getPackageName();
  return new AggregatedStats[]{getOrCreateAggregatedStatsLocked(user),getOrCreateAggregatedStatsLocked(userPackage),getOrCreateAggregatedStatsLocked(n.getKey())};
}
