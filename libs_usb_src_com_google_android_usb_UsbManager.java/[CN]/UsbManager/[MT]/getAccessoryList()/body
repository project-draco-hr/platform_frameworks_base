{
  try {
    android.hardware.usb.UsbAccessory accessory=mService.getCurrentAccessory();
    if (accessory == null) {
      return null;
    }
 else {
      return new UsbAccessory[]{new UsbAccessory(accessory)};
    }
  }
 catch (  RemoteException e) {
    Log.e(TAG,"RemoteException in getAccessoryList",e);
    return null;
  }
}
