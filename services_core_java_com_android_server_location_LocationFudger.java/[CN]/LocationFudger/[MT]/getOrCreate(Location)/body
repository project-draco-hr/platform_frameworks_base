{
synchronized (mLock) {
    Location coarse=location.getExtraLocation(Location.EXTRA_COARSE_LOCATION);
    if (coarse == null) {
      return addCoarseLocationExtraLocked(location);
    }
    if (coarse.getAccuracy() < mAccuracyInMeters) {
      return addCoarseLocationExtraLocked(location);
    }
    return coarse;
  }
}
