{
  if (arguments == null) {
    arguments=new Bundle();
  }
  final boolean includeRealDeviceTest=Boolean.parseBoolean(arguments.getString("realDeviceTest","false"));
  if (!includeRealDeviceTest) {
    arguments.putString("notAnnotation","com.android.mtp.RealDeviceTest");
  }
  super.onCreate(arguments);
  if (includeRealDeviceTest) {
    addTestListener(this);
  }
}
