{
  long start=System.currentTimeMillis();
  InputStream is=openStream("cube_env.png");
  if (is == null) {
    return new Boolean(false);
  }
  Bitmap b=BitmapFactory.decodeStream(is);
  tempEnv=Allocation.createCubemapFromBitmap(mRS,b,MipmapControl.MIPMAP_ON_SYNC_TO_TEXTURE,Allocation.USAGE_GRAPHICS_TEXTURE);
  is=openStream("cube_spec.png");
  if (is == null) {
    return new Boolean(false);
  }
  b=BitmapFactory.decodeStream(is);
  tempDiff=Allocation.createCubemapFromBitmap(mRS,b,MipmapControl.MIPMAP_ON_SYNC_TO_TEXTURE,Allocation.USAGE_GRAPHICS_TEXTURE);
  long end=System.currentTimeMillis();
  Log.v("TIMER","Image load time: " + (end - start));
  return new Boolean(true);
}
