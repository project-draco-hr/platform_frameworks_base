{
  if (mApplicationObject != null) {
    try {
      Log.e(TAG,Log.getStackTraceString(t));
      IActivityManager am=ActivityManagerNative.getDefault();
      am.handleApplicationError(mApplicationObject,tag,new ApplicationErrorReport.CrashInfo(t));
    }
 catch (    Throwable t2) {
      try {
        Log.e(TAG,"Error reporting crash: " + Log.getStackTraceString(t2));
      }
 catch (      Throwable t3) {
      }
    }
 finally {
      Process.killProcess(Process.myPid());
      System.exit(10);
    }
  }
 else {
    try {
      Log.e(TAG,"*** EXCEPTION IN SYSTEM PROCESS.  System will crash.");
      Log.e(tag,Log.getStackTraceString(t));
    }
 catch (    Throwable t2) {
    }
 finally {
      Process.killProcess(Process.myPid());
      System.exit(10);
    }
  }
}
