{
  if (mStatsChanged) {
    Slog.i(TAG,mLogPrefix + "Flushing usage stats to disk");
    try {
      for (int i=0; i < mCurrentStats.length; i++) {
        mDatabase.putUsageStats(i,mCurrentStats[i]);
      }
      mStatsChanged=false;
    }
 catch (    IOException e) {
      Slog.e(TAG,mLogPrefix + "Failed to persist active stats",e);
    }
  }
}
