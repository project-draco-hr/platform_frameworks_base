{
synchronized (mMutex) {
    try {
      ManagedServiceInfo info=newServiceInfo(service,component,userid,true,null,Build.VERSION_CODES.LOLLIPOP);
      service.asBinder().linkToDeath(info,0);
      mServices.add(info);
      return info;
    }
 catch (    RemoteException e) {
    }
  }
  return null;
}
