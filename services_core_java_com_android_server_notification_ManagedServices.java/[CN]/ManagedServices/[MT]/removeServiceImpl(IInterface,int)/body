{
  ManagedServiceInfo serviceInfo=null;
synchronized (mMutex) {
    final int N=mServices.size();
    for (int i=N - 1; i >= 0; i--) {
      final ManagedServiceInfo info=mServices.get(i);
      if (info.service.asBinder() == service.asBinder() && info.userid == userid) {
        serviceInfo=mServices.remove(i);
      }
    }
  }
  return serviceInfo;
}
