{
  super(tag,TagTechnology.MIFARE_CLASSIC);
  NfcA a=NfcA.get(tag);
  mIsEmulated=false;
switch (a.getSak()) {
case 0x08:
    mType=TYPE_CLASSIC;
  mSize=SIZE_1K;
break;
case 0x09:
mType=TYPE_CLASSIC;
mSize=SIZE_MINI;
break;
case 0x10:
mType=TYPE_PLUS;
mSize=SIZE_2K;
break;
case 0x11:
mType=TYPE_PLUS;
mSize=SIZE_4K;
break;
case 0x18:
mType=TYPE_CLASSIC;
mSize=SIZE_4K;
break;
case 0x28:
mType=TYPE_CLASSIC;
mSize=SIZE_1K;
mIsEmulated=true;
break;
case 0x38:
mType=TYPE_CLASSIC;
mSize=SIZE_4K;
mIsEmulated=true;
break;
case 0x88:
mType=TYPE_CLASSIC;
mSize=SIZE_1K;
break;
case 0x98:
case 0xB8:
mType=TYPE_PRO;
mSize=SIZE_4K;
break;
default :
throw new RuntimeException("Tag incorrectly enumerated as MIFARE Classic, SAK = " + a.getSak());
}
}
