{
  if (this.left < right && left < this.right && this.top < bottom && top < this.bottom) {
    if (this.left < left) {
      this.left=left;
    }
    if (this.top < top) {
      this.top=top;
    }
    if (this.right > right) {
      this.right=right;
    }
    if (this.bottom > bottom) {
      this.bottom=bottom;
    }
    return true;
  }
  return false;
}
