{
  int newRotation;
  while (mWaitCounter < WAIT_TIMES_MS.length) {
    updateOrientation();
    int waitTimeMs;
synchronized (mCounterLock) {
      waitTimeMs=WAIT_TIMES_MS[mWaitCounter];
      mWaitCounter++;
    }
    try {
      sleep(waitTimeMs);
    }
 catch (    InterruptedException e) {
    }
  }
}
