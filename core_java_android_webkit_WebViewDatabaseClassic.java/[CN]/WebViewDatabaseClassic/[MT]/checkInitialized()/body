{
synchronized (this) {
    while (!mInitialized) {
      try {
        wait();
      }
 catch (      InterruptedException e) {
        Log.e(LOGTAG,"Caught exception while checking " + "initialization");
        Log.e(LOGTAG,Log.getStackTraceString(e));
      }
    }
  }
  return sDatabase != null;
}
