{
switch (mWifiApState.get()) {
case WIFI_AP_STATE_DISABLING:
    return "disabling";
case WIFI_AP_STATE_DISABLED:
  return "disabled";
case WIFI_AP_STATE_ENABLING:
return "enabling";
case WIFI_AP_STATE_ENABLED:
return "enabled";
case WIFI_AP_STATE_FAILED:
return "failed";
default :
return "[invalid state]";
}
}
