{
  String status=mWifiNative.status();
  if (status == null) {
    return null;
  }
  String[] lines=status.split("\n");
  for (  String line : lines) {
    String[] prop=line.split(" *= *");
    if (prop.length < 2)     continue;
    String name=prop[0];
    String value=prop[1];
    if (name.equalsIgnoreCase("ssid"))     return value;
  }
  return null;
}
