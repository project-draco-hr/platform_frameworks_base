{
  if (mNotedBatchedScanWorkSource != null && (mNotedBatchedScanWorkSource.equals(mBatchedScanWorkSource) == false || mNotedBatchedScanCsph != mBatchedScanCsph)) {
    try {
      mBatteryStats.noteWifiBatchedScanStoppedFromSource(mNotedBatchedScanWorkSource);
    }
 catch (    RemoteException e) {
      log(e.toString());
    }
 finally {
      mNotedBatchedScanWorkSource=null;
      mNotedBatchedScanCsph=0;
    }
  }
  try {
    mBatteryStats.noteWifiBatchedScanStartedFromSource(mBatchedScanWorkSource,mBatchedScanCsph);
    mNotedBatchedScanWorkSource=mBatchedScanWorkSource;
    mNotedBatchedScanCsph=mBatchedScanCsph;
  }
 catch (  RemoteException e) {
    log(e.toString());
  }
}
