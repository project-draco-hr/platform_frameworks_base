{
  if (DBG)   Log.d(TAG,getName() + message.toString() + "\n");
switch (message.what) {
case CMD_START_DRIVER:
    mWakeLock.acquire();
  WifiNative.startDriverCommand();
transitionTo(mDriverStartingState);
mWakeLock.release();
break;
default :
return NOT_HANDLED;
}
EventLog.writeEvent(EVENTLOG_WIFI_EVENT_HANDLED,message.what);
return HANDLED;
}
