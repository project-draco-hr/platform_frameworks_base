{
  Graphics2D g=getGraphics2d();
  g=(Graphics2D)g.create();
  if (paint.isAntiAliased()) {
    g.setRenderingHint(RenderingHints.KEY_ANTIALIASING,RenderingHints.VALUE_ANTIALIAS_ON);
    g.setRenderingHint(RenderingHints.KEY_TEXT_ANTIALIASING,RenderingHints.VALUE_TEXT_ANTIALIAS_ON);
  }
  g.setColor(new Color(paint.getColor()));
  int alpha=paint.getAlpha();
  float falpha=alpha / 255.f;
  int style=paint.getStyle();
  if (style == Paint.Style.STROKE.nativeInt || style == Paint.Style.FILL_AND_STROKE.nativeInt) {
    g.setStroke(new BasicStroke(paint.getStrokeWidth(),paint.getJavaCap(),paint.getJavaJoin(),paint.getStrokeMiter()));
  }
  int nativeShader=paint.getShader();
  Shader_Delegate shaderDelegate=Shader_Delegate.getDelegate(nativeShader);
  if (shaderDelegate != null) {
    java.awt.Paint shaderPaint=shaderDelegate.getJavaPaint();
    if (shaderPaint != null) {
      g.setPaint(shaderPaint);
    }
 else {
      if (mLogger != null) {
        mLogger.warning(String.format("Shader '%1$s' is not supported in the Layout Editor.",shaderDelegate.getClass().getCanonicalName()));
      }
    }
  }
  return g;
}
