{
  userState.mIsAccessibilityEnabled=Settings.Secure.getIntForUser(mContext.getContentResolver(),Settings.Secure.ACCESSIBILITY_ENABLED,0,userState.mUserId) == 1;
  if (userState.mIsAccessibilityEnabled) {
    manageServicesLocked(userState);
  }
 else {
    unbindAllServicesLocked(userState);
  }
}
