{
synchronized (mLock) {
    mSecurityPolicy.resolveCallingUserIdEnforcingPermissionsLocked(UserHandle.getCallingUserId());
    IBinder token=window.asBinder();
    final boolean removedGlobal=removeAccessibilityInteractionConnectionInternalLocked(token,mGlobalWindowTokens,mGlobalInteractionConnections);
    if (removedGlobal) {
      return;
    }
    final int userCount=mUserStates.size();
    for (int i=0; i < userCount; i++) {
      UserState userState=mUserStates.valueAt(i);
      final boolean removedForUser=removeAccessibilityInteractionConnectionInternalLocked(token,userState.mWindowTokens,userState.mInteractionConnections);
      if (removedForUser) {
        return;
      }
    }
  }
}
