{
synchronized (mLock) {
    if (mUiAutomationService != null && mUiAutomationService.mServiceInterface != null && serviceClient != null && mUiAutomationService.mServiceInterface.asBinder() == serviceClient.asBinder()) {
      mUiAutomationService.binderDied();
    }
 else {
      throw new IllegalStateException("UiAutomationService " + serviceClient + " not registered!");
    }
  }
}
