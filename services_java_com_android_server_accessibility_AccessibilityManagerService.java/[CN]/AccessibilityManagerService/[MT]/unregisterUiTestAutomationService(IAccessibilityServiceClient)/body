{
synchronized (mLock) {
    UserState userState=getCurrentUserStateLocked();
    if (userState.mUiAutomationService != null && userState.mUiAutomationService.mServiceInterface != null && serviceClient != null && userState.mUiAutomationService.mServiceInterface.asBinder() == serviceClient.asBinder()) {
      userState.mUiAutomationService.binderDied();
    }
 else {
      throw new IllegalStateException("UiAutomationService " + serviceClient + " not registered!");
    }
  }
}
