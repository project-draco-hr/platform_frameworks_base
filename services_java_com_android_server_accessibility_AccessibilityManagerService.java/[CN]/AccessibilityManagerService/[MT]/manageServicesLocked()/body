{
  unbindAutomationService();
  populateEnabledServicesLocked(mEnabledServices);
  final int enabledInstalledServicesCount=updateServicesStateLocked(mInstalledServices,mEnabledServices);
  if (mIsAccessibilityEnabled && enabledInstalledServicesCount == 0) {
    Settings.Secure.putInt(mContext.getContentResolver(),Settings.Secure.ACCESSIBILITY_ENABLED,0);
  }
}
