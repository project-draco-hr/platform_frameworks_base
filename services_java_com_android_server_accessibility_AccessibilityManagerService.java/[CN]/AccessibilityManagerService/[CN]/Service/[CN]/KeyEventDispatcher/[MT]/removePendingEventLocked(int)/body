{
  PendingEvent previous=null;
  PendingEvent current=mPendingEvents;
  while (current != null) {
    if (current.sequence == sequence) {
      if (previous != null) {
        previous.next=current.next;
      }
 else {
        mPendingEvents=current.next;
      }
      current.next=null;
      return current;
    }
    previous=current;
    current=current.next;
  }
  return null;
}
