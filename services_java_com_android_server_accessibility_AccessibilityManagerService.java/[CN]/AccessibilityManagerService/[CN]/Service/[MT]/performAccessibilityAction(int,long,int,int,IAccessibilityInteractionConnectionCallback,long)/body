{
  final int resolvedWindowId=resolveAccessibilityWindowId(accessibilityWindowId);
  IAccessibilityInteractionConnection connection=null;
synchronized (mLock) {
    final boolean permissionGranted=mSecurityPolicy.canPerformActionLocked(this,resolvedWindowId,action);
    if (!permissionGranted) {
      return false;
    }
 else {
      connection=getConnectionLocked(resolvedWindowId);
      if (connection == null) {
        return false;
      }
    }
  }
  final int interrogatingPid=Binder.getCallingPid();
  final long identityToken=Binder.clearCallingIdentity();
  try {
    connection.performAccessibilityAction(accessibilityNodeId,action,interactionId,callback,interrogatingPid,interrogatingTid);
  }
 catch (  RemoteException re) {
    if (DEBUG) {
      Slog.e(LOG_TAG,"Error calling performAccessibilityAction()");
    }
  }
 finally {
    Binder.restoreCallingIdentity(identityToken);
  }
  return true;
}
