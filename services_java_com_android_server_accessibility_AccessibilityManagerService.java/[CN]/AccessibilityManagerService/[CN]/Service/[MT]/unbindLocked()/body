{
  if (mService == null) {
    return false;
  }
  if (!mIsAutomation) {
    mContext.unbindService(this);
  }
 else {
    UserState userState=getUserStateLocked(mUserId);
    userState.mUiAutomationService=null;
    userState.mUiAutomationServiceClient=null;
  }
  removeServiceLocked(this);
  return true;
}
