{
switch (action) {
case AccessibilityNodeInfo.ACTION_NEXT_AT_GRANULARITY:
case AccessibilityNodeInfo.ACTION_PREVIOUS_AT_GRANULARITY:
{
      if (arguments.size() == 1) {
        final int granularity=arguments.getInt(AccessibilityNodeInfo.ACTION_ARGUMENT_GRANULARITY_INT);
        return (granularity & VALID_GRANULARITIES) != 0 && Integer.bitCount(granularity) == 1;
      }
    }
  break;
case AccessibilityNodeInfo.ACTION_NEXT_HTML_ELEMENT:
case AccessibilityNodeInfo.ACTION_PREVIOUS_HTML_ELEMENT:
{
  if (arguments.size() == 1) {
    String element=arguments.getString(AccessibilityNodeInfo.ACTION_ARGUMENT_HTML_ELEMENT_STRING);
    return !TextUtils.isEmpty(element);
  }
}
break;
}
return false;
}
