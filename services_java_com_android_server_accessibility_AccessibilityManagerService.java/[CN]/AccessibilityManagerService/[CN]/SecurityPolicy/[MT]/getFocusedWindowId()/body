{
  try {
    IBinder token=mWindowManagerService.getFocusedWindowToken();
    if (token != null) {
synchronized (mLock) {
        int windowId=getFocusedWindowIdLocked(token,mGlobalWindowTokens);
        if (windowId < 0) {
          windowId=getFocusedWindowIdLocked(token,getCurrentUserStateLocked().mWindowTokens);
        }
        return windowId;
      }
    }
  }
 catch (  RemoteException re) {
  }
  return -1;
}
