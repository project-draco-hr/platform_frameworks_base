{
  PorterDuff.Mode m=getMode(mode);
switch (m) {
case CLEAR:
    return AlphaComposite.getInstance(AlphaComposite.CLEAR,1.0f);
case DARKEN:
  break;
case DST:
return AlphaComposite.getInstance(AlphaComposite.DST,1.0f);
case DST_ATOP:
return AlphaComposite.getInstance(AlphaComposite.DST_ATOP,1.0f);
case DST_IN:
return AlphaComposite.getInstance(AlphaComposite.DST_IN,1.0f);
case DST_OUT:
return AlphaComposite.getInstance(AlphaComposite.DST_OUT,1.0f);
case DST_OVER:
return AlphaComposite.getInstance(AlphaComposite.DST_OVER,1.0f);
case LIGHTEN:
break;
case MULTIPLY:
break;
case SCREEN:
break;
case SRC:
return AlphaComposite.getInstance(AlphaComposite.SRC,1.0f);
case SRC_ATOP:
return AlphaComposite.getInstance(AlphaComposite.SRC_ATOP,1.0f);
case SRC_IN:
return AlphaComposite.getInstance(AlphaComposite.SRC_IN,1.0f);
case SRC_OUT:
return AlphaComposite.getInstance(AlphaComposite.SRC_OUT,1.0f);
case SRC_OVER:
return AlphaComposite.getInstance(AlphaComposite.SRC_OVER,1.0f);
case XOR:
return AlphaComposite.getInstance(AlphaComposite.XOR,1.0f);
}
Bridge.getLog().fidelityWarning(null,String.format("Unsupported PorterDuff Mode: %s",m.name()),null);
return AlphaComposite.getInstance(AlphaComposite.SRC_OVER);
}
