{
  StringBuilder sb=new StringBuilder();
  sb.append("[");
  if (mPsc != UNKNOWN_CID) {
    sb.append(Integer.toHexString(mPsc)).append("@").append(((mRssi == UNKNOWN_RSSI) ? "-" : mRssi));
  }
 else   if (mLac != UNKNOWN_CID && mCid != UNKNOWN_CID) {
    sb.append(Integer.toHexString(mLac)).append(Integer.toHexString(mCid)).append("@").append(((mRssi == UNKNOWN_RSSI) ? "-" : mRssi));
  }
  sb.append("]");
  return sb.toString();
}
