{
  List<InputMethodInfo> enabled=mSettings.getEnabledInputMethodListLocked();
  if (enabled != null && enabled.size() > 0) {
    int i=enabled.size();
    while (i > 0) {
      i--;
      final InputMethodInfo imi=enabled.get(i);
      if (isSystemIme(imi) && !imi.isAuxiliaryIme()) {
        break;
      }
    }
    return enabled.get(i);
  }
  return null;
}
