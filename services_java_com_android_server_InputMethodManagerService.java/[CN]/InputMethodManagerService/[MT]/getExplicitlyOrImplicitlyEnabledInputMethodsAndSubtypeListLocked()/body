{
  HashMap<InputMethodInfo,List<InputMethodSubtype>> enabledInputMethodAndSubtypes=new HashMap<InputMethodInfo,List<InputMethodSubtype>>();
  for (  InputMethodInfo imi : mSettings.getEnabledInputMethodListLocked()) {
    enabledInputMethodAndSubtypes.put(imi,mSettings.getEnabledInputMethodSubtypeListLocked(mContext,imi,true));
  }
  return enabledInputMethodAndSubtypes;
}
