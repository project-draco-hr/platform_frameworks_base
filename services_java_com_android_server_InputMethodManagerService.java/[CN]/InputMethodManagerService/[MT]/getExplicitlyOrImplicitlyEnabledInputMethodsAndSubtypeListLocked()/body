{
  HashMap<InputMethodInfo,List<InputMethodSubtype>> enabledInputMethodAndSubtypes=new HashMap<InputMethodInfo,List<InputMethodSubtype>>();
  for (  InputMethodInfo imi : getEnabledInputMethodList()) {
    enabledInputMethodAndSubtypes.put(imi,getEnabledInputMethodSubtypeListLocked(imi,true));
  }
  return enabledInputMethodAndSubtypes;
}
