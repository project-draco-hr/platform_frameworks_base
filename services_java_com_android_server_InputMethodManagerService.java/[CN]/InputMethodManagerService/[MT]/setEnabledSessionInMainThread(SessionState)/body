{
  if (mEnabledSession != session) {
    if (mEnabledSession != null) {
      try {
        if (DEBUG)         Log.v(TAG,"Disabling: " + mEnabledSession);
        mEnabledSession.method.setSessionEnabled(mEnabledSession.session,false);
      }
 catch (      RemoteException e) {
      }
    }
    mEnabledSession=session;
    try {
      if (DEBUG)       Log.v(TAG,"Enabling: " + mEnabledSession);
      session.method.setSessionEnabled(session.session,true);
    }
 catch (    RemoteException e) {
    }
  }
}
