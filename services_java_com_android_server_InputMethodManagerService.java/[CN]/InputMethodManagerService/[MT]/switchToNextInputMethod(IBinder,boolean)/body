{
synchronized (mMethodMap) {
    final ImeSubtypeListItem nextSubtype=mImListManager.getNextInputMethod(onlyCurrentIme,mMethodMap.get(mCurMethodId),mCurrentSubtype);
    if (nextSubtype == null) {
      return false;
    }
    setInputMethodWithSubtypeId(token,nextSubtype.mImi.getId(),nextSubtype.mSubtypeId);
    return true;
  }
}
