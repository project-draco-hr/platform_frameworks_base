{
  if (DBG)   log("isAudioOn()");
  if (mService != null && isEnabled()) {
    try {
      return mService.isAudioOn();
    }
 catch (    RemoteException e) {
      Log.e(TAG,Log.getStackTraceString(new Throwable()));
    }
  }
  if (mService == null)   Log.w(TAG,"Proxy not attached to service");
  return false;
}
