{
  if (mService != null) {
    try {
      if (mService.connectHeadset(address)) {
        return true;
      }
    }
 catch (    RemoteException e) {
      Log.e(TAG,e.toString());
    }
  }
 else {
    Log.w(TAG,"Proxy not attached to service");
    if (DBG)     Log.d(TAG,Log.getStackTraceString(new Throwable()));
  }
  return false;
}
