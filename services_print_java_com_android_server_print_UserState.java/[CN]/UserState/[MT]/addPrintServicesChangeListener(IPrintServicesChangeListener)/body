{
synchronized (mLock) {
    throwIfDestroyedLocked();
    if (mPrintServicesChangeListenerRecords == null) {
      mPrintServicesChangeListenerRecords=new ArrayList<>();
    }
    mPrintServicesChangeListenerRecords.add(new ListenerRecord<IPrintServicesChangeListener>(listener){
      @Override public void onBinderDied(){
        mPrintServicesChangeListenerRecords.remove(this);
      }
    }
);
  }
}
