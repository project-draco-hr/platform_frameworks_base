{
synchronized (mLock) {
    throwIfDestroyedLocked();
    if (mPrintServiceRecommendationsChangeListenerRecords == null) {
      mPrintServiceRecommendationsChangeListenerRecords=new ArrayList<>();
      mPrintServiceRecommendationsService=new RemotePrintServiceRecommendationService(mContext,UserHandle.getUserHandleForUid(mUserId),this);
    }
    mPrintServiceRecommendationsChangeListenerRecords.add(new ListenerRecord<IRecommendationsChangeListener>(listener){
      @Override public void onBinderDied(){
        mPrintServiceRecommendationsChangeListenerRecords.remove(this);
      }
    }
);
  }
}
