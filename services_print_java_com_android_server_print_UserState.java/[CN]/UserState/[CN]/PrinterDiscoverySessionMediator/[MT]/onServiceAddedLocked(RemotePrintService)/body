{
  if (mIsDestroyed) {
    Log.w(LOG_TAG,"Not updating added service - session destroyed");
    return;
  }
  mHandler.obtainMessage(SessionHandler.MSG_CREATE_PRINTER_DISCOVERY_SESSION,service).sendToTarget();
  if (!mStartedPrinterDiscoveryTokens.isEmpty()) {
    mHandler.obtainMessage(SessionHandler.MSG_START_PRINTER_DISCOVERY,service).sendToTarget();
  }
  final int trackedPrinterCount=mStateTrackedPrinters.size();
  for (int i=0; i < trackedPrinterCount; i++) {
    PrinterId printerId=mStateTrackedPrinters.get(i);
    if (printerId.getServiceName().equals(service.getComponentName())) {
      SomeArgs args=SomeArgs.obtain();
      args.arg1=service;
      args.arg2=printerId;
      mHandler.obtainMessage(SessionHandler.MSG_START_PRINTER_STATE_TRACKING,args).sendToTarget();
    }
  }
}
