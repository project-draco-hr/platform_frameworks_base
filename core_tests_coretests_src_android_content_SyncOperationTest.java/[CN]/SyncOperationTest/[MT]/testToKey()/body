{
  Account account1=new Account("account1","type1");
  Account account2=new Account("account2","type2");
  Bundle b1=new Bundle();
  Bundle b2=new Bundle();
  b2.putBoolean("b2",true);
  SyncOperation op1=new SyncOperation(account1,1,"authority1",b1,100,1000,10000);
  SyncOperation op2=new SyncOperation(account1,1,"authority1",b1,200,2000,20000);
  SyncOperation op3=new SyncOperation(account1,1,"authority2",b1,100,1000,10000);
  SyncOperation op4=new SyncOperation(account2,1,"authority1",b1,100,1000,10000);
  SyncOperation op5=new SyncOperation(account1,1,"authority1",b2,100,1000,10000);
  assertEquals(op1.key,op2.key);
  assertNotSame(op1.key,op3.key);
  assertNotSame(op1.key,op4.key);
  assertNotSame(op1.key,op5.key);
}
