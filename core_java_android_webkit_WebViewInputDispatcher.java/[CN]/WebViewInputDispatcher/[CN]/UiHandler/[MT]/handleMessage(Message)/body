{
switch (msg.what) {
case MSG_DISPATCH_UI_EVENTS:
    dispatchUiEvents(true);
  break;
case MSG_WEBKIT_TIMEOUT:
handleWebKitTimeout();
break;
case MSG_LONG_PRESS:
postLongPress();
break;
case MSG_CLICK:
postClick();
break;
default :
throw new IllegalStateException("Unknown message type: " + msg.what);
}
}
