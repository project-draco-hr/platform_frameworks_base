{
  Slog.v(TAG,"Poll all devices.");
  pollDevices(new DevicePollingCallback(){
    @Override public void onPollingFinished(    List<Integer> ackedAddress){
      checkHotplug(ackedAddress,false);
    }
  }
,Constants.POLL_ITERATION_IN_ORDER | Constants.POLL_STRATEGY_REMOTES_DEVICES,HdmiConfig.HOTPLUG_DETECTION_RETRY);
}
