{
  if (keyCode == KeyEvent.KEYCODE_BACK) {
    return true;
  }
  final char match=event.getMatch(DIGITS);
  if (match != 0) {
    reportDigit(match - '0');
    return true;
  }
  if (keyCode == KeyEvent.KEYCODE_DEL) {
    mPasswordTextView.onKeyDown(keyCode,event);
    return true;
  }
  if (keyCode == KeyEvent.KEYCODE_ENTER) {
    verifyPasswordAndUnlock();
    return true;
  }
  return false;
}
