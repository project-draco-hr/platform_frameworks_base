{
synchronized (this) {
    if (!mBootCompleted || !mProvisioned) {
      if (msg.what == MESSAGE_START_SYNC) {
        deferSyncH((SyncOperation)msg.obj,60 * 1000);
      }
      Message m=Message.obtain(msg);
      mUnreadyQueue.add(m);
      return true;
    }
 else {
      return false;
    }
  }
}
