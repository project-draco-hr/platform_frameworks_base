{
synchronized (this) {
    if (!mBootCompleted || !mProvisioned) {
      if (msg.what == MESSAGE_START_SYNC) {
        SyncOperation op=(SyncOperation)msg.obj;
        addSyncOperationToCache(op);
      }
      Message m=Message.obtain(msg);
      mUnreadyQueue.add(m);
      return true;
    }
 else {
      return false;
    }
  }
}
