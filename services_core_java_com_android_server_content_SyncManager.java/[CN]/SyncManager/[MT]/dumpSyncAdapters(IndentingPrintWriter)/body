{
  pw.println();
  final List<UserInfo> users=getAllUsers();
  if (users != null) {
    for (    UserInfo user : users) {
      pw.println("Sync adapters for " + user + ":");
      pw.increaseIndent();
      for (      RegisteredServicesCache.ServiceInfo<?> info : mSyncAdapters.getAllServices(user.id)) {
        pw.println(info);
      }
      pw.decreaseIndent();
      pw.println();
    }
  }
}
