{
  checkLock();
  BridgeXmlBlockParser blockParser=new BridgeXmlBlockParser(childXml,mContext,false);
  View child=mInflater.inflate(blockParser,parentView,false);
  try {
    parentView.addView(child,index);
  }
 catch (  UnsupportedOperationException e) {
    return new SceneResult(SceneStatus.ERROR_VIEWGROUP_NO_CHILDREN);
  }
  invalidateRenderingSize();
  SceneResult result=render();
  if (result.isSuccess()) {
    result.setData(child);
  }
  return result;
}
