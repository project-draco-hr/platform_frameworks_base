{
  final int length=Math.min(mHead,mLog.length);
  if (mHead > length) {
    final int start=mHead % length;
    for (int i=start; i < length; ++i) {
      pw.print(prefix);
      pw.println(mLog[i]);
    }
    for (int i=0; i < start; ++i) {
      pw.print(prefix);
      pw.println(mLog[i]);
    }
  }
 else {
    for (int i=0; i < length; ++i) {
      pw.print(prefix);
      pw.println(mLog[i]);
    }
  }
}
