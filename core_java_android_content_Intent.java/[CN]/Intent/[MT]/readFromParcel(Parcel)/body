{
  mAction=in.readString();
  mData=Uri.CREATOR.createFromParcel(in);
  mType=in.readString();
  mFlags=in.readInt();
  mComponent=ComponentName.readFromParcel(in);
  int N=in.readInt();
  if (N > 0) {
    mCategories=new HashSet<String>();
    int i;
    for (i=0; i < N; i++) {
      mCategories.add(in.readString());
    }
  }
 else {
    mCategories=null;
  }
  mExtras=in.readBundle();
}
