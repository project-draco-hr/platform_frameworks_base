{
  if (obj == null) {
    return false;
  }
  if (this == obj) {
    return true;
  }
  if (obj instanceof TonemapCurve) {
    final TonemapCurve other=(TonemapCurve)obj;
    return Arrays.equals(mRed,other.mRed) && Arrays.equals(mGreen,other.mGreen) && Arrays.equals(mBlue,other.mBlue);
  }
  return false;
}
