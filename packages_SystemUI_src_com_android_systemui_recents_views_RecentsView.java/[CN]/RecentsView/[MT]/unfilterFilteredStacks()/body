{
  if (mStacks != null) {
    boolean stacksUnfiltered=false;
    int numStacks=mStacks.size();
    for (int i=0; i < numStacks; i++) {
      TaskStack stack=mStacks.get(i);
      if (stack.hasFilteredTasks()) {
        stack.unfilterTasks();
        stacksUnfiltered=true;
      }
    }
    return stacksUnfiltered;
  }
  return false;
}
