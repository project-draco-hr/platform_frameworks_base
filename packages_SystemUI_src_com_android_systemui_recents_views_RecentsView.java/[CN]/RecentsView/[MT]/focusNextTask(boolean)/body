{
  TaskStackView stackView=null;
  int childCount=getChildCount();
  for (int i=0; i < childCount; i++) {
    View child=getChildAt(i);
    if (child != mSearchBar) {
      stackView=(TaskStackView)child;
      break;
    }
  }
  if (stackView != null) {
    stackView.focusNextTask(forward);
  }
}
