{
  super.onDrawForeground(canvas);
  ArrayList<TaskStack.DockState> visDockStates=mTouchHandler.getVisibleDockStates();
  for (int i=visDockStates.size() - 1; i >= 0; i--) {
    Drawable d=visDockStates.get(i).viewState.dockAreaOverlay;
    if (d.getAlpha() > 0) {
      d.draw(canvas);
    }
  }
}
