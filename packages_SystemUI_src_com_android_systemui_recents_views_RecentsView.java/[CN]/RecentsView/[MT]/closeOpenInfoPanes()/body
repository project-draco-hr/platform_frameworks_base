{
  if (mBSP != null) {
    int childCount=getChildCount();
    for (int i=0; i < childCount; i++) {
      View child=getChildAt(i);
      if (child instanceof TaskStackView) {
        TaskStackView stackView=(TaskStackView)child;
        if (stackView.closeOpenInfoPanes()) {
          return true;
        }
      }
    }
  }
  return false;
}
