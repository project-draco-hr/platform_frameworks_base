{
  List<AppTransitionAnimationSpec> specs=getAppTransitionAnimationSpecs(stackView,clickedTask,offsetX,offsetY,stackScroll);
  if (specs == null) {
    return;
  }
  IRemoteCallback.Stub callback=new IRemoteCallback.Stub(){
    @Override public void sendResult(    Bundle data) throws RemoteException {
      post(new Runnable(){
        @Override public void run(){
          if (animStartedListener != null) {
            animStartedListener.onAnimationStarted();
          }
        }
      }
);
    }
  }
;
  AppTransitionAnimationSpec[] specsArray=new AppTransitionAnimationSpec[specs.size()];
  try {
    WindowManagerGlobal.getWindowManagerService().overridePendingAppTransitionMultiThumb(specs.toArray(specsArray),callback,true);
  }
 catch (  RemoteException e) {
    Log.w(TAG,"Error overriding app transition",e);
  }
}
