{
  if (familyName == null) {
    familyName=DEFAULT_FAMILY;
  }
  if (style < 0) {
    style=Typeface.NORMAL;
  }
  Typeface_Delegate newDelegate=new Typeface_Delegate(familyName,style);
  if (sFontLoader != null) {
    newDelegate.init();
  }
 else {
    sPostInitDelegate.add(newDelegate);
  }
  return sManager.addNewDelegate(newDelegate);
}
