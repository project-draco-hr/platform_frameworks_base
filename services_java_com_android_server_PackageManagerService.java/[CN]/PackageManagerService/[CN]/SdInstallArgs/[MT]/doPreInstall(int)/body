{
  if (status != PackageManager.INSTALL_SUCCEEDED) {
    destroySdDir(cid);
  }
 else {
    boolean mounted=false;
    if (!mounted) {
      cachePath=mountSdDir(cid,Process.SYSTEM_UID);
      if (cachePath == null) {
        return PackageManager.INSTALL_FAILED_CONTAINER_ERROR;
      }
    }
  }
  return status;
}
