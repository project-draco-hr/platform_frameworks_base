{
  String v=parser.getAttributeValue(ns,name);
  try {
    if (v == null) {
      return defValue;
    }
    return Integer.parseInt(v);
  }
 catch (  NumberFormatException e) {
    reportSettingsProblem(Log.WARN,"Error in package manager settings: attribute " + name + " has bad integer value "+ v+ " at "+ parser.getPositionDescription());
  }
  return defValue;
}
