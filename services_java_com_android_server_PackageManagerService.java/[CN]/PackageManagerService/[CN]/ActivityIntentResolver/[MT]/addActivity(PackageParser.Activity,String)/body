{
  mActivities.put(a.component,a);
  if (SHOW_INFO || Config.LOGV)   Log.v(TAG,"  " + type + " "+ (a.info.nonLocalizedLabel != null ? a.info.nonLocalizedLabel : a.info.name)+ ":");
  if (SHOW_INFO || Config.LOGV)   Log.v(TAG,"    Class=" + a.info.name);
  int NI=a.intents.size();
  for (int j=0; j < NI; j++) {
    PackageParser.ActivityIntentInfo intent=a.intents.get(j);
    if (SHOW_INFO || Config.LOGV) {
      Log.v(TAG,"    IntentFilter:");
      intent.dump(new LogPrinter(Log.VERBOSE,TAG),"      ");
    }
    if (!intent.debugCheck()) {
      Log.w(TAG,"==> For Activity " + a.info.name);
    }
    addFilter(intent);
  }
}
