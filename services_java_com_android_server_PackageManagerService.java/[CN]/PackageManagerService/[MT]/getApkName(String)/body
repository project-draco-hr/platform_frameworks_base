{
  if (codePath == null) {
    return null;
  }
  int sidx=codePath.lastIndexOf("/");
  int eidx=codePath.lastIndexOf(".");
  if (eidx == -1) {
    eidx=codePath.length();
  }
 else   if (eidx == 0) {
    Slog.w(TAG," Invalid code path, " + codePath + " Not a valid apk name");
    return null;
  }
  return codePath.substring(sidx + 1,eidx);
}
