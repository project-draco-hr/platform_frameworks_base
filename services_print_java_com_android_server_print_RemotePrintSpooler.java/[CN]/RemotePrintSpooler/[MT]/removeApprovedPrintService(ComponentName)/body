{
  throwIfCalledOnMainThread();
synchronized (mLock) {
    throwIfDestroyedLocked();
    mCanUnbind=false;
  }
  try {
    getRemoteInstanceLazy().removeApprovedPrintService(serviceToRemove);
  }
 catch (  RemoteException re) {
    Slog.e(LOG_TAG,"Error removing approved print service.",re);
  }
catch (  TimeoutException te) {
    Slog.e(LOG_TAG,"Error removing approved print service.",te);
  }
 finally {
    if (DEBUG) {
      Slog.i(LOG_TAG,"[user: " + mUserHandle.getIdentifier() + "] removing approved print service()");
    }
synchronized (mLock) {
      mCanUnbind=true;
      mLock.notifyAll();
    }
  }
}
