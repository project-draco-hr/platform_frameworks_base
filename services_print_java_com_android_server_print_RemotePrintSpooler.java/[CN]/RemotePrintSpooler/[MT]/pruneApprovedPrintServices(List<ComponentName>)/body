{
  throwIfCalledOnMainThread();
synchronized (mLock) {
    throwIfDestroyedLocked();
    mCanUnbind=false;
  }
  try {
    getRemoteInstanceLazy().pruneApprovedPrintServices(servicesToKeep);
  }
 catch (  RemoteException|TimeoutException re) {
    Slog.e(LOG_TAG,"Error pruning approved print services.",re);
  }
 finally {
    if (DEBUG) {
      Slog.i(LOG_TAG,"[user: " + mUserHandle.getIdentifier() + "] pruneApprovedPrintServices()");
    }
synchronized (mLock) {
      mCanUnbind=true;
      mLock.notifyAll();
    }
  }
}
