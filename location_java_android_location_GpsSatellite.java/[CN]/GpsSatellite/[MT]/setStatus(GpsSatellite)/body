{
  if (satellite == null) {
    mValid=false;
  }
 else {
    mValid=satellite.mValid;
    mHasEphemeris=satellite.mHasEphemeris;
    mHasAlmanac=satellite.mHasAlmanac;
    mUsedInFix=satellite.mUsedInFix;
    mSnr=satellite.mSnr;
    mElevation=satellite.mElevation;
    mAzimuth=satellite.mAzimuth;
  }
}
