{
  Vector<FontProperty> props=fProperties.get(fpName);
  if (props == null) {
    return null;
  }
  int size=props.size();
  if (size == 0) {
    return null;
  }
  FontProperty[] fps=new FontProperty[size];
  for (int i=0; i < fps.length; i++) {
    fps[i]=props.elementAt(i);
  }
  return fps;
}
