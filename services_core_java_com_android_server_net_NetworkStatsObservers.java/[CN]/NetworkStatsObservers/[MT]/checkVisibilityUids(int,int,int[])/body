{
  if (uids == null) {
    return;
  }
  for (int i=0; i < uids.length; i++) {
    if (!NetworkStatsAccess.isAccessibleToUser(uids[i],callingUid,accessLevel)) {
      throw new SecurityException("Caller " + callingUid + " cannot monitor network stats"+ " for uid "+ uids[i]+ " with accessLevel "+ accessLevel);
    }
  }
}
