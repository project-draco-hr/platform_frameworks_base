{
  if (view == null) {
    return null;
  }
  LayoutViewInfo result=new LayoutViewInfo(view.getClass().getName(),context.getViewKey(view),view.getLeft(),view.getTop(),view.getRight(),view.getBottom());
  if (view instanceof ViewGroup) {
    ViewGroup group=((ViewGroup)view);
    int n=group.getChildCount();
    ILayoutViewInfo[] children=new ILayoutViewInfo[n];
    for (int i=0; i < group.getChildCount(); i++) {
      children[i]=visit(group.getChildAt(i),context);
    }
    result.setChildren(children);
  }
  return result;
}
