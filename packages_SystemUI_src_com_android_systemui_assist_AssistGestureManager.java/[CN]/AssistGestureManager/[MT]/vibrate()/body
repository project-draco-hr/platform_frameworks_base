{
  if (Settings.System.getIntForUser(mContext.getContentResolver(),Settings.System.HAPTIC_FEEDBACK_ENABLED,1,UserHandle.USER_CURRENT) != 0) {
    Resources res=mContext.getResources();
    Vibrator vibrator=(Vibrator)mContext.getSystemService(Context.VIBRATOR_SERVICE);
    vibrator.vibrate(res.getInteger(R.integer.config_search_panel_view_vibration_duration),VIBRATION_ATTRIBUTES);
  }
}
