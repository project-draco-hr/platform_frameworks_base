{
  checkNotNull(entry,"entry must not be null");
  int numInputs=0;
  int left=entry.length;
  for (int i=0; i < entry.length; ) {
    final int format=entry[i];
    left--;
    i++;
    if (left < 1) {
      throw new IllegalArgumentException(String.format("Input %x had no output format length listed",format));
    }
    final int length=entry[i];
    left--;
    i++;
    if (length > 0) {
      if (left < length) {
        throw new IllegalArgumentException(String.format("Input %x had too few output formats listed (actual: %d, " + "expected: %d)",format,left,length));
      }
      i+=length;
      left-=length;
    }
    numInputs++;
  }
  mEntry=entry;
  mInputCount=numInputs;
}
