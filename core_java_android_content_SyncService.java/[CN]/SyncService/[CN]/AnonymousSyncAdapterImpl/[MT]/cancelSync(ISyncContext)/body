{
  AnonymousSyncThread runningSync=null;
synchronized (mSyncThreadLock) {
    for (    AnonymousSyncThread thread : mSyncThreads.values()) {
      if (thread.mSyncContext.getSyncContextBinder() == syncContext.asBinder()) {
        runningSync=thread;
        break;
      }
    }
  }
  if (runningSync != null) {
    runningSync.interrupt();
  }
}
