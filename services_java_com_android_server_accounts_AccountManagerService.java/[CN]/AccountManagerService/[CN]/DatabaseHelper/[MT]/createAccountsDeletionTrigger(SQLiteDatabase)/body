{
  db.execSQL("" + " CREATE TRIGGER " + TABLE_ACCOUNTS + "Delete DELETE ON "+ TABLE_ACCOUNTS+ " BEGIN"+ "   DELETE FROM "+ TABLE_AUTHTOKENS+ "     WHERE "+ AUTHTOKENS_ACCOUNTS_ID+ "=OLD."+ ACCOUNTS_ID+ " ;"+ "   DELETE FROM "+ TABLE_EXTRAS+ "     WHERE "+ EXTRAS_ACCOUNTS_ID+ "=OLD."+ ACCOUNTS_ID+ " ;"+ "   DELETE FROM "+ TABLE_GRANTS+ "     WHERE "+ GRANTS_ACCOUNTS_ID+ "=OLD."+ ACCOUNTS_ID+ " ;"+ " END");
}
