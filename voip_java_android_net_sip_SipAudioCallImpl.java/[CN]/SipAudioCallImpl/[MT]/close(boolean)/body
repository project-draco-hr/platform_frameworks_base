{
  if (closeRtp)   stopCall(RELEASE_SOCKET);
  stopRingbackTone();
  stopRinging();
  mInCall=false;
  mHold=false;
  mSessionId=System.currentTimeMillis();
  mErrorCode=SipErrorCode.NO_ERROR;
  mErrorMessage=null;
  if (mSipSession != null) {
    try {
      mSipSession.setListener(null);
    }
 catch (    RemoteException e) {
    }
    mSipSession=null;
  }
}
