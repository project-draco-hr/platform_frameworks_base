{
  try {
    if ((mSipSession == null) || !mInCall || !session.getCallId().equals(mSipSession.getCallId())) {
      session.endCall();
      return;
    }
    try {
      mPeerSd=new SdpSessionDescription(sessionDescription);
      answerCall(SESSION_TIMEOUT);
    }
 catch (    Throwable e) {
      Log.e(TAG,"onRinging()",e);
      session.endCall();
    }
  }
 catch (  RemoteException e) {
    Log.e(TAG,"onRinging()",e);
  }
}
