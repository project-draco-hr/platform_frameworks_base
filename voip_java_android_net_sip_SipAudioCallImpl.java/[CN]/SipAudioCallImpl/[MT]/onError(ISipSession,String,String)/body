{
  Log.d(TAG,"sip session error: " + errorCode + ": "+ message);
synchronized (this) {
    if (!isInCall())     close(true);
  }
  Listener listener=mListener;
  if (listener != null) {
    try {
      listener.onError(SipAudioCallImpl.this,errorCode,message);
    }
 catch (    Throwable t) {
      Log.e(TAG,"onError()",t);
    }
  }
}
