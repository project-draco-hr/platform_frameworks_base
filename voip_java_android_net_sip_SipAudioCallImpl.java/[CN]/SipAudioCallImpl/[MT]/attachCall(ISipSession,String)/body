{
  mSipSession=session;
  mPeerSd=sessionDescription;
  Log.v(TAG,"attachCall()" + mPeerSd);
  try {
    session.setListener(this);
    if (getState() == SipSessionState.INCOMING_CALL)     startRinging();
  }
 catch (  Throwable e) {
    Log.e(TAG,"attachCall()",e);
    throwSipException(e);
  }
}
