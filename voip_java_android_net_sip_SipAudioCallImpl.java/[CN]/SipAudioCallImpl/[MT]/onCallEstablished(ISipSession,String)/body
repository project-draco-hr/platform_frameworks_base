{
  stopRingbackTone();
  stopRinging();
  mPeerSd=sessionDescription;
  Log.v(TAG,"onCallEstablished()" + mPeerSd);
  Listener listener=mListener;
  if (listener != null) {
    try {
      if (mHold) {
        listener.onCallHeld(this);
      }
 else {
        listener.onCallEstablished(this);
      }
    }
 catch (    Throwable t) {
      Log.e(TAG,"onCallEstablished()",t);
    }
  }
}
