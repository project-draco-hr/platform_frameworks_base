{
  try {
    Log.d(LOGTAG,"test starting for url " + url);
    mActivity.runOnUiThread(new Runnable(){
      @Override public void run(){
        mView.loadUrl(url);
      }
    }
);
synchronized (mStats) {
      mStats.wait();
    }
    mStats.aggregate();
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
    return false;
  }
  return true;
}
