{
  if (mController.hasStarted()) {
    mController.finish();
  }
  if (mEditor.isPrintConfirmed() && mController.isFinished()) {
    PrintSpooler.peekInstance().setPrintJobState(mPrintJobId,PrintJobInfo.STATE_QUEUED,null);
  }
 else {
    PrintSpooler.peekInstance().setPrintJobState(mPrintJobId,PrintJobInfo.STATE_CANCELED,null);
  }
  mIPrintDocumentAdapter.unlinkToDeath(mDeathRecipient,0);
  if (mGeneratingPrintJobDialog != null) {
    mGeneratingPrintJobDialog.dismiss();
    mGeneratingPrintJobDialog=null;
  }
  super.onDestroy();
}
