{
switch (document.getContentType()) {
case PrintDocumentInfo.CONTENT_TYPE_DOCUMENT:
{
      mCurrPrintAttributes.setFittingMode(PrintAttributes.FITTING_MODE_SCALE_TO_FIT);
    }
  break;
case PrintDocumentInfo.CONTENT_TYPE_PHOTO:
{
  mCurrPrintAttributes.setFittingMode(PrintAttributes.FITTING_MODE_SCALE_TO_FILL);
}
}
}
