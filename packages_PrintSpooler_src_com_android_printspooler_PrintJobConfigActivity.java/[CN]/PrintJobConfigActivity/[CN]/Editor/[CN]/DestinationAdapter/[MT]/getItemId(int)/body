{
  if (mPrinters.isEmpty()) {
    if (mFakePdfPrinter != null) {
      if (position == 0) {
        return DEST_ADAPTER_ITEM_ID_SAVE_AS_PDF;
      }
 else       if (position == 1) {
        return DEST_ADAPTER_ITEM_ID_ALL_PRINTERS;
      }
    }
  }
 else {
    if (position == 1 && mFakePdfPrinter != null) {
      return DEST_ADAPTER_ITEM_ID_SAVE_AS_PDF;
    }
    if (position == getCount() - 1) {
      return DEST_ADAPTER_ITEM_ID_ALL_PRINTERS;
    }
  }
  return position;
}
