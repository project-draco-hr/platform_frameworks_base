{
  if (mPrinters.isEmpty()) {
    if (position == 0 && mFakePdfPrinter != null) {
      return mFakePdfPrinter;
    }
  }
 else {
    if (position < 1) {
      return mPrinters.get(position);
    }
    if (position == 1 && mFakePdfPrinter != null) {
      return mFakePdfPrinter;
    }
    if (position < getCount() - 1) {
      return mPrinters.get(position - 1);
    }
  }
  return null;
}
