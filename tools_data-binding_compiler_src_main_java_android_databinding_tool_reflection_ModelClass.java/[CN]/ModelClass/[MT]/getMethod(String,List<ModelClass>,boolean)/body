{
  ModelMethod[] methods=getMethods(name,args,staticAccess);
  if (methods.length == 0) {
    return null;
  }
  ModelMethod bestMethod=methods[0];
  for (int i=1; i < methods.length; i++) {
    if (methods[i].isBetterArgMatchThan(bestMethod,args)) {
      bestMethod=methods[i];
    }
  }
  return bestMethod;
}
