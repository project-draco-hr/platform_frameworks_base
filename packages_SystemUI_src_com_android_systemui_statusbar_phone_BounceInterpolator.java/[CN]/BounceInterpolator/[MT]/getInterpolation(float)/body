{
  if (t < 4f / 11f) {
    return SCALE_FACTOR * t * t;
  }
 else   if (t < 8f / 11f) {
    float t2=t - 6f / 11f;
    return SCALE_FACTOR * t2 * t2 + 3f / 4f;
  }
 else   if (t < 10f / 11f) {
    float t2=t - 9f / 11f;
    return SCALE_FACTOR * t2 * t2 + 15f / 16f;
  }
 else {
    float t2=t - 21f / 22f;
    return SCALE_FACTOR * t2 * t2 + 63f / 64f;
  }
}
