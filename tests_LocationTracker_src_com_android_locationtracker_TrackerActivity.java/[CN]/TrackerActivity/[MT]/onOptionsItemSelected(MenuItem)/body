{
switch (item.getItemId()) {
case R.id.start_service_menu:
{
      startService(new Intent(TrackerActivity.this,TrackerService.class));
      break;
    }
case R.id.stop_service_menu:
{
    stopService(new Intent(TrackerActivity.this,TrackerService.class));
    break;
  }
case R.id.settings_menu:
{
  launchSettings();
  break;
}
case R.id.export_kml:
{
exportKML();
break;
}
case R.id.export_csv:
{
exportCSV();
break;
}
case R.id.clear_data_menu:
{
clearData();
break;
}
}
return super.onOptionsItemSelected(item);
}
