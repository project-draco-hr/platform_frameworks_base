{
  ActivityRecord r=mActivities.get(token);
  if (r != null) {
    final boolean resumed=!r.paused;
    if (resumed) {
      mInstrumentation.callActivityOnPause(r.activity);
    }
    deliverNewIntents(r,intents);
    if (resumed) {
      mInstrumentation.callActivityOnResume(r.activity);
    }
  }
}
