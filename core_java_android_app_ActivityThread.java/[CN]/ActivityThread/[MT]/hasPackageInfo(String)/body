{
synchronized (mPackages) {
    WeakReference<PackageInfo> ref;
    ref=mPackages.get(packageName);
    if (ref != null && ref.get() != null) {
      return true;
    }
    ref=mResourcePackages.get(packageName);
    if (ref != null && ref.get() != null) {
      return true;
    }
    return false;
  }
}
