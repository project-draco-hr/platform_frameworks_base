{
  try {
    ActivityClientRecord r=mActivities.get(info.token);
    if (r != null && r.activity != null) {
      PrintWriter pw=new PrintWriter(new FileOutputStream(info.fd));
      r.activity.dump(info.fd,pw,info.args);
      pw.close();
    }
  }
  finally {
synchronized (info) {
      info.dumped=true;
      info.notifyAll();
    }
  }
}
