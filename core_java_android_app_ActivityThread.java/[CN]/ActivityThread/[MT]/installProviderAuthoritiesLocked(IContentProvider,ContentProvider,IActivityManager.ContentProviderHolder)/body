{
  String names[]=PATTERN_SEMICOLON.split(holder.info.authority);
  ProviderClientRecord pcr=new ProviderClientRecord(names,provider,localProvider,holder);
  for (int i=0; i < names.length; i++) {
    ProviderClientRecord existing=mProviderMap.get(names[i]);
    if (existing != null) {
      Slog.w(TAG,"Content provider " + pcr.mHolder.info.name + " already published as "+ names[i]);
    }
 else {
      mProviderMap.put(names[i],pcr);
    }
  }
  return pcr;
}
