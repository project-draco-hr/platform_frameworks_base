{
  try {
    Service s=mServices.get(info.service);
    if (s != null) {
      PrintWriter pw=new PrintWriter(new FileOutputStream(info.fd));
      s.dump(info.fd,pw,info.args);
      pw.close();
    }
  }
  finally {
synchronized (info) {
      info.dumped=true;
      info.notifyAll();
    }
  }
}
