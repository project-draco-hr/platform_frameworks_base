{
  if (mContext != context) {
    throw new RuntimeException("ServiceConnection " + mConnection + " registered with differing Context (was "+ mContext+ " now "+ context+ ")");
  }
  if (mActivityThread != activityThread) {
    throw new RuntimeException("ServiceConnection " + mConnection + " registered with differing handler (was "+ mActivityThread+ " now "+ activityThread+ ")");
  }
}
