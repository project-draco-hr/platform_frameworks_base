{
  if (mContext != context) {
    throw new IllegalStateException("Receiver " + mReceiver + " registered with differing Context (was "+ mContext+ " now "+ context+ ")");
  }
  if (mActivityThread != activityThread) {
    throw new IllegalStateException("Receiver " + mReceiver + " registered with differing handler (was "+ mActivityThread+ " now "+ activityThread+ ")");
  }
}
