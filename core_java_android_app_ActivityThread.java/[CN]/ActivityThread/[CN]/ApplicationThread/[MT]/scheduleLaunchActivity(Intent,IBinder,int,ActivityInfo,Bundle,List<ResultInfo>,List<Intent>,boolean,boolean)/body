{
  ActivityRecord r=new ActivityRecord();
  r.token=token;
  r.ident=ident;
  r.intent=intent;
  r.activityInfo=info;
  r.state=state;
  r.pendingResults=pendingResults;
  r.pendingIntents=pendingNewIntents;
  r.startsNotResumed=notResumed;
  r.isForward=isForward;
  queueOrSendMessage(H.LAUNCH_ACTIVITY,r);
}
