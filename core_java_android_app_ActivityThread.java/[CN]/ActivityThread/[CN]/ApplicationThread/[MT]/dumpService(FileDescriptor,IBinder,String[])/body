{
  DumpServiceInfo data=new DumpServiceInfo();
  data.fd=fd;
  data.service=servicetoken;
  data.args=args;
  data.dumped=false;
  queueOrSendMessage(H.DUMP_SERVICE,data);
synchronized (data) {
    while (!data.dumped) {
      try {
        data.wait();
      }
 catch (      InterruptedException e) {
      }
    }
  }
}
