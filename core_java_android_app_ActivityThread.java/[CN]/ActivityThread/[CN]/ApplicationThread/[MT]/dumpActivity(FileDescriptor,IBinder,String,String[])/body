{
  DumpComponentInfo data=new DumpComponentInfo();
  data.fd=fd;
  data.token=activitytoken;
  data.prefix=prefix;
  data.args=args;
  data.dumped=false;
  queueOrSendMessage(H.DUMP_ACTIVITY,data);
synchronized (data) {
    while (!data.dumped) {
      try {
        data.wait();
      }
 catch (      InterruptedException e) {
      }
    }
  }
}
