{
  DumpComponentInfo data=new DumpComponentInfo();
  try {
    data.fd=ParcelFileDescriptor.dup(fd);
    data.token=activitytoken;
    data.prefix=prefix;
    data.args=args;
    sendMessage(H.DUMP_ACTIVITY,data,0,0,true);
  }
 catch (  IOException e) {
    Slog.w(TAG,"dumpActivity failed",e);
  }
}
