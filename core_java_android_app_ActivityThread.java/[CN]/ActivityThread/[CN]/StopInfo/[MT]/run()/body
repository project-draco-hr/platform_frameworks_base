{
  try {
    if (DEBUG_MEMORY_TRIM)     Slog.v(TAG,"Reporting activity stopped: " + activity);
    ActivityManagerNative.getDefault().activityStopped(activity.token,state,persistentState,description);
  }
 catch (  RemoteException ex) {
    if (ex instanceof TransactionTooLargeException && "com.google.android.gms".equals(activity.packageInfo.getPackageName())) {
      Log.d(TAG,"STAHP SENDING SO MUCH DATA KTHX: " + ex);
      return;
    }
    throw ex.rethrowFromSystemServer();
  }
}
