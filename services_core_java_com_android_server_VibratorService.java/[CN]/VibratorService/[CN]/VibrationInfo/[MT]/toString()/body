{
  return new StringBuilder().append("timeout: ").append(timeout).append(", startTime: ").append(startTime).append(", pattern: ").append(Arrays.toString(pattern)).append(", repeat: ").append(repeat).append(", usageHint: ").append(usageHint).append(", uid: ").append(uid).append(", opPkg: ").append(opPkg).toString();
}
