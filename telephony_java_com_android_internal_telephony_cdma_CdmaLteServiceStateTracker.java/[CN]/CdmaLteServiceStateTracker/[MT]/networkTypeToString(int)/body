{
  String ret="unknown";
switch (type) {
case DATA_ACCESS_CDMA_IS95A:
case DATA_ACCESS_CDMA_IS95B:
    ret="CDMA";
  break;
case DATA_ACCESS_CDMA_1xRTT:
ret="CDMA - 1xRTT";
break;
case DATA_ACCESS_CDMA_EvDo_0:
ret="CDMA - EvDo rev. 0";
break;
case DATA_ACCESS_CDMA_EvDo_A:
ret="CDMA - EvDo rev. A";
break;
case DATA_ACCESS_CDMA_EvDo_B:
ret="CDMA - EvDo rev. B";
break;
case DATA_ACCESS_LTE:
ret="LTE";
break;
case DATA_ACCESS_EHRPD:
ret="CDMA - eHRPD";
default :
if (DBG) {
Log.e(LOG_TAG," [CdmaLteServiceStateTracker] Wrong network." + " Can not return a string.");
}
break;
}
return ret;
}
