{
  int px=x;
  while (count-- > 0) {
    if (e >= 0) {
      if (sx > 0) {
        dst.addRect(px,y,x,y);
      }
 else {
        dst.addRect(x,y,px,y);
      }
      x+=sx;
      y+=sy;
      e+=ePos;
      px=x;
    }
 else {
      e+=eNeg;
      x+=sx;
    }
  }
  if (sx > 0) {
    dst.addRect(px,y,x,y);
  }
 else {
    dst.addRect(x,y,px,y);
  }
}
