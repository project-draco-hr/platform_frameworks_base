{
  if (y1 >= clip[index + 1] && y1 <= clip[index + 3]) {
    int cx1=clip[index + 0];
    int cx2=clip[index + 2];
    if (x1 <= cx2 && x2 >= cx1) {
      int nx1, nx2;
      if (dx > 0) {
        nx1=Math.max(x1,cx1);
        nx2=Math.min(x2,cx2);
      }
 else {
        nx2=Math.max(x2,cx1);
        nx1=Math.min(x1,cx2);
      }
      rasterizeClipped(nx1,nx2);
    }
  }
}
