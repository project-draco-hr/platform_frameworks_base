{
  LineDasher local=new LineDasher();
  local.dash=new float[dash.length];
  if (invert) {
    move(length);
    local.phase=dash[index] - phase;
    local.visible=visible;
    local.index=inv.length - index - 1;
    local.dash=inv;
  }
 else {
    local.phase=phase;
    local.visible=visible;
    local.index=index;
    local.dash=dash;
    move(length);
  }
  return local;
}
