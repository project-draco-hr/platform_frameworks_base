{
  Bitmap thumbnail=mThumbTransitionBitmapCache;
  Rect rect=TaskCardView.getStartingCardThumbnailRect(mContext);
  if (thumbnail != null) {
    return ActivityOptions.makeThumbnailAspectScaleDownAnimation(mDummyStackView,null,(int)rect.left,(int)rect.top,(int)rect.width(),(int)rect.height(),mHandler,null);
  }
  return getUnknownTransitionActivityOptions();
}
