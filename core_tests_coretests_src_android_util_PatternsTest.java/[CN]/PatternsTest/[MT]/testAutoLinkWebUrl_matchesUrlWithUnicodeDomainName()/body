{
  String url="http://\uD604\uAE08\uC601\uC218\uC99D.kr";
  assertTrue("Should match URL with Unicode domain name",Patterns.AUTOLINK_WEB_URL.matcher(url).matches());
  url="\uD604\uAE08\uC601\uC218\uC99D.kr";
  assertTrue("hould match URL without protocol and with Unicode domain name",Patterns.AUTOLINK_WEB_URL.matcher(url).matches());
}
