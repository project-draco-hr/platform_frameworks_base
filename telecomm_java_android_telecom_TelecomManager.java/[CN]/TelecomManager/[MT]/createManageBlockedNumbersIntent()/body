{
  ITelecomService service=getTelecomService();
  Intent result=null;
  if (service != null) {
    try {
      result=service.createManageBlockedNumbersIntent();
    }
 catch (    RemoteException e) {
      Log.e(TAG,"Error calling ITelecomService#createManageBlockedNumbersIntent",e);
    }
  }
  return result;
}
