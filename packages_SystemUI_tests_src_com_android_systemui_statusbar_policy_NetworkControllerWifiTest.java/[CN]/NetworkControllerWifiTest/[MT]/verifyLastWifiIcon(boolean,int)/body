{
  ArgumentCaptor<IconState> iconArg=ArgumentCaptor.forClass(IconState.class);
  Mockito.verify(mCallbackHandler,Mockito.atLeastOnce()).setWifiIndicators(Mockito.anyBoolean(),iconArg.capture(),Mockito.any(IconState.class),Mockito.anyBoolean(),Mockito.anyBoolean(),Mockito.anyString());
  IconState iconState=iconArg.getValue();
  assertEquals("WiFi visible, in status bar",visible,iconState.visible);
  assertEquals("WiFi signal, in status bar",icon,iconState.icon);
}
