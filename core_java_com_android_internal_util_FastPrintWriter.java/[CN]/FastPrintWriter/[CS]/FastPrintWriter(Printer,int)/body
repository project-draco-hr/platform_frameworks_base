{
  super(sDummyWriter,true);
  if (pr == null) {
    throw new NullPointerException("pr is null");
  }
  mBufferLen=bufferLen;
  mText=new char[bufferLen];
  mBytes=null;
  mOutputStream=null;
  mWriter=null;
  mPrinter=pr;
  mAutoFlush=true;
  mSeparator=System.lineSeparator();
  initDefaultEncoder();
}
