{
  if (which == POSITIVE_BUTTON) {
    ConnectivityManager connManager=(ConnectivityManager)getSystemService(Context.CONNECTIVITY_SERVICE);
    if (mWantTethering) {
      if (!connManager.tether("ppp0")) {
        showTetheringError();
      }
    }
 else {
      if (!connManager.untether("ppp0")) {
        showUnTetheringError();
      }
    }
  }
  finish();
}
