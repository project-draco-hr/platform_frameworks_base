{
  if (DEBUG)   Slog.d(TAG,"onAnimationEnd: mTarget=" + mTarget + " mMoveToFullScreen="+ mMoveToFullScreen+ " mWillReplace="+ mWillReplace);
  if (mAppTransition.isRunning() && !mFinishAnimationAfterTransition) {
    mFinishAnimationAfterTransition=true;
    return;
  }
  if (mMoveToFullScreen && !mWillReplace) {
    mTarget.moveToFullscreen();
  }
  finishAnimation();
}
