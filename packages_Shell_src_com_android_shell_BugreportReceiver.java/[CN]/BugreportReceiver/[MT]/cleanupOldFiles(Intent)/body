{
  if (!INTENT_BUGREPORT_FINISHED.equals(intent.getAction())) {
    return;
  }
  final File bugreportFile=getFileExtra(intent,EXTRA_BUGREPORT);
  if (bugreportFile == null || !bugreportFile.exists()) {
    Log.e(TAG,"Not deleting old files because file " + bugreportFile + " doesn't exist");
    return;
  }
  final PendingResult result=goAsync();
  new AsyncTask<Void,Void,Void>(){
    @Override protected Void doInBackground(    Void... params){
      FileUtils.deleteOlderFiles(bugreportFile.getParentFile(),MIN_KEEP_COUNT,MIN_KEEP_AGE);
      result.finish();
      return null;
    }
  }
.execute();
}
