{
  try {
    if (mOnClick != null) {
      mOnClick.send();
    }
 else     if (mOnClickUri != null) {
      final Intent intent=Intent.parseUri(mOnClickUri,Intent.URI_INTENT_SCHEME);
      mContext.sendBroadcastAsUser(intent,new UserHandle(mCurrentUserId));
    }
  }
 catch (  Throwable t) {
    Log.w(TAG,"Error sending click intent",t);
  }
}
