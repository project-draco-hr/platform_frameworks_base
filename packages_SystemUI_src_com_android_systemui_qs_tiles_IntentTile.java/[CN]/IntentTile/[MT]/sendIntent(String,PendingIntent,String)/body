{
  try {
    if (pi != null) {
      pi.send();
    }
 else     if (uri != null) {
      final Intent intent=Intent.parseUri(uri,Intent.URI_INTENT_SCHEME);
      mContext.sendBroadcastAsUser(intent,new UserHandle(mCurrentUserId));
    }
  }
 catch (  Throwable t) {
    Log.w(TAG,"Error sending " + type + " intent",t);
  }
}
