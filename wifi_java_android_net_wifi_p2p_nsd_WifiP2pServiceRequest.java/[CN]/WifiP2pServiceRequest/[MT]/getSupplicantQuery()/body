{
  StringBuffer sb=new StringBuffer();
  sb.append(String.format("%02x",(mLength) & 0xff));
  sb.append(String.format("%02x",(mLength >> 8) & 0xff));
  sb.append(String.format("%02x",mServiceType));
  sb.append(String.format("%02x",mTransId));
  if (mQuery != null) {
    sb.append(mQuery);
  }
  return sb.toString();
}
