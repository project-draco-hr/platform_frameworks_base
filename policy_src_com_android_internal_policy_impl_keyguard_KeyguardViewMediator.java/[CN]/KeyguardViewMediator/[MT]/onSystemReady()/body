{
synchronized (this) {
    if (DEBUG)     Log.d(TAG,"onSystemReady");
    mSystemReady=true;
    mUpdateMonitor.registerCallback(mUpdateCallback);
    mUpdateMonitor.setAlternateUnlockEnabled(false);
    mUpdateMonitor.setIsFirstBoot(true);
    doKeyguardLocked();
  }
  maybeSendUserPresentBroadcast();
}
