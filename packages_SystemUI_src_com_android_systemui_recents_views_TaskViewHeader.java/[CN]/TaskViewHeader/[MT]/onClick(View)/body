{
  if (v == mApplicationIcon) {
    EventBus.getDefault().send(new ShowApplicationInfoEvent(mTask));
  }
 else   if (v == mDismissButton) {
    TaskView tv=(TaskView)getParent().getParent();
    tv.dismissTask();
    MetricsLogger.histogram(getContext(),"overview_task_dismissed_source",Constants.Metrics.DismissSourceHeaderButton);
  }
 else   if (v == mMoveTaskButton) {
    EventBus.getDefault().send(new ResizeTaskEvent(mTask));
  }
}
