{
  Resources r=Resources.getSystem();
  String appName=getAppNameByIntent(tracker.mSentIntent);
  AlertDialog d=new AlertDialog.Builder(mContext).setTitle(r.getString(R.string.sms_control_title)).setMessage(appName + " " + r.getString(R.string.sms_control_message)).setPositiveButton(r.getString(R.string.sms_control_yes),mListener).setNegativeButton(r.getString(R.string.sms_control_no),null).create();
  d.getWindow().setType(WindowManager.LayoutParams.TYPE_SYSTEM_ALERT);
  d.show();
  mSTracker=tracker;
  sendMessageDelayed(obtainMessage(EVENT_ALERT_TIMEOUT,d),DEFAULT_SMS_TIMOUEOUT);
}
