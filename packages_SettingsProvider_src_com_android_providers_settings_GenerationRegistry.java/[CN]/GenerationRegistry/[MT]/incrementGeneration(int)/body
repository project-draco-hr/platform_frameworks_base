{
synchronized (mLock) {
    if (mImpl != null) {
      try {
        final int index=getKeyIndexLocked(key);
        if (index >= 0) {
          final int generation=mImpl.get(index) + 1;
          mImpl.set(index,generation);
        }
      }
 catch (      IOException e) {
        Slog.e(LOG_TAG,"Error updating generation id",e);
      }
    }
  }
}
