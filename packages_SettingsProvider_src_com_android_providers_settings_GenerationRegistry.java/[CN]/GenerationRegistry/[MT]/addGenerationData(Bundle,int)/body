{
synchronized (mLock) {
    MemoryIntArray backingStore=getBackingStoreLocked();
    try {
      if (backingStore != null) {
        final int index=getKeyIndexLocked(key,mKeyToIndexMap,backingStore);
        if (index >= 0) {
          bundle.putParcelable(Settings.CALL_METHOD_TRACK_GENERATION_KEY,backingStore);
          bundle.putInt(Settings.CALL_METHOD_GENERATION_INDEX_KEY,index);
          if (DEBUG) {
            Slog.i(LOG_TAG,"Exported index:" + index + " for key:"+ SettingsProvider.keyToString(key));
          }
        }
      }
    }
 catch (    IOException e) {
      Slog.e(LOG_TAG,"Error adding generation data",e);
      destroyBackingStore();
    }
  }
}
