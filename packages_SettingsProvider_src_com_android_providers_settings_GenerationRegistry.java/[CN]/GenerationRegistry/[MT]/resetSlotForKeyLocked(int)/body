{
  final int index=mKeyToIndexMap.get(key,-1);
  if (index >= 0) {
    mKeyToIndexMap.delete(key);
    try {
      mImpl.set(index,0);
      if (DEBUG) {
        Slog.i(LOG_TAG,"Freed index:" + index + " for key:"+ SettingsProvider.keyToString(key));
      }
    }
 catch (    IOException e) {
      Slog.e(LOG_TAG,"Cannot write to generation memory array",e);
    }
  }
}
