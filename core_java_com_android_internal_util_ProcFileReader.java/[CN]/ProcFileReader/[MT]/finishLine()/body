{
  if (mLineFinished) {
    mLineFinished=false;
    return;
  }
  int i=0;
  do {
    for (; i < mTail; i++) {
      if (mBuffer[i] == '\n') {
        consumeBuf(i + 1);
        return;
      }
    }
  }
 while (fillBuf() > 0);
  throw new IOException("end of stream while looking for line boundary");
}
