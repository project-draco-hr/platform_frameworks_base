{
  mAudioDevices=new HashMap<String,SinkState>();
  String[] paths=(String[])listHeadsetsNative();
  if (paths != null) {
    for (    String path : paths) {
      mAudioDevices.put(path,new SinkState(getAddressNative(path),isSinkConnectedNative(path) ? BluetoothA2dp.STATE_CONNECTED : BluetoothA2dp.STATE_DISCONNECTED));
    }
  }
}
