{
  Set<BluetoothDevice> sinks=new HashSet<BluetoothDevice>();
  if (mAudioDevices.isEmpty()) {
    return sinks;
  }
  for (  BluetoothDevice device : mAudioDevices.keySet()) {
    int sinkState=getSinkState(device);
    for (    int state : states) {
      if (state == sinkState) {
        sinks.add(device);
        break;
      }
    }
  }
  return sinks;
}
