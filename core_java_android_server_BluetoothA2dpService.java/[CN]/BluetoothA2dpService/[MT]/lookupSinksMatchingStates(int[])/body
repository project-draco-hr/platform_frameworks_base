{
  List<String> sinks=new ArrayList<String>();
  if (mAudioDevices.isEmpty()) {
    return sinks;
  }
  for (  String path : mAudioDevices.keySet()) {
    int sinkState=getSinkState(path);
    for (    int state : states) {
      if (state == sinkState) {
        sinks.add(path);
        break;
      }
    }
  }
  return sinks;
}
