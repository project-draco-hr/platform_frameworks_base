{
  List<String> sinks=new ArrayList<String>();
  if (mAudioDevices == null) {
    return sinks;
  }
  for (  SinkState sink : mAudioDevices.values()) {
    for (    int state : states) {
      if (sink.state == state) {
        sinks.add(sink.address);
        break;
      }
    }
  }
  return sinks;
}
