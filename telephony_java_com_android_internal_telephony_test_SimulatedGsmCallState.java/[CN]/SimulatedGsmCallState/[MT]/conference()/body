{
  int countCalls=0;
  for (int i=0; i < calls.length; i++) {
    CallInfo c=calls[i];
    if (c != null) {
      countCalls++;
      if (c.isConnecting()) {
        return false;
      }
    }
  }
  for (int i=0; i < calls.length; i++) {
    CallInfo c=calls[i];
    if (c != null) {
      c.state=CallInfo.State.ACTIVE;
      if (countCalls > 0) {
        c.isMpty=true;
      }
    }
  }
  return true;
}
