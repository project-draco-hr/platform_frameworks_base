{
  boolean found=false;
  for (int i=0; i < calls.length; i++) {
    CallInfo c=calls[i];
    if (c != null && c.isRinging()) {
      found=true;
      calls[i]=null;
      break;
    }
  }
  if (!found) {
    for (int i=0; i < calls.length; i++) {
      CallInfo c=calls[i];
      if (c != null && c.state == CallInfo.State.HOLDING) {
        found=true;
        calls[i]=null;
      }
    }
  }
  return true;
}
