{
  if (this != mSessionCallback) {
    return;
  }
  if (DEBUG) {
    Log.d(TAG,"onLayoutSurface (left=" + left + ", top="+ top+ ", right="+ right+ ", bottom="+ bottom+ ",)");
  }
  mSurfaceViewLeft=left;
  mSurfaceViewTop=top;
  mSurfaceViewRight=right;
  mSurfaceViewBottom=bottom;
  mUseRequestedSurfaceLayout=true;
  requestLayout();
}
