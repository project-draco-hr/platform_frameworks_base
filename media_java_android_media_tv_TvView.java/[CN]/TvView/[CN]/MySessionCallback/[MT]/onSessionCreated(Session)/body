{
  if (this != mSessionCallback) {
    if (session != null) {
      session.release();
    }
    return;
  }
  mSession=session;
  if (session != null) {
    if (mSurface != null) {
      setSessionSurface(mSurface);
    }
    createSessionOverlayView();
    mSession.tune(mChannelUri);
  }
 else {
    if (mListener != null) {
      mListener.onError(mInputId,ERROR_BUSY);
    }
  }
}
