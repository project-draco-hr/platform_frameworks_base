{
  if (this != mSessionCallback) {
    if (session != null) {
      session.release();
    }
    return;
  }
  mSession=session;
  if (session != null) {
    if (mSurface != null) {
      setSessionSurface(mSurface);
    }
    createSessionOverlayView();
  }
  if (mExternalCallback != null) {
    mExternalCallback.onSessionCreated(session);
  }
}
