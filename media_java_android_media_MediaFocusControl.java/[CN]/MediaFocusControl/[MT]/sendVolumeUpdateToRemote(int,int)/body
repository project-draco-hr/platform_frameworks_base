{
  if (DEBUG_VOL) {
    Log.d(TAG,"sendVolumeUpdateToRemote(rccId=" + rccId + " , dir="+ direction);
  }
  if (direction == 0) {
    return;
  }
  IRemoteVolumeObserver rvo=null;
synchronized (mMCStack) {
    try {
      for (int index=mMCStack.size() - 1; index >= 0; index--) {
        final MediaController mcse=mMCStack.elementAt(index);
        if (mcse.getRccId() == rccId) {
          rvo=mcse.mRemoteVolumeObs;
          break;
        }
      }
    }
 catch (    ArrayIndexOutOfBoundsException e) {
      Log.e(TAG,"Wrong index accessing media button stack, lock error? ",e);
    }
  }
  if (rvo != null) {
    try {
      rvo.dispatchRemoteVolumeUpdate(direction,-1);
    }
 catch (    RemoteException e) {
      Log.e(TAG,"Error dispatching relative volume update",e);
    }
  }
}
