{
  int reg=checkRcdRegistrationAuthorization(null);
  if (reg != RCD_REG_FAILURE) {
    registerRemoteControlDisplay_int(rcd,w,h,null);
    return true;
  }
 else {
    Slog.w(TAG,"Access denied to process: " + Binder.getCallingPid() + ", must have permission "+ android.Manifest.permission.MEDIA_CONTENT_CONTROL+ " to register IRemoteControlDisplay");
    return false;
  }
}
