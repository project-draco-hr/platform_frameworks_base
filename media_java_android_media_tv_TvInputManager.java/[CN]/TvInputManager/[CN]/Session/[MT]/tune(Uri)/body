{
  if (channelUri == null) {
    throw new IllegalArgumentException("channelUri cannot be null");
  }
  if (mToken == null) {
    Log.w(TAG,"The session has been already released");
    return;
  }
  try {
    mService.tune(mToken,channelUri,mUserId);
  }
 catch (  RemoteException e) {
    throw new RuntimeException(e);
  }
}
