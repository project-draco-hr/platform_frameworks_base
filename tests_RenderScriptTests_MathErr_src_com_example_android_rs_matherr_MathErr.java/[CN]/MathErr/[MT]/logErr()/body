{
  mAllocationRes.copyTo(mRes);
  for (int i=0; i < BUF_SIZE; i++) {
    int err=Float.floatToRawIntBits(mRef[i]) - Float.floatToRawIntBits(mRes[i]);
    err=Math.abs(err);
    if (err > 8096) {
      android.util.Log.v("err","error " + err + " src "+ mSrc[i]+ " ref "+ mRef[i]+ " res "+ mRes[i]);
    }
  }
}
