{
  String whereClause;
  String date=BookmarkColumns.DATE;
  if (-1 == begin) {
    if (-1 == end) {
      clearHistory(cr);
      return;
    }
    whereClause=date + " < " + Long.toString(end);
  }
 else   if (-1 == end) {
    whereClause=date + " >= " + Long.toString(begin);
  }
 else {
    whereClause=date + " >= " + Long.toString(begin)+ " AND "+ date+ " < "+ Long.toString(end);
  }
  deleteHistoryWhere(cr,whereClause);
}
