{
  Cursor cursor=null;
  boolean ret=false;
  try {
    cursor=cr.query(History.CONTENT_URI,new String[]{History._ID,History.VISITS},null,null,null);
    ret=cursor.getCount() > 0;
  }
 catch (  IllegalStateException e) {
    Log.e(LOGTAG,"canClearHistory",e);
  }
 finally {
    if (cursor != null)     cursor.close();
  }
  return ret;
}
