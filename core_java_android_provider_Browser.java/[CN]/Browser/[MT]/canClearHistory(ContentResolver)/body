{
  try {
    Cursor c=cr.query(BOOKMARKS_URI,new String[]{BookmarkColumns._ID,BookmarkColumns.BOOKMARK,BookmarkColumns.VISITS},"bookmark = 0 OR visits > 0",null,null);
    boolean ret=c.moveToFirst();
    c.deactivate();
    return ret;
  }
 catch (  IllegalStateException e) {
    return false;
  }
}
