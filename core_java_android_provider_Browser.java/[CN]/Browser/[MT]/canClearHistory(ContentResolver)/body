{
  Cursor c=null;
  boolean ret=false;
  try {
    c=cr.query(BOOKMARKS_URI,new String[]{BookmarkColumns._ID,BookmarkColumns.BOOKMARK,BookmarkColumns.VISITS},"bookmark = 0 OR visits > 0",null,null);
    ret=c.moveToFirst();
  }
 catch (  IllegalStateException e) {
    Log.e(LOGTAG,"canClearHistory",e);
  }
 finally {
    if (c != null)     c.close();
  }
  return ret;
}
