{
  Cursor c=null;
  String[] str=null;
  try {
    String[] projection=new String[]{"url"};
    c=cr.query(BOOKMARKS_URI,projection,"visits > 0",null,null);
    str=new String[c.getCount()];
    int i=0;
    while (c.moveToNext()) {
      str[i]=c.getString(0);
      i++;
    }
  }
 catch (  IllegalStateException e) {
    Log.e(LOGTAG,"getVisitedHistory",e);
    str=new String[0];
  }
 finally {
    if (c != null)     c.close();
  }
  return str;
}
