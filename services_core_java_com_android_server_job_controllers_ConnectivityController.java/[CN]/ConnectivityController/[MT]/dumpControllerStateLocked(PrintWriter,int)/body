{
  pw.println("Conn.");
  for (int i=0; i < mTrackedJobs.size(); i++) {
    final JobStatus js=mTrackedJobs.get(i);
    if (js.shouldDump(filterUid)) {
      pw.println(String.valueOf(js.getJobId() + "," + js.getUid()) + ": C=" + js.hasConnectivityConstraint()+ ", UM="+ js.hasUnmeteredConstraint()+ ", NR="+ js.hasNotRoamingConstraint());
    }
  }
}
