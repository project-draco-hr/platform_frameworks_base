{
  pw.println("Conn.");
  pw.println("connected: " + mNetworkConnected + " unmetered: "+ mNetworkUnmetered);
  for (  JobStatus js : mTrackedJobs) {
    pw.println(String.valueOf(js.getJobId() + "," + js.getUid()) + ": C=" + js.hasConnectivityConstraint()+ ", UM="+ js.hasUnmeteredConstraint());
  }
}
