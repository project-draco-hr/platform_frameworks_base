{
synchronized (mLock) {
    for (int i=0; i < mTrackedJobs.size(); i++) {
      final JobStatus js=mTrackedJobs.get(i);
      if (js.isReady()) {
        if (DEBUG) {
          Slog.d(TAG,"Running " + js + " due to network activity.");
        }
        mStateChangedListener.onRunJobNow(js);
      }
    }
  }
}
