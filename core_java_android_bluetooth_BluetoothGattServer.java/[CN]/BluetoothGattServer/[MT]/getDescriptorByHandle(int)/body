{
  for (  BluetoothGattService svc : mServices) {
    for (    BluetoothGattCharacteristic charac : svc.getCharacteristics()) {
      for (      BluetoothGattDescriptor desc : charac.getDescriptors()) {
        if (desc.getInstanceId() == handle)         return desc;
      }
    }
  }
  return null;
}
