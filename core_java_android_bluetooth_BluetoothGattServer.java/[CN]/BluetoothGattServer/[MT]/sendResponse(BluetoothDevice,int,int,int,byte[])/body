{
  if (DBG)   Log.d(TAG,"sendResponse() - device: " + device.getAddress());
  if (mService == null || mServerIf == 0)   return false;
  try {
    mService.sendResponse(mServerIf,device.getAddress(),requestId,status,offset,value);
  }
 catch (  RemoteException e) {
    Log.e(TAG,"",e);
    return false;
  }
  return true;
}
