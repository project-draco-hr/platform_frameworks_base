{
  if (DBG)   Log.d(TAG,"addService() - service: " + service.getUuid());
  if (mService == null || mServerIf == 0)   return false;
  mPendingService=service;
  try {
    mService.addService(mServerIf,service);
  }
 catch (  RemoteException e) {
    Log.e(TAG,"",e);
    return false;
  }
  return true;
}
