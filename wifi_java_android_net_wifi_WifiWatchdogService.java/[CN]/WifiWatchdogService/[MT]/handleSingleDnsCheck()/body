{
  mStatus.lastSingleCheckTime=SystemClock.elapsedRealtime();
  long responseTime=mDnsPinger.pingDns(mDnsPinger.getDns(),DNS_PING_TIMEOUT_MS);
  if (DBG) {
    Slog.d(WWS_TAG,"Ran a single DNS ping. Response time: " + responseTime);
  }
  if (responseTime < 0) {
    return false;
  }
  return true;
}
