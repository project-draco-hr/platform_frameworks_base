{
  final FinishedEventCallback callback;
synchronized (mH) {
    PendingEvent p=dequeuePendingEventLocked(seq);
    if (p == null) {
      return;
    }
    mH.removeMessages(MSG_EVENT_TIMEOUT,p);
    callback=p.mCallback;
    recyclePendingEventLocked(p);
  }
  callback.finishedEvent(seq,handled);
}
