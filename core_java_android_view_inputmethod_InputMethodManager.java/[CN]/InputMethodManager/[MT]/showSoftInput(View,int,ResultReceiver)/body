{
  checkFocus();
synchronized (mH) {
    if (mServedView != view && (mServedView == null || !mServedView.checkInputConnectionProxy(view))) {
      return false;
    }
    try {
      return mService.showSoftInput(mClient,flags,resultReceiver);
    }
 catch (    RemoteException e) {
      throw e.rethrowFromSystemServer();
    }
  }
}
