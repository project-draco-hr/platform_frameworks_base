{
synchronized (mH) {
    final boolean isImmediate=(mCursorAnchorInfoMonitorMode & CursorAnchorInfoRequest.FLAG_CURSOR_ANCHOR_INFO_IMMEDIATE) != 0;
    final boolean isMonitoring=(mCursorAnchorInfoMonitorMode & CursorAnchorInfoRequest.FLAG_CURSOR_ANCHOR_INFO_MONITOR) != 0;
    return isImmediate || isMonitoring;
  }
}
