{
synchronized (mH) {
    final boolean isImmediate=(mRequestUpdateCursorAnchorInfoMonitorMode & InputConnection.REQUEST_UPDATE_CURSOR_ANCHOR_INFO_IMMEDIATE) != 0;
    final boolean isMonitoring=(mRequestUpdateCursorAnchorInfoMonitorMode & InputConnection.REQUEST_UPDATE_CURSOR_ANCHOR_INFO_MONITOR) != 0;
    return isImmediate || isMonitoring;
  }
}
