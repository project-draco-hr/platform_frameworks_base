{
synchronized (mH) {
    if (view != mLastServedView) {
      return;
    }
    if (DEBUG)     Log.v(TAG,"checkFocus: view=" + mServedView + " last="+ mLastServedView);
    if (mServedView == mLastServedView) {
      finishInputLocked();
      closeCurrentInput();
    }
    mLastServedView=null;
  }
}
