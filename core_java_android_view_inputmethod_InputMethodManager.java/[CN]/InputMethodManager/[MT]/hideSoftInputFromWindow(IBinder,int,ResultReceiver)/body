{
  checkFocus();
synchronized (mH) {
    if (mServedView == null || mServedView.getWindowToken() != windowToken) {
      return false;
    }
    try {
      return mService.hideSoftInput(mClient,flags,resultReceiver);
    }
 catch (    RemoteException e) {
      throw e.rethrowFromSystemServer();
    }
  }
}
