{
  mH.removeMessages(MSG_EVENT_TIMEOUT);
  PendingEvent curr, prev, next;
  curr=mFirstPendingEvent;
  prev=null;
  while (curr != null) {
    next=curr.mNext;
    curr.mNext=prev;
    prev=curr;
    curr=next;
  }
  curr=prev;
  prev=null;
  while (curr != null) {
    Message msg=mH.obtainMessage(MSG_EVENT_TIMEOUT,curr.mSeq,0,curr);
    msg.setAsynchronous(true);
    mH.sendMessage(msg);
    next=curr.mNext;
    curr.mNext=prev;
    prev=curr;
    curr=next;
  }
}
