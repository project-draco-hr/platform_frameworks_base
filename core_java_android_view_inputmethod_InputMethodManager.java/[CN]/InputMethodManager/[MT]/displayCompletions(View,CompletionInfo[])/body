{
  checkFocus();
synchronized (mH) {
    if (mServedView != view && (mServedView == null || !mServedView.checkInputConnectionProxy(view))) {
      return;
    }
    mCompletions=completions;
    if (mCurMethod != null) {
      try {
        mCurMethod.displayCompletions(mCompletions);
      }
 catch (      RemoteException e) {
      }
    }
  }
}
