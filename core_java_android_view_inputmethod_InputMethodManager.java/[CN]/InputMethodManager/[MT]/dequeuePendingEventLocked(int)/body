{
  PendingEvent p=mPendingEventHead;
  if (p == null) {
    return null;
  }
  if (p.mSeq == seq) {
    mPendingEventHead=p.mNext;
    if (mPendingEventHead == null) {
      mPendingEventTail=null;
    }
  }
 else {
    PendingEvent prev;
    do {
      prev=p;
      p=p.mNext;
      if (p == null) {
        return null;
      }
    }
 while (p.mSeq != seq);
    prev.mNext=p.mNext;
    if (mPendingEventTail == p) {
      mPendingEventTail=prev;
    }
  }
  p.mNext=null;
  return p;
}
