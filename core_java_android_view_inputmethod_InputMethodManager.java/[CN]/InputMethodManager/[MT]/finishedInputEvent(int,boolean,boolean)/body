{
  final PendingEvent p;
synchronized (mH) {
    int index=mPendingEvents.indexOfKey(seq);
    if (index < 0) {
      return;
    }
    p=mPendingEvents.valueAt(index);
    mPendingEvents.removeAt(index);
    Trace.traceCounter(Trace.TRACE_TAG_INPUT,PENDING_EVENT_COUNTER,mPendingEvents.size());
    if (timeout) {
      Log.w(TAG,"Timeout waiting for IME to handle input event after " + INPUT_METHOD_NOT_RESPONDING_TIMEOUT + " ms: "+ p.mInputMethodId);
    }
 else {
      mH.removeMessages(MSG_TIMEOUT_INPUT_EVENT,p);
    }
  }
  invokeFinishedInputEventCallback(p,handled);
}
