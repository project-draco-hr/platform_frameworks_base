{
synchronized (mH) {
    if (rootView == null) {
      mCurRootView=null;
    }
    if (hasWindowFocus) {
      mCurRootView=rootView;
    }
 else     if (rootView == mCurRootView) {
      mCurRootView=null;
    }
 else {
      if (DEBUG) {
        Log.v(TAG,"Ignoring onPreWindowFocus()." + " mCurRootView=" + mCurRootView + " rootView="+ rootView);
      }
    }
  }
}
