{
synchronized (mH) {
    if (mServedView != null) {
      if (DEBUG)       Log.v(TAG,"FINISH INPUT: " + mServedView);
      updateStatusIcon(0,null);
      if (mCurrentTextBoxAttribute != null) {
        try {
          mService.finishInput(mClient);
        }
 catch (        RemoteException e) {
        }
      }
      mServedView=null;
      mCompletions=null;
      mServedConnecting=false;
      clearConnectionLocked();
    }
  }
}
