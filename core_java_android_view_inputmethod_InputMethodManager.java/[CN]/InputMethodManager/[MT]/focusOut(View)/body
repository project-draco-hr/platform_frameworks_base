{
synchronized (mH) {
    if (DEBUG)     Log.v(TAG,"focusOut: view=" + dumpViewInfo(view) + " mServedView="+ dumpViewInfo(mServedView));
    if (mServedView != view) {
      if (false && view.hasWindowFocus()) {
        mNextServedView=null;
        scheduleCheckFocusLocked(view);
      }
    }
  }
}
