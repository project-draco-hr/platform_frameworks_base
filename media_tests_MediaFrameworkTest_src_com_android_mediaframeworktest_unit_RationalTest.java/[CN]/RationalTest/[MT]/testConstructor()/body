{
  Rational r=new Rational(1,2);
  assertEquals(1,r.getNumerator());
  assertEquals(2,r.getDenominator());
  r=new Rational(-1,2);
  assertEquals(-1,r.getNumerator());
  assertEquals(2,r.getDenominator());
  r=new Rational(1,-2);
  assertEquals(-1,r.getNumerator());
  assertEquals(2,r.getDenominator());
  r=new Rational(-1,-2);
  assertEquals(1,r.getNumerator());
  assertEquals(2,r.getDenominator());
  r=new Rational(1,0);
  assertEquals(0,r.getNumerator());
  assertEquals(0,r.getDenominator());
  r=new Rational(-1,0);
  assertEquals(0,r.getNumerator());
  assertEquals(0,r.getDenominator());
  r=new Rational(0,0);
  assertEquals(0,r.getNumerator());
  assertEquals(0,r.getDenominator());
}
