{
  Rational r=new Rational(1,2);
  assertEquals(1,r.getNumerator());
  assertEquals(2,r.getDenominator());
  r=new Rational(-1,2);
  assertEquals(-1,r.getNumerator());
  assertEquals(2,r.getDenominator());
  r=new Rational(1,-2);
  assertEquals(-1,r.getNumerator());
  assertEquals(2,r.getDenominator());
  r=new Rational(-1,-2);
  assertEquals(1,r.getNumerator());
  assertEquals(2,r.getDenominator());
  try {
    r=new Rational(1,0);
    fail("Expected Rational constructor to throw an IllegalArgumentException");
  }
 catch (  IllegalArgumentException e) {
  }
}
