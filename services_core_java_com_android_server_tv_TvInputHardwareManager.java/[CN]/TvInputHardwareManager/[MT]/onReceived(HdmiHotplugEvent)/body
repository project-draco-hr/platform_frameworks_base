{
  String inputId=null;
synchronized (mLock) {
    mHdmiStateMap.put(event.getPort(),event.isConnected());
    inputId=findInputIdForHdmiPortLocked(event.getPort());
    if (inputId == null) {
      return;
    }
    mHandler.obtainMessage(ClientHandler.DO_SET_AVAILABLE,convertConnectedToState(event.isConnected()),0,inputId).sendToTarget();
  }
}
