{
synchronized (mLock) {
    mHdmiStateMap.put(event.getPort(),event.isConnected());
    String inputId=findInputIdForHdmiPortLocked(event.getPort());
    if (inputId == null) {
      return;
    }
    mHandler.obtainMessage(ListenerHandler.STATE_CHANGED,convertConnectedToState(event.isConnected()),0,inputId).sendToTarget();
  }
}
