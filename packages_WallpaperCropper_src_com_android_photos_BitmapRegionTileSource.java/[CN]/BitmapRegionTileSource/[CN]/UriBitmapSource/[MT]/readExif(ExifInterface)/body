{
  try {
    ei.readExif(regenerateInputStream());
    return true;
  }
 catch (  FileNotFoundException e) {
    Log.e("BitmapRegionTileSource","Failed to load URI " + mUri,e);
    return false;
  }
catch (  IOException e) {
    Log.e("BitmapRegionTileSource","Failure while reading URI " + mUri,e);
    return false;
  }
}
