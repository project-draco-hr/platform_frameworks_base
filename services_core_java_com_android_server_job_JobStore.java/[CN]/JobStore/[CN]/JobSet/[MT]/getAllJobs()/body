{
  ArrayList<JobStatus> allJobs=new ArrayList<JobStatus>(size());
  for (int i=mJobs.size() - 1; i >= 0; i--) {
    ArraySet<JobStatus> jobs=mJobs.valueAt(i);
    if (jobs != null) {
      for (int j=jobs.size() - 1; j >= 0; j--) {
        allJobs.add(jobs.valueAt(j));
      }
    }
  }
  return allJobs;
}
