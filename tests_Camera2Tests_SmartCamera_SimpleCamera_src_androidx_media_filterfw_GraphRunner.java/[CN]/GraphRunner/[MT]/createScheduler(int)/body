{
switch (strategy) {
case STRATEGY_LRU:
    mScheduler=new LruScheduler();
  break;
case STRATEGY_LFU:
mScheduler=new LfuScheduler();
break;
case STRATEGY_ONESHOT:
mScheduler=new OneShotScheduler();
break;
default :
throw new IllegalArgumentException("Unknown schedule-strategy constant " + strategy + "!");
}
}
