{
switch (key) {
case KEY_FAST_TOGGLE:
    mFastToggleRecents=(newValue != null) && (Integer.parseInt(newValue) != 0);
  break;
case KEY_PAGE_ON_TOGGLE:
mPageOnToggle=(newValue != null) && (Integer.parseInt(newValue) != 0);
break;
case KEY_FULLSCREEN_THUMBNAILS:
mUseFullscreenThumbnails=(newValue != null) && (Integer.parseInt(newValue) != 0);
break;
case KEY_SHOW_HISTORY:
mShowHistory=(newValue != null) && (Integer.parseInt(newValue) != 0);
break;
}
EventBus.getDefault().send(new DebugFlagsChangedEvent());
}
