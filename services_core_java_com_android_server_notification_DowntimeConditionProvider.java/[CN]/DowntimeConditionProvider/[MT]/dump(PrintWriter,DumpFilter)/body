{
  pw.println("    DowntimeConditionProvider:");
  pw.print("      mConnected=");
  pw.println(mConnected);
  pw.print("      mSubscriptions=");
  pw.println(mSubscriptions);
  pw.print("      mLookaheadThreshold=");
  pw.print(mLookaheadThreshold);
  pw.print(" (");
  TimeUtils.formatDuration(mLookaheadThreshold,pw);
  pw.println(")");
  pw.print("      mCalendar=");
  pw.println(mCalendar);
  pw.print("      mFiredAlarms=");
  pw.println(mFiredAlarms);
  pw.print("      mDowntimed=");
  pw.println(mDowntimed);
  pw.print("      mConditionClearing=");
  pw.println(mConditionClearing);
  pw.print("      mRequesting=");
  pw.println(mRequesting);
}
