{
  if (super.performAccessibilityAction(action,args)) {
    return true;
  }
switch (action) {
case AccessibilityNodeInfo.ACTION_SCROLL_FORWARD:
    if (canScrollHorizontally(1)) {
      setCurrentItem(mCurItem + 1);
      return true;
    }
  return false;
case AccessibilityNodeInfo.ACTION_SCROLL_BACKWARD:
if (canScrollHorizontally(-1)) {
  setCurrentItem(mCurItem - 1);
  return true;
}
return false;
}
return false;
}
