{
  super.onCreate(icicle);
  String ssid=icicle.getString("ssid");
  if (ssid != null) {
    mSsid=ssid;
  }
  String password=(String)icicle.get("password");
  if (password != null) {
    mPassword=password;
  }
  String wifiOnlyFlag=(String)icicle.get("wifi-only");
  if (wifiOnlyFlag != null) {
    mWifiOnly=true;
  }
}
