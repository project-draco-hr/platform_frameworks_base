{
  StringBuilder str=new StringBuilder(GestureConstants.STROKE_STRING_BUFFER_SIZE);
  float[] pts=points;
  long[] times=timestamps;
  int count=points.length;
  for (int i=0; i < count; i+=2) {
    str.append(points[i] + GestureConstants.STRING_STROKE_DELIIMITER + points[i + 1]+ GestureConstants.STRING_STROKE_DELIIMITER+ times[i / 2]+ GestureConstants.STRING_STROKE_DELIIMITER);
  }
  return str.toString();
}
