{
  StringBuilder str=new StringBuilder(GestureConstants.STROKE_STRING_BUFFER_SIZE);
  float[] xpts=xPoints;
  float[] ypts=yPoints;
  long[] times=timestamps;
  int count=xpts.length;
  for (int i=0; i < count; i++) {
    str.append(xpts[i] + GestureConstants.STRING_STROKE_DELIIMITER + ypts[i]+ GestureConstants.STRING_STROKE_DELIIMITER+ times[i]+ GestureConstants.STRING_STROKE_DELIIMITER);
  }
  return str.toString();
}
