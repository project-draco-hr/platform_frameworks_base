{
  new Thread(){
    @Override public void run(){
      try {
        logBootEvents(context);
      }
 catch (      Exception e) {
        Slog.e(TAG,"Can't log boot events",e);
      }
      try {
        removeOldUpdatePackages(context);
      }
 catch (      Exception e) {
        Slog.e(TAG,"Can't remove old update packages",e);
      }
    }
  }
.start();
}
