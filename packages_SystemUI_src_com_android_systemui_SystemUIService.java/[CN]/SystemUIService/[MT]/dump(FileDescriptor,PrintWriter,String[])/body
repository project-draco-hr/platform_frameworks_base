{
  if (checkCallingOrSelfPermission(android.Manifest.permission.DUMP) != PackageManager.PERMISSION_GRANTED) {
    pw.println("Permission Denial: can't dump StatusBar from from pid=" + Binder.getCallingPid() + ", uid="+ Binder.getCallingUid());
    return;
  }
  for (  SystemUI ui : mServices) {
    pw.println("dumping service: " + ui.getClass().getName());
    ui.dump(fd,pw,args);
  }
}
