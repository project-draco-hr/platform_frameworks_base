{
  WifiAwareManager mgr=mMgr.get();
  if (mgr == null) {
    Log.e(TAG,"publish: called post GC on WifiAwareManager");
    return;
  }
  if (mTerminated) {
    Log.e(TAG,"publish: called after termination");
    return;
  }
  mgr.publish(mClientId,(handler == null) ? Looper.getMainLooper() : handler.getLooper(),publishConfig,callback);
}
