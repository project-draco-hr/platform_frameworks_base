{
  if (mMediaSize != null) {
    parcel.writeInt(1);
    mMediaSize.writeToParcel(parcel);
  }
 else {
    parcel.writeInt(0);
  }
  if (mResolution != null) {
    parcel.writeInt(1);
    mResolution.writeToParcel(parcel);
  }
 else {
    parcel.writeInt(0);
  }
  if (mMargins != null) {
    parcel.writeInt(1);
    mMargins.writeToParcel(parcel);
  }
 else {
    parcel.writeInt(0);
  }
  parcel.writeInt(mColorMode);
}
