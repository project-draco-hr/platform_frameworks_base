{
  if (mMediaSize != null) {
    parcel.writeInt(1);
    mMediaSize.writeToParcel(parcel);
  }
 else {
    parcel.writeInt(0);
  }
  if (mResolution != null) {
    parcel.writeInt(1);
    mResolution.writeToParcel(parcel);
  }
 else {
    parcel.writeInt(0);
  }
  if (mMargins != null) {
    parcel.writeInt(1);
    mMargins.writeToParcel(parcel);
  }
 else {
    parcel.writeInt(0);
  }
  if (mInputTray != null) {
    parcel.writeInt(1);
    mInputTray.writeToParcel(parcel);
  }
 else {
    parcel.writeInt(0);
  }
  if (mOutputTray != null) {
    parcel.writeInt(1);
    mOutputTray.writeToParcel(parcel);
  }
 else {
    parcel.writeInt(0);
  }
  parcel.writeInt(mDuplexMode);
  parcel.writeInt(mColorMode);
  parcel.writeInt(mFittingMode);
  parcel.writeInt(mOrientation);
  parcel.writeInt(mCopies);
}
