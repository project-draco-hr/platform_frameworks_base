{
  mVerifier.initForExportTest(vcardType);
  ContactEntry entry=mVerifier.addInputEntry();
  entry.addContentValues(Phone.CONTENT_ITEM_TYPE).put(Phone.NUMBER,"0312341234").put(Phone.TYPE,Phone.TYPE_HOME);
  entry.addContentValues(Phone.CONTENT_ITEM_TYPE).put(Phone.NUMBER,"09012341234").put(Phone.TYPE,Phone.TYPE_MOBILE);
  mVerifier.addPropertyNodesVerifierElemWithEmptyName().addExpectedNode("TEL","03-1234-1234",new TypeSet("HOME")).addExpectedNode("TEL","090-1234-1234",new TypeSet("CELL"));
}
