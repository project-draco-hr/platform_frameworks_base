{
  checkPermissionBluetoothAdmin();
  if (mIsAirplaneSensitive && isAirplaneModeOn()) {
    return false;
  }
  if (mIsEnabled) {
    return false;
  }
  if (mEnableThread != null && mEnableThread.isAlive()) {
    return false;
  }
  mEnableThread=new EnableThread(callback);
  mEnableThread.start();
  return true;
}
