{
  super(context,attrs);
  setFocusable(false);
  LayoutInflater inflater=(LayoutInflater)context.getSystemService(Context.LAYOUT_INFLATER_SERVICE);
  inflater.inflate(R.layout.zoom_controls,this,true);
  mZoomIn=(ZoomButton)findViewById(R.id.zoomIn);
