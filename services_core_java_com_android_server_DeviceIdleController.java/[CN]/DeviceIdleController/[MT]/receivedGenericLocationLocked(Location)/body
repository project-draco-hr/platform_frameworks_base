{
  if (mState != STATE_LOCATING) {
    cancelLocatingLocked();
    return;
  }
  if (DEBUG)   Slog.d(TAG,"Generic location: " + location);
  mLastGenericLocation=new Location(location);
  if (location.getAccuracy() > mConstants.LOCATION_ACCURACY && mHasGps) {
    return;
  }
  mLocated=true;
  if (mNotMoving) {
    stepIdleStateLocked();
  }
}
