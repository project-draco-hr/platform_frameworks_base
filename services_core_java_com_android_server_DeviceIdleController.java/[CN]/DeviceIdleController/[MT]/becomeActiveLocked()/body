{
  if (mState != STATE_ACTIVE) {
    try {
      mNetworkPolicyManager.setDeviceIdleMode(false);
      mBatteryStats.noteDeviceIdleMode(false,true,false);
    }
 catch (    RemoteException e) {
    }
    mState=STATE_ACTIVE;
    mNextIdlePendingDelay=0;
    mNextIdleDelay=0;
    cancelAlarmLocked();
    stopMonitoringSignificantMotion();
  }
}
