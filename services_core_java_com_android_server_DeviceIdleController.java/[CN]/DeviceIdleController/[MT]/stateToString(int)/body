{
switch (state) {
case STATE_ACTIVE:
    return "ACTIVE";
case STATE_INACTIVE:
  return "INACTIVE";
case STATE_IDLE_PENDING:
return "IDLE_PENDING";
case STATE_SENSING:
return "SENSING";
case STATE_LOCATING:
return "LOCATING";
case STATE_IDLE:
return "IDLE";
case STATE_IDLE_MAINTENANCE:
return "IDLE_MAINTENANCE";
default :
return Integer.toString(state);
}
}
