{
  if (callback == mDiscoveryCallback) {
    if (DEBUG) {
      Log.d(TAG,"discoveryFailed: error=" + error + ", message="+ message+ ", extras="+ extras);
    }
    try {
      mClientCallback.onDiscoveryFailed(mDiscoverySeq,error,message,extras);
    }
 catch (    RemoteException ex) {
    }
  }
}
