{
  if (callback == mConnectionCallback) {
    if (DEBUG) {
      Log.d(TAG,"connectionFailed: error=" + error + ", message="+ message+ ", extras="+ extras);
    }
    try {
      mClientCallback.onConnectionFailed(mConnectionSeq,error,message,extras);
    }
 catch (    RemoteException ex) {
    }
  }
}
