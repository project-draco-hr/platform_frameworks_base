{
  mHandler.post(new Runnable(){
    @Override public void run(){
      ClientRecord record=mClientRecords.get(callback.asBinder());
      if (record == null) {
        return;
      }
      if (DEBUG) {
        Log.d(TAG,"pauseStream: client=" + record.getClientInfo());
      }
      record.pauseStream();
    }
  }
);
}
