{
  StringBuilder word=new StringBuilder();
  try {
    for (; ; ) {
      char ch=mBuffer.charAt(offset);
      if (isLetterOrDigit(ch) || ch == '-') {
        word.append(ch);
        offset++;
      }
 else {
        break;
      }
    }
  }
 catch (  IndexOutOfBoundsException e) {
    ;
  }
  return word.toString();
}
