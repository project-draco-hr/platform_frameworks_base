{
  int ret=0, sum=0;
  ret=parseTag(offset);
  if (PARSE_ERROR == ret) {
    return PARSE_ERROR;
  }
  offset+=ret;
  sum+=ret;
  for (; ; ) {
    ret=parseString(offset,"-",false);
    if (PARSE_ERROR == ret) {
      break;
    }
    offset+=ret;
    sum+=ret;
    ret=parseTag(offset);
    if (PARSE_ERROR == ret) {
      break;
    }
    offset+=ret;
    sum+=ret;
  }
  return sum;
}
