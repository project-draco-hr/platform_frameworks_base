{
  int sum=0;
  try {
    for (; ; ) {
      char ch;
      ch=mBuffer.charAt(offset);
      if (ch == '\r') {
        int ret=parseString(offset,"\r\n\r\n",false);
        sum+=ret;
        break;
      }
 else {
        sum++;
        offset++;
      }
    }
  }
 catch (  IndexOutOfBoundsException e) {
    return PARSE_ERROR;
  }
  sum-=2;
  return sum;
}
