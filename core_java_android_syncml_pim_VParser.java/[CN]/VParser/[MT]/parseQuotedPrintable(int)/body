{
  int ret=0, sum=0;
  ret=removeWs(offset);
  offset+=ret;
  sum+=ret;
  for (; ; ) {
    ret=parsePtext(offset);
    if (PARSE_ERROR == ret)     break;
    offset+=ret;
    sum+=ret;
    ret=removeWs(offset);
    offset+=ret;
    sum+=ret;
  }
  ret=parseString(offset,"=",false);
  if (ret != PARSE_ERROR) {
    sum+=ret;
  }
  return sum;
}
