{
  int sum=0;
  if (tar == null) {
    return PARSE_ERROR;
  }
  if (ignoreCase) {
    int len=tar.length();
    try {
      if (mBuffer.substring(offset,offset + len).equalsIgnoreCase(tar)) {
        sum=len;
      }
 else {
        return PARSE_ERROR;
      }
    }
 catch (    IndexOutOfBoundsException e) {
      return PARSE_ERROR;
    }
  }
 else {
    if (mBuffer.startsWith(tar,offset)) {
      sum=tar.length();
    }
 else {
      return PARSE_ERROR;
    }
  }
  return sum;
}
