{
  final int id=view.getId();
  boolean visitChildren=true;
  if (id > 0) {
    int index;
    if (viewsWithIds != null && (index=viewsWithIds.get(id,-1)) >= 0) {
      views[index]=view;
    }
 else     if (includes != null && (index=includes.get(id,-1)) >= 0) {
      views[index]=view;
      visitChildren=false;
    }
  }
  return visitChildren;
}
