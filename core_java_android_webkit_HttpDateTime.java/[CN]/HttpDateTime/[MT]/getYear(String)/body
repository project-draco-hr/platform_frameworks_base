{
  if (yearString.length() == 2) {
    int year=(yearString.charAt(0) - '0') * 10 + (yearString.charAt(1) - '0');
    if (year >= 70) {
      return year + 1900;
    }
 else {
      return year + 2000;
    }
  }
 else   return (yearString.charAt(0) - '0') * 1000 + (yearString.charAt(1) - '0') * 100 + (yearString.charAt(2) - '0') * 10 + (yearString.charAt(3) - '0');
}
