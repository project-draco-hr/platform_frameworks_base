{
  String str="Stack Tasks (" + mStackTaskList.size() + "):\n";
  for (  Task t : mStackTaskList.getTasks()) {
    str+="    " + t.toString() + "\n";
  }
  str+="Historical Tasks(" + mHistoryTaskList.size() + "):\n";
  for (  Task t : mHistoryTaskList.getTasks()) {
    str+="    " + t.toString() + "\n";
  }
  return str;
}
