{
  ArrayList<Task> stackTasks=new ArrayList<>();
  ArrayList<Task> historyTasks=new ArrayList<>();
  for (  Task task : tasks) {
    if (task.isHistorical) {
      historyTasks.add(task);
    }
 else {
      stackTasks.add(task);
    }
  }
  mStackTaskList.set(stackTasks);
  mHistoryTaskList.set(historyTasks);
}
