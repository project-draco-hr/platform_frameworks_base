{
  ArrayList<Task> taskList=mTaskList.getTasks();
  int taskCount=taskList.size();
  for (int i=taskCount - 1; i >= 0; i--) {
    Task t=taskList.get(i);
    removeTaskImpl(t);
    if (mCb != null) {
      mCb.onStackTaskRemoved(this,t,false,null);
    }
  }
  mTaskList.set(tasks);
  for (  Task t : tasks) {
    if (mCb != null) {
      mCb.onStackTaskAdded(this,t);
    }
  }
}
