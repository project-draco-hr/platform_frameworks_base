{
  int taskCount=mTaskList.getTasks().size();
  for (int i=0; i < taskCount; i++) {
    Task t=mTaskList.getTasks().get(i);
    if (mCb != null) {
      mCb.onStackTaskRemoved(this,t);
    }
  }
  mTaskList.set(tasks);
  for (  Task t : tasks) {
    if (mCb != null) {
      mCb.onStackTaskAdded(this,t);
    }
  }
}
