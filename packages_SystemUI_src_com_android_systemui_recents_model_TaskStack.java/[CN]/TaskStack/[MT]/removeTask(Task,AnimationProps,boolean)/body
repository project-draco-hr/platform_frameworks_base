{
  if (mStackTaskList.contains(t)) {
    boolean wasFrontMostTask=(getStackFrontMostTask(false) == t);
    removeTaskImpl(mStackTaskList,t);
    Task newFrontMostTask=getStackFrontMostTask(false);
    if (mCb != null) {
      mCb.onStackTaskRemoved(this,t,wasFrontMostTask,newFrontMostTask,animation,fromDockGesture);
    }
  }
  mRawTaskList.remove(t);
}
