{
  if (mSD.rate == 0) {
    mSD.r=((x & 0x1) != 0) ? 0.f : 1.f;
    mSD.g=((x & 0x2) != 0) ? 0.f : 1.f;
    mSD.b=((x & 0x4) != 0) ? 0.f : 1.f;
    if ((mSD.r + mSD.g + mSD.b) < 0.9f) {
      mSD.r=0.8f;
      mSD.g=0.5f;
      mSD.b=1.f;
    }
  }
  mSD.rate=rate;
  mSD.x=x;
  mSD.y=y;
  mIntAlloc.data(mSD);
}
