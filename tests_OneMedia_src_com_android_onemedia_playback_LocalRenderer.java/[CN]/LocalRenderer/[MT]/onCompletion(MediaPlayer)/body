{
  if (!isCurrentPlayer(player)) {
    return;
  }
  if (DEBUG) {
    Log.d(TAG,mDebugId + ": Completed item. Have next item? " + (mNextPlayer != null));
  }
  if (mNextPlayer != null) {
    if (mPlayer != null) {
      mPlayer.release();
    }
    mPlayer=mNextPlayer;
    mContent=mNextContent;
    mNextPlayer=null;
    mNextContent=null;
    pushOnNextStarted();
    return;
  }
  setState(STATE_ENDED);
}
