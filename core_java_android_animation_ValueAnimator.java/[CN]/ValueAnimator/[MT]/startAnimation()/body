{
  if (Trace.isTagEnabled(Trace.TRACE_TAG_VIEW)) {
    Trace.asyncTraceBegin(Trace.TRACE_TAG_VIEW,getNameForTrace(),System.identityHashCode(this));
  }
  initAnimation();
  mRunning=true;
  if (mSeekFraction >= 0) {
    mOverallFraction=mSeekFraction;
  }
 else {
    mOverallFraction=0f;
  }
  if (mListeners != null) {
    notifyStartListeners();
  }
}
