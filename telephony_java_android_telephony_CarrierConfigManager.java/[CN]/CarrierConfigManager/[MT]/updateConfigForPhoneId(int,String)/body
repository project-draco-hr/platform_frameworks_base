{
  try {
    getICarrierConfigLoader().updateConfigForPhoneId(phoneId,simState);
  }
 catch (  RemoteException ex) {
    Rlog.e(TAG,"Error updating config for phoneId=" + phoneId + ": "+ ex.toString());
  }
catch (  NullPointerException ex) {
    Rlog.e(TAG,"Error updating config for phoneId=" + phoneId + ": "+ ex.toString());
  }
}
