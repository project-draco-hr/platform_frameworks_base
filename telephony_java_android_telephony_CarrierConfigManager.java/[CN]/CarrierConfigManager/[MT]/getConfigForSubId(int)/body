{
  try {
    return getICarrierConfigLoader().getConfigForSubId(subId);
  }
 catch (  RemoteException ex) {
    Rlog.e(TAG,"Error getting config for subId " + Integer.toString(subId) + ": "+ ex.toString());
  }
catch (  NullPointerException ex) {
    Rlog.e(TAG,"Error getting config for subId " + Integer.toString(subId) + ": "+ ex.toString());
  }
  return null;
}
