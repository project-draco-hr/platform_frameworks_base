{
  try {
    getICarrierConfigLoader().reloadCarrierConfigForSubId(subId);
  }
 catch (  RemoteException ex) {
    Rlog.e(TAG,"Error reloading config for subId=" + subId + ": "+ ex.toString());
  }
catch (  NullPointerException ex) {
    Rlog.e(TAG,"Error reloading config for subId=" + subId + ": "+ ex.toString());
  }
}
