{
  Log.d(TAG,"onDisconnect: display.getId()=" + display.getId());
  if (display == mTestDisplay1 || display == mTestDisplay2 || display == mTestDisplay6 || display == mTestDisplay8 || display == mTestDisplay9 || display == mTestDisplay10) {
    display.setStatus(RemoteDisplay.STATUS_AVAILABLE);
    updateDisplay(display);
  }
}
