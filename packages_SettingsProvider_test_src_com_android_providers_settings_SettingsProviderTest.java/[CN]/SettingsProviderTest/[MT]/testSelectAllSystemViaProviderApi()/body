{
  setSettingViaProviderApiAndAssertSuccessfulChange(SETTING_TYPE_SYSTEM,FAKE_SETTING_NAME,FAKE_SETTING_VALUE,true);
  try {
    queryAllSettingsViaProviderApiSettingAndAssertSettingPresent(SETTING_TYPE_SYSTEM,FAKE_SETTING_NAME);
  }
  finally {
    deleteStringViaProviderApi(SETTING_TYPE_SYSTEM,FAKE_SETTING_NAME);
  }
}
