{
  mGSMPhone.dial("+13125551212");
  do {
    mTestHandler.getNextMessage();
  }
 while (mGSMPhone.getForegroundCall().getConnections().isEmpty());
  mRadioControl.shutdown();
  do {
    mTestHandler.getNextMessage();
    mGSMPhone.clearDisconnected();
  }
 while (!mGSMPhone.getForegroundCall().getConnections().isEmpty());
}
