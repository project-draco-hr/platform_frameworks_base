{
  return String.valueOf(hashCode()).substring(0,3) + ".." + ":["+ job.getService()+ ",jId="+ job.getId()+ ",u"+ getUserId()+ ",R=("+ formatRunTime(earliestRunTimeElapsedMillis,NO_EARLIEST_RUNTIME)+ ","+ formatRunTime(latestRunTimeElapsedMillis,NO_LATEST_RUNTIME)+ ")"+ ",N="+ job.getNetworkType()+ ",C="+ job.isRequireCharging()+ ",I="+ job.isRequireDeviceIdle()+ ",F="+ numFailures+ ",P="+ job.isPersisted()+ ",ANI="+ appNotIdleConstraintSatisfied.get()+ (isReady() ? "(READY)" : "")+ "]";
}
