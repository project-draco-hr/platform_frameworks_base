{
  return String.valueOf(hashCode()).substring(0,3) + ".." + ":["+ job.getService()+ ",jId="+ job.getId()+ ",u"+ getUserId()+ ",R=("+ formatRunTime(earliestRunTimeElapsedMillis,NO_EARLIEST_RUNTIME)+ ","+ formatRunTime(latestRunTimeElapsedMillis,NO_LATEST_RUNTIME)+ ")"+ ",N="+ job.getNetworkType()+ ",C="+ job.isRequireCharging()+ ",I="+ job.isRequireDeviceIdle()+ ",U="+ (job.getTriggerContentUris() != null)+ ",F="+ numFailures+ ",P="+ job.isPersisted()+ ",ANI="+ ((satisfiedConstraints & CONSTRAINT_APP_NOT_IDLE) != 0)+ ",DND="+ ((satisfiedConstraints & CONSTRAINT_DEVICE_NOT_DOZING) != 0)+ (isReady() ? "(READY)" : "")+ "]";
}
