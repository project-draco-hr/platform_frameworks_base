{
  return (!hasChargingConstraint() || chargingConstraintSatisfied.get()) && (!hasTimingDelayConstraint() || timeDelayConstraintSatisfied.get()) && (!hasConnectivityConstraint() || connectivityConstraintSatisfied.get())&& (!hasUnmeteredConstraint() || unmeteredConstraintSatisfied.get())&& (!hasIdleConstraint() || idleConstraintSatisfied.get());
}
