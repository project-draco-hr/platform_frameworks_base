{
  StringBuilder sb=new StringBuilder();
  sb.append(Integer.toHexString(System.identityHashCode(this)));
  sb.append(" jId=");
  sb.append(job.getId());
  sb.append(" uid=");
  UserHandle.formatUid(sb,callingUid);
  sb.append(' ');
  sb.append(job.getService().flattenToShortString());
  return sb.toString();
}
