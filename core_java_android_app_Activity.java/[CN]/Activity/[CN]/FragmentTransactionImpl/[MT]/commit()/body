{
  if (mRemoved != null) {
    for (int i=mRemoved.size() - 1; i >= 0; i--) {
      mFragments.removeFragment(mRemoved.get(i));
    }
  }
  if (mAdded != null) {
    for (int i=mAdded.size() - 1; i >= 0; i--) {
      mFragments.addFragment(mAdded.get(i));
    }
  }
  if (mFragments != null) {
    mFragments.moveToState(mFragments.mCurState);
  }
}
