{
  final View view=mDecor;
  if (view == null) {
    return false;
  }
  final int vw=view.getWidth();
  final int vh=view.getHeight();
  final int dw=outBitmap.getWidth();
  final int dh=outBitmap.getHeight();
  canvas.save();
  canvas.scale(((float)dw) / vw,((float)dh) / vh);
  view.draw(canvas);
  canvas.restore();
  return true;
}
