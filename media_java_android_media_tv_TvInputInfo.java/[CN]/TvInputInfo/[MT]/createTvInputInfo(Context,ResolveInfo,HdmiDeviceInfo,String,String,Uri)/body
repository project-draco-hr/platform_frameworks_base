{
  boolean isConnectedToHdmiSwitch=(hdmiDeviceInfo.getPhysicalAddress() & 0x0FFF) != 0;
  TvInputInfo input=createTvInputInfo(context,service,generateInputIdForHdmiDevice(new ComponentName(service.serviceInfo.packageName,service.serviceInfo.name),hdmiDeviceInfo),parentId,TYPE_HDMI,label,iconUri,isConnectedToHdmiSwitch);
  input.mHdmiDeviceInfo=hdmiDeviceInfo;
  return input;
}
