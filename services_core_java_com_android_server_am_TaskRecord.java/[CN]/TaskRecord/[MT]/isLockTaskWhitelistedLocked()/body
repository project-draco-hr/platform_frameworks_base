{
  if (mCallingPackage == null) {
    return false;
  }
  String[] packages=mService.mLockTaskPackages.get(userId);
  if (packages == null) {
    return false;
  }
  for (int i=packages.length - 1; i >= 0; --i) {
    if (mCallingPackage.equals(packages[i])) {
      return true;
    }
  }
  return false;
}
