{
  if (event == mLastEvent && nestingLevel < mLastNestingLevel && eventType == mLastEventType) {
    return false;
  }
  if (nestingLevel > 0) {
    mLastEvent=event;
    mLastEventType=eventType;
    mLastNestingLevel=nestingLevel;
  }
 else {
    mLastEvent=null;
    mLastEventType=null;
    mLastNestingLevel=0;
  }
  mCurrentEvent=event;
  mCurrentEventType=eventType;
  return true;
}
