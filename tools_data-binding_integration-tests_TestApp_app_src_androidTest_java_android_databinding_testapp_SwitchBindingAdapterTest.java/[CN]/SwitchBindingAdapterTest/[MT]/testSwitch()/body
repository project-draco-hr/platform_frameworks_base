{
  if (Build.VERSION.SDK_INT >= Build.VERSION_CODES.JELLY_BEAN) {
    assertEquals(mBindingObject.getThumb(),((ColorDrawable)mView.getThumbDrawable()).getColor());
    assertEquals(mBindingObject.getTrack(),((ColorDrawable)mView.getTrackDrawable()).getColor());
    changeValues();
    assertEquals(mBindingObject.getThumb(),((ColorDrawable)mView.getThumbDrawable()).getColor());
    assertEquals(mBindingObject.getTrack(),((ColorDrawable)mView.getTrackDrawable()).getColor());
  }
}
