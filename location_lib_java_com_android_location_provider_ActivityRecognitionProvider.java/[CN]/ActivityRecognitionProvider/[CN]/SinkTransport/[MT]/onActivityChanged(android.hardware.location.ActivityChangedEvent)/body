{
  Collection<Sink> sinks;
synchronized (mSinkSet) {
    if (mSinkSet.isEmpty()) {
      return;
    }
    sinks=new ArrayList<Sink>(mSinkSet);
  }
  ArrayList<ActivityRecognitionEvent> gmsEvents=new ArrayList<ActivityRecognitionEvent>();
  for (  android.hardware.location.ActivityRecognitionEvent event : activityChangedEvent.getActivityRecognitionEvents()) {
    ActivityRecognitionEvent gmsEvent=new ActivityRecognitionEvent(event.getActivity(),event.getEventType(),event.getTimestampNs());
    gmsEvents.add(gmsEvent);
  }
  ActivityChangedEvent gmsEvent=new ActivityChangedEvent(gmsEvents);
  for (  Sink sink : sinks) {
    sink.onActivityChanged(gmsEvent);
  }
}
