{
  SmsMessage sms;
  String s="03029111000c9194981492631000f269206190022000a053e4534a05358bd3" + "69f05804259da0219418a40641536a110a0aea408080604028180e888462c1" + "50341c0f484432a1542c174c46b3e1743c9f9068442a994ea8946ac56ab95e"+ "b0986c46abd96eb89c6ec7ebf97ec0a070482c1a8fc8a472c96c3a9fd0a874"+ "4aad5aafd8ac76cbed7abfe0b0784c2e9bcfe8b47acd6ebbdff0b87c4eafdb"+ "eff8bc7ecfeffbffffffffffffffffffffffffffff";
  byte[] data=IccUtils.hexStringToBytes(s);
  sms=SmsMessage.createFromEfRecord(1,data);
  assertNotNull(sms.getMessageBody());
}
