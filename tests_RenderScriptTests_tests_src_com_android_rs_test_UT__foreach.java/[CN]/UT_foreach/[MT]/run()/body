{
  RenderScript pRS=RenderScript.create(mCtx);
  ScriptC_foreach s=new ScriptC_foreach(pRS);
  pRS.setMessageHandler(mRsMessage);
  initializeGlobals(pRS,s);
  s.forEach_root(A);
  s.invoke_verify_root();
  s.forEach_foo(A,A);
  s.invoke_verify_foo();
  s.invoke_foreach_test();
  pRS.finish();
  waitForMessage();
  pRS.destroy();
}
