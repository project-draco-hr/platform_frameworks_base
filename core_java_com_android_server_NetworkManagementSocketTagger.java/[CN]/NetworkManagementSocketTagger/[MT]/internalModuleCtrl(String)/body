{
  final FileOutputStream procOut;
  try {
    procOut=new FileOutputStream("/proc/net/xt_qtaguid/ctrl");
  }
 catch (  FileNotFoundException e) {
    if (LOGI) {
      System.logI("Can't talk to kernel module:" + e);
    }
    return;
  }
  try {
    procOut.write(cmd.getBytes(Charsets.US_ASCII));
  }
  finally {
    procOut.close();
  }
}
