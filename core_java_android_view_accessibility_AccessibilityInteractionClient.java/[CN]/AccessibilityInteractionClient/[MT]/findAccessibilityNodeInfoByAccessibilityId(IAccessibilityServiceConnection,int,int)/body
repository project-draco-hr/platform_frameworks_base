{
  try {
    final int interactionId=mInteractionIdCounter.getAndIncrement();
    final float windowScale=connection.findAccessibilityNodeInfoByAccessibilityId(accessibilityWindowId,accessibilityViewId,interactionId,this,Thread.currentThread().getId());
    if (windowScale > 0) {
      handleSameThreadMessageIfNeeded();
      AccessibilityNodeInfo info=getFindAccessibilityNodeInfoResultAndClear(interactionId);
      finalizeAccessibilityNodeInfo(info,connection,windowScale);
      return info;
    }
  }
 catch (  RemoteException re) {
  }
  return null;
}
