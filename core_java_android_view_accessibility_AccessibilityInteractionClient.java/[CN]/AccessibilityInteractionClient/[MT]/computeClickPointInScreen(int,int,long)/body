{
  try {
    IAccessibilityServiceConnection connection=getConnection(connectionId);
    if (connection != null) {
      final int interactionId=mInteractionIdCounter.getAndIncrement();
      final boolean success=connection.computeClickPointInScreen(accessibilityWindowId,accessibilityNodeId,interactionId,this,Thread.currentThread().getId());
      if (success) {
        return getComputeClickPointInScreenResultAndClear(interactionId);
      }
    }
 else {
      if (DEBUG) {
        Log.w(LOG_TAG,"No connection for connection id: " + connectionId);
      }
    }
  }
 catch (  RemoteException re) {
    Log.w(LOG_TAG,"Error while calling remote computeClickPointInScreen",re);
  }
  return null;
}
