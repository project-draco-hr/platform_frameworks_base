{
  try {
    final int interactionId=mInteractionIdCounter.getAndIncrement();
    final boolean success=connection.performAccessibilityAction(accessibilityWindowId,accessibilityNodeId,action,interactionId,this,Thread.currentThread().getId());
    if (success) {
      return getPerformAccessibilityActionResult(interactionId);
    }
  }
 catch (  RemoteException re) {
  }
  return false;
}
