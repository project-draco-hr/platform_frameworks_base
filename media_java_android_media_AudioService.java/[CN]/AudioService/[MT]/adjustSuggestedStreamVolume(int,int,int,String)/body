{
  if (DEBUG_VOL)   Log.d(TAG,"adjustSuggestedStreamVolume() stream=" + suggestedStreamType + ", flags="+ flags);
  int streamType;
  if (mVolumeControlStream != -1) {
    streamType=mVolumeControlStream;
  }
 else {
    streamType=getActiveStreamType(suggestedStreamType);
  }
  if ((streamType != STREAM_REMOTE_MUSIC) && (flags & AudioManager.FLAG_PLAY_SOUND) != 0 && (mStreamVolumeAlias[streamType] != AudioSystem.STREAM_RING)) {
    flags&=~AudioManager.FLAG_PLAY_SOUND;
  }
  if (streamType == STREAM_REMOTE_MUSIC) {
  }
 else {
    adjustStreamVolume(streamType,direction,flags,callingPackage);
  }
}
