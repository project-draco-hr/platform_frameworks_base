{
  if (mRcDisplay != null) {
    IBinder b=mRcDisplay.asBinder();
    mRcDisplayDeathHandler=new RcDisplayDeathHandler(b);
    try {
      b.linkToDeath(mRcDisplayDeathHandler,0);
    }
 catch (    RemoteException e) {
      Log.w(TAG,"registerRemoteControlDisplay() has a dead client " + b);
      mRcDisplay=null;
    }
  }
}
