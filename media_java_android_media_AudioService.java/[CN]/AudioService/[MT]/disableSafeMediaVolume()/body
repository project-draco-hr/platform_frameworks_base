{
synchronized (mSafeMediaVolumeState) {
    setSafeMediaVolumeEnabled(false);
    if (mPendingVolumeCommand != null) {
      onSetStreamVolume(mPendingVolumeCommand.mStreamType,mPendingVolumeCommand.mIndex,mPendingVolumeCommand.mFlags,mPendingVolumeCommand.mDevice);
      mPendingVolumeCommand=null;
    }
  }
}
