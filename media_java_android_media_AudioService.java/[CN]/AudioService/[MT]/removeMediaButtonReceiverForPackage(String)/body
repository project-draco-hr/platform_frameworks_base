{
synchronized (mRCStack) {
    if (mRCStack.empty()) {
      return;
    }
 else {
      RemoteControlStackEntry oldTop=mRCStack.peek();
      Iterator<RemoteControlStackEntry> stackIterator=mRCStack.iterator();
      while (stackIterator.hasNext()) {
        RemoteControlStackEntry rcse=(RemoteControlStackEntry)stackIterator.next();
        if (packageName.equals(rcse.mMediaIntent.getCreatorPackage())) {
          stackIterator.remove();
          rcse.unlinkToRcClientDeath();
        }
      }
      if (mRCStack.empty()) {
        mAudioHandler.sendMessage(mAudioHandler.obtainMessage(MSG_PERSIST_MEDIABUTTONRECEIVER,0,0,null));
      }
 else       if (oldTop != mRCStack.peek()) {
        RemoteControlStackEntry rcse=mRCStack.peek();
        if (rcse.mReceiverComponent != null) {
          mAudioHandler.sendMessage(mAudioHandler.obtainMessage(MSG_PERSIST_MEDIABUTTONRECEIVER,0,0,rcse.mReceiverComponent));
        }
      }
    }
  }
}
