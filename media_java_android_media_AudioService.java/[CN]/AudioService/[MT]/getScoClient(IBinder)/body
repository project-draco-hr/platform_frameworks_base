{
synchronized (mScoClients) {
    ScoClient client;
    int size=mScoClients.size();
    for (int i=0; i < size; i++) {
      client=mScoClients.get(i);
      if (client.getBinder() == cb)       return client;
    }
    client=new ScoClient(cb);
    mScoClients.add(client);
    return client;
  }
}
