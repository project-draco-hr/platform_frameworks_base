{
synchronized (mSafeMediaVolumeEnabled) {
    if (mSafeMediaVolumeEnabled && (mStreamVolumeAlias[streamType] == AudioSystem.STREAM_MUSIC) && ((device & mSafeMediaVolumeDevices) != 0)&& (index > mSafeMediaVolumeIndex)) {
      mVolumePanel.postDisplaySafeVolumeWarning();
      return false;
    }
    return true;
  }
}
