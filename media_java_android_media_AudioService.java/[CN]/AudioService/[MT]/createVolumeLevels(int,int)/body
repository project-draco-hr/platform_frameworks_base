{
  double curve=1.0f;
  int[] volumes=new int[numlevels + offset];
  for (int i=0; i < offset; i++) {
    volumes[i]=0;
  }
  double val=0;
  double max=Math.pow(numlevels - 1,curve);
  for (int i=0; i < numlevels; i++) {
    val=Math.pow(i,curve) / max;
    volumes[offset + i]=(int)(val * 100.0f);
  }
  return volumes;
}
