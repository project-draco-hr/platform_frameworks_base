{
  mContext.enforceCallingOrSelfPermission(android.Manifest.permission.DUMP,TAG);
  dumpFocusStack(pw);
  dumpRCStack(pw);
  dumpRCCStack(pw);
  dumpRCDList(pw);
  dumpStreamStates(pw);
  dumpRingerMode(pw);
  pw.println("\nAudio routes:");
  pw.print("  mMainType=0x");
  pw.println(Integer.toHexString(mCurAudioRoutes.mMainType));
  pw.print("  mBluetoothName=");
  pw.println(mCurAudioRoutes.mBluetoothName);
}
