{
  if (USE_SESSIONS) {
    if (DEBUG_SESSIONS) {
      int pid=getCallingPid();
      Log.w(TAG,"Call to dispatchMediaKeyEvent from " + pid);
    }
    MediaSessionLegacyHelper.getHelper(mContext).sendMediaButtonEvent(keyEvent,false);
  }
 else {
    mMediaFocusControl.dispatchMediaKeyEvent(keyEvent);
  }
}
