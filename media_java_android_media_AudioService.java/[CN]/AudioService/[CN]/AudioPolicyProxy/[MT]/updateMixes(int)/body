{
  for (  AudioMix mix : mMixes) {
    if (DEBUG_AP) {
      Log.v(TAG,"AudioPolicyProxy connect mix state=" + connectionState + " addr="+ mix.getRegistration());
    }
    AudioSystem.setDeviceConnectionState(AudioSystem.DEVICE_IN_REMOTE_SUBMIX,connectionState,mix.getRegistration());
    AudioSystem.setDeviceConnectionState(AudioSystem.DEVICE_OUT_REMOTE_SUBMIX,connectionState,mix.getRegistration());
  }
}
