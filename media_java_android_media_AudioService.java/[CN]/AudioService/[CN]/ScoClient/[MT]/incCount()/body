{
synchronized (mScoClients) {
    requestScoState(BluetoothHeadset.AUDIO_STATE_CONNECTED);
    if (mStartcount == 0) {
      try {
        mCb.linkToDeath(this,0);
      }
 catch (      RemoteException e) {
        Log.w(TAG,"ScoClient  incCount() could not link to " + mCb + " binder death");
      }
    }
    mStartcount++;
  }
}
