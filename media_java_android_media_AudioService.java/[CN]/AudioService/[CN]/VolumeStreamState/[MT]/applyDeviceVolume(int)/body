{
  int index;
  if (isMuted()) {
    index=0;
  }
 else   if (mStreamVolumeAlias[mStreamType] == AudioSystem.STREAM_MUSIC && (device & AudioSystem.DEVICE_OUT_ALL_A2DP) != 0 && mAvrcpAbsVolSupported) {
    index=(mIndexMax + 5) / 10;
  }
 else {
    index=(getIndex(device) + 5) / 10;
  }
  AudioSystem.setStreamVolumeIndex(mStreamType,index,device);
}
