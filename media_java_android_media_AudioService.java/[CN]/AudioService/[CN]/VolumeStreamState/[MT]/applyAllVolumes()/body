{
  AudioSystem.setStreamVolumeIndex(mStreamType,(getIndex(AudioSystem.DEVICE_OUT_DEFAULT,false) + 5) / 10,AudioSystem.DEVICE_OUT_DEFAULT);
  Set set=mIndex.entrySet();
  Iterator i=set.iterator();
  while (i.hasNext()) {
    Map.Entry entry=(Map.Entry)i.next();
    int device=((Integer)entry.getKey()).intValue();
    if (device != AudioSystem.DEVICE_OUT_DEFAULT) {
      AudioSystem.setStreamVolumeIndex(mStreamType,((Integer)entry.getValue() + 5) / 10,device);
    }
  }
}
