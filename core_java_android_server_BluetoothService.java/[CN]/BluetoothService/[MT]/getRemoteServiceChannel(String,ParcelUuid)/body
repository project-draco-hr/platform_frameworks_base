{
  mContext.enforceCallingOrSelfPermission(BLUETOOTH_PERM,"Need BLUETOOTH permission");
  if (!BluetoothAdapter.checkBluetoothAddress(address)) {
    return BluetoothDevice.ERROR;
  }
  if (mDeviceProperties.isEmpty()) {
    if (!updateRemoteDevicePropertiesCache(address))     return -1;
  }
  Map<ParcelUuid,Integer> value=mDeviceServiceChannelCache.get(address);
  if (value != null && value.containsKey(uuid))   return value.get(uuid);
  return -1;
}
