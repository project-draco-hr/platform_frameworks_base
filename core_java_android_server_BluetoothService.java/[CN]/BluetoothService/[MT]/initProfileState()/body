{
  String[] bonds=null;
  String val=getProperty("Devices",false);
  if (val != null) {
    bonds=val.split(",");
  }
  if (bonds == null) {
    return;
  }
  for (  String path : bonds) {
    String address=getAddressFromObjectPath(path);
    BluetoothDeviceProfileState state=addProfileState(address,false);
  }
}
