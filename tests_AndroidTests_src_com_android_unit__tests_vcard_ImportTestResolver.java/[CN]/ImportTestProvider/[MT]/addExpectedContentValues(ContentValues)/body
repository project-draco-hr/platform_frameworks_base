{
  final String mimeType=expectedContentValues.getAsString(Data.MIMETYPE);
  if (!sKnownMimeTypeSet.contains(mimeType)) {
    mTestCase.fail(String.format("Unknow MimeType %s in the test code. Test code should be broken.",mimeType));
  }
  final Collection<ContentValues> contentValuesCollection=mMimeTypeToExpectedContentValues.get(mimeType);
  contentValuesCollection.add(expectedContentValues);
}
