{
  super(parserMode);
  if (detector != null) {
    final int type=detector.getType();
    if (type == VCardSourceDetector.TYPE_FOMA) {
      mNestCount=1;
    }
 else     if (type == VCardSourceDetector.TYPE_JAPANESE_MOBILE_PHONE) {
      mIgnoreInvalidAgentLine=true;
    }
  }
  if (parserMode == PARSER_MODE_SCAN) {
    mIgnoreInvalidAgentLine=true;
  }
}
