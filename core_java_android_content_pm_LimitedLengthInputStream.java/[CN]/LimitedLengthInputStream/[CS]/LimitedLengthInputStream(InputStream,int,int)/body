{
  super(in);
  if (in == null) {
    throw new IOException("in == null");
  }
  if (offset < 0) {
    throw new IOException("offset == " + offset);
  }
  if (length < 0) {
    throw new IOException("length must be non-negative; is " + length);
  }
  mEnd=offset + length;
  skip(offset);
  mOffset=offset;
}
