{
  Context context=mPhone.getContext();
  context.enforceCallingPermission("android.permission.SEND_SMS","Sending SMS message");
  if (DBG)   log("sendMultipartText");
  mDispatcher.sendMultipartText(destinationAddress,scAddress,(ArrayList<String>)parts,(ArrayList<PendingIntent>)sentIntents,(ArrayList<PendingIntent>)deliveryIntents);
}
