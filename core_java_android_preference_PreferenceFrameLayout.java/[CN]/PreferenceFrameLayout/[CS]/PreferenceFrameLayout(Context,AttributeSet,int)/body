{
  super(context,attrs,defStyle);
  TypedArray a=context.obtainStyledAttributes(attrs,com.android.internal.R.styleable.PreferenceFrameLayout,defStyle,0);
  mTopPadding=(int)a.getDimension(com.android.internal.R.styleable.PreferenceFrameLayout_topPadding,DEFAULT_TOP_PADDING);
  mBottomPadding=(int)a.getDimension(com.android.internal.R.styleable.PreferenceFrameLayout_bottomPadding,DEFAULT_BOTTOM_PADDING);
  a.recycle();
}
