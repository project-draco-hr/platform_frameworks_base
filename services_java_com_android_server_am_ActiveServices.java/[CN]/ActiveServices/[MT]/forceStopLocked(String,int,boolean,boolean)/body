{
  boolean didSomething=false;
  ArrayList<ServiceRecord> services=new ArrayList<ServiceRecord>();
  if (userId == UserHandle.USER_ALL) {
    for (int i=0; i < mServiceMap.mServicesByNamePerUser.size(); i++) {
      didSomething|=collectForceStopServicesLocked(name,userId,evenPersistent,doit,mServiceMap.mServicesByNamePerUser.valueAt(i),services);
      if (!doit && didSomething) {
        return true;
      }
    }
  }
 else {
    didSomething=collectForceStopServicesLocked(name,userId,evenPersistent,doit,mServiceMap.mServicesByNamePerUser.get(userId),services);
  }
  int N=services.size();
  for (int i=0; i < N; i++) {
    bringDownServiceLocked(services.get(i),true);
  }
  return didSomething;
}
