{
  ServiceLookupResult r=findServiceLocked(service,resolvedType,UserId.getCallingUserId());
  IBinder ret=null;
  if (r != null) {
    if (r.record == null) {
      throw new SecurityException("Permission Denial: Accessing service " + r.record.name + " from pid="+ Binder.getCallingPid()+ ", uid="+ Binder.getCallingUid()+ " requires "+ r.permission);
    }
    IntentBindRecord ib=r.record.bindings.get(r.record.intent);
    if (ib != null) {
      ret=ib.binder;
    }
  }
  return ret;
}
