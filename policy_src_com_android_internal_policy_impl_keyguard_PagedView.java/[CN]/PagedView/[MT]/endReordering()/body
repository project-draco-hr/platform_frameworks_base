{
  if (!mReorderingStarted)   return;
  mReorderingStarted=false;
  Runnable onCompleteRunnable=new Runnable(){
    @Override public void run(){
      onEndReordering();
    }
  }
;
  zoomIn(onCompleteRunnable);
  if (!mIsFlingingToDelete) {
    snapToDestination();
    animateChildrenToOriginalPosition();
  }
}
