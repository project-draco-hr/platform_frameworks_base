{
  for (  ActivityManager.RecentTaskInfo task : recentTasks) {
    if (getChildCount() >= MAX_RECENTS) {
      return;
    }
    ComponentName component=task.baseIntent.getComponent();
    if (component == null) {
      continue;
    }
    if (mCurrentTasks.contains(component)) {
      continue;
    }
    addRecentAppButton(task);
  }
}
