{
  StringBuffer sb=new StringBuffer();
  String none="<none>";
  sb.append("SSID: ").append(mSSID == null ? none : mSSID).append(", BSSID: ").append(mBSSID == null ? none : mBSSID).append(", MAC: ").append(mMacAddress == null ? none : mMacAddress).append(", Supplicant state: ").append(mSupplicantState == null ? none : mSupplicantState).append(", RSSI: ").append(mRssi).append(", Link speed: ").append(mLinkSpeed).append(", Net ID: ").append(mNetworkId);
  return sb.toString();
}
