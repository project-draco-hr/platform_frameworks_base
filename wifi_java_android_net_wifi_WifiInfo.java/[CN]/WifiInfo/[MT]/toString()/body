{
  StringBuffer sb=new StringBuffer();
  String none="<none>";
  sb.append("SSID: ").append(mWifiSsid == null ? WifiSsid.NONE : mWifiSsid).append(", BSSID: ").append(mBSSID == null ? none : mBSSID).append(", MAC: ").append(mMacAddress == null ? none : mMacAddress).append(", Supplicant state: ").append(mSupplicantState == null ? none : mSupplicantState).append(", RSSI: ").append(mRssi).append(", Link speed: ").append(mLinkSpeed).append(LINK_SPEED_UNITS).append(", Frequency: ").append(mFrequency).append(FREQUENCY_UNITS).append(", Net ID: ").append(mNetworkId).append(", Metered hint: ").append(mMeteredHint);
  return sb.toString();
}
