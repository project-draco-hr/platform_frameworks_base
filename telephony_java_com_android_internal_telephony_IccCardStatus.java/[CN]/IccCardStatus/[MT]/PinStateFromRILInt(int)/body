{
  PinState newState;
switch (state) {
case 0:
    newState=PinState.PINSTATE_UNKNOWN;
  break;
case 1:
newState=PinState.PINSTATE_ENABLED_NOT_VERIFIED;
break;
case 2:
newState=PinState.PINSTATE_ENABLED_VERIFIED;
break;
case 3:
newState=PinState.PINSTATE_DISABLED;
break;
case 4:
newState=PinState.PINSTATE_ENABLED_BLOCKED;
break;
case 5:
newState=PinState.PINSTATE_ENABLED_PERM_BLOCKED;
break;
default :
throw new RuntimeException("Unrecognized RIL_PinState: " + state);
}
return newState;
}
