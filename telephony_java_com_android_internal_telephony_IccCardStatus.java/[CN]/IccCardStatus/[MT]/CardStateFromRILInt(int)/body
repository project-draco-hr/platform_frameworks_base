{
  CardState newState;
switch (state) {
case 0:
    newState=CardState.CARDSTATE_ABSENT;
  break;
case 1:
newState=CardState.CARDSTATE_PRESENT;
break;
case 2:
newState=CardState.CARDSTATE_ERROR;
break;
default :
throw new RuntimeException("Unrecognized RIL_CardState: " + state);
}
return newState;
}
