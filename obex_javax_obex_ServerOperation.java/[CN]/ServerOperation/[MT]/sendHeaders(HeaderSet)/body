{
  ensureOpen();
  if (headers == null) {
    throw new IOException("Headers may not be null");
  }
  int[] headerList=headers.getHeaderList();
  if (headerList != null) {
    for (int i=0; i < headerList.length; i++) {
      replyHeader.setHeader(headerList[i],headers.getHeader(headerList[i]));
    }
  }
}
