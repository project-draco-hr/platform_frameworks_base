{
  if (!isGet) {
    if (!finalBitSet) {
      if (sendEmpty) {
        sendReply(ObexHelper.OBEX_HTTP_CONTINUE);
        return true;
      }
 else {
        if ((responseSize > 3) || (privateOutput.size() > 0)) {
          sendReply(ObexHelper.OBEX_HTTP_CONTINUE);
          return true;
        }
 else {
          return false;
        }
      }
    }
 else {
      return false;
    }
  }
 else {
    sendReply(ObexHelper.OBEX_HTTP_CONTINUE);
    return true;
  }
}
