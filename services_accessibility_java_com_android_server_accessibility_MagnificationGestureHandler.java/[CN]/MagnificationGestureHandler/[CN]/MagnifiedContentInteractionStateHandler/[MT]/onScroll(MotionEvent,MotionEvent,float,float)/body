{
  if (mCurrentState != STATE_MAGNIFIED_INTERACTION) {
    return true;
  }
  if (DEBUG_PANNING) {
    Slog.i(LOG_TAG,"Panned content by scrollX: " + distanceX + " scrollY: "+ distanceY);
  }
  mMagnificationController.offsetMagnifiedRegionCenter(distanceX,distanceY,AccessibilityManagerService.MAGNIFICATION_GESTURE_HANDLER_ID);
  return true;
}
