{
  super(loadListener);
  mIsAsset=asset;
  mContext=context;
  int index=url.indexOf('?');
  if (mIsAsset) {
    mPath=index > 0 ? URLUtil.stripAnchor(url.substring(URLUtil.ASSET_BASE.length(),index)) : URLUtil.stripAnchor(url.substring(URLUtil.ASSET_BASE.length()));
  }
 else {
    mPath=index > 0 ? URLUtil.stripAnchor(url.substring(URLUtil.FILE_BASE.length(),index)) : URLUtil.stripAnchor(url.substring(URLUtil.FILE_BASE.length()));
  }
}
