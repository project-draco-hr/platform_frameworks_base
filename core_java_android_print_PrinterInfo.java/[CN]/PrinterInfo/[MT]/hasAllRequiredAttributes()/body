{
  return (mMediaSizes != null && !mMediaSizes.isEmpty() && mResolutions != null && !mResolutions.isEmpty() && mColorModes != 0 || mOrientations != 0 && mDefaults[PROPERTY_MEDIA_SIZE] != DEFAULT_UNDEFINED && mDefaults[PROPERTY_RESOLUTION] != DEFAULT_UNDEFINED && mDefaults[PROPERTY_COLOR_MODE] != DEFAULT_UNDEFINED && mDefaults[PROPERTY_ORIENTATION] != DEFAULT_UNDEFINED);
}
