{
  if (margins.getLeftMils() > defaultMargins.getLeftMils() || margins.getTopMils() > defaultMargins.getTopMils() || margins.getRightMils() < defaultMargins.getRightMils() || margins.getBottomMils() < defaultMargins.getBottomMils()) {
    throw new IllegalArgumentException("Default margins" + " cannot be outside of the min margins.");
  }
  mPrinterInfo.mMinMargins=margins;
  mPrinterInfo.mDefaultMargins=defaultMargins;
  return this;
}
