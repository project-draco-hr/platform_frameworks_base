{
  if (mSelfChange || mStopFormatting) {
    return;
  }
  if (mState == STATE_OTHER) {
    if (count > 0 && !hasSeparator(s,start,count)) {
      mState=STATE_MODIFY_DIGITS;
    }
  }
  if (mState == STATE_APPEND && count > 0 && hasSeparator(s,start,count)) {
    stopFormatting();
  }
 else   if (mState == STATE_OTHER) {
    stopFormatting();
  }
}
