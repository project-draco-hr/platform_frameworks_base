{
  super(c);
  mSweepGradient=new SweepGradient(0.0f,0.0f,0xff000000,0xffffffff);
  mRadialGradient=new RadialGradient(0.0f,0.0f,100.0f,0xff000000,0xffffffff,Shader.TileMode.MIRROR);
