{
  final ReentrantLock lock=this.mLock;
  lock.lock();
  try {
    CaptureHolder h=mJpegCaptureQueue.poll();
    if (h == null) {
      Log.w(TAG,"jpegCaptured called with no jpeg request on queue!");
      return null;
    }
    h.setJpegTimestamp(timestamp);
    return h.mRequest;
  }
  finally {
    lock.unlock();
  }
}
