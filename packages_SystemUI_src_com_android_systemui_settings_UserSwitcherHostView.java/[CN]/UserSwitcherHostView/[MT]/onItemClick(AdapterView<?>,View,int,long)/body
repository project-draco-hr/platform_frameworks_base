{
  if (position == mUserInfo.size()) {
    postDelayed(new Runnable(){
      public void run(){
        switchToGuestUser();
      }
    }
,100);
  }
 else {
    final int userId=mAdapter.getItem(position).id;
    postDelayed(new Runnable(){
      public void run(){
        switchUser(userId);
      }
    }
,100);
  }
}
