{
  EnforcedAdmin admin=RestrictedLockUtils.checkIfRestrictionEnforced(mContext,UserManager.DISALLOW_ADD_USER,UserHandle.myUserId());
  if (admin != null) {
    record.isDisabledByAdmin=true;
    record.enforcedAdmin=admin;
  }
 else {
    record.isDisabledByAdmin=false;
    record.enforcedAdmin=null;
  }
}
