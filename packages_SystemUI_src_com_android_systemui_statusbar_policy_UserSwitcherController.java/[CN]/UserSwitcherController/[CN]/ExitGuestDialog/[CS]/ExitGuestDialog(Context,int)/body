{
  super(context);
  setTitle(R.string.guest_exit_guest_dialog_title);
  setMessage(context.getString(R.string.guest_exit_guest_dialog_message));
  setButton(DialogInterface.BUTTON_NEGATIVE,context.getString(android.R.string.no),this);
  setButton(DialogInterface.BUTTON_POSITIVE,context.getString(android.R.string.yes),this);
  setCanceledOnTouchOutside(false);
  mGuestId=guestId;
}
