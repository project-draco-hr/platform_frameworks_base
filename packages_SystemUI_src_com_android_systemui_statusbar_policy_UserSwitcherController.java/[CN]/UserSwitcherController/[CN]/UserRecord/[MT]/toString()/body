{
  StringBuilder sb=new StringBuilder();
  sb.append("UserRecord(");
  if (info != null) {
    sb.append("name=\"" + info.name + "\" id="+ info.id);
  }
 else {
    sb.append("<add guest placeholder>");
  }
  if (isGuest) {
    sb.append(" <isGuest>");
  }
  if (isCurrent) {
    sb.append(" <isCurrent>");
  }
  if (picture != null) {
    sb.append(" <hasPicture>");
  }
  sb.append(')');
  return sb.toString();
}
