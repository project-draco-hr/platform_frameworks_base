{
  if (Config.LOGD) {
    Log.d(TAG,"processEventNotify begins");
  }
  String text=null;
  List<TextAttribute> textAttrs=null;
  try {
    ComprehensionTlv ctlv=searchForTag(ComprehensionTlvTag.ALPHA_ID,ctlvs);
    if (ctlv != null) {
      text=retrieveAlphaId(ctlv);
    }
 else {
      throw new ResultException(ResultCode.REQUIRED_VALUES_MISSING);
    }
    ctlv=searchForTag(ComprehensionTlvTag.TEXT_ATTRIBUTE,ctlvs);
    if (ctlv != null) {
      textAttrs=retrieveTextAttribute(ctlv);
    }
  }
 catch (  ResultException e) {
    return;
  }
  Toast toast=Toast.makeText(mContext.getApplicationContext(),text,Toast.LENGTH_LONG);
  toast.setGravity(Gravity.BOTTOM,0,0);
  toast.show();
}
