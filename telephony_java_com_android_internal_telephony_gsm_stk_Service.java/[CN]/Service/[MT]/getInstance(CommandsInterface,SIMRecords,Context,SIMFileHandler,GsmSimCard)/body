{
  if (sInstance == null) {
    if (ci == null || sr == null || context == null || fh == null || sc == null) {
      return null;
    }
    HandlerThread thread=new HandlerThread("Stk Telephony service");
    thread.start();
    sInstance=new Service(ci,sr,context,fh,sc);
  }
  return sInstance;
}
