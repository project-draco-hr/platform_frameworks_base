{
  ByteArrayOutputStream buf=new ByteArrayOutputStream();
  int cause=isErrorTermination ? 1 : 0;
  int tag=0x80 | ComprehensionTlvTag.BROWSER_TERMINATION_CAUSE.value();
  buf.write(tag);
  buf.write(0x01);
  buf.write(cause);
  byte[] info=buf.toByteArray();
  eventDownload(UICC_EVENT_BROWSER_TERMINATION,DEV_ID_TERMINAL,DEV_ID_UICC,info,true);
}
