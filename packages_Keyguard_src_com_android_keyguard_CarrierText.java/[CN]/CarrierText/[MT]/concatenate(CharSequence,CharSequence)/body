{
  final boolean plmnValid=!TextUtils.isEmpty(plmn);
  final boolean spnValid=!TextUtils.isEmpty(spn);
  if (plmnValid && spnValid) {
    if (plmn.equals(spn)) {
      return plmn;
    }
 else {
      return new StringBuilder().append(plmn).append(mSeparator).append(spn).toString();
    }
  }
 else   if (plmnValid) {
    return plmn;
  }
 else   if (spnValid) {
    return spn;
  }
 else {
    return "";
  }
}
