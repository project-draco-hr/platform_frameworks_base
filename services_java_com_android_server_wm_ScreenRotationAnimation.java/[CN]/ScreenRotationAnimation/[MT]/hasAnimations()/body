{
  return (TWO_PHASE_ANIMATION && (mStartEnterAnimation != null || mStartExitAnimation != null || mFinishEnterAnimation != null || mFinishExitAnimation != null)) || (USE_CUSTOM_BLACK_FRAME && (mStartFrameAnimation != null || mRotateFrameAnimation != null || mFinishFrameAnimation != null)) || mRotateEnterAnimation != null || mRotateExitAnimation != null;
}
