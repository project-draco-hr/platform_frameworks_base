{
  LoaderInfo<D> info=mLoaders.get(id);
  if (info != null) {
    mInactiveLoaders.put(id,info);
  }
  info=new LoaderInfo<D>();
  info.args=args;
  mLoaders.put(id,info);
  Loader<D> loader=onCreateLoader(id,args);
  info.loader=loader;
  if (mStarted) {
    loader.registerListener(id,this);
    loader.startLoading();
  }
  return loader;
}
