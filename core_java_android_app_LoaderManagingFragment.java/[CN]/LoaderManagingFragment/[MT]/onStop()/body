{
  super.onStop();
  for (  HashMap.Entry<Integer,LoaderInfo<D>> entry : mLoaders.entrySet()) {
    LoaderInfo<D> info=entry.getValue();
    Loader<D> loader=info.loader;
    if (loader == null) {
      continue;
    }
    loader.unregisterListener(this);
    if (!getActivity().isChangingConfigurations()) {
      loader.stopLoading();
    }
  }
  mStarted=false;
}
