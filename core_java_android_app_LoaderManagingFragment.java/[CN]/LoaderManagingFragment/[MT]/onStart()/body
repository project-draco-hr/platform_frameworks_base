{
  super.onStart();
  for (  HashMap.Entry<Integer,LoaderInfo<D>> entry : mLoaders.entrySet()) {
    LoaderInfo<D> info=entry.getValue();
    Loader<D> loader=info.loader;
    int id=entry.getKey();
    if (loader == null) {
      loader=onCreateLoader(id,info.args);
      info.loader=loader;
    }
    loader.registerListener(id,this);
    loader.startLoading();
  }
  mStarted=true;
}
