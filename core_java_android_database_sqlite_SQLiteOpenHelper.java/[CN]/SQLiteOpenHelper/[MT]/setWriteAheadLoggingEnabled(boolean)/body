{
synchronized (this) {
    if (mEnableWriteAheadLogging != enabled) {
      if (mDatabase != null && mDatabase.isOpen() && !mDatabase.isReadOnly()) {
        if (enabled) {
          mDatabase.enableWriteAheadLogging();
        }
 else {
          mDatabase.disableWriteAheadLogging();
        }
      }
      mEnableWriteAheadLogging=enabled;
    }
  }
}
