{
  ContentResolver res=getContext().getContentResolver();
  String info=null;
  final boolean ownerInfoEnabled=mLockPatternUtils.isOwnerInfoEnabled(mLockPatternUtils.getCurrentUser());
  if (ownerInfoEnabled) {
    info=mLockPatternUtils.getOwnerInfo(mLockPatternUtils.getCurrentUser());
  }
  return info;
}
