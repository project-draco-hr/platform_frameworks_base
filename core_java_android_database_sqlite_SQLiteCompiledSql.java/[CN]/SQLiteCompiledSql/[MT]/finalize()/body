{
  try {
    if (nStatement == 0)     return;
    if ((nState & NSTATE_INUSE) == 0) {
    }
 else {
      int len=mSqlStmt.length();
      Log.w(TAG,"Releasing SQL statement in finalizer. " + "Could be due to close() not being called on the cursor or on the database. " + toString(),mStackTrace);
    }
    releaseFromDatabase();
  }
  finally {
    super.finalize();
  }
}
