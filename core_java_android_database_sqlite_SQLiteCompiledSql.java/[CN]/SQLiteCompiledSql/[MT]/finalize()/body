{
  try {
    if (nStatement == 0)     return;
    int len=mSqlStmt.length();
    Log.w(TAG,"Releasing statement in a finalizer. Please ensure " + "that you explicitly call close() on your cursor: " + mSqlStmt.substring(0,(len > 100) ? 100 : len),mStackTrace);
    releaseSqlStatement();
  }
  finally {
    super.finalize();
  }
}
