{
  try {
    if (nStatement == 0)     return;
    if (SQLiteDebug.DEBUG_ACTIVE_CURSOR_FINALIZATION) {
      Log.v(TAG,"** warning ** Finalized DbObj (id#" + nStatement + ")");
    }
    Log.w(TAG,"finalizer should never be called on sql: " + mSqlStmt,mStackTrace);
    releaseSqlStatement();
  }
  finally {
    super.finalize();
  }
}
