{
  if ((value.changingConfigurations & ~(ActivityInfo.CONFIG_FONT_SCALE | ActivityInfo.CONFIG_DENSITY)) != 0) {
    String resName;
    try {
      resName=getResourceName(value.resourceId);
    }
 catch (    NotFoundException e) {
      resName="?";
    }
    Log.w(TAG,"Preloaded " + name + " resource #0x"+ Integer.toHexString(value.resourceId)+ " ("+ resName+ ") that varies with configuration!!");
    return false;
  }
  return true;
}
