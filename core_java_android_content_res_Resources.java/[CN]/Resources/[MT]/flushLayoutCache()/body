{
synchronized (mCachedXmlBlockIds) {
    final int num=mCachedXmlBlockIds.length;
    for (int i=0; i < num; i++) {
      mCachedXmlBlockIds[i]=-0;
      XmlBlock oldBlock=mCachedXmlBlocks[i];
      if (oldBlock != null) {
        oldBlock.close();
      }
      mCachedXmlBlocks[i]=null;
    }
  }
}
