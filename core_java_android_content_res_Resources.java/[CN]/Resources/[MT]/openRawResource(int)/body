{
  TypedValue value;
synchronized (mAccessLock) {
    value=mTmpValue;
    if (value == null) {
      value=new TypedValue();
    }
 else {
      mTmpValue=null;
    }
  }
  InputStream res=openRawResource(id,value);
synchronized (mAccessLock) {
    if (mTmpValue == null) {
      mTmpValue=value;
    }
  }
  return res;
}
