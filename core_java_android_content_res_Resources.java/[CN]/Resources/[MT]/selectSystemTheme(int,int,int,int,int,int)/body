{
  if (curTheme != 0) {
    return curTheme;
  }
  if (targetSdkVersion < Build.VERSION_CODES.HONEYCOMB) {
    return orig;
  }
  if (targetSdkVersion < Build.VERSION_CODES.ICE_CREAM_SANDWICH) {
    return holo;
  }
  if (targetSdkVersion < Build.VERSION_CODES.N) {
    return dark;
  }
  return deviceDefault;
}
