{
synchronized (mAccessLock) {
    WeakReference<Drawable.ConstantState> wr=drawableCache.get(key);
    if (wr != null) {
      Drawable.ConstantState entry=wr.get();
      if (entry != null) {
        return entry.newDrawable(this);
      }
 else {
        drawableCache.delete(key);
      }
    }
  }
  return null;
}
