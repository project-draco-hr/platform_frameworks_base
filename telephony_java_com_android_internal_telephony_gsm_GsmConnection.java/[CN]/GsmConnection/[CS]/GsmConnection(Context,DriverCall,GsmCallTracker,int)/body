{
  createWakeLock(context);
  acquireWakeLock();
  owner=ct;
  h=new MyHandler(owner.getLooper());
  address=dc.number;
  isIncoming=dc.isMT;
  createTime=System.currentTimeMillis();
  numberPresentation=dc.numberPresentation;
  uusInfo=dc.uusInfo;
  this.index=index;
  parent=parentFromDCState(dc.state);
  parent.attach(this,dc);
}
