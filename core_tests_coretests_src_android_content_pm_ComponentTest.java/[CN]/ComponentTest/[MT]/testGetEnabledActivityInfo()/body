{
  mPackageManager.setComponentEnabledSetting(ENABLED_ACTIVITY_COMPONENTNAME,COMPONENT_ENABLED_STATE_DEFAULT,PackageManager.DONT_KILL_APP);
  ActivityInfo activityInfo=mPackageManager.getActivityInfo(ENABLED_ACTIVITY_COMPONENTNAME,0);
  assertNotNull(activityInfo);
  assertTrue(activityInfo.enabled);
}
