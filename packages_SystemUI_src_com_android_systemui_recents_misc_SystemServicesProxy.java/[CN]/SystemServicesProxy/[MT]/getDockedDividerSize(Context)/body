{
  Resources res=context.getResources();
  int dividerWindowWidth=res.getDimensionPixelSize(com.android.internal.R.dimen.docked_stack_divider_thickness);
  int dividerInsets=res.getDimensionPixelSize(com.android.internal.R.dimen.docked_stack_divider_insets);
  return dividerWindowWidth - 2 * dividerInsets;
}
