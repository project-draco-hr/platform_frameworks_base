{
  BitmapRegionDecoder decoder=null;
  InputStream stream=null;
  try {
    stream=new FileInputStream(pathName);
    decoder=newInstance(stream,isShareable);
  }
  finally {
    if (stream != null) {
      try {
        stream.close();
      }
 catch (      IOException e) {
      }
    }
  }
  return decoder;
}
