{
  if (c >= '0' && c <= '9') {
    return c - '0';
  }
 else   if (c == '*') {
    return 0xa;
  }
 else   if (c == '#') {
    return 0xb;
  }
 else   if (c == PAUSE) {
    return 0xc;
  }
 else   if (c == WILD) {
    return 0xd;
  }
 else   if (c == WAIT) {
    return 0xe;
  }
 else {
    throw new RuntimeException("invalid char for BCD " + c);
  }
}
