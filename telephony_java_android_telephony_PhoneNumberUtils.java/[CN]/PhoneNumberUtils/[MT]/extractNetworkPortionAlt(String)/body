{
  if (phoneNumber == null) {
    return null;
  }
  int len=phoneNumber.length();
  StringBuilder ret=new StringBuilder(len);
  boolean haveSeenPlus=false;
  for (int i=0; i < len; i++) {
    char c=phoneNumber.charAt(i);
    if (c == '+') {
      if (haveSeenPlus) {
        continue;
      }
      haveSeenPlus=true;
    }
    if (isDialable(c)) {
      ret.append(c);
    }
 else     if (isStartsPostDial(c)) {
      break;
    }
  }
  return ret.toString();
}
