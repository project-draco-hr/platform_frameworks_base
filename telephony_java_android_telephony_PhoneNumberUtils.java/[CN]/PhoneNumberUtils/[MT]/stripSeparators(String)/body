{
  if (phoneNumber == null) {
    return null;
  }
  int len=phoneNumber.length();
  StringBuilder ret=new StringBuilder(len);
  for (int i=0; i < len; i++) {
    char c=phoneNumber.charAt(i);
    int digit=Character.digit(c,10);
    if (digit != -1) {
      ret.append(digit);
    }
 else     if (isNonSeparator(c)) {
      ret.append(c);
    }
  }
  return ret.toString();
}
