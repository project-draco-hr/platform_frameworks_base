{
  Method[] methods=mClass.getDeclaredMethods();
  if (methods == null) {
    return new ModelMethod[0];
  }
 else {
    ModelMethod[] classMethods=new ModelMethod[methods.length];
    for (int i=0; i < methods.length; i++) {
      classMethods[i]=new JavaMethod(methods[i]);
    }
    return classMethods;
  }
}
