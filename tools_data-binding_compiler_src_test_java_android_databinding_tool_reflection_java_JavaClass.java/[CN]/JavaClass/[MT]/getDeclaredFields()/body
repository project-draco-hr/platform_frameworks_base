{
  Field[] fields=mClass.getDeclaredFields();
  ModelField[] modelFields;
  if (fields == null) {
    modelFields=new ModelField[0];
  }
 else {
    modelFields=new ModelField[fields.length];
    for (int i=0; i < fields.length; i++) {
      modelFields[i]=new JavaField(fields[i]);
    }
  }
  return modelFields;
}
