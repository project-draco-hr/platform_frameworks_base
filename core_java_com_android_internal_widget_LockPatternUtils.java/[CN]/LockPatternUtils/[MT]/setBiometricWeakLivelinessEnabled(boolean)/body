{
  long currentFlag=getLong(Settings.Secure.LOCK_BIOMETRIC_WEAK_FLAGS,0L);
  long newFlag;
  if (enabled) {
    newFlag=currentFlag | FLAG_BIOMETRIC_WEAK_LIVELINESS;
  }
 else {
    newFlag=currentFlag & ~FLAG_BIOMETRIC_WEAK_LIVELINESS;
  }
  setLong(Settings.Secure.LOCK_BIOMETRIC_WEAK_FLAGS,newFlag);
}
