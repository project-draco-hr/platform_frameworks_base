{
  int quality=(int)getLong(PASSWORD_TYPE_KEY,DevicePolicyManager.PASSWORD_QUALITY_UNSPECIFIED,userHandle);
  if (quality == DevicePolicyManager.PASSWORD_QUALITY_BIOMETRIC_WEAK) {
    quality=(int)getLong(PASSWORD_TYPE_ALTERNATE_KEY,DevicePolicyManager.PASSWORD_QUALITY_UNSPECIFIED,userHandle);
  }
  return quality;
}
