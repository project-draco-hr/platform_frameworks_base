{
  ImageInputStream markable=(ImageInputStream)source;
  try {
    markable.mark();
    byte[] signature=new byte[3];
    markable.seek(0);
    markable.read(signature,0,3);
    markable.reset();
    if ((signature[0] & 0xFF) == 0xFF && (signature[1] & 0xFF) == JPEGConsts.SOI && (signature[2] & 0xFF) == 0xFF) {
      return true;
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  return false;
}
