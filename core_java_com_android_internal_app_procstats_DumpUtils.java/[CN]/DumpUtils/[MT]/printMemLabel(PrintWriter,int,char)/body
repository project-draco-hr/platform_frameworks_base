{
switch (offset) {
case ADJ_NOTHING:
    pw.print("    ");
  if (sep != 0)   pw.print(' ');
break;
case ADJ_MEM_FACTOR_NORMAL:
pw.print("Norm");
if (sep != 0) pw.print(sep);
break;
case ADJ_MEM_FACTOR_MODERATE:
pw.print("Mod ");
if (sep != 0) pw.print(sep);
break;
case ADJ_MEM_FACTOR_LOW:
pw.print("Low ");
if (sep != 0) pw.print(sep);
break;
case ADJ_MEM_FACTOR_CRITICAL:
pw.print("Crit");
if (sep != 0) pw.print(sep);
break;
default :
pw.print("????");
if (sep != 0) pw.print(sep);
break;
}
}
