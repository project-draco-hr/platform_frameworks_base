{
  if (itemName.startsWith(pkgName)) {
    final int ITEMLEN=itemName.length();
    final int PKGLEN=pkgName.length();
    if (ITEMLEN == PKGLEN) {
      return "";
    }
 else     if (ITEMLEN >= PKGLEN) {
      if (itemName.charAt(PKGLEN) == '.') {
        return itemName.substring(PKGLEN);
      }
    }
  }
  return itemName;
}
