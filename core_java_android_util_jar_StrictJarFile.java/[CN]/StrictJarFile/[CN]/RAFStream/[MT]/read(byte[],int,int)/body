{
synchronized (sharedRaf) {
    final long length=endOffset - offset;
    if (byteCount > length) {
      byteCount=(int)length;
    }
    sharedRaf.seek(offset);
    int count=sharedRaf.read(buffer,byteOffset,byteCount);
    if (count > 0) {
      offset+=count;
      return count;
    }
 else {
      return -1;
    }
  }
}
