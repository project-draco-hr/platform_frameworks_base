{
  cmActivity.finish();
  log("tear down ConnectivityManagerTestActivity");
  wl.release();
  cmActivity.removeConfiguredNetworksAndDisableWifi();
  if (Settings.Global.getInt(getInstrumentation().getContext().getContentResolver(),Settings.Global.AIRPLANE_MODE_ON) == 1) {
    log("disable airplane mode if it is enabled");
    cmActivity.setAirplaneMode(getInstrumentation().getContext(),false);
  }
  super.tearDown();
}
