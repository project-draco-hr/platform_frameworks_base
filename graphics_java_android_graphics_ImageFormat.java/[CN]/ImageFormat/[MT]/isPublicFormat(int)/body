{
switch (format) {
case RGB_565:
case NV16:
case YUY2:
case YV12:
case JPEG:
case NV21:
case YUV_420_888:
case YUV_422_888:
case YUV_444_888:
case FLEX_RGB_888:
case FLEX_RGBA_8888:
case RAW_SENSOR:
case RAW_PRIVATE:
case RAW10:
case RAW12:
case DEPTH16:
case DEPTH_POINT_CLOUD:
case PRIVATE:
    return true;
}
return false;
}
