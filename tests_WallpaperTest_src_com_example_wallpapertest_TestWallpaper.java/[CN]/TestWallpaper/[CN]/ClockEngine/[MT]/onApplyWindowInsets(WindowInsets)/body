{
  super.onApplyWindowInsets(insets);
  mMainInsets.set(insets.getSystemWindowInsetLeft(),insets.getSystemWindowInsetTop(),insets.getSystemWindowInsetRight(),insets.getSystemWindowInsetBottom());
  mStableInsets.set(insets.getStableInsetLeft(),insets.getStableInsetTop(),insets.getStableInsetRight(),insets.getStableInsetBottom());
  mRound=insets.isRound();
  drawFrame();
}
