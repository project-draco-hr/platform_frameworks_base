{
  assertArraysEqual(new String[]{"deu","DEU",""},TtsEngines.toOldLocaleStringFormat(new Locale("de","DE")));
  assertArraysEqual(new String[]{"deu","",""},TtsEngines.toOldLocaleStringFormat(new Locale("de")));
  assertArraysEqual(new String[]{"eng","",""},TtsEngines.toOldLocaleStringFormat(new Locale("en")));
  assertArraysEqual(new String[]{"eng","USA",""},TtsEngines.toOldLocaleStringFormat(new Locale("foo")));
}
