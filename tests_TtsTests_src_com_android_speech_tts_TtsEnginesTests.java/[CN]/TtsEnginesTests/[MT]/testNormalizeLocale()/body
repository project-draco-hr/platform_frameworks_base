{
  assertEquals(Locale.UK,TtsEngines.normalizeTTSLocale(new Locale("eng","gbr")));
  assertEquals(Locale.UK,TtsEngines.normalizeTTSLocale(new Locale("eng","GBR")));
  assertEquals(Locale.GERMANY,TtsEngines.normalizeTTSLocale(new Locale("deu","deu")));
  assertEquals(Locale.GERMAN,TtsEngines.normalizeTTSLocale(new Locale("deu")));
  assertEquals(new Locale("yyy","DE"),TtsEngines.normalizeTTSLocale(new Locale("yyy","DE")));
}
