{
  try {
    MidiDeviceServer server=new MidiDeviceServer(mService);
    MidiDeviceInfo deviceInfo=mService.registerDeviceServer(server.getBinderInterface(),numInputPorts,numOutputPorts,properties,isPrivate,type);
    if (deviceInfo == null) {
      Log.e(TAG,"registerVirtualDevice failed");
      return null;
    }
    server.setDeviceInfo(deviceInfo);
    return server;
  }
 catch (  RemoteException e) {
    Log.e(TAG,"RemoteException in createVirtualDevice");
    return null;
  }
}
