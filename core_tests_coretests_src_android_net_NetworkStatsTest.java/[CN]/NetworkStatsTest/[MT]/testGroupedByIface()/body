{
  final NetworkStats uidStats=new NetworkStats(TEST_START,7).addValues(TEST_IFACE,100,SET_DEFAULT,TAG_NONE,ROAMING_NO,128L,8L,0L,2L,20L).addValues(TEST_IFACE2,100,SET_DEFAULT,TAG_NONE,ROAMING_NO,512L,32L,0L,0L,0L).addValues(TEST_IFACE2,100,SET_DEFAULT,0xF00D,ROAMING_NO,64L,4L,0L,0L,0L).addValues(TEST_IFACE2,100,SET_FOREGROUND,TAG_NONE,ROAMING_NO,512L,32L,0L,0L,0L).addValues(TEST_IFACE,101,SET_DEFAULT,TAG_NONE,ROAMING_NO,128L,8L,0L,0L,0L).addValues(TEST_IFACE,101,SET_DEFAULT,0xF00D,ROAMING_NO,128L,8L,0L,0L,0L).addValues(TEST_IFACE,101,SET_DEFAULT,TAG_NONE,ROAMING_YES,128L,8L,0L,0L,0L);
  final NetworkStats grouped=uidStats.groupedByIface();
  assertEquals(7,uidStats.size());
  assertEquals(2,grouped.size());
  assertValues(grouped,0,TEST_IFACE,UID_ALL,SET_ALL,TAG_NONE,ROAMING_ALL,384L,24L,0L,2L,0L);
  assertValues(grouped,1,TEST_IFACE2,UID_ALL,SET_ALL,TAG_NONE,ROAMING_ALL,1024L,64L,0L,0L,0L);
}
