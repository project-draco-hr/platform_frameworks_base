{
  final NetworkStats stats=new NetworkStats.Builder(SystemClock.elapsedRealtime(),3).addEntry(TEST_IFACE,100,1024,0).addEntry(TEST_IFACE,101,0,1024).addEntry(TEST_IFACE,102,1024,1024).build();
  assertEquals(2,stats.findIndex(TEST_IFACE,102));
  assertEquals(2,stats.findIndex(TEST_IFACE,102));
  assertEquals(0,stats.findIndex(TEST_IFACE,100));
  assertEquals(-1,stats.findIndex(TEST_IFACE,6));
}
