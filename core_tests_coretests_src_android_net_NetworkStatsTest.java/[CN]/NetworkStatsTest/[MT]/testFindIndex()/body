{
  final NetworkStats stats=new NetworkStats(TEST_START,3).addValues(TEST_IFACE,100,TAG_NONE,1024L,8L,0L,0L).addValues(TEST_IFACE,101,TAG_NONE,0L,0L,1024L,8L).addValues(TEST_IFACE,102,TAG_NONE,1024L,8L,1024L,8L);
  assertEquals(2,stats.findIndex(TEST_IFACE,102,TAG_NONE));
  assertEquals(2,stats.findIndex(TEST_IFACE,102,TAG_NONE));
  assertEquals(0,stats.findIndex(TEST_IFACE,100,TAG_NONE));
  assertEquals(-1,stats.findIndex(TEST_IFACE,6,TAG_NONE));
}
