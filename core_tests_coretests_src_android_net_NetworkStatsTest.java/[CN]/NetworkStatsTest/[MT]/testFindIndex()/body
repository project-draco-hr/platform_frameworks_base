{
  final NetworkStats stats=new NetworkStats(TEST_START,3).addValues(TEST_IFACE,100,SET_DEFAULT,TAG_NONE,1024L,8L,0L,0L,10).addValues(TEST_IFACE,101,SET_DEFAULT,TAG_NONE,0L,0L,1024L,8L,11).addValues(TEST_IFACE,102,SET_DEFAULT,TAG_NONE,1024L,8L,1024L,8L,12);
  assertEquals(2,stats.findIndex(TEST_IFACE,102,SET_DEFAULT,TAG_NONE));
  assertEquals(2,stats.findIndex(TEST_IFACE,102,SET_DEFAULT,TAG_NONE));
  assertEquals(0,stats.findIndex(TEST_IFACE,100,SET_DEFAULT,TAG_NONE));
  assertEquals(-1,stats.findIndex(TEST_IFACE,6,SET_DEFAULT,TAG_NONE));
}
