{
  final NetworkStats before=new NetworkStats(TEST_START,2).addEntry(TEST_IFACE,100,TAG_NONE,1024L,0L).addEntry(TEST_IFACE,101,TAG_NONE,0L,1024L);
  final NetworkStats after=new NetworkStats(TEST_START,2).addEntry(TEST_IFACE,100,TAG_NONE,1024L,0L).addEntry(TEST_IFACE,101,TAG_NONE,0L,1024L);
  final NetworkStats result=after.subtract(before);
  assertEquals(0,result.rx[0]);
  assertEquals(0,result.tx[0]);
  assertEquals(0,result.rx[1]);
  assertEquals(0,result.tx[1]);
}
