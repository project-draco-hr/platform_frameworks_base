{
  final NetworkStats before=new NetworkStats(TEST_START,2).addEntry(TEST_IFACE,100,1024,0).addEntry(TEST_IFACE,101,0,1024);
  final NetworkStats after=new NetworkStats(TEST_START,2).addEntry(TEST_IFACE,100,1025,2).addEntry(TEST_IFACE,101,3,1028);
  final NetworkStats result=after.subtract(before);
  assertEquals(1,result.rx[0]);
  assertEquals(2,result.tx[0]);
  assertEquals(3,result.rx[1]);
  assertEquals(4,result.tx[1]);
}
