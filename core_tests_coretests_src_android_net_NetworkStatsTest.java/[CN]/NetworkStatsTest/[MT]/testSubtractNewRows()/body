{
  final NetworkStats before=new NetworkStats.Builder(SystemClock.elapsedRealtime(),2).addEntry(TEST_IFACE,100,1024,0).addEntry(TEST_IFACE,101,0,1024).build();
  final NetworkStats after=new NetworkStats.Builder(SystemClock.elapsedRealtime(),3).addEntry(TEST_IFACE,100,1024,0).addEntry(TEST_IFACE,101,0,1024).addEntry(TEST_IFACE,102,1024,1024).build();
  final NetworkStats result=after.subtract(before);
  assertEquals(0,result.rx[0]);
  assertEquals(0,result.tx[0]);
  assertEquals(0,result.rx[1]);
  assertEquals(0,result.tx[1]);
  assertEquals(1024,result.rx[2]);
  assertEquals(1024,result.tx[2]);
}
