{
  final NetworkStats before=new NetworkStats(TEST_START,2).addValues(TEST_IFACE,100,TAG_NONE,1024L,8L,0L,0L).addValues(TEST_IFACE,101,TAG_NONE,0L,0L,1024L,8L);
  final NetworkStats after=new NetworkStats(TEST_START,3).addValues(TEST_IFACE,100,TAG_NONE,1024L,8L,0L,0L).addValues(TEST_IFACE,101,TAG_NONE,0L,0L,1024L,8L).addValues(TEST_IFACE,102,TAG_NONE,1024L,8L,1024L,8L);
  final NetworkStats result=after.subtract(before);
  assertEntry(result,0,TEST_IFACE,100,TAG_NONE,0L,0L,0L,0L);
  assertEntry(result,1,TEST_IFACE,101,TAG_NONE,0L,0L,0L,0L);
  assertEntry(result,2,TEST_IFACE,102,TAG_NONE,1024L,8L,1024L,8L);
}
