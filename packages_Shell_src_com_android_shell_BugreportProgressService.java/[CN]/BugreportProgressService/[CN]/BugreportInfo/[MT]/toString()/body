{
  final float percent=((float)progress * 100 / max);
  return "id: " + id + ", pid: "+ pid+ ", name: "+ name+ ", finished: "+ finished+ "\n\ttitle: "+ title+ "\n\tdescription: "+ description+ "\n\tfile: "+ bugreportFile+ "\n\tscreenshots: "+ screenshotFiles+ "\n\tprogress: "+ progress+ "/"+ max+ " ("+ percent+ ")"+ "\n\tlast_update: "+ getFormattedLastUpdate()+ "\naddingDetailsToZip: "+ addingDetailsToZip+ " addedDetailsToZip: "+ addedDetailsToZip;
}
