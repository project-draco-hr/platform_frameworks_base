{
  for (int i=0; i < mAlarmBatches.size(); i++) {
    Batch b=mAlarmBatches.get(i);
    if (b.hasPackage(packageName)) {
      return true;
    }
  }
  for (int i=0; i < mPendingWhileIdleAlarms.size(); i++) {
    final Alarm a=mPendingWhileIdleAlarms.get(i);
    if (a.matches(packageName)) {
      return true;
    }
  }
  return false;
}
