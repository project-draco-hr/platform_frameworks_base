{
  final long start1=b1.start;
  final long start2=b2.start;
  if (start1 > start2) {
    return 1;
  }
  if (start1 < start2) {
    return -1;
  }
  final long interval1=b1.end - b1.start;
  final long interval2=b2.end - b2.start;
  if (interval1 > interval2) {
    return 1;
  }
  if (interval2 < interval1) {
    return -1;
  }
  return 0;
}
