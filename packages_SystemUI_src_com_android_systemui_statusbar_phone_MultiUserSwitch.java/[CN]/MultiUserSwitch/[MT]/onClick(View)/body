{
  if (UserSwitcherController.isUserSwitcherAvailable(mUserManager)) {
    if (mKeyguardMode) {
      if (mKeyguardUserSwitcher != null) {
        mKeyguardUserSwitcher.show(true);
      }
    }
 else {
      if (mQsPanel != null) {
        UserSwitcherController userSwitcherController=mQsPanel.getHost().getUserSwitcherController();
        if (userSwitcherController != null) {
          mQsPanel.showDetailAdapter(true,userSwitcherController.userDetailAdapter);
        }
      }
    }
  }
 else {
    Intent intent=ContactsContract.QuickContact.composeQuickContactsIntent(getContext(),v,ContactsContract.Profile.CONTENT_URI,ContactsContract.QuickContact.MODE_LARGE,null);
    getContext().startActivityAsUser(intent,new UserHandle(UserHandle.USER_CURRENT));
  }
}
