{
  final UserManager um=UserManager.get(getContext());
  if (um.isUserSwitcherEnabled()) {
    final UserSwitcherHostView switcher=(UserSwitcherHostView)LayoutInflater.from(getContext()).inflate(R.layout.user_switcher_host,mOverlayParent,false);
    switcher.setFinishRunnable(new Runnable(){
      @Override public void run(){
        mOverlayParent.removeView(switcher);
      }
    }
);
    switcher.refreshUsers();
    mOverlayParent.addView(switcher);
  }
 else {
    Intent intent=ContactsContract.QuickContact.composeQuickContactsIntent(getContext(),v,ContactsContract.Profile.CONTENT_URI,ContactsContract.QuickContact.MODE_LARGE,null);
    getContext().startActivityAsUser(intent,new UserHandle(UserHandle.USER_CURRENT));
  }
}
