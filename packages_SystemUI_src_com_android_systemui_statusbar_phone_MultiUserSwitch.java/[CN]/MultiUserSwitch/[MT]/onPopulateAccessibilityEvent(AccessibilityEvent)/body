{
  super.onPopulateAccessibilityEvent(event);
  if (isClickable()) {
    final UserManager um=UserManager.get(getContext());
    String text=mContext.getString(um.isUserSwitcherEnabled() ? R.string.accessibility_multi_user_switch_switcher : R.string.accessibility_multi_user_switch_quick_contact);
    if (!TextUtils.isEmpty(text)) {
      event.getText().add(text);
    }
  }
}
