{
  super.onPopulateAccessibilityEvent(event);
  if (isClickable()) {
    String text;
    if (opensUserSwitcherWhenClicked()) {
      String currentUser=null;
      if (mQsPanel != null) {
        UserSwitcherController controller=mQsPanel.getHost().getUserSwitcherController();
        currentUser=controller.getCurrentUserName(mContext);
      }
      if (TextUtils.isEmpty(currentUser)) {
        text=mContext.getString(R.string.accessibility_multi_user_switch_switcher);
      }
 else {
        text=mContext.getString(R.string.accessibility_multi_user_switch_switcher_with_current,currentUser);
      }
    }
 else {
      text=mContext.getString(R.string.accessibility_multi_user_switch_quick_contact);
    }
    if (!TextUtils.isEmpty(text)) {
      event.getText().add(text);
    }
  }
}
