{
  mIsAutoFillable=getSettings().getAutoFillEnabled() && (queryId != WebTextView.FORM_NOT_AUTOFILLABLE);
  int variation=mInputType & EditorInfo.TYPE_MASK_VARIATION;
  if (variation != EditorInfo.TYPE_TEXT_VARIATION_WEB_PASSWORD && (mIsAutoFillable || mIsAutoCompleteEnabled)) {
    if (mName != null && mName.length() > 0) {
      requestFormData(mName,mFieldPointer,mIsAutoFillable,mIsAutoCompleteEnabled);
    }
  }
}
