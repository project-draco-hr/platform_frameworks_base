{
  int maxScrollX=mEditTextContent.width() - mEditTextBounds.width();
  int maxScrollY=mEditTextContent.height() - mEditTextBounds.height();
  int contentVelocityX=viewToContentDimension((int)-velocityX);
  int contentVelocityY=viewToContentDimension((int)-velocityY);
  mEditTextScroller.fling(-mEditTextContent.left,-mEditTextContent.top,contentVelocityX,contentVelocityY,0,maxScrollX,0,maxScrollY);
  return true;
}
