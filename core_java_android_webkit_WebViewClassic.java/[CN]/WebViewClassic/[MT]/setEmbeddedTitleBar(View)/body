{
  if (mWebView instanceof TitleBarDelegate) {
    ((TitleBarDelegate)mWebView).onSetEmbeddedTitleBar(v);
  }
  if (mTitleBar == v)   return;
  if (mTitleBar != null) {
    mWebView.removeView(mTitleBar);
  }
  if (null != v) {
    mWebView.addView(v,new AbsoluteLayout.LayoutParams(ViewGroup.LayoutParams.MATCH_PARENT,ViewGroup.LayoutParams.WRAP_CONTENT,0,0));
  }
  mTitleBar=v;
}
