{
  clearHelpers();
  if (mListBoxDialog != null) {
    mListBoxDialog.dismiss();
    mListBoxDialog=null;
  }
  if (mWebTextView != null) {
    mWebTextView.remove();
    mWebTextView=null;
  }
  if (mNativeClass != 0)   nativeStopGL();
  if (mWebViewCore != null) {
synchronized (this) {
      WebViewCore webViewCore=mWebViewCore;
      mWebViewCore=null;
      webViewCore.destroy();
    }
    mPrivateHandler.removeCallbacksAndMessages(null);
  }
  if (mNativeClass != 0) {
    nativeDestroy();
    mNativeClass=0;
  }
}
