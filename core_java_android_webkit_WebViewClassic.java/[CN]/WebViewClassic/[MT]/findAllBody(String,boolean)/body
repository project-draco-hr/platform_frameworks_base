{
  checkThread();
  if (0 == mNativeClass)   return 0;
  mLastFind=find;
  mWebViewCore.removeMessages(EventHub.FIND_ALL);
  WebViewCore.FindAllRequest request=new WebViewCore.FindAllRequest(find);
  if (isAsync) {
    mWebViewCore.sendMessage(EventHub.FIND_ALL,request);
    return 0;
  }
synchronized (request) {
    try {
      mWebViewCore.sendMessageAtFrontOfQueue(EventHub.FIND_ALL,request);
      while (request.mMatchCount == -1) {
        request.wait();
      }
    }
 catch (    InterruptedException e) {
      return 0;
    }
  }
  return request.mMatchCount;
}
