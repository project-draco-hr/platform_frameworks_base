{
  handles[0]=mSelectCursorBase.x;
  handles[1]=mSelectCursorBase.y;
  handles[2]=mSelectCursorExtent.x;
  handles[3]=mSelectCursorExtent.y;
  if (!nativeIsBaseFirst(mNativeClass)) {
    int swap=handles[0];
    handles[0]=handles[2];
    handles[2]=swap;
    swap=handles[1];
    handles[1]=handles[3];
    handles[3]=swap;
  }
}
