{
  if (!mIsPaused) {
    mIsPaused=true;
    mWebViewCore.sendMessage(EventHub.ON_PAUSE);
    if (mHTML5VideoViewProxy != null) {
      mHTML5VideoViewProxy.pauseAndDispatch();
    }
    if (mNativeClass != 0) {
      nativeSetPauseDrawing(mNativeClass,true);
    }
    cancelDialogs();
    WebCoreThreadWatchdog.pause();
  }
}
