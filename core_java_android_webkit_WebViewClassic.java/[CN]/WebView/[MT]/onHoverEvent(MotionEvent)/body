{
  if (mNativeClass == 0) {
    return false;
  }
  WebViewCore.CursorData data=cursorDataNoPosition();
  data.mX=viewToContentX((int)event.getX() + getScrollX());
  data.mY=viewToContentY((int)event.getY() + getScrollY());
  mWebViewCore.sendMessage(EventHub.SET_MOVE_MOUSE,data);
  return true;
}
