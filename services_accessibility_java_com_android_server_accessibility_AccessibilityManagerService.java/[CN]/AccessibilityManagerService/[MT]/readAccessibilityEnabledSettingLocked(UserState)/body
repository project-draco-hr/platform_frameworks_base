{
  final boolean accessibilityEnabled=Settings.Secure.getIntForUser(mContext.getContentResolver(),Settings.Secure.ACCESSIBILITY_ENABLED,0,userState.mUserId) == 1;
  if (accessibilityEnabled != userState.mIsAccessibilityEnabled) {
    userState.mIsAccessibilityEnabled=accessibilityEnabled;
    return true;
  }
  return false;
}
