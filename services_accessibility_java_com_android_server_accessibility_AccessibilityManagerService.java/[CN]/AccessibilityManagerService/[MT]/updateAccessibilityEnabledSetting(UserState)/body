{
  final long identity=Binder.clearCallingIdentity();
  try {
    Settings.Secure.putIntForUser(mContext.getContentResolver(),Settings.Secure.ACCESSIBILITY_ENABLED,userState.isHandlingAccessibilityEvents() ? 0 : 1,userState.mUserId);
  }
  finally {
    Binder.restoreCallingIdentity(identity);
  }
}
