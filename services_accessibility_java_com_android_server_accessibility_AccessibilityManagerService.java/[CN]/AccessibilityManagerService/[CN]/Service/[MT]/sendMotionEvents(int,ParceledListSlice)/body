{
synchronized (mLock) {
    if (mSecurityPolicy.canPerformGestures(this)) {
      final long endMillis=SystemClock.uptimeMillis() + WAIT_MOTION_INJECTOR_TIMEOUT_MILLIS;
      while ((mMotionEventInjector == null) && (SystemClock.uptimeMillis() < endMillis)) {
        try {
          mLock.wait(endMillis - SystemClock.uptimeMillis());
        }
 catch (        InterruptedException ie) {
        }
      }
      if (mMotionEventInjector != null) {
        mMotionEventInjector.injectEvents((List<MotionEvent>)events.getList(),mServiceInterface,sequence);
        return;
      }
 else {
        Slog.e(LOG_TAG,"MotionEventInjector installation timed out");
      }
    }
  }
  try {
    mServiceInterface.onPerformGestureResult(sequence,false);
  }
 catch (  RemoteException re) {
    Slog.e(LOG_TAG,"Error sending motion event injection failure to " + mServiceInterface,re);
  }
}
