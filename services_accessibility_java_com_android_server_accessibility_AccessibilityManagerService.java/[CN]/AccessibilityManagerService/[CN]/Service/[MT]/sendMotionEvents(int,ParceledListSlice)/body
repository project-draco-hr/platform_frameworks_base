{
synchronized (mLock) {
    if (mSecurityPolicy.canPerformGestures(this) && (mMotionEventInjector != null)) {
      mMotionEventInjector.injectEvents((List<MotionEvent>)events.getList(),mServiceInterface,sequence);
      return;
    }
  }
  try {
    mServiceInterface.onPerformGestureResult(sequence,false);
  }
 catch (  RemoteException re) {
    Slog.e(LOG_TAG,"Error sending motion event injection failure to " + mServiceInterface,re);
  }
}
