{
  final IAccessibilityServiceClient listener;
synchronized (mLock) {
    listener=mServiceInterface;
  }
  if (listener != null) {
    try {
      listener.onWindowsChanged(windowIds);
    }
 catch (    RemoteException re) {
      Slog.e(LOG_TAG,"Error during sending windows to: " + mService,re);
    }
  }
}
