{
  if (mService == null) {
    return false;
  }
  UserState userState=getUserStateLocked(mUserId);
  mKeyEventDispatcher.flush();
  if (!mIsAutomation) {
    mContext.unbindService(this);
  }
 else {
    userState.destroyUiAutomationService();
  }
  removeServiceLocked(this,userState);
  resetLocked();
  return true;
}
