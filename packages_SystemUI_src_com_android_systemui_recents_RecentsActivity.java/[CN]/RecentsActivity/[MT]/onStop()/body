{
  if (Console.Enabled) {
    Console.log(Constants.Log.App.SystemUIHandshake,"[RecentsActivity|onStop]","",Console.AnsiRed);
  }
  super.onStop();
  unregisterReceiver(mServiceBroadcastReceiver);
  if (mConfig.searchBarAppWidgetId >= 0) {
    mAppWidgetHost.stopListening();
  }
  mVisible=false;
}
