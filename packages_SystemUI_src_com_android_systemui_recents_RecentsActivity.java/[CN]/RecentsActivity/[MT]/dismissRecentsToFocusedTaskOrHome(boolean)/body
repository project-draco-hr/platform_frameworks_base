{
  if (mVisible) {
    if (mFullScreenOverlayView != null && mFullScreenOverlayView.cancelAnimateOnEnterRecents(mFinishRunnable))     return true;
    if (checkFilteredStackState && mRecentsView.unfilterFilteredStacks())     return true;
    if (mRecentsView.launchFocusedTask())     return true;
    if (mConfig.launchedFromHome) {
      dismissRecentsToHomeRaw(true);
      return true;
    }
    if (mRecentsView.launchFirstTask())     return true;
    dismissRecentsToHomeRaw(true);
    return true;
  }
  return false;
}
