{
  if (Console.Enabled) {
    Console.log(Constants.Log.App.SystemUIHandshake,"[RecentsActivity|onStart]","",Console.AnsiRed);
  }
  super.onStart();
  RecentsConfiguration config=RecentsConfiguration.getInstance();
  if (config.searchBarAppWidgetId >= 0) {
    mAppWidgetHost.startListening();
  }
  mVisible=true;
}
