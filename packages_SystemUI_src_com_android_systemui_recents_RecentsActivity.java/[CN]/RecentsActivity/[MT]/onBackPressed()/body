{
  if (mFullScreenshotView == null || !mFullScreenshotView.cancelAnimateOnEnterRecents(mFinishRunnable)) {
    if (!mRecentsView.unfilterFilteredStacks()) {
      if (mConfig.launchedFromHome) {
        ReferenceCountedTrigger exitTrigger=new ReferenceCountedTrigger(this,null,mFinishRunnable,null);
        mRecentsView.startOnExitAnimation(new ViewAnimation.TaskViewExitContext(exitTrigger));
      }
 else {
        if (!mRecentsView.launchFirstTask()) {
          ReferenceCountedTrigger exitTrigger=new ReferenceCountedTrigger(this,null,mFinishRunnable,null);
          mRecentsView.startOnExitAnimation(new ViewAnimation.TaskViewExitContext(exitTrigger));
        }
      }
    }
  }
}
