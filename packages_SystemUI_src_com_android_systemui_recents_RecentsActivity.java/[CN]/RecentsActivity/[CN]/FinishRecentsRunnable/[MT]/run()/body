{
  if (mLaunchIntent != null) {
    try {
      if (mLaunchOpts != null) {
        startActivityAsUser(mLaunchIntent,mLaunchOpts.toBundle(),UserHandle.CURRENT);
      }
 else {
        startActivityAsUser(mLaunchIntent,UserHandle.CURRENT);
      }
    }
 catch (    Exception e) {
      Console.logError(RecentsActivity.this,getString(R.string.recents_launch_error_message,"Home"));
    }
  }
 else {
    finish();
    overridePendingTransition(R.anim.recents_to_launcher_enter,R.anim.recents_to_launcher_exit);
  }
}
