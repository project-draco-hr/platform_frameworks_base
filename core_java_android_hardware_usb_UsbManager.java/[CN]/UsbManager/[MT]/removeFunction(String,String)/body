{
  String[] split=functions.split(",");
  for (int i=0; i < split.length; i++) {
    if (function.equals(split[i])) {
      split[i]=null;
    }
  }
  if (split.length == 1 && split[0] == null) {
    return USB_FUNCTION_NONE;
  }
  StringBuilder builder=new StringBuilder();
  for (int i=0; i < split.length; i++) {
    String s=split[i];
    if (s != null) {
      if (builder.length() > 0) {
        builder.append(",");
      }
      builder.append(s);
    }
  }
  return builder.toString();
}
