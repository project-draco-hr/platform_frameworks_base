{
  pw.print(prefix);
  pw.print("mDismissKeyguardOnNextActivity:");
  pw.println(mDismissKeyguardOnNextActivity);
  pw.print(prefix);
  pw.print("mStackState=");
  pw.println(stackStateToString(mStackState));
  pw.print(prefix);
  pw.println("mSleepTimeout: " + mSleepTimeout);
  pw.print(prefix);
  pw.println("mCurTaskId: " + mCurTaskId);
  pw.print(prefix);
  pw.print("mHomePackageNames:");
  for (int i=0; i < mHomePackageNames.size(); ++i) {
    pw.print(" (");
    pw.print(mHomePackageNames.keyAt(i));
    pw.print(",");
    pw.print(mHomePackageNames.valueAt(i));
    pw.print(")");
  }
  pw.println();
}
