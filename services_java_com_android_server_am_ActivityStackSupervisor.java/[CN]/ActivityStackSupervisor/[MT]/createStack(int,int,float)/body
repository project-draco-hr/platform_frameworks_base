{
synchronized (this) {
    while (true) {
      if (++mLastStackId <= HOME_STACK_ID) {
        mLastStackId=HOME_STACK_ID + 1;
      }
      if (getStack(mLastStackId) == null) {
        break;
      }
    }
    mStacks.add(new ActivityStack(mService,mContext,mLooper,mLastStackId,this));
    return mLastStackId;
  }
}
