{
  RenderScript pRS=RenderScript.create(mCtx);
  ScriptC_primitives s=new ScriptC_primitives(pRS,mRes,R.raw.primitives);
  pRS.setMessageHandler(mRsMessage);
  if (!initializeGlobals(s)) {
    result=-1;
  }
 else {
    s.invoke_primitives_test(0,0);
    pRS.finish();
    waitForMessage();
  }
  pRS.destroy();
}
