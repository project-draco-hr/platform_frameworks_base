{
  switchOutDrawHistory();
  float scale=mActualScale * 0.8f;
  if (scale < (mMinZoomScale + 0.1f) && mWebViewCore.getSettings().getUseWideViewPort() && mZoomOverviewWidth > Math.ceil(getViewWidth() * mInvActualScale)) {
    doDoubleTap();
    return true;
  }
 else {
    mZoomCenterX=getViewWidth() * .5f;
    mZoomCenterY=getViewHeight() * .5f;
    return zoomWithPreview(scale);
  }
}
