{
  int keyEventAction;
  int eventHubAction;
  if (down) {
    keyEventAction=KeyEvent.ACTION_DOWN;
    eventHubAction=EventHub.KEY_DOWN;
    playSoundEffect(keyCodeToSoundsEffect(keyCode));
  }
 else {
    keyEventAction=KeyEvent.ACTION_UP;
    eventHubAction=EventHub.KEY_UP;
  }
  KeyEvent event=new KeyEvent(time,time,keyEventAction,keyCode,1,(metaState & KeyEvent.META_SHIFT_ON) | (metaState & KeyEvent.META_ALT_ON) | (metaState & KeyEvent.META_SYM_ON),0,0,0);
  mWebViewCore.sendMessage(eventHubAction,event);
}
