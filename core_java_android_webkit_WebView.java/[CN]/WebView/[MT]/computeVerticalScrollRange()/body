{
  if (mDrawHistory) {
    return mHistoryHeight;
  }
 else {
    int height;
    if (mLastHeightSent == mContentHeight) {
      height=getViewHeight();
    }
 else {
      height=contentToViewDimension(mContentHeight);
    }
    if (mFindIsUp) {
      height+=FIND_HEIGHT;
    }
    return height;
  }
}
