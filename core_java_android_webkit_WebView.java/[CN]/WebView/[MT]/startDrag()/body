{
  WebViewCore.reducePriority();
  WebViewCore.pauseUpdatePicture(mWebViewCore);
  if (!mDragFromTextInput) {
    nativeHideCursor();
  }
  WebSettings settings=getSettings();
  if (settings.supportZoom() && settings.getBuiltInZoomControls() && !getZoomButtonsController().isVisible()&& mMinZoomScale < mMaxZoomScale && (mHorizontalScrollBarMode != SCROLLBAR_ALWAYSOFF || mVerticalScrollBarMode != SCROLLBAR_ALWAYSOFF)) {
    mZoomButtonsController.setVisible(true);
    int count=settings.getDoubleTapToastCount();
    if (mInZoomOverview && count > 0) {
      settings.setDoubleTapToastCount(--count);
      Toast.makeText(mContext,com.android.internal.R.string.double_tap_toast,Toast.LENGTH_LONG).show();
    }
  }
}
