{
  switchOutDrawHistory();
  if (mInZoomOverview) {
    mLastTouchX=getViewWidth() * .5f;
    mLastTouchY=getViewHeight() * .5f;
    doDoubleTap();
    return true;
  }
 else {
    mZoomCenterX=getViewWidth() * .5f;
    mZoomCenterY=getViewHeight() * .5f;
    return zoomWithPreview(mActualScale * 1.25f);
  }
}
