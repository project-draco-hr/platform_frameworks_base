{
  boolean dispatch=true;
  boolean inEditingMode=inEditingMode();
  if (!inEditingMode && (mNativeClass == 0 || !nativePageShouldHandleShiftAndArrows())) {
    if (event.getAction() == KeyEvent.ACTION_DOWN) {
      mGotKeyDown=true;
    }
 else {
      if (!mGotKeyDown) {
        dispatch=false;
      }
      mGotKeyDown=false;
    }
  }
  if (dispatch) {
    if (inEditingMode) {
      return mWebTextView.dispatchKeyEvent(event);
    }
 else {
      return super.dispatchKeyEvent(event);
    }
  }
 else {
    return false;
  }
}
