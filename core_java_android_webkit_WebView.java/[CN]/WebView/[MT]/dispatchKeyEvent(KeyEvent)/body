{
  boolean dispatch=true;
  if (!inEditingMode()) {
    if (event.getAction() == KeyEvent.ACTION_DOWN) {
      mGotKeyDown=true;
    }
 else {
      if (!mGotKeyDown) {
        dispatch=false;
      }
      mGotKeyDown=false;
    }
  }
  if (dispatch) {
    return super.dispatchKeyEvent(event);
  }
 else {
    return false;
  }
}
