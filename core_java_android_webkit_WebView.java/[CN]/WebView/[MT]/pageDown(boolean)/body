{
  if (mNativeClass == 0) {
    return false;
  }
  nativeClearCursor(-1,-1);
  if (bottom) {
    return pinScrollTo(mScrollX,mContentHeight,true,0);
  }
  int h=getHeight();
  int y;
  if (h > 2 * PAGE_SCROLL_OVERLAP) {
    y=h - PAGE_SCROLL_OVERLAP;
  }
 else {
    y=h / 2;
  }
  mUserScroll=true;
  return mScroller.isFinished() ? pinScrollBy(0,y,true,0) : extendScroll(y);
}
