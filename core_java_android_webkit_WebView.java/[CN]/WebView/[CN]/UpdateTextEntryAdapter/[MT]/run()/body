{
  ArrayList<String> pastEntries=mDatabase.getFormData(mUrl,mName);
  if (pastEntries.size() > 0) {
    ArrayAdapter<String> adapter=new ArrayAdapter<String>(mContext,com.android.internal.R.layout.simple_list_item_1,pastEntries);
    ((HashMap)mUpdateMessage.obj).put("adapter",adapter);
    mUpdateMessage.sendToTarget();
  }
}
