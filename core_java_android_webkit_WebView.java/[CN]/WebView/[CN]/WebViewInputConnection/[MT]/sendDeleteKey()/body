{
  long eventTime=SystemClock.uptimeMillis();
  sendKeyEvent(new KeyEvent(eventTime,eventTime,KeyEvent.ACTION_DOWN,KeyEvent.KEYCODE_DEL,0,0,KeyCharacterMap.VIRTUAL_KEYBOARD,0,KeyEvent.FLAG_SOFT_KEYBOARD));
  sendKeyEvent(new KeyEvent(SystemClock.uptimeMillis(),eventTime,KeyEvent.ACTION_UP,KeyEvent.KEYCODE_DEL,0,0,KeyCharacterMap.VIRTUAL_KEYBOARD,0,KeyEvent.FLAG_SOFT_KEYBOARD));
}
