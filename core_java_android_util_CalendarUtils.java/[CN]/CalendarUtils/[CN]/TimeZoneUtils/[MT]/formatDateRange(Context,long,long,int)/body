{
  String date;
  String tz;
  if ((flags & DateUtils.FORMAT_UTC) != 0) {
    tz=Time.TIMEZONE_UTC;
  }
 else {
    tz=getTimeZone(context,null);
  }
synchronized (mSB) {
    mSB.setLength(0);
    date=DateUtils.formatDateRange(context,mF,startMillis,endMillis,flags,tz).toString();
  }
  return date;
}
