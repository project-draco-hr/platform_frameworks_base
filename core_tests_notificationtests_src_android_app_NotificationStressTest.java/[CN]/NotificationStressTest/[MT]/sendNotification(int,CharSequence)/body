{
  Notification notification=new Notification(ICONS[mRandom.nextInt(ICONS.length)],text,System.currentTimeMillis());
  Intent intent=new Intent(Intent.ACTION_VIEW,Uri.parse("http://www.google.com"));
  PendingIntent pendingIntent=PendingIntent.getActivity(mContext,0,intent,0);
  CharSequence title=text + " " + id;
  CharSequence subtitle=String.valueOf(System.currentTimeMillis());
  notification.setLatestEventInfo(mContext,title,subtitle,pendingIntent);
  mNotificationManager.notify(id,notification);
  SystemClock.sleep(10);
}
