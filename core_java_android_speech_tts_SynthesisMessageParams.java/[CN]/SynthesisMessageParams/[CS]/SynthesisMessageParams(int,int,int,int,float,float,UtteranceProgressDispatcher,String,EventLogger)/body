{
  super(dispatcher,callingApp);
  mStreamType=streamType;
  mSampleRateInHz=sampleRate;
  mAudioFormat=audioFormat;
  mChannelCount=channelCount;
  mVolume=volume;
  mPan=pan;
  mLogger=logger;
  mBytesPerFrame=getBytesPerFrame(mAudioFormat) * mChannelCount;
  mAudioTrack=null;
  mBytesWritten=0;
  mAudioBufferSize=0;
  mIsError=false;
}
