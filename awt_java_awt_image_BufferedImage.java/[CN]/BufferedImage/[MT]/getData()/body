{
  int w=raster.getWidth();
  int h=raster.getHeight();
  int minX=raster.getMinX();
  int minY=raster.getMinY();
  WritableRaster outr=Raster.createWritableRaster(raster.getSampleModel(),new Point(raster.getSampleModelTranslateX(),raster.getSampleModelTranslateY()));
  Object data=null;
  data=raster.getDataElements(minX,minY,w,h,data);
  outr.setDataElements(minX,minY,w,h,data);
  return outr;
}
