{
  if (mainThumbnail != null) {
    dest.writeInt(1);
    mainThumbnail.writeToParcel(dest,flags);
  }
 else {
    dest.writeInt(0);
  }
  if (thumbnailFileDescriptor != null) {
    dest.writeInt(1);
    thumbnailFileDescriptor.writeToParcel(dest,flags);
  }
 else {
    dest.writeInt(0);
  }
  if (thumbnailInfo != null) {
    dest.writeInt(1);
    thumbnailInfo.writeToParcel(dest,flags);
  }
 else {
    dest.writeInt(0);
  }
}
