{
  dest.writeInt(id);
  if (baseIntent != null) {
    dest.writeInt(1);
    baseIntent.writeToParcel(dest,0);
  }
 else {
    dest.writeInt(0);
  }
  ComponentName.writeToParcel(origActivity,dest);
  if (thumbnail != null) {
    dest.writeInt(1);
    thumbnail.writeToParcel(dest,0);
  }
 else {
    dest.writeInt(0);
  }
  TextUtils.writeToParcel(description,dest,Parcelable.PARCELABLE_WRITE_RETURN_VALUE);
}
