{
synchronized (this) {
    if (false) {
      Log.d(TAG,"pulseBreathingLight mAttentionLightOn=" + mAttentionLightOn + " mPulsing="+ mPulsing);
    }
    if (!mAttentionLightOn && !mPulsing) {
      mPulsing=true;
      setLight_native(mNativePointer,LIGHT_ID_ATTENTION,0x00ffffff,LIGHT_FLASH_HARDWARE,7,0,0);
      mH.sendMessageDelayed(Message.obtain(mH,1),3000);
    }
  }
}
