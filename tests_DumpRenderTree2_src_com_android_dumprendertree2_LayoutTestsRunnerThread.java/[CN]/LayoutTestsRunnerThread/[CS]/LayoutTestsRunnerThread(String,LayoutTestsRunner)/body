{
  mFileFilter=new FileFilter(TESTS_ROOT_DIR_PATH);
  mRelativePath=path;
  mUiDisplayHandler=activity.getHandler();
  mActivity=activity;
  mHandlerOnUiThread=new Handler(){
    @Override public void handleMessage(    Message msg){
switch (msg.what) {
case MSG_RUN_TEST:
        ((LayoutTest)msg.obj).run();
      break;
  }
}
}
;
}
