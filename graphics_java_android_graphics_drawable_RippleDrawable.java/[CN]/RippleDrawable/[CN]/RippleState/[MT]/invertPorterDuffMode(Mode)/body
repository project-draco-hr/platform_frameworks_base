{
switch (src) {
case SRC_ATOP:
    return Mode.DST_ATOP;
case SRC_IN:
  return Mode.DST_IN;
case SRC_OUT:
return Mode.DST_OUT;
case SRC_OVER:
return Mode.DST_OVER;
case DST_ATOP:
return Mode.SRC_ATOP;
case DST_IN:
return Mode.SRC_IN;
case DST_OUT:
return Mode.SRC_OUT;
case DST_OVER:
return Mode.SRC_OVER;
default :
return src;
}
}
