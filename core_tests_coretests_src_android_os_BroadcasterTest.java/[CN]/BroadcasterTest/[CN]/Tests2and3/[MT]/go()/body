{
  Broadcaster b=new Broadcaster();
  mHandlers=new Handler[N];
  mSuccess=new boolean[N];
  for (int i=0; i < N; i++) {
    mHandlers[i]=new H();
    mSuccess[i]=false;
    b.request(MESSAGE_A,mHandlers[i],MESSAGE_B + i);
  }
  Message msg=new Message();
  msg.what=MESSAGE_A;
  b.broadcast(msg);
}
