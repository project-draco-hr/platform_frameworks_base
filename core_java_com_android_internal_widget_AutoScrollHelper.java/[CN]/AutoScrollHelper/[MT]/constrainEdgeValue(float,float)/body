{
  if (leading == 0) {
    return 0;
  }
switch (mEdgeType) {
case EDGE_TYPE_INSIDE:
case EDGE_TYPE_INSIDE_EXTEND:
    if (current < leading) {
      if (current > 0) {
        return 1f - current / leading;
      }
 else       if (mAnimating && (mEdgeType == EDGE_TYPE_INSIDE_EXTEND)) {
        return 1f;
      }
    }
  break;
case EDGE_TYPE_OUTSIDE:
if (current < 0) {
  return current / -leading;
}
break;
}
return 0;
}
