{
  RenderScript pRS=RenderScript.create(mCtx);
  ScriptC_unsigned s=new ScriptC_unsigned(pRS,mRes,R.raw.unsigned);
  pRS.setMessageHandler(mRsMessage);
  if (!initializeGlobals(s)) {
    failTest();
  }
 else {
    s.invoke_unsigned_test();
    pRS.finish();
    waitForMessage();
  }
  pRS.destroy();
}
