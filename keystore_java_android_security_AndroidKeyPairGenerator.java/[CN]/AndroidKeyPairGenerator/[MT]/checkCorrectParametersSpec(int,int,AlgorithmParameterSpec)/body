{
  if (keyType == NativeConstants.EVP_PKEY_RSA && spec != null) {
    if (spec instanceof RSAKeyGenParameterSpec) {
      RSAKeyGenParameterSpec rsaSpec=(RSAKeyGenParameterSpec)spec;
      if (keySize != -1 && keySize != rsaSpec.getKeysize()) {
        throw new InvalidAlgorithmParameterException("RSA key size must match: " + keySize + " vs "+ rsaSpec.getKeysize());
      }
    }
 else {
      throw new InvalidAlgorithmParameterException("RSA may only use RSAKeyGenParameterSpec");
    }
  }
}
