{
  if (params == null) {
    throw new InvalidAlgorithmParameterException("must supply params of type AndroidKeyPairGenericSpec");
  }
 else   if (!(params instanceof AndroidKeyPairGeneratorSpec)) {
    throw new InvalidAlgorithmParameterException("params must be of type AndroidKeyPairGeneratorSpec");
  }
  AndroidKeyPairGeneratorSpec spec=(AndroidKeyPairGeneratorSpec)params;
  mSpec=spec;
  mKeyStore=android.security.KeyStore.getInstance();
}
