{
  StringBuffer sb=new StringBuffer();
  for (  byte b : data) {
    String s=null;
    try {
      s=Integer.toHexString(b & 0xff);
    }
 catch (    Exception e) {
      e.printStackTrace();
      return null;
    }
    if (s.length() == 1) {
      sb.append('0');
    }
    sb.append(s);
  }
  return sb.toString();
}
