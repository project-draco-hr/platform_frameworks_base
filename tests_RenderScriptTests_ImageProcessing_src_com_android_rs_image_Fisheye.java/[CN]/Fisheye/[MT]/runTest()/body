{
  if (approx) {
    if (relaxed)     mScript_approx_relaxed.forEach_root(mOutPixelsAllocation);
 else     mScript_approx_full.forEach_root(mOutPixelsAllocation);
  }
 else   if (relaxed)   mScript_relaxed.forEach_root(mOutPixelsAllocation);
 else   mScript_full.forEach_root(mOutPixelsAllocation);
}
