{
  if (deviceInfo != null && !deviceInfo.isSource()) {
    return false;
  }
  int preferredDeviceId=deviceInfo != null ? deviceInfo.getId() : 0;
  boolean status=native_setInputDevice(preferredDeviceId);
  if (status == true) {
synchronized (this) {
      mPreferredDevice=deviceInfo;
    }
  }
  return status;
}
