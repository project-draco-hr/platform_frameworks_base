{
  int deviceId=native_getRoutedDeviceId();
  if (deviceId == 0) {
    return null;
  }
  AudioDeviceInfo[] devices=AudioManager.getDevicesStatic(AudioManager.GET_DEVICES_INPUTS);
  for (int i=0; i < devices.length; i++) {
    if (devices[i].getId() == deviceId) {
      return devices[i];
    }
  }
  return null;
}
