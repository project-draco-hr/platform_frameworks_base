{
  if (mState != STATE_INITIALIZED) {
    int[] session={0};
    int[] rates={0};
    int initResult=native_setup(new WeakReference<AudioRecord>(this),null,rates,0,0,0,0,session,ActivityThread.currentOpPackageName(),nativeRecordInJavaObj);
    if (initResult != SUCCESS) {
      loge("Error code " + initResult + " when initializing native AudioRecord object.");
      return;
    }
    mSessionId=session[0];
    mState=STATE_INITIALIZED;
  }
}
