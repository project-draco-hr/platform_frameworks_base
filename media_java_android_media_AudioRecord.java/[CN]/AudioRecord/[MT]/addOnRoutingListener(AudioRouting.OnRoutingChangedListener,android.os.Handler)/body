{
  if (listener != null && !mNewRoutingChangeListeners.containsKey(listener)) {
synchronized (mNewRoutingChangeListeners) {
      testEnableNativeRoutingCallbacks();
      mNewRoutingChangeListeners.put(listener,new NativeNewRoutingEventHandlerDelegate(this,listener,handler != null ? handler : new Handler(mInitializationLooper)));
    }
  }
}
