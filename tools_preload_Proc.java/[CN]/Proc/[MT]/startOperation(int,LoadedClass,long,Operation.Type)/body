{
  Operation o=new Operation(this,loadedClass,time,operationCount++,type);
  operations.add(o);
  LinkedList<Operation> stack=stacks.get(threadId);
  if (stack == null) {
    stack=new LinkedList<Operation>();
    stacks.put(threadId,stack);
  }
  if (!stack.isEmpty()) {
    stack.getLast().subops.add(o);
  }
  stack.add(o);
}
