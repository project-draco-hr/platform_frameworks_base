{
  if (mDebug)   Slog.v(TAG,"finishInternal() mFinished = " + mFinished);
  if (!mFinished) {
    mFinished=true;
    if (mWindowToken == null) {
      Slog.w(TAG,"Finish was called before the dream was attached.");
    }
 else {
      try {
        mSandman.finishSelf(mWindowToken);
      }
 catch (      RemoteException ex) {
      }
    }
    stopSelf();
  }
}
