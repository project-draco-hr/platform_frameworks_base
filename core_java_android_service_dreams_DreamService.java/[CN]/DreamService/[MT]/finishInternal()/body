{
  if (DEBUG)   Slog.v(TAG,"finishInternal() mFinished = " + mFinished);
  if (mFinished)   return;
  try {
    mFinished=true;
    if (mSandman != null) {
      mSandman.finishSelf(mWindowToken);
    }
 else {
      Slog.w(TAG,"No dream manager found");
    }
    stopSelf();
  }
 catch (  Throwable t) {
    Slog.w(TAG,"Crashed in finishInternal()",t);
  }
}
