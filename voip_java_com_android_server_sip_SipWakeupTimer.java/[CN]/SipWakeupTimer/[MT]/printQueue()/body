{
  int count=0;
  for (  MyEvent event : mEventQueue) {
    log("     " + event + ": scheduled at "+ showTime(event.mTriggerTime)+ ": last at "+ showTime(event.mLastTriggerTime));
    if (++count >= 5)     break;
  }
  if (mEventQueue.size() > count) {
    log("     .....");
  }
 else   if (count == 0) {
    log("     <empty>");
  }
}
