{
  if (account == null) {
    Log.e(TAG,"peekAuthToken: the account must not be null");
    return null;
  }
  if (authTokenType == null) {
    return null;
  }
  try {
    return mService.peekAuthToken(account,authTokenType);
  }
 catch (  RemoteException e) {
    throw new RuntimeException(e);
  }
}
