{
  mPackageManager.setComponentEnabledSetting(ENABLED_RECEIVER_COMPONENTNAME,COMPONENT_ENABLED_STATE_DEFAULT,PackageManager.DONT_KILL_APP);
  final List<ResolveInfo> infoList=mPackageManager.queryBroadcastReceivers(mEnabledReceiverIntent,0);
  assertEquals(1,infoList.size());
  final ResolveInfo info=infoList.get(0);
  assertNotNull(info);
  assertNotNull(info.activityInfo);
  assertTrue(info.activityInfo.enabled);
}
