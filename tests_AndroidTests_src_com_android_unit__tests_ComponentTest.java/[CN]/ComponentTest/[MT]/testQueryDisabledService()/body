{
  mPackageManager.setComponentEnabledSetting(DISABLED_SERVICE_COMPONENTNAME,COMPONENT_ENABLED_STATE_DEFAULT,PackageManager.DONT_KILL_APP);
  final List<ResolveInfo> infoList=mPackageManager.queryIntentServices(mDisabledServiceIntent,0);
  assertEquals(0,infoList.size());
  final List<ResolveInfo> infoList2=mPackageManager.queryIntentServices(mDisabledServiceIntent,GET_DISABLED_COMPONENTS);
  assertEquals(1,infoList2.size());
  final ResolveInfo info=infoList2.get(0);
  assertNotNull(info);
  assertNotNull(info.serviceInfo);
  assertFalse(info.serviceInfo.enabled);
}
