{
  Handler handler=mTargetView.getHandler();
  KeyEvent[] events=KeyCharacterMap.load(KeyCharacterMap.ALPHA).getEvents(new char[]{(char)keyEventCode});
  if (events != null) {
    for (    KeyEvent event : events) {
      handler.sendMessage(handler.obtainMessage(ViewRoot.DISPATCH_KEY,event));
    }
  }
}
