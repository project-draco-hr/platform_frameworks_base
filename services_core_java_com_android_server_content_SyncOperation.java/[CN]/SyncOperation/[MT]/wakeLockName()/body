{
  if (wakeLockName != null) {
    return wakeLockName;
  }
  if (target.target_provider) {
    return (wakeLockName=target.provider + "/" + target.account.type+ "/"+ target.account.name);
  }
 else   if (target.target_service) {
    return (wakeLockName=target.service.getPackageName() + "/" + target.service.getClassName());
  }
 else {
    Log.wtf(TAG,"Invalid target getting wakelock name for operation - " + key);
    return null;
  }
}
