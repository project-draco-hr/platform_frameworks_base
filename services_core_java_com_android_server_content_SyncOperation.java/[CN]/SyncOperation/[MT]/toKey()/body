{
  StringBuilder sb=new StringBuilder();
  if (service == null) {
    sb.append("authority: ").append(authority);
    sb.append(" account {name=" + account.name + ", user="+ userId+ ", type="+ account.type+ "}");
  }
 else {
    sb.append("service {package=").append(service.getPackageName()).append(" user=").append(userId).append(", class=").append(service.getClassName()).append("}");
  }
  sb.append(" extras: ");
  extrasToStringBuilder(extras,sb);
  return sb.toString();
}
