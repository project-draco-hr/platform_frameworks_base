{
  if (!mStarted) {
    if (Config.LOGV)     Log.v(TAG,"startNavigating");
    mStarted=true;
    if (!native_start(mPositionMode,false,mFixInterval)) {
      mStarted=false;
      Log.e(TAG,"native_start failed in startNavigating()");
    }
    updateStatus(LocationProvider.TEMPORARILY_UNAVAILABLE,0);
  }
}
