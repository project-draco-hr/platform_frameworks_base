{
  if (Config.LOGD)   Log.d(TAG,"disable");
  mEnabled=false;
  stopNavigating();
  native_disable();
  if (mEventThread != null) {
    try {
      mEventThread.join();
    }
 catch (    InterruptedException e) {
      Log.w(TAG,"InterruptedException when joining mEventThread");
    }
    mEventThread=null;
  }
  native_cleanup();
}
