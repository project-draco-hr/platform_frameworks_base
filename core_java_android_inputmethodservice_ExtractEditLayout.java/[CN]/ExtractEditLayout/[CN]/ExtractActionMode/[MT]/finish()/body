{
  if (mActionMode != this) {
    return;
  }
  mCallback.onDestroyActionMode(this);
  mCallback=null;
  mMenu.close();
  mExtractActionButton.setVisibility(VISIBLE);
  mEditButton.setVisibility(INVISIBLE);
  sendAccessibilityEvent(AccessibilityEvent.TYPE_WINDOW_STATE_CHANGED);
  mActionMode=null;
}
