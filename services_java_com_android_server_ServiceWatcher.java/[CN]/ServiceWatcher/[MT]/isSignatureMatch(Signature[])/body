{
  if (signatures == null)   return false;
  HashSet<Signature> inputSet=new HashSet<Signature>();
  for (  Signature s : signatures) {
    inputSet.add(s);
  }
  for (  HashSet<Signature> referenceSet : mSignatureSets) {
    if (referenceSet.equals(inputSet)) {
      return true;
    }
  }
  return false;
}
