{
  if (mBlockMessages) {
    return;
  }
  if (what == EventHub.WEBKIT_DRAW) {
    mDrawIsScheduled=false;
  }
  if (mMessages != null) {
    Iterator<Message> iter=mMessages.iterator();
    while (iter.hasNext()) {
      Message m=iter.next();
      if (m.what == what) {
        iter.remove();
      }
    }
  }
 else {
    mHandler.removeMessages(what);
  }
}
