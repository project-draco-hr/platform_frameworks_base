{
  WebStorageClassic webStorage=WebStorageClassic.getInstance();
  Collection<WebStorage.Origin> origins=webStorage.getOriginsSync();
  if (origins == null) {
    return 0;
  }
  long usedQuota=0;
  for (  WebStorage.Origin website : origins) {
    usedQuota+=website.getQuota();
  }
  return usedQuota;
}
