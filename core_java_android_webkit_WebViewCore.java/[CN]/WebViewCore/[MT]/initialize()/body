{
  mBrowserFrame=new BrowserFrame(mContext,this,mCallbackProxy,mSettings);
  mSettings.syncSettingsAndCreateHandler(mBrowserFrame);
  WebIconDatabase.getInstance().createHandler();
  WebStorage.getInstance().createHandler();
  mEventHub.transferMessages();
  if (mWebView != null) {
    Message.obtain(mWebView.mPrivateHandler,WebView.WEBCORE_INITIALIZED_MSG_ID,mNativeClass,0).sendToTarget();
  }
}
