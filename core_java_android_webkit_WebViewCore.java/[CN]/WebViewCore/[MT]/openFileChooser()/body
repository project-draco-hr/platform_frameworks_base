{
  Uri uri=mCallbackProxy.openFileChooser();
  if (uri == null)   return "";
  Cursor cursor=mContext.getContentResolver().query(uri,new String[]{OpenableColumns.DISPLAY_NAME},null,null,null);
  String name="";
  if (cursor != null) {
    try {
      if (cursor.moveToNext()) {
        name=cursor.getString(0);
      }
    }
  finally {
      cursor.close();
    }
  }
  return uri.toString() + "/" + name;
}
