{
  if (mWebView == null) {
    return;
  }
  PluginFullScreenData data=new PluginFullScreenData();
  data.mView=childView.mView;
  data.mNpp=npp;
  data.mDocX=x;
  data.mDocY=y;
  data.mDocWidth=width;
  data.mDocHeight=height;
  mWebView.mPrivateHandler.obtainMessage(WebView.SHOW_FULLSCREEN,data).sendToTarget();
}
