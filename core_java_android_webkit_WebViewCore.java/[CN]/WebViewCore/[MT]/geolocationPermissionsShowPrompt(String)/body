{
  mCallbackProxy.onGeolocationPermissionsShowPrompt(origin,new GeolocationPermissions.Callback(){
    @Override public void invoke(    String origin,    boolean allow,    boolean remember){
      GeolocationPermissionsData data=new GeolocationPermissionsData();
      data.mOrigin=origin;
      data.mAllow=allow;
      data.mRemember=remember;
      sendMessage(EventHub.GEOLOCATION_PERMISSIONS_PROVIDE,data);
    }
  }
);
}
