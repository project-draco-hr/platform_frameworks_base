{
  Uri uri=mCallbackProxy.openFileChooser(acceptType);
  if (uri != null) {
    String fileName="";
    Cursor cursor=mContext.getContentResolver().query(uri,new String[]{OpenableColumns.DISPLAY_NAME},null,null,null);
    if (cursor != null) {
      try {
        if (cursor.moveToNext()) {
          fileName=cursor.getString(0);
        }
      }
  finally {
        cursor.close();
      }
    }
 else {
      fileName=uri.getLastPathSegment();
    }
    String uriString=uri.toString();
    BrowserFrame.sJavaBridge.storeFileNameForContentUri(fileName,uriString);
    return uriString;
  }
  return "";
}
