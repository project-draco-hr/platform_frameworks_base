{
  if (mWebViewClassic == null) {
    return;
  }
  Message.obtain(mWebViewClassic.mPrivateHandler,WebViewClassic.INIT_EDIT_FIELD,initData).sendToTarget();
  Message.obtain(mWebViewClassic.mPrivateHandler,WebViewClassic.REQUEST_KEYBOARD_WITH_SELECTION_MSG_ID,initData.mFieldPointer,0,new TextSelectionData(start,end,selectionPtr)).sendToTarget();
}
