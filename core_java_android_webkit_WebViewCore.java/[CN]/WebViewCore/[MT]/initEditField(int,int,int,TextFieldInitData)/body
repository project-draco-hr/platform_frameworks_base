{
  if (mWebView == null) {
    return;
  }
  Message.obtain(mWebView.mPrivateHandler,WebViewClassic.INIT_EDIT_FIELD,initData).sendToTarget();
  Message.obtain(mWebView.mPrivateHandler,WebViewClassic.REQUEST_KEYBOARD_WITH_SELECTION_MSG_ID,initData.mFieldPointer,0,new TextSelectionData(start,end,selectionPtr)).sendToTarget();
}
