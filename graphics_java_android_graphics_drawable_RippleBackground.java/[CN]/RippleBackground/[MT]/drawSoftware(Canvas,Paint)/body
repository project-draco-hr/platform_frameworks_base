{
  boolean hasContent=false;
  final int paintAlpha=p.getAlpha();
  final int alpha=(int)(paintAlpha * mOuterOpacity + 0.5f);
  final float radius=mOuterRadius;
  if (alpha > 0 && radius > 0) {
    p.setAlpha(alpha);
    c.drawCircle(mOuterX,mOuterY,radius,p);
    p.setAlpha(paintAlpha);
    hasContent=true;
  }
  return hasContent;
}
