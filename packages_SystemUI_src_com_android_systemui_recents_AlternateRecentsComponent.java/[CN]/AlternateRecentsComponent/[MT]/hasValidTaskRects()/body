{
  return mSingleCountFirstTaskRect != null && mSingleCountFirstTaskRect.width() > 0 && mSingleCountFirstTaskRect.height() > 0 && mTwoCountFirstTaskRect != null && mTwoCountFirstTaskRect.width() > 0 && mTwoCountFirstTaskRect.height() > 0 && mMultipleCountFirstTaskRect != null && mMultipleCountFirstTaskRect.width() > 0 && mMultipleCountFirstTaskRect.height() > 0;
