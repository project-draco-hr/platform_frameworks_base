{
  if (Console.Enabled) {
    Console.logStartTracingTime(Constants.Log.App.TimeRecentsStartup,Constants.Log.App.TimeRecentsStartupKey);
    Console.logStartTracingTime(Constants.Log.App.TimeRecentsLaunchTask,Constants.Log.App.TimeRecentsLaunchKey);
    Console.log(Constants.Log.App.RecentsComponent,"[RecentsComponent|toggleRecents]","serviceIsBound: " + mServiceIsBound);
  }
  mStatusBarView=statusBarView;
  mTriggeredFromAltTab=false;
  if (!mServiceIsBound) {
    bindToRecentsService(true);
    return;
  }
  try {
    toggleRecentsActivity();
  }
 catch (  ActivityNotFoundException e) {
    Console.logRawError("Failed to launch RecentAppsIntent",e);
  }
}
