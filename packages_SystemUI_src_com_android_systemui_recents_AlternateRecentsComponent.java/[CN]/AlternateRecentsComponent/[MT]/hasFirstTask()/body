{
  SystemServicesProxy ssp=mSystemServicesProxy;
  List<ActivityManager.RecentTaskInfo> tasks=ssp.getRecentTasks(1,UserHandle.CURRENT.getIdentifier());
  for (  ActivityManager.RecentTaskInfo t : tasks) {
    if (ssp.isInHomeStack(t.persistentId)) {
      continue;
    }
    return true;
  }
  return false;
}
