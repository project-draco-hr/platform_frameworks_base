{
  Slog.v(TAG,"Move " + moveId + " status "+ status);
  final SomeArgs args=SomeArgs.obtain();
  args.argi1=moveId;
  args.arg2=moveTitle;
  args.argi3=status;
  args.arg4=estMillis;
  obtainMessage(MSG_STATUS_CHANGED,args).sendToTarget();
synchronized (mLastStatus) {
    mLastStatus.put(moveId,status);
  }
}
