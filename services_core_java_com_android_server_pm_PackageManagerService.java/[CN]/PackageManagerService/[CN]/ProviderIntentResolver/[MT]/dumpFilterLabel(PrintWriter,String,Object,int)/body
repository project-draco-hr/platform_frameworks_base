{
  PackageParser.Provider provider=(PackageParser.Provider)label;
  out.print(prefix);
  out.print(Integer.toHexString(System.identityHashCode(provider)));
  out.print(' ');
  provider.printComponentShortName(out);
  if (count > 1) {
    out.print(" (");
    out.print(count);
    out.print(" filters)");
  }
  out.println();
}
