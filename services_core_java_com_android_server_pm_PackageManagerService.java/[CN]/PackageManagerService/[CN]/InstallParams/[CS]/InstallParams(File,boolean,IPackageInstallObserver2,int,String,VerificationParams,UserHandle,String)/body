{
  super(user);
  this.originFile=Preconditions.checkNotNull(originFile);
  this.originStaged=originStaged;
  this.observer=observer;
  this.flags=flags;
  this.installerPackageName=installerPackageName;
  this.verificationParams=verificationParams;
  this.packageAbiOverride=packageAbiOverride;
  this.packageInstructionSetOverride=(packageAbiOverride == null) ? packageAbiOverride : VMRuntime.getInstructionSet(packageAbiOverride);
}
