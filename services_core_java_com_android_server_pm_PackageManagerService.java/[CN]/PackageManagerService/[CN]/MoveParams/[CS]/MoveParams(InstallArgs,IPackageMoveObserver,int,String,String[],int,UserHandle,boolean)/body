{
  super(user);
  this.srcArgs=srcArgs;
  this.observer=observer;
  this.flags=flags;
  this.packageName=packageName;
  this.uid=uid;
  if (srcArgs != null) {
    final String codePath=srcArgs.getCodePath();
    targetArgs=createInstallArgsForMoveTarget(codePath,flags,packageName,instructionSets,isMultiArch);
  }
 else {
    targetArgs=null;
  }
}
