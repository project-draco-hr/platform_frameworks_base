{
  final File codeFile=new File(Environment.getDataAppDirectory(volumeUuid),move.dataAppName);
  Slog.d(TAG,"Cleaning up " + move.packageName + " on "+ volumeUuid);
synchronized (mInstallLock) {
    try {
      mInstaller.destroyAppData(volumeUuid,move.packageName,UserHandle.USER_ALL,StorageManager.FLAG_STORAGE_DE | StorageManager.FLAG_STORAGE_CE,0);
    }
 catch (    InstallerException e) {
      Slog.w(TAG,String.valueOf(e));
    }
    removeCodePathLI(codeFile);
  }
  return true;
}
