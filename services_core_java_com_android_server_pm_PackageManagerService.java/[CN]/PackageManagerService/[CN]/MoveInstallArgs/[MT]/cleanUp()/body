{
  if (codeFile == null || !codeFile.exists()) {
    return false;
  }
  if (codeFile.isDirectory()) {
    mInstaller.rmPackageDir(codeFile.getAbsolutePath());
  }
 else {
    codeFile.delete();
  }
  if (resourceFile != null && !FileUtils.contains(codeFile,resourceFile)) {
    resourceFile.delete();
  }
  return true;
}
