{
  if (temp) {
    createCopyFile();
  }
 else {
    PackageHelper.destroySdDir(cid);
  }
  final String newCachePath=imcs.copyResourceToContainer(fromFile.getAbsolutePath(),cid,getEncryptKey(),RES_FILE_NAME,PUBLIC_RES_FILE_NAME,isExternal(),isFwdLocked(),abiOverride);
  if (newCachePath != null) {
    setCachePath(newCachePath);
    return PackageManager.INSTALL_SUCCEEDED;
  }
 else {
    return PackageManager.INSTALL_FAILED_CONTAINER_ERROR;
  }
}
