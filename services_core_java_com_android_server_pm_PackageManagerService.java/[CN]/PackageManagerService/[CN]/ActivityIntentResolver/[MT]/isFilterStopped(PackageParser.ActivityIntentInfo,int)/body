{
  if (!sUserManager.exists(userId))   return true;
  PackageParser.Package p=filter.activity.owner;
  if (p != null) {
    PackageSetting ps=(PackageSetting)p.mExtras;
    if (ps != null) {
      return (ps.pkgFlags & ApplicationInfo.FLAG_SYSTEM) == 0 && ps.getStopped(userId);
    }
  }
  return false;
}
