{
  PackageParser.Activity activity=(PackageParser.Activity)label;
  out.print(prefix);
  out.print(Integer.toHexString(System.identityHashCode(activity)));
  out.print(' ');
  activity.printComponentShortName(out);
  if (count > 1) {
    out.print(" (");
    out.print(count);
    out.print(" filters)");
  }
  out.println();
}
