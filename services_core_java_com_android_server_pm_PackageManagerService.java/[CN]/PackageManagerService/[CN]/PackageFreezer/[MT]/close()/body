{
  mCloseGuard.close();
  if (mClosed.compareAndSet(false,true)) {
synchronized (mPackages) {
      if (mWeFroze) {
        mFrozenPackages.remove(mPackageName);
      }
      if (mChildren != null) {
        for (        PackageFreezer freezer : mChildren) {
          freezer.close();
        }
      }
    }
  }
}
