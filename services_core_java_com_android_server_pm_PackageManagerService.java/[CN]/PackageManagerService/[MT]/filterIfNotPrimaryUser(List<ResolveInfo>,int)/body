{
  if (userId == UserHandle.USER_OWNER) {
    return resolveInfos;
  }
  for (int i=resolveInfos.size() - 1; i >= 0; i--) {
    ResolveInfo info=resolveInfos.get(i);
    if ((info.activityInfo.flags & ActivityInfo.FLAG_PRIMARY_USER_ONLY) != 0) {
      resolveInfos.remove(i);
    }
  }
  return resolveInfos;
}
