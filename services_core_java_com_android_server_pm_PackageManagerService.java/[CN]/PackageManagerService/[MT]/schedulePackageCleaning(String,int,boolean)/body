{
  final Message msg=mHandler.obtainMessage(START_CLEANING_PACKAGE,userId,andCode ? 1 : 0,packageName);
  if (mSystemReady) {
    msg.sendToTarget();
  }
 else {
    if (mPostSystemReadyMessages == null) {
      mPostSystemReadyMessages=new ArrayList<>();
    }
    mPostSystemReadyMessages.add(msg);
  }
}
