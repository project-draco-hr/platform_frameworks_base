{
  if (outInfo != null) {
    outInfo.uid=ps.appId;
  }
  removePackageDataLI(ps,allUserHandles,perUserInstalled,outInfo,flags,writeSettings);
  if (deleteCodeAndResources && (outInfo != null)) {
    outInfo.args=createInstallArgsForExisting(packageFlagsToInstallFlags(ps),ps.codePathString,ps.resourcePathString,ps.legacyNativeLibraryPathString,getAppDexInstructionSets(ps),isMultiArch(ps));
    if (DEBUG_SD_INSTALL)     Slog.i(TAG,"args=" + outInfo.args);
  }
  return true;
}
