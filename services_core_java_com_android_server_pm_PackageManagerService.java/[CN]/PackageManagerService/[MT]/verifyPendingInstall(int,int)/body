{
  mContext.enforceCallingOrSelfPermission(android.Manifest.permission.PACKAGE_VERIFICATION_AGENT,"Only package verification agents can verify applications");
  final Message msg=mHandler.obtainMessage(PACKAGE_VERIFIED);
  final PackageVerificationResponse response=new PackageVerificationResponse(verificationCode,Binder.getCallingUid());
  msg.arg1=id;
  msg.obj=response;
  mHandler.sendMessage(msg);
}
