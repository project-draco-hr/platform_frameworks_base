{
  boolean hasHTTPorHTTPS=filter.hasDataScheme(IntentFilter.SCHEME_HTTP) || filter.hasDataScheme(IntentFilter.SCHEME_HTTPS);
  if (!hasHTTPorHTTPS) {
    if (DEBUG_DOMAIN_VERIFICATION)     Slog.d(TAG,"IntentFilter does not contain any HTTP or HTTPS data scheme");
    return false;
  }
  return true;
}
