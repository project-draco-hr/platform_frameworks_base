{
  final VerificationParams verifParams=new VerificationParams(null,params.originatingUri,params.referrerUri,installerUid,null);
  final Message msg=mHandler.obtainMessage(INIT_COPY);
  final OriginInfo origin=new OriginInfo(stagedDir,stagedCid,true);
  msg.obj=new InstallParams(origin,observer,params.installFlags,installerPackageName,verifParams,user,params.abiOverride);
  mHandler.sendMessage(msg);
}
