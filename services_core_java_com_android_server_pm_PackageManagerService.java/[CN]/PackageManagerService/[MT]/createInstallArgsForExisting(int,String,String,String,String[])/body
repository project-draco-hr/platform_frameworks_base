{
  final boolean isInAsec;
  if (installOnSd(flags)) {
    isInAsec=true;
  }
 else   if (installForwardLocked(flags) && !codePath.startsWith(mDrmAppPrivateInstallDir.getAbsolutePath())) {
    isInAsec=true;
  }
 else {
    isInAsec=false;
  }
  if (isInAsec) {
    return new AsecInstallArgs(codePath,instructionSets,installOnSd(flags),installForwardLocked(flags));
  }
 else {
    return new FileInstallArgs(codePath,resourcePath,nativeLibraryRoot,instructionSets);
  }
}
