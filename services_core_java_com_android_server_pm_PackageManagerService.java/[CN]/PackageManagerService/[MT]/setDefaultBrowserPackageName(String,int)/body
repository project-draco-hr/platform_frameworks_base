{
  mContext.enforceCallingOrSelfPermission(android.Manifest.permission.SET_PREFERRED_APPLICATIONS,null);
synchronized (mPackages) {
    boolean result=mSettings.setDefaultBrowserPackageNameLPw(packageName,userId);
    if (packageName != null) {
      result|=updateIntentVerificationStatus(packageName,PackageManager.INTENT_FILTER_DOMAIN_VERIFICATION_STATUS_ALWAYS,UserHandle.myUserId());
    }
    return result;
  }
}
