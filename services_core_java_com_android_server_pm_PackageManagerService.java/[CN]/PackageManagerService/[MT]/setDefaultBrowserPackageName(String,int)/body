{
synchronized (mPackages) {
    boolean result=mSettings.setDefaultBrowserPackageNameLPr(packageName,userId);
    if (packageName != null) {
      result|=updateIntentVerificationStatus(packageName,PackageManager.INTENT_FILTER_DOMAIN_VERIFICATION_STATUS_ALWAYS,UserHandle.myUserId());
    }
    return result;
  }
}
