{
  mContext.enforceCallingOrSelfPermission(android.Manifest.permission.INTERACT_ACROSS_USERS_FULL,null);
  if (filter.countActions() == 0) {
    Slog.w(TAG,"Cannot set a forwarding intent filter with no filter actions");
    return;
  }
synchronized (mPackages) {
    mSettings.editForwardingIntentResolverLPw(userIdOrig).addFilter(new ForwardingIntentFilter(filter,removable,userIdDest));
    mSettings.writePackageRestrictionsLPr(userIdOrig);
  }
}
