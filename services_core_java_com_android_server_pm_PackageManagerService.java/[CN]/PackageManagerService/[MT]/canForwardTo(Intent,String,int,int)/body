{
  mContext.enforceCallingOrSelfPermission(android.Manifest.permission.INTERACT_ACROSS_USERS_FULL,null);
  List<ForwardingIntentFilter> matches=getMatchingForwardingIntentFilters(intent,resolvedType,userIdFrom);
  if (matches != null) {
    int size=matches.size();
    for (int i=0; i < size; i++) {
      if (matches.get(i).getUserIdDest() == userIdDest)       return true;
    }
  }
  return false;
}
