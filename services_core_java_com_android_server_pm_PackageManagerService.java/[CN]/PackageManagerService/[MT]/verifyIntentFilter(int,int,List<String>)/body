{
  mContext.enforceCallingOrSelfPermission(Manifest.permission.INTENT_FILTER_VERIFICATION_AGENT,"Only intentfilter verification agents can verify applications");
  final Message msg=mHandler.obtainMessage(INTENT_FILTER_VERIFIED);
  final IntentFilterVerificationResponse response=new IntentFilterVerificationResponse(Binder.getCallingUid(),verificationCode,outFailedDomains);
  msg.arg1=id;
  msg.obj=response;
  mHandler.sendMessage(msg);
}
