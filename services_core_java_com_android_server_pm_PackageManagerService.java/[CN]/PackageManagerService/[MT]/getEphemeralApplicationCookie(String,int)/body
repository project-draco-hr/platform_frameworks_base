{
  if (DISABLE_EPHEMERAL_APPS) {
    return null;
  }
  enforceCrossUserPermission(Binder.getCallingUid(),userId,true,false,"getCookie");
  if (!isCallerSameApp(packageName)) {
    return null;
  }
synchronized (mPackages) {
    return mEphemeralApplicationRegistry.getEphemeralApplicationCookieLPw(packageName,userId);
  }
}
