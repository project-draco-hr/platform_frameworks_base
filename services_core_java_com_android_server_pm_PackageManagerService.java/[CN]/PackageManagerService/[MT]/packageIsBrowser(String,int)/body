{
  List<ResolveInfo> list=queryIntentActivities(sBrowserIntent,null,PackageManager.MATCH_ALL,userId);
  final int N=list.size();
  for (int i=0; i < N; i++) {
    ResolveInfo info=list.get(i);
    if (packageName.equals(info.activityInfo.packageName)) {
      return true;
    }
  }
  return false;
}
