{
synchronized (mPackages) {
    PackageParser.Activity a=mActivities.mActivities.get(component);
    if (a == null) {
      return false;
    }
    for (int i=0; i < a.intents.size(); i++) {
      if (a.intents.get(i).match(intent.getAction(),resolvedType,intent.getScheme(),intent.getData(),intent.getCategories(),TAG) >= 0) {
        return true;
      }
    }
    return false;
  }
}
