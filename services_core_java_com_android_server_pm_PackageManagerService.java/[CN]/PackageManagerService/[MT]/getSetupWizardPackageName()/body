{
  final Intent intent=new Intent(Intent.ACTION_MAIN);
  intent.addCategory(Intent.CATEGORY_SETUP_WIZARD);
  final List<ResolveInfo> matches=queryIntentActivitiesInternal(intent,null,MATCH_SYSTEM_ONLY | MATCH_DISABLED_COMPONENTS,UserHandle.myUserId());
  if (matches.size() == 1) {
    return matches.get(0).getComponentInfo().packageName;
  }
 else {
    Slog.e(TAG,"There should probably be exactly one setup wizard; found " + matches.size() + ": matches="+ matches);
    return null;
  }
}
