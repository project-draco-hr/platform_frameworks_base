{
  int[] users=sUserManager.getUserIds();
  int res=mInstaller.install(volumeUuid,packageName,uid,uid,seinfo);
  if (res < 0) {
    return res;
  }
  for (  int user : users) {
    if (user != 0) {
      res=mInstaller.createUserData(volumeUuid,packageName,UserHandle.getUid(user,uid),user,seinfo);
      if (res < 0) {
        return res;
      }
    }
  }
  return res;
}
