{
  final File codeFile=new File(codePath);
  if (installOnSd(flags) || installForwardLocked(flags)) {
    String cid=getNextCodePath(codePath,pkgName,"/" + AsecInstallArgs.RES_FILE_NAME);
    return new AsecInstallArgs(codeFile,cid,instructionSets,installOnSd(flags),installForwardLocked(flags));
  }
 else {
    return new FileInstallArgs(codeFile,instructionSets);
  }
}
