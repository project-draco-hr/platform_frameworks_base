{
  updateSettingsInternalLI(newPackage,installerPackageName,allUsers,perUserInstalled,res,user);
  final int childCount=(newPackage.childPackages != null) ? newPackage.childPackages.size() : 0;
  for (int i=0; i < childCount; i++) {
    PackageParser.Package childPackage=newPackage.childPackages.get(i);
    updateSettingsInternalLI(childPackage,installerPackageName,allUsers,perUserInstalled,res,user);
  }
}
