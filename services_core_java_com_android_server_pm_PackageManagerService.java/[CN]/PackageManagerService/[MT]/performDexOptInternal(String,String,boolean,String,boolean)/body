{
  PackageParser.Package p;
  final String targetInstructionSet;
synchronized (mPackages) {
    p=mPackages.get(packageName);
    if (p == null) {
      return PackageDexOptimizer.DEX_OPT_FAILED;
    }
    mPackageUsage.write(false);
    targetInstructionSet=instructionSet != null ? instructionSet : getPrimaryInstructionSet(p.applicationInfo);
  }
  long callingId=Binder.clearCallingIdentity();
  try {
synchronized (mInstallLock) {
      final String[] instructionSets=new String[]{targetInstructionSet};
      return performDexOptInternalWithDependenciesLI(p,instructionSets,checkProfiles,targetCompilerFilter,force);
    }
  }
  finally {
    Binder.restoreCallingIdentity(callingId);
  }
}
