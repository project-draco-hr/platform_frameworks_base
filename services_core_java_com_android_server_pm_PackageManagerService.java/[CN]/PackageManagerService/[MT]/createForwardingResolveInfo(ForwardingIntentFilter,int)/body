{
  String className;
  int userIdDest=fif.getUserIdDest();
  if (userIdDest == UserHandle.USER_OWNER) {
    className=FORWARD_INTENT_TO_USER_OWNER;
  }
 else {
    className=FORWARD_INTENT_TO_MANAGED_PROFILE;
  }
  ComponentName forwardingActivityComponentName=new ComponentName(mAndroidApplication.packageName,className);
  ActivityInfo forwardingActivityInfo=getActivityInfo(forwardingActivityComponentName,0,userIdFrom);
  ResolveInfo forwardingResolveInfo=new ResolveInfo();
  forwardingResolveInfo.activityInfo=forwardingActivityInfo;
  forwardingResolveInfo.priority=0;
  forwardingResolveInfo.preferredOrder=0;
  forwardingResolveInfo.match=0;
  forwardingResolveInfo.isDefault=true;
  forwardingResolveInfo.filter=fif;
  return forwardingResolveInfo;
}
