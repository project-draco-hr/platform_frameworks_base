{
  PackageSetting ps=mSettings.mPackages.get(packageName);
  if (ps == null) {
    return;
  }
  if (!ps.isAnyInstalled(sUserManager.getUserIds())) {
    mHandler.post(new Runnable(){
      public void run(){
        deletePackageX(packageName,0,PackageManager.DELETE_ALL_USERS);
      }
    }
);
  }
}
