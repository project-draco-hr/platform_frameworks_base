{
  int dBm;
  if (isGsm()) {
    dBm=getLteDbm();
    if (dBm == INVALID) {
      dBm=getGsmDbm();
    }
  }
 else {
    int cdmaDbm=getCdmaDbm();
    int evdoDbm=getEvdoDbm();
    return (evdoDbm == -120) ? cdmaDbm : ((cdmaDbm == -120) ? evdoDbm : (cdmaDbm < evdoDbm ? cdmaDbm : evdoDbm));
  }
  if (DBG)   log("getDbm=" + dBm);
  return dBm;
}
