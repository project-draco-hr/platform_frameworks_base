{
  int dBm;
  if (isGsm()) {
    if ((mLteSignalStrength == -1) && (mLteRsrp == -1) && (mLteRsrq == -1)&& (mLteRssnr == INVALID_SNR)&& (mLteCqi == -1)) {
      dBm=getGsmDbm();
    }
 else {
      dBm=getLteDbm();
    }
  }
 else {
    dBm=getCdmaDbm();
  }
  if (DBG)   log("getDbm=" + dBm);
  return dBm;
}
