{
  if (isEmpty() || rw <= 0.0 || rh <= 0.0) {
    return false;
  }
  double cx=getX() + getWidth() / 2.0;
  double cy=getY() + getHeight() / 2.0;
  double rx1=rx;
  double ry1=ry;
  double rx2=rx + rw;
  double ry2=ry + rh;
  double nx=cx < rx1 ? rx1 : (cx > rx2 ? rx2 : cx);
  double ny=cy < ry1 ? ry1 : (cy > ry2 ? ry2 : cy);
  return contains(nx,ny);
}
