{
  mTestedSm=new TetherInterfaceStateMachine(IFACE_NAME,mLooper.getLooper(),ConnectivityManager.TETHERING_BLUETOOTH,mNMService,mStatsService,mTetherHelper);
  mTestedSm.start();
  mLooper.dispatchAll();
  verify(mTetherHelper).notifyInterfaceStateChange(IFACE_NAME,mTestedSm,STATE_AVAILABLE,TETHER_ERROR_NO_ERROR);
  verifyNoMoreInteractions(mTetherHelper,mNMService,mStatsService);
}
