{
  try {
    File stressImagesDirectory=new File(String.format("%s/%s",Environment.getExternalStorageDirectory(),CAMERA_STRESS_IMAGES_DIRECTORY));
    File[] stressImages=stressImagesDirectory.listFiles();
    for (    File f : stressImages) {
      f.delete();
    }
  }
 catch (  SecurityException e) {
    Log.e(TAG,"Security manager access violation: " + e.toString());
  }
}
