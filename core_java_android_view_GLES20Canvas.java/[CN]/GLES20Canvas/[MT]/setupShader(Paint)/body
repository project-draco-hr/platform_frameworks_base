      nSetupBitmapShader(mRenderer,bs.native_instance,bs.mBitmap.mNativeBitmap,bs.mTileX,bs.mTileY,bs.mLocalMatrix);
      return true;
    }
 else     if (shader instanceof LinearGradient) {
