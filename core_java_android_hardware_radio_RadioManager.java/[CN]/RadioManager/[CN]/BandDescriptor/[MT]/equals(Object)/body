{
  if (this == obj)   return true;
  if (!(obj instanceof BandDescriptor))   return false;
  BandDescriptor other=(BandDescriptor)obj;
  if (mRegion != other.getRegion())   return false;
  if (mType != other.getType())   return false;
  if (mLowerLimit != other.getLowerLimit())   return false;
  if (mUpperLimit != other.getUpperLimit())   return false;
  if (mSpacing != other.getSpacing())   return false;
  return true;
}
