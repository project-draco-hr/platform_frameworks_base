{
  begin("glGetFramebufferAttachmentParameterivOES");
  arg("target",target);
  arg("attachment",attachment);
  arg("pname",pname);
  arg("params",params.toString());
  arg("offset",offset);
  end();
  mgl11ExtensionPack.glGetFramebufferAttachmentParameterivOES(target,attachment,pname,params,offset);
  checkError();
}
