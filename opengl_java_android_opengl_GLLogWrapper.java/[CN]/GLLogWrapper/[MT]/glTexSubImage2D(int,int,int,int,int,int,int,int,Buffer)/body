{
  begin("glTexSubImage2D");
  arg("target",getTextureTarget(target));
  arg("level",level);
  arg("xoffset",xoffset);
  arg("yoffset",yoffset);
  arg("width",width);
  arg("height",height);
  arg("format",format);
  arg("type",type);
  arg("pixels",pixels.toString());
  end();
  mgl.glTexSubImage2D(target,level,xoffset,yoffset,width,height,format,type,pixels);
  checkError();
}
