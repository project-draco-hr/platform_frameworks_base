{
  begin("glTexImage2D");
  arg("target",target);
  arg("level",level);
  arg("internalformat",internalformat);
  arg("width",width);
  arg("height",height);
  arg("border",border);
  arg("format",format);
  arg("type",type);
  arg("pixels",pixels.toString());
  end();
  mgl.glTexImage2D(target,level,internalformat,width,height,border,format,type,pixels);
  checkError();
}
