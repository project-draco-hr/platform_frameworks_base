{
  begin("glCopyTexImage2D");
  arg("target",getTextureTarget(target));
  arg("level",level);
  arg("internalformat",internalformat);
  arg("x",x);
  arg("y",y);
  arg("width",width);
  arg("height",height);
  arg("border",border);
  end();
  mgl.glCopyTexImage2D(target,level,internalformat,x,y,width,height,border);
  checkError();
}
