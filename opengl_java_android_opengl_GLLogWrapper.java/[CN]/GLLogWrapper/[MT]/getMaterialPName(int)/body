{
switch (pname) {
case GL_AMBIENT:
    return "GL_AMBIENT";
case GL_DIFFUSE:
  return "GL_DIFFUSE";
case GL_SPECULAR:
return "GL_SPECULAR";
case GL_EMISSION:
return "GL_EMISSION";
case GL_SHININESS:
return "GL_SHININESS";
case GL_AMBIENT_AND_DIFFUSE:
return "GL_AMBIENT_AND_DIFFUSE";
default :
return getHex(pname);
}
}
