{
  int iparam=(int)param;
  if (param == (float)iparam) {
switch (iparam) {
case GL_REPLACE:
      return "GL_REPLACE";
case GL_MODULATE:
    return "GL_MODULATE";
case GL_DECAL:
  return "GL_DECAL";
case GL_BLEND:
return "GL_BLEND";
case GL_ADD:
return "GL_ADD";
case GL_COMBINE:
return "GL_COMBINE";
default :
return getHex(iparam);
}
}
return Float.toString(param);
}
