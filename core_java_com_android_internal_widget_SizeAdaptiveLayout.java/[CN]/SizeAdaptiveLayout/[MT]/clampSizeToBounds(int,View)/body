{
  SizeAdaptiveLayout.LayoutParams lp=(SizeAdaptiveLayout.LayoutParams)child.getLayoutParams();
  int heightIn=View.MEASURED_SIZE_MASK & measuredHeight;
  int height=Math.max(heightIn,lp.minHeight);
  if (lp.maxHeight != SizeAdaptiveLayout.LayoutParams.UNBOUNDED) {
    height=Math.min(height,lp.maxHeight);
  }
  if (DEBUG && heightIn != height) {
    Log.d(TAG,this + "child view " + child+ " "+ "measured out of bounds at "+ heightIn+ "px "+ "clamped to "+ height+ "px");
  }
  return height;
}
