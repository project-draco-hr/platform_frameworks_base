{
  final String[] selectionArgs=new String[]{syncId,account};
  Cursor c=mDb.query(mTable,getDeleteRowProjection(),SELECT_BY_ID_AND_ACCOUNT,selectionArgs,null,null,null);
  try {
    c.moveToFirst();
    while (!c.isAfterLast()) {
      deleteRow(c);
      syncResult.stats.numDeletes++;
    }
  }
  finally {
    c.deactivate();
  }
  if (mDeletedTable != null) {
    mDb.delete(mDeletedTable,SELECT_BY_ID_AND_ACCOUNT,selectionArgs);
  }
}
