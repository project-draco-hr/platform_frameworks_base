{
  assert(enStr != null);
  int charset=enStr.getCharacterSet();
  byte[] textString=enStr.getTextString();
  if (null == textString) {
    return;
  }
  mStack.newbuf();
  PositionMarker start=mStack.mark();
  appendShortInteger(charset);
  appendTextString(textString);
  int len=start.getLength();
  mStack.pop();
  appendValueLength(len);
  mStack.copy();
}
