{
  if (keyType == null) {
    throw new NullPointerException("keyType == null");
  }
 else {
    try {
      KeyStore.getKeyTypeForAlgorithm(keyType);
    }
 catch (    IllegalArgumentException e) {
      throw new NoSuchAlgorithmException("Unsupported key type: " + keyType);
    }
  }
  mKeyType=keyType;
  return this;
}
