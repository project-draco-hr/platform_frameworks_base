{
  if (redrawManager == null) {
    return;
  }
  Rectangle clipRect=event.getUpdateRect();
  if ((clipRect.width <= 0) || (clipRect.height <= 0)) {
    return;
  }
  Graphics g=getGraphics();
  if (g == null) {
    return;
  }
  initGraphics(g,event);
  if (!getIgnoreRepaint()) {
    if (event.getID() == PaintEvent.PAINT) {
      paint(g);
    }
 else {
      update(g);
    }
  }
  g.dispose();
}
