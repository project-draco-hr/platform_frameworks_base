{
  InputContext ic=getInputContext();
  if (ic == null) {
    return false;
  }
  int id=e.getID();
  boolean isInputEvent=((id >= KeyEvent.KEY_FIRST) && (id <= KeyEvent.KEY_LAST)) || ((id >= MouseEvent.MOUSE_FIRST) && (id <= MouseEvent.MOUSE_LAST));
  if (((id >= FocusEvent.FOCUS_FIRST) && (id <= FocusEvent.FOCUS_LAST)) || isInputEvent) {
    ic.dispatchEvent(e);
  }
  return e.isConsumed();
}
