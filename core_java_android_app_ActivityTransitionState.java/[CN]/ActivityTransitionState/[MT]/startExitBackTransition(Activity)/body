{
  if (mEnteringNames == null) {
    return false;
  }
 else {
    if (!mHasExited) {
      mHasExited=true;
      if (mEnterTransitionCoordinator != null) {
        mEnterTransitionCoordinator.stop();
        mEnterTransitionCoordinator=null;
      }
      ArrayMap<String,View> sharedElements=new ArrayMap<String,View>();
      activity.getWindow().getDecorView().findNamedViews(sharedElements);
      ExitTransitionCoordinator exitCoordinator=new ExitTransitionCoordinator(activity,mEnteringNames,mEnteringFrom,mEnteringTo,true);
      exitCoordinator.startExit(activity.mResultCode,activity.mResultData);
    }
    return true;
  }
}
