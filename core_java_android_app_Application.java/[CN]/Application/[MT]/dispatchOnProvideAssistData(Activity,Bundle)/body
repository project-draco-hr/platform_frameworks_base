{
  Object[] callbacks;
synchronized (this) {
    if (mAssistCallbacks == null) {
      return;
    }
    callbacks=mAssistCallbacks.toArray();
  }
  if (callbacks != null) {
    for (int i=0; i < callbacks.length; i++) {
      ((OnProvideAssistDataListener)callbacks[i]).onProvideAssistData(activity,data);
    }
  }
}
