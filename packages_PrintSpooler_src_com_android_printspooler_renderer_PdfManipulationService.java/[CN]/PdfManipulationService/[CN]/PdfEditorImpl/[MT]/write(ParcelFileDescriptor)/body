{
synchronized (mLock) {
    try {
      throwIfNotOpened();
      if (DEBUG) {
        Log.i(LOG_TAG,"write()");
      }
      mEditor.write(destination);
    }
 catch (    IOException|IllegalStateException e) {
      IoUtils.closeQuietly(destination);
      Log.e(LOG_TAG,"Error writing PDF to file.",e);
      throw new RemoteException(e.toString());
    }
  }
}
