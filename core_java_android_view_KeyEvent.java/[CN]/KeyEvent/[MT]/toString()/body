{
  return "KeyEvent{action=" + actionToString(mAction) + " keycode="+ keyCodeToString(mKeyCode)+ " scancode="+ mScanCode+ " meta="+ metaStateToString(mMetaState)+ " flags=0x"+ Integer.toHexString(mFlags)+ " repeat="+ mRepeatCount+ " device="+ mDeviceId+ " source=0x"+ Integer.toHexString(mSource)+ "}";
}
