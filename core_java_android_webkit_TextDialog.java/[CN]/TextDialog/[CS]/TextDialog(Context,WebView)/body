{
  super(context);
  mWebView=webView;
  ShapeDrawable background=new ShapeDrawable(new RectShape());
  Paint shapePaint=background.getPaint();
  shapePaint.setStyle(Paint.Style.STROKE);
  ColorDrawable color=new ColorDrawable(Color.WHITE);
  Drawable[] array=new Drawable[2];
  array[0]=color;
  array[1]=background;
  LayerDrawable layers=new LayerDrawable(array);
  setBackgroundDrawable(layers);
  setPadding(3,2,0,0);
  mMaxLength=-1;
  TextPaint paint=getPaint();
  int flags=paint.getFlags() | Paint.SUBPIXEL_TEXT_FLAG | Paint.ANTI_ALIAS_FLAG & ~Paint.DEV_KERN_TEXT_FLAG;
  paint.setFlags(flags);
  setTextColor(Color.BLACK);
  setImeOptions(EditorInfo.IME_ACTION_NONE);
}
