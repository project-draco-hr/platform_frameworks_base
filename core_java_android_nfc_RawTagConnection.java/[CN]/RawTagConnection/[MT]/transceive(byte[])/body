{
  try {
    byte[] response=mTagService.transceive(mTag.mServiceHandle,data);
    if (response == null) {
      throw new IOException("transcieve failed");
    }
    return response;
  }
 catch (  RemoteException e) {
    Log.e(TAG,"NFC service died",e);
    throw new IOException("NFC service died");
  }
}
