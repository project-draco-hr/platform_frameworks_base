{
  if (DEBUG_AUDIO) {
    Slog.d(TAG,"usb:UsbHostManager.usbDeviceRemoved() nm:" + deviceName);
  }
  sendDeviceNotification(1,0,false,false,false,false);
synchronized (mLock) {
    UsbDevice device=mDevices.remove(deviceName);
    if (device != null) {
      getCurrentSettings().deviceDetached(device);
    }
  }
}
