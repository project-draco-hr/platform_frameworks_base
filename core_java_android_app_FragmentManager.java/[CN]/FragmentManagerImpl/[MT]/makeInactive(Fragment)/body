{
  if (f.mIndex < 0) {
    return;
  }
  if (DEBUG)   Log.v(TAG,"Freeing fragment index " + f.mIndex);
  mActive.set(f.mIndex,null);
  if (mAvailIndices == null) {
    mAvailIndices=new ArrayList<Integer>();
  }
  mAvailIndices.add(f.mIndex);
  mActivity.invalidateFragmentIndex(f.mIndex);
  f.initState();
}
