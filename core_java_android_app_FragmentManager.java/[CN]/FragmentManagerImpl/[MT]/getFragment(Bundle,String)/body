{
  int index=bundle.getInt(key,-1);
  if (index == -1) {
    return null;
  }
  if (index >= mActive.size()) {
    throwException(new IllegalStateException("Fragment no longer exists for key " + key + ": index "+ index));
  }
  Fragment f=mActive.get(index);
  if (f == null) {
    throwException(new IllegalStateException("Fragment no longer exists for key " + key + ": index "+ index));
  }
  return f;
}
