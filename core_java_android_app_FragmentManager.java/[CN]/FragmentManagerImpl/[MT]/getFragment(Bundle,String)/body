{
  int index=bundle.getInt(key);
  if (index >= mActive.size()) {
    throw new IllegalStateException("Fragement no longer exists for key " + key + ": index "+ index);
  }
  Fragment f=mActive.get(index);
  if (f == null) {
    throw new IllegalStateException("Fragement no longer exists for key " + key + ": index "+ index);
  }
  return f;
}
