{
  Animation animObj=fragment.onCreateAnimation(transitionStyle,enter);
  if (animObj != null) {
    return animObj;
  }
  if (transit == 0) {
    return null;
  }
  int styleIndex=transitToStyleIndex(transit,enter);
  if (styleIndex < 0) {
    return null;
  }
  if (transitionStyle == 0 && mActivity.getWindow() != null) {
    transitionStyle=mActivity.getWindow().getAttributes().windowAnimations;
  }
  if (transitionStyle == 0) {
    return null;
  }
  TypedArray attrs=mActivity.obtainStyledAttributes(transitionStyle,com.android.internal.R.styleable.WindowAnimation);
  int anim=attrs.getResourceId(styleIndex,0);
  attrs.recycle();
  if (anim == 0) {
    return null;
  }
  return AnimationUtils.loadAnimation(mActivity,anim);
}
