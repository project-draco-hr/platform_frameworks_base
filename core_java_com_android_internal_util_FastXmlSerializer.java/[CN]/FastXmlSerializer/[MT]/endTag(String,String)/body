{
  mNesting--;
  if (mInTag) {
    append(" />\n");
  }
 else {
    if (mIndent && mLineStart) {
      appendIndent(mNesting);
    }
    append("</");
    if (namespace != null) {
      append(namespace);
      append(':');
    }
    append(name);
    append(">\n");
  }
  mLineStart=true;
  mInTag=false;
  return this;
}
