{
  mClockNotificationsMarginMin=res.getDimensionPixelSize(R.dimen.keyguard_clock_notifications_margin_min);
  mClockNotificationsMarginMax=res.getDimensionPixelSize(R.dimen.keyguard_clock_notifications_margin_max);
  mClockYFractionMin=res.getFraction(R.fraction.keyguard_clock_y_fraction_min,1,1);
  mClockYFractionMax=res.getFraction(R.fraction.keyguard_clock_y_fraction_max,1,1);
  mMoreCardNotificationAmount=(float)res.getDimensionPixelSize(R.dimen.notification_summary_height) / res.getDimensionPixelSize(R.dimen.notification_min_height);
  mDensity=res.getDisplayMetrics().density;
}
