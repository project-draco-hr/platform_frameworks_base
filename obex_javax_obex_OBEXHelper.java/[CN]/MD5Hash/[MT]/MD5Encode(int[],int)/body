{
  int i, j;
  byte output[]=new byte[length];
  for (i=0, j=0; j < length; i++, j+=4) {
    output[j]=(byte)(input[i] & 0xff);
    output[j + 1]=(byte)((input[i] >>> 8) & 0xff);
    output[j + 2]=(byte)((input[i] >>> 16) & 0xff);
    output[j + 3]=(byte)((input[i] >>> 24) & 0xff);
  }
  return output;
}
