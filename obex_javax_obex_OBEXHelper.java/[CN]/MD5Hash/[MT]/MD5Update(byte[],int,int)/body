{
  int i, index, partLen;
  index=(count[0] >>> 3) & 0x3F;
  int slen=inputLen << 3;
  if ((count[0]+=slen) < slen) {
    count[1]++;
  }
  count[1]+=(inputLen >>> 29);
  partLen=64 - index;
  if (inputLen >= partLen) {
    copy(index,input,offset,partLen);
    MD5Transform(buffer,0,0);
    for (i=partLen; i + 63 < inputLen; i+=64) {
      MD5Transform(input,offset,i);
    }
    index=0;
  }
 else {
    i=0;
  }
  copy(index,input,i + offset,inputLen - i);
}
