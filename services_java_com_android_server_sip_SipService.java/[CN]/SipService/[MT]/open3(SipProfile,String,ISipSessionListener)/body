{
  localProfile.setCallingUid(Binder.getCallingUid());
  if (TextUtils.isEmpty(incomingCallBroadcastAction)) {
    throw new RuntimeException("empty broadcast action for incoming call");
  }
  if (DEBUG)   Log.d(TAG,"open3: " + localProfile.getUriString() + ": "+ incomingCallBroadcastAction+ ": "+ listener);
  try {
    SipSessionGroupExt group=createGroup(localProfile,incomingCallBroadcastAction,listener);
    if (localProfile.getAutoRegistration()) {
      group.openToReceiveCalls();
      if (isWifiOn())       grabWifiLock();
    }
  }
 catch (  SipException e) {
    Log.e(TAG,"openToReceiveCalls()",e);
  }
}
