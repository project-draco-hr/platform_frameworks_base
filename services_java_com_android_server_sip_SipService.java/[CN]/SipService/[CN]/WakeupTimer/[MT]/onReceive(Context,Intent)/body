{
  String action=intent.getAction();
  if (getAction().equals(action) && intent.getExtras().containsKey(TRIGGER_TIME)) {
    mPendingIntent=null;
    long triggerTime=intent.getLongExtra(TRIGGER_TIME,-1L);
    execute(triggerTime);
  }
 else {
    Log.d(TAG,"unrecognized intent: " + intent);
  }
}
