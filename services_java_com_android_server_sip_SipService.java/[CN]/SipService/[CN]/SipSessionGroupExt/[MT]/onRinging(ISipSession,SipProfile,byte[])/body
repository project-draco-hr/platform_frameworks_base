{
synchronized (SipService.this) {
    try {
      if (!isRegistered()) {
        session.endCall();
        return;
      }
      Log.d(TAG," ringing~~ " + getUri() + ": "+ caller.getUri()+ ": "+ session.getCallId());
      addPendingSession(session);
      Intent intent=SipManager.createIncomingCallBroadcast(mIncomingCallBroadcastAction,session.getCallId(),sessionDescription);
      Log.d(TAG,"   send out intent: " + intent);
      mContext.sendBroadcast(intent);
    }
 catch (    RemoteException e) {
      Log.e(TAG,"processCall()",e);
    }
  }
}
