{
  if (mInitialized) {
    AndroidTestCase.fail("Already initialized");
  }
  mExportTestResolver=new ExportTestResolver(mAndroidTestCase);
  mVCardType=vcardType;
  mIsDoCoMo=VCardConfig.isDoCoMo(vcardType);
  mInitialized=true;
  if (TextUtils.isEmpty(charset)) {
    mCharset="UTF-8";
  }
 else {
    mCharset=charset;
  }
}
