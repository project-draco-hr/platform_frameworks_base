{
  Log.d(LOG_TAG,vcard);
  InputStream is=null;
  try {
    is=new ByteArrayInputStream(vcard.getBytes(mCharset));
    verifyWithInputStream(is);
  }
 catch (  IOException e) {
    AndroidTestCase.fail("Unexpected IOException: " + e.getMessage());
  }
 finally {
    if (is != null) {
      try {
        is.close();
      }
 catch (      IOException e) {
        AndroidTestCase.fail("Unexpected IOException: " + e.getMessage());
      }
    }
  }
}
