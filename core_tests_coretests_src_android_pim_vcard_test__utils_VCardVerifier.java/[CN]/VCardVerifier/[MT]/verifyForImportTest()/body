{
  if (mLineVerifier != null) {
    AndroidTestCase.fail("Not supported now.");
  }
  try {
    verifyWithInputStream(mInputStream);
  }
 catch (  IOException e) {
    AndroidTestCase.fail("IOException was thrown: " + e.getMessage());
  }
 finally {
    if (mInputStream != null) {
      try {
        mInputStream.close();
      }
 catch (      IOException e) {
      }
    }
  }
}
