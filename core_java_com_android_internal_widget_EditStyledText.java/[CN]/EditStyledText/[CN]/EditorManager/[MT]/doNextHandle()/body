{
  if (DBG) {
    Log.d(LOG_TAG,"--- doNextHandle: " + mMode + ","+ mState);
  }
switch (mMode) {
case MODE_COPY:
    handleCopy();
  break;
case MODE_CUT:
handleCut();
break;
case MODE_PASTE:
handlePaste();
break;
case MODE_SIZE:
handleSize();
break;
case MODE_COLOR:
handleColor();
break;
case MODE_ALIGN:
handleAlign();
break;
default :
break;
}
}
