{
  if (DBG) {
    Log.d(LOG_TAG,"--- handleComplete");
  }
  if (!mEditFlag) {
    return;
  }
  if (mState == STATE_SELECTED) {
    mState=STATE_SELECT_FIX;
  }
switch (mMode) {
case MODE_COPY:
    handleCopy();
  break;
case MODE_COLOR:
handleColor();
break;
case MODE_SIZE:
handleSize();
break;
default :
break;
}
}
