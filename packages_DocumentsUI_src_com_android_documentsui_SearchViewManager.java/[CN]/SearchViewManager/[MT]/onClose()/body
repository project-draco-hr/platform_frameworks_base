{
  mSearchExpanded=false;
  if (mIgnoreNextClose) {
    mIgnoreNextClose=false;
    return false;
  }
  mView.setBackgroundColor(mView.getResources().getColor(android.R.color.transparent,null));
  if (mCurrentSearch != null) {
    mCurrentSearch=null;
    if (mListener != null) {
      mListener.onSearchChanged(mCurrentSearch);
    }
  }
  return false;
}
