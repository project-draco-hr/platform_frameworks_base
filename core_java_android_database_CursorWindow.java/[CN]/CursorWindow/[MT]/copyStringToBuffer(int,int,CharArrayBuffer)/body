{
  if (buffer == null) {
    throw new IllegalArgumentException("CharArrayBuffer should not be null");
  }
  if (buffer.data == null) {
    buffer.data=new char[64];
  }
  acquireReference();
  try {
    char[] newbuf=copyStringToBuffer_native(row - mStartPos,col,buffer.data.length,buffer);
    if (newbuf != null) {
      buffer.data=newbuf;
    }
  }
  finally {
    releaseReference();
  }
}
