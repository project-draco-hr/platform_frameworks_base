{
  deleteDatabase();
  DatabaseSessionCache cache=new DatabaseSessionCache(getContext());
  putSessionsIn(cache);
  closeDatabase();
  System.err.println("Size of ssl_sessions.db w/ 10 sessions: " + new File(dbDir,"ssl_sessions.db").length());
  Stopwatch stopwatch=new Stopwatch();
  cache=new DatabaseSessionCache(getContext());
  cache.getSessionData("crazybob.org",443);
  stopwatch.stop();
}
