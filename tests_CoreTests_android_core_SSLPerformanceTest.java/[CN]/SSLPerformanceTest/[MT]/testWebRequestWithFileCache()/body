{
  deleteDirectory();
  OpenSSLContextImpl sslContext=new OpenSSLContextImpl();
  sslContext.engineInit(null,null,null,FileClientSessionCache.usingDirectory(getCacheDirectory()),null);
  getVerisignDotCom(sslContext);
  sslContext.engineInit(null,null,null,FileClientSessionCache.usingDirectory(getCacheDirectory()),null);
  Stopwatch stopwatch=new Stopwatch();
  getVerisignDotCom(sslContext);
  stopwatch.stop();
}
