{
switch (protocol) {
case PROTOCOL_GOOGLE_TALK:
    return Im.ProviderCategories.GTALK;
case PROTOCOL_AIM:
  return Im.ProviderCategories.AIM;
case PROTOCOL_MSN:
return Im.ProviderCategories.MSN;
case PROTOCOL_YAHOO:
return Im.ProviderCategories.YAHOO;
case PROTOCOL_ICQ:
return Im.ProviderCategories.ICQ;
}
return null;
}
