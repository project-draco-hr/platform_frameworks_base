{
  List<BluetoothDevice> devices=mBluetoothService.lookupInputDevicesMatchingStates(new int[]{BluetoothInputDevice.STATE_CONNECTING,BluetoothInputDevice.STATE_CONNECTED});
  for (  BluetoothDevice device : devices) {
    if (!device.getAddress().equals(address))     return true;
  }
  return false;
}
