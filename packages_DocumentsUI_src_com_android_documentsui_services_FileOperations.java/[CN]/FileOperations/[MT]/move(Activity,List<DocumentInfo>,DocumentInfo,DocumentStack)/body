{
  String jobId=createJobId();
  if (DEBUG)   Log.d(TAG,"Initiating 'move' operation id: " + jobId);
  Intent intent=createBaseIntent(OPERATION_MOVE,activity,jobId,srcDocs,srcParent,destination);
  createSharedSnackBar(activity,R.plurals.move_begin,srcDocs.size()).show();
  activity.startService(intent);
  return jobId;
}
