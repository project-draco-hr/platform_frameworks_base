{
  if (mSpecs.contains(spec)) {
    return;
  }
  TileInfo info=new TileInfo();
  info.state=state;
  info.state.minimalAccessibilityClassName=info.state.expandedAccessibilityClassName=Button.class.getName();
  info.spec=spec;
  info.appLabel=appLabel;
  info.isSystem=isSystem;
  mTiles.add(info);
  mSpecs.add(spec);
}
