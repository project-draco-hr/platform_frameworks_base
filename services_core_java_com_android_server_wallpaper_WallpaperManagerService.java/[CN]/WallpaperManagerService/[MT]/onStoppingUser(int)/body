{
  if (userId < 1)   return;
synchronized (mLock) {
    WallpaperData wallpaper=mWallpaperMap.get(userId);
    if (wallpaper != null) {
      if (wallpaper.wallpaperObserver != null) {
        wallpaper.wallpaperObserver.stopWatching();
        wallpaper.wallpaperObserver=null;
      }
      mWallpaperMap.remove(userId);
    }
  }
}
