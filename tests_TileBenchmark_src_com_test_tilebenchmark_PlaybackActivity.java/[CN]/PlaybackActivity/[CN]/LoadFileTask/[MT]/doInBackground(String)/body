{
  TileData[][] data=null;
  try {
    FileInputStream fis=openFileInput(params[0]);
    ObjectInputStream in=new ObjectInputStream(fis);
    data=(TileData[][])in.readObject();
    in.close();
  }
 catch (  IOException ex) {
    ex.printStackTrace();
  }
catch (  ClassNotFoundException ex) {
    ex.printStackTrace();
  }
  return data;
}
