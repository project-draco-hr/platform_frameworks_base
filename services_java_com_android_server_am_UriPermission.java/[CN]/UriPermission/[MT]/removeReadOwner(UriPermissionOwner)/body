{
  if (!mReadOwners.remove(owner)) {
    Log.wtf(TAG,"Unknown read owner " + owner + " in "+ this);
  }
  if (mReadOwners.size() == 0) {
    mReadOwners=null;
    if ((globalModeFlags & Intent.FLAG_GRANT_READ_URI_PERMISSION) == 0) {
      modeFlags&=~Intent.FLAG_GRANT_READ_URI_PERMISSION;
    }
  }
}
