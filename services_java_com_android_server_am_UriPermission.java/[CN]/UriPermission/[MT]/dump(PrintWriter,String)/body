{
  pw.println(prefix + this);
  pw.println(prefix + "  modeFlags=0x" + Integer.toHexString(modeFlags)+ " uid="+ uid+ " globalModeFlags=0x"+ Integer.toHexString(globalModeFlags));
  pw.println(prefix + "  readActivities=" + readActivities);
  pw.println(prefix + "  writeActivities=" + writeActivities);
}
