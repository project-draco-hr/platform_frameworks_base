{
  if (params.result > 0) {
    GlobalScreenshot.notifyScreenshotError(params.context,mNotificationManager);
  }
 else {
    Resources r=params.context.getResources();
    mNotificationBuilder.setContentTitle(r.getString(R.string.screenshot_saved_title)).setContentText(r.getString(R.string.screenshot_saved_text)).setContentIntent(PendingIntent.getActivity(params.context,0,mLaunchIntent,0)).setWhen(System.currentTimeMillis()).setAutoCancel(true);
    Notification n=mNotificationBuilder.getNotification();
    n.flags&=~Notification.FLAG_NO_CLEAR;
    mNotificationManager.notify(mNotificationId,n);
  }
  params.finisher.run();
}
