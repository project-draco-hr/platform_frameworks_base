{
  final LockdownVpnTracker existing=mLockdownTracker;
  mLockdownTracker=null;
  if (existing != null) {
    existing.shutdown();
  }
  try {
    if (tracker != null) {
      mNetd.setFirewallEnabled(true);
      mLockdownTracker=tracker;
      mLockdownTracker.init();
    }
 else {
      mNetd.setFirewallEnabled(false);
    }
  }
 catch (  RemoteException e) {
  }
}
