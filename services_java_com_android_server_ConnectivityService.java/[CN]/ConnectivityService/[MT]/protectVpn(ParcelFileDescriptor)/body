{
  try {
    int type=mActiveDefaultNetwork;
    if (ConnectivityManager.isNetworkTypeValid(type)) {
      mVpn.protect(socket,mNetTrackers[type].getLinkProperties().getInterfaceName());
      return true;
    }
  }
 catch (  Exception e) {
  }
 finally {
    try {
      socket.close();
    }
 catch (    Exception e) {
    }
  }
  return false;
}
