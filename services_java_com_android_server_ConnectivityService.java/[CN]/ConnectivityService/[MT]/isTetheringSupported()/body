{
  enforceTetherAccessPermission();
  int defaultVal=(SystemProperties.get("ro.tether.denied").equals("true") ? 0 : 1);
  return ((Settings.Secure.getInt(mContext.getContentResolver(),Settings.Secure.TETHER_SUPPORTED,defaultVal) != 0) && (mNetTrackers[ConnectivityManager.TYPE_MOBILE_DUN] != null));
}
