{
  boolean v4=false;
  boolean v6=false;
  for (  RouteInfo r : p.getRoutes()) {
    if (r.getDestination().getAddress() instanceof Inet6Address) {
      v6=true;
    }
 else {
      v4=true;
    }
  }
  for (  InetAddress i : p.getDnses()) {
    if (i instanceof Inet6Address) {
      v6=true;
    }
 else {
      v4=true;
    }
  }
  return (v4 ? "v4" : "") + (v6 ? "v6" : "");
}
