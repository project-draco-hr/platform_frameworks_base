{
  if (delayMs <= 0) {
    sendStickyBroadcast(intent);
  }
 else {
    if (VDBG) {
      log("sendStickyBroadcastDelayed: delayMs=" + delayMs + ", action="+ intent.getAction());
    }
    mHandler.sendMessageDelayed(mHandler.obtainMessage(EVENT_SEND_STICKY_BROADCAST_INTENT,intent),delayMs);
  }
}
