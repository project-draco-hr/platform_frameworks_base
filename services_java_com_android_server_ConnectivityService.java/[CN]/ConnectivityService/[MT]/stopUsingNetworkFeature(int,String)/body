{
  enforceChangePermission();
  int pid=getCallingPid();
  int uid=getCallingUid();
  FeatureUser u=null;
  boolean found=false;
synchronized (this) {
    for (int i=0; i < mFeatureUsers.size(); i++) {
      u=(FeatureUser)mFeatureUsers.get(i);
      if (uid == u.mUid && pid == u.mPid && networkType == u.mNetworkType && TextUtils.equals(feature,u.mFeature)) {
        found=true;
        break;
      }
    }
  }
  if (found && u != null) {
    return stopUsingNetworkFeature(u,true);
  }
 else {
    if (DBG)     log("ignoring stopUsingNetworkFeature - not a live request");
    return 1;
  }
}
