{
  int j=1;
  boolean changed=false;
  for (  InetAddress dns : dnses) {
    String dnsString=dns.getHostAddress();
    if (changed || !dnsString.equals(SystemProperties.get("net.dns" + j + "."+ pid))) {
      changed=true;
      SystemProperties.set("net.dns" + j + "."+ pid,dns.getHostAddress());
    }
    j++;
  }
  return changed;
}
