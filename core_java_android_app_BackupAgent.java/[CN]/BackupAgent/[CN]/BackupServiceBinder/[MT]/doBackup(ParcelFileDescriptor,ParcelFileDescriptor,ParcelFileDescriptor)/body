{
  Log.v(TAG,"doBackup() invoked");
  BackupDataOutput output=new BackupDataOutput(BackupAgent.this,data.getFileDescriptor());
  try {
    BackupAgent.this.onBackup(oldState,output,newState);
  }
 catch (  RuntimeException ex) {
    Log.d("BackupAgent","onBackup (" + BackupAgent.this.getClass().getName() + ") threw",ex);
    throw ex;
  }
}
