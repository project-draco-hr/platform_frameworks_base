{
  if (mListeners == null) {
    return false;
  }
  final int keyIndex=mListeners.indexOfKey(listener);
  final boolean hasKey=keyIndex >= 0;
  if (hasKey) {
    mListeners.removeAt(keyIndex);
  }
  if (hasKey && mListeners.isEmpty()) {
    setMagnificationCallbackEnabled(false);
  }
  return hasKey;
}
