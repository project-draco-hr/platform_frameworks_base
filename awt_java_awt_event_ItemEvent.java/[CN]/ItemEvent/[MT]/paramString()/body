{
  String stateString=null;
switch (stateChange) {
case SELECTED:
    stateString="SELECTED";
  break;
case DESELECTED:
stateString="DESELECTED";
break;
default :
stateString="unknown type";
}
return ((id == ITEM_STATE_CHANGED ? "ITEM_STATE_CHANGED" : "unknown type") + ",item=" + item+ ",stateChange="+ stateString);
}
