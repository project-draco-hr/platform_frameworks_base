{
  final String taskString=Integer.toString(task.taskId);
  for (int queueNdx=mWriteQueue.size() - 1; queueNdx >= 0; --queueNdx) {
    final WriteQueueItem item=mWriteQueue.get(queueNdx);
    if (item instanceof ImageWriteQueueItem && ((ImageWriteQueueItem)item).mFilePath.startsWith(taskString)) {
      if (DEBUG)       Slog.d(TAG,"Removing " + ((ImageWriteQueueItem)item).mFilePath + " from write queue");
      mWriteQueue.remove(queueNdx);
    }
  }
}
