{
  final String taskString=Integer.toString(task.taskId);
  for (int queueNdx=mWriteQueue.size() - 1; queueNdx >= 0; --queueNdx) {
    final WriteQueueItem item=mWriteQueue.get(queueNdx);
    if (item instanceof ImageWriteQueueItem && ((ImageWriteQueueItem)item).mFilename.startsWith(taskString)) {
      if (DEBUG_PERSISTER)       Slog.d(TAG,"Removing " + ((ImageWriteQueueItem)item).mFilename + " from write queue");
      mWriteQueue.remove(queueNdx);
    }
  }
}
