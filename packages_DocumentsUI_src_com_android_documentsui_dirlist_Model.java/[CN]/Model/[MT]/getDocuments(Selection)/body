{
  final int size=(items != null) ? items.size() : 0;
  final List<DocumentInfo> docs=new ArrayList<>(size);
  for (  String modelId : items.getAll()) {
    final Cursor cursor=getItem(modelId);
    assert(cursor != null);
    docs.add(DocumentInfo.fromDirectoryCursor(cursor));
  }
  return docs;
}
