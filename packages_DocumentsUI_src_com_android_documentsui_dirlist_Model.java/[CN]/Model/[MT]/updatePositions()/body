{
  mPositions.clear();
  mCursor.moveToPosition(-1);
  for (int pos=0; pos < mCursorCount; ++pos) {
    mCursor.moveToNext();
    String modelId=getCursorString(mCursor,RootCursorWrapper.COLUMN_AUTHORITY) + "|" + getCursorString(mCursor,Document.COLUMN_DOCUMENT_ID);
    mPositions.put(modelId,pos);
  }
}
