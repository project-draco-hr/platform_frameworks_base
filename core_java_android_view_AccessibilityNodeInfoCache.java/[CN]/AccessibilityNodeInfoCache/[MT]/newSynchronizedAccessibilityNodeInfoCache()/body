{
  return new AccessibilityNodeInfoCache(){
    private final Object mLock=new Object();
    @Override public void clear(){
synchronized (mLock) {
        super.clear();
      }
    }
    @Override public AccessibilityNodeInfo get(    long accessibilityNodeId){
synchronized (mLock) {
        return super.get(accessibilityNodeId);
      }
    }
    @Override public void put(    long accessibilityNodeId,    AccessibilityNodeInfo info){
synchronized (mLock) {
        super.put(accessibilityNodeId,info);
      }
    }
    @Override public void remove(    long accessibilityNodeId){
synchronized (mLock) {
        super.remove(accessibilityNodeId);
      }
    }
  }
;
}
