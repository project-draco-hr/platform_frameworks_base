{
  CallbackRecord callback=obtainCallbackLocked(dueTime,action,token);
  CallbackRecord entry=mHead;
  if (entry == null) {
    mHead=callback;
    return;
  }
  if (dueTime < entry.dueTime) {
    callback.next=entry;
    mHead=callback;
    return;
  }
  while (entry.next != null) {
    if (dueTime < entry.next.dueTime) {
      callback.next=entry.next;
      break;
    }
    entry=entry.next;
  }
  entry.next=callback;
}
