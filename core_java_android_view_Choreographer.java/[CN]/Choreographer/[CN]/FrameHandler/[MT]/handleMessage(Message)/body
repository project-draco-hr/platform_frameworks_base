{
switch (msg.what) {
case MSG_DO_FRAME:
    doFrame(System.nanoTime(),0);
  break;
case MSG_DO_SCHEDULE_VSYNC:
doScheduleVsync();
break;
case MSG_DO_SCHEDULE_CALLBACK:
doScheduleCallback(msg.arg1);
break;
case MSG_FAKE_VSYNC:
if (DEBUG) {
Log.d(TAG,"Handling fake vsync while screen is off.");
}
doFrame(System.nanoTime(),0);
break;
}
}
