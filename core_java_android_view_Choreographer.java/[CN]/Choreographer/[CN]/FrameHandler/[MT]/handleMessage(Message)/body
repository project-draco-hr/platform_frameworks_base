{
switch (msg.what) {
case MSG_DO_ANIMATION:
    doAnimation();
  break;
case MSG_DO_DRAW:
doDraw();
break;
case MSG_DO_SCHEDULE_VSYNC:
doScheduleVsync();
break;
case MSG_POST_DELAYED_ANIMATION:
postAnimationCallbackUnchecked((Runnable)msg.obj);
break;
case MSG_POST_DELAYED_DRAW:
postDrawCallbackUnchecked((Runnable)msg.obj);
break;
}
}
