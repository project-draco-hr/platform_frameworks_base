{
  if (listener == null) {
    throw new IllegalArgumentException("listener must not be null");
  }
  if (DEBUG) {
    Log.d(TAG,"Adding onAnimate listener: " + listener);
  }
synchronized (mLock) {
    mOnAnimateListeners=ArrayUtils.appendElement(OnAnimateListener.class,mOnAnimateListeners,listener);
  }
}
