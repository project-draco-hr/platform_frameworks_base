{
  if (DEBUG) {
    Log.d(TAG,"RemoveAnimationCallbacks: " + action + ", token="+ token);
  }
synchronized (mLock) {
    mAnimationCallbackQueue.removeCallbacksLocked(action,token);
    if (action != null && token == null) {
      mHandler.removeMessages(MSG_DO_SCHEDULE_ANIMATION,action);
    }
  }
}
