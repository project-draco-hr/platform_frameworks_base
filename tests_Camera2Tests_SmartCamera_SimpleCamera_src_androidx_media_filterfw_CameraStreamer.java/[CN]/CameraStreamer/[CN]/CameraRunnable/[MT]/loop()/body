{
  while (true) {
    try {
      Event event=nextEvent();
      if (event == null)       continue;
switch (event.code) {
case Event.START:
        onStart();
      break;
case Event.STOP:
    onStop();
  break;
case Event.FRAME:
onFrame();
break;
case Event.HALT:
onHalt();
break;
case Event.RESTART:
onRestart();
break;
case Event.UPDATE:
onUpdate();
break;
case Event.TEARDOWN:
onTearDown();
break;
}
}
 catch (Exception e) {
e.printStackTrace();
}
}
}
