{
  StringBuffer buf=new StringBuffer();
  int nextSetBit=-1;
  set=set.get(0,strings.length);
  while ((nextSetBit=set.nextSetBit(nextSetBit + 1)) != -1) {
    buf.append(strings[nextSetBit].replace('_','-')).append(' ');
  }
  if (set.cardinality() > 0) {
    buf.setLength(buf.length() - 1);
  }
  return buf.toString();
}
