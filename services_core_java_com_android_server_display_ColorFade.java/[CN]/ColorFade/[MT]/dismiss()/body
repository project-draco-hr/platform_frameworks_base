{
  if (DEBUG) {
    Slog.d(TAG,"dismiss");
  }
  if (mPrepared) {
    attachEglContext();
    try {
      destroyScreenshotTexture();
      destroyGLShaders();
      destroyGLBuffers();
      destroyEglSurface();
    }
  finally {
      detachEglContext();
    }
    destroySurface();
    GLES20.glFlush();
    mPrepared=false;
  }
}
