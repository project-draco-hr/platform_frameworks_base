{
  if (DEBUG) {
    Slog.d(TAG,"dismissResources");
  }
  if (mCreatedResources) {
    attachEglContext();
    try {
      destroyScreenshotTexture();
      destroyGLShaders();
      destroyGLBuffers();
      destroyEglSurface();
    }
  finally {
      detachEglContext();
    }
    GLES20.glFlush();
    mCreatedResources=false;
  }
}
