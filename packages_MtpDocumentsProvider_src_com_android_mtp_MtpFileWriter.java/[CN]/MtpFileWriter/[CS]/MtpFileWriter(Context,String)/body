{
  mDocumentId=documentId;
  mDirty=false;
  final File tempFile=File.createTempFile("mtp","tmp",context.getCacheDir());
  mCacheFd=ParcelFileDescriptor.open(tempFile,ParcelFileDescriptor.MODE_READ_WRITE | ParcelFileDescriptor.MODE_TRUNCATE | ParcelFileDescriptor.MODE_CREATE);
  tempFile.delete();
}
