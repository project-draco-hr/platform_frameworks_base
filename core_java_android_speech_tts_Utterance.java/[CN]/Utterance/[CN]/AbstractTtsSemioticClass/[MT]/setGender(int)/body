{
  if (gender < 0 || gender > 3) {
    throw new IllegalArgumentException("Only four types of gender can be set: " + "unknown, neutral, maculine and female.");
  }
  if (gender != GENDER_UNKNOWN) {
    setParameter(KEY_GENDER,String.valueOf(gender));
  }
 else {
    setParameter(KEY_GENDER,null);
  }
  return (C)this;
}
