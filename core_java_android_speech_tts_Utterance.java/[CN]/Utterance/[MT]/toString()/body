{
  String out="type: \"" + TYPE_UTTERANCE + "\"";
  if (mNoWarningOnFallback != null) {
    out+=" no_warning_on_fallback: \"" + (mNoWarningOnFallback ? "true" : "false") + "\"";
  }
  for (  AbstractTts<? extends AbstractTts<?>> say : says) {
    out+=" markup { " + say.getMarkup().toString() + " }";
  }
  return out;
}
