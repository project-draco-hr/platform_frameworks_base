{
  if (this == o)   return true;
  if (!(o instanceof Utterance))   return false;
  Utterance utt=(Utterance)o;
  if (says.size() != utt.says.size()) {
    return false;
  }
  for (int i=0; i < says.size(); i++) {
    if (!says.get(i).getMarkup().equals(utt.says.get(i).getMarkup())) {
      return false;
    }
  }
  return true;
}
