{
  boolean disabledNetwork=false;
  if (0 <= networkId) {
    disabledNetwork=mWM.disableNetwork(networkId);
    if (LOCAL_LOGD) {
      if (disabledNetwork) {
        Log.v(TAG,"Disabled network: " + networkId);
      }
    }
  }
  if (LOCAL_LOGD) {
    if (!disabledNetwork) {
      Log.e(TAG,"Failed to disable network:" + " invalid network id: " + networkId);
    }
  }
  return disabledNetwork;
}
