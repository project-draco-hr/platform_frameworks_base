{
switch (mode) {
case DRIVER_POWER_MODE_ACTIVE:
    mPowerModeRefCount++;
  break;
case DRIVER_POWER_MODE_AUTO:
mPowerModeRefCount--;
if (mPowerModeRefCount > 0) {
return;
}
 else {
mPowerModeRefCount=0;
}
break;
}
if (mWifiState.get() != WIFI_STATE_ENABLED || isDriverStopped()) {
return;
}
WifiNative.setPowerModeCommand(mode);
}
