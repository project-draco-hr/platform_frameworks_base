{
  requestConnectionStatus(mWifiInfo);
  requestPolledInfo(mWifiInfo);
  WifiInfo info=mWifiInfo;
  if (false)   Log.v(TAG,"Status from supplicant: NID=" + info.getNetworkId() + " SSID="+ (info.getSSID() == null ? "<none>" : info.getSSID())+ " BSSID="+ (info.getBSSID() == null ? "<none>" : info.getBSSID())+ " WPA_STATE="+ (info.getSupplicantState() == null ? "<none>" : info.getSupplicantState())+ " RSSI="+ info.getRssi());
  return mWifiInfo;
}
