{
  if (other == null)   throw new IllegalArgumentException();
  if (!other.getClass().equals(getClass()))   throw new IllegalArgumentException();
  final boolean changed=other.visible != visible || other.iconId != iconId || !Objects.equals(other.icon,icon) || !Objects.equals(other.label,label) || !Objects.equals(other.contentDescription,contentDescription);
  other.visible=visible;
  other.iconId=iconId;
  other.icon=icon;
  other.label=label;
  other.contentDescription=contentDescription;
  return changed;
}
