{
  ArrayList<ArrayList> list=Lists.newArrayList();
  Random generator=new Random();
  for (int i=0; i < rows; i++) {
    ArrayList<Integer> col=Lists.newArrayList();
    list.add(col);
    for (int j=0; j < cols; j++) {
      Integer r=generator.nextInt();
      col.add(r);
    }
    col.add(i);
  }
  return list;
}
