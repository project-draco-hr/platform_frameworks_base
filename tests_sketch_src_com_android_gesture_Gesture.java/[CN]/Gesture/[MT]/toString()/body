{
  String str="";
  str+="#" + this.mBBX.left + ","+ this.mBBX.top+ ","+ this.mBBX.right+ ","+ this.mBBX.bottom;
  str+="#";
  Iterator<PointF> it=this.mPtsBuffer.iterator();
  while (it.hasNext()) {
    PointF fp=it.next();
    str+=fp.x + "," + fp.y+ ",";
  }
  str+="#";
  str+=this.mColor;
  str+="#";
  str+=this.mWidth;
  str+="#";
  str+=this.mLength;
  str+="#";
  str+=this.mTimestamp;
  return str;
}
