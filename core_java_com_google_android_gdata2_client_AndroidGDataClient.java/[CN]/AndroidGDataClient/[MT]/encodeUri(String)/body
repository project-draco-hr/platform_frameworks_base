{
  String encodedUri;
  try {
    encodedUri=URLEncoder.encode(uri,"UTF-8");
  }
 catch (  UnsupportedEncodingException uee) {
    Log.e("JakartaGDataClient","UTF-8 not supported -- should not happen.  " + "Using default encoding.",uee);
    encodedUri=URLEncoder.encode(uri);
  }
  return encodedUri;
}
