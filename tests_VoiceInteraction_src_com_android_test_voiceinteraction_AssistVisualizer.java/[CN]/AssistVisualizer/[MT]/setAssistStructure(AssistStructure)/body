{
  mAssistStructure=as;
  mTextRects.clear();
  final int N=as.getWindowNodeCount();
  if (N > 0) {
    for (int i=0; i < N; i++) {
      AssistStructure.WindowNode windowNode=as.getWindowNodeAt(i);
      buildTextRects(windowNode.getRootViewNode(),windowNode.getLeft(),windowNode.getTop());
    }
  }
  invalidate();
}
