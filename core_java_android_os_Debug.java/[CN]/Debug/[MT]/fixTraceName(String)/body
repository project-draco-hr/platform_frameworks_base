{
  if (traceName == null)   traceName=NoPreloadHolder.DEFAULT_TRACE_FILE_PATH;
  if (traceName.charAt(0) != '/')   traceName=NoPreloadHolder.DEFAULT_TRACE_PATH_PREFIX + traceName;
  if (!traceName.endsWith(DEFAULT_TRACE_EXTENSION))   traceName=traceName + DEFAULT_TRACE_EXTENSION;
  return traceName;
}
