{
  if (Config.DEBUG) {
    if (debugProperties != null) {
      for (      Field field : cl.getDeclaredFields()) {
        final String propertyName=cl.getName() + "." + field.getName();
        boolean isStatic=Modifier.isStatic(field.getModifiers());
        boolean isFinal=Modifier.isFinal(field.getModifiers());
        if (!isStatic || isFinal) {
          throw new IllegalArgumentException(propertyName + " must be static and non-final");
        }
        modifyFieldIfSet(field,propertyName);
      }
    }
  }
 else {
    Log.w("android.os.Debug","setPropertiesOn(" + (cl == null ? "null" : cl.getName()) + ") called in non-DEBUG build");
  }
}
