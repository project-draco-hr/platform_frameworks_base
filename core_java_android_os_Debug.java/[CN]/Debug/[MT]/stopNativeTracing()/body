{
  VMDebug.stopEmulatorTracing();
  PrintWriter outStream=null;
  try {
    FileOutputStream fos=new FileOutputStream(SYSFS_QEMU_TRACE_STATE);
    outStream=new FastPrintWriter(fos);
    outStream.println("0");
  }
 catch (  Exception e) {
  }
 finally {
    if (outStream != null)     outStream.close();
  }
}
