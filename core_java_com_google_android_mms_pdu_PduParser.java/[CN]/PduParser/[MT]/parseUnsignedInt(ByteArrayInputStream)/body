{
  assert(null != pduDataStream);
  int result=0;
  int temp=pduDataStream.read();
  if (temp == -1) {
    return temp;
  }
  while ((temp & 0x80) != 0) {
    result=result << 7;
    result|=temp & 0x7F;
    temp=pduDataStream.read();
    if (temp == -1) {
      return temp;
    }
  }
  result=result << 7;
  result|=temp & 0x7F;
  return result;
}
