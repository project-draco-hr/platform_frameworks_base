{
  if (DEBUG_WARP)   Log.v(TAG,"pageBeginMoving(" + mIsPageMoving + ")");
  if (!mIsPageMoving) {
    mIsPageMoving=true;
    if (isWarping()) {
      onPageBeginWarp();
      if (mPageSwapIndex != -1) {
        swapPages(mPageSwapIndex,mPageWarpIndex);
      }
    }
    onPageBeginMoving();
  }
}
