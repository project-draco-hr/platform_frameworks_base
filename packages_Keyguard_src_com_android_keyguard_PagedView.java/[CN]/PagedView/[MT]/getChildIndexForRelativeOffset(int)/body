{
  final int childCount=getChildCount();
  int left;
  int right;
  for (int i=0; i < childCount; ++i) {
    left=getRelativeChildOffset(i);
    right=(left + getScaledMeasuredWidth(getPageAt(i)));
    if (left <= relativeOffset && relativeOffset <= right) {
      return i;
    }
  }
  return -1;
}
