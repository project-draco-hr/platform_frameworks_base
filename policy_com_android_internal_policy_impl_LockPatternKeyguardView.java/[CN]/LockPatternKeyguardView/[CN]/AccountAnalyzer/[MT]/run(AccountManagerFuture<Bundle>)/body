{
  try {
    Bundle result=future.getResult();
    if (result.getParcelable(AccountManager.KEY_INTENT) != null) {
      mEnableFallback=true;
    }
  }
 catch (  OperationCanceledException e) {
  }
catch (  IOException e) {
  }
catch (  AuthenticatorException e) {
  }
 finally {
    mAccountIndex++;
    next();
  }
}
