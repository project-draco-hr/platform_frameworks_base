{
  throwIfDestroyed();
  if (isBound()) {
    if (DEBUG) {
      Slog.i(LOG_TAG,"[user: " + mUserId + "] requestCancelPrintJob()");
    }
    try {
      mPrintService.requestCancelPrintJob(printJob);
    }
 catch (    RemoteException re) {
      Slog.e(LOG_TAG,"Error canceling a pring job.",re);
    }
  }
}
