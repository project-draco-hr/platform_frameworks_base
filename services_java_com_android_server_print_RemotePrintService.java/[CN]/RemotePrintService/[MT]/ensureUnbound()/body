{
  if (!isBound() && !mBinding) {
    return;
  }
  if (DEBUG) {
    Slog.i(LOG_TAG,"[user: " + mUserId + "] ensureUnbound()");
  }
  mBinding=false;
  mPendingCommands.clear();
  if (isBound()) {
    try {
      mPrintService.setClient(null);
    }
 catch (    RemoteException re) {
    }
    mPrintService=null;
    mContext.unbindService(mServiceConnection);
  }
}
