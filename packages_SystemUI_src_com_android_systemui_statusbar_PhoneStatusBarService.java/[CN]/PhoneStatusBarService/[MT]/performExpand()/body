{
  if (SPEW)   Slog.d(TAG,"performExpand: mExpanded=" + mExpanded);
  if ((mDisabled & StatusBarManager.DISABLE_EXPAND) != 0) {
    return;
  }
  if (mExpanded) {
    return;
  }
  mExpanded=true;
  makeExpandedVisible();
  updateExpandedViewPos(EXPANDED_FULL_OPEN);
  if (false)   postStartTracing();
}
