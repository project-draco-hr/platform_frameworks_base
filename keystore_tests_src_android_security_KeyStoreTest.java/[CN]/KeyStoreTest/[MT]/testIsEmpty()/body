{
  assertTrue(mKeyStore.isEmpty());
  mKeyStore.onUserPasswordChanged(TEST_PASSWD);
  assertTrue(mKeyStore.isEmpty());
  mKeyStore.put(TEST_KEYNAME,TEST_KEYVALUE,KeyStore.UID_SELF,KeyStore.FLAG_ENCRYPTED);
  assertFalse(mKeyStore.isEmpty());
  mKeyStore.reset();
  assertTrue(mKeyStore.isEmpty());
}
