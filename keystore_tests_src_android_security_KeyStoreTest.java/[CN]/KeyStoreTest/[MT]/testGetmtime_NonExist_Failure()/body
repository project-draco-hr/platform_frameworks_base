{
  assertTrue("Password should work for keystore",mKeyStore.password(TEST_PASSWD));
  assertTrue("Should be able to import key when unlocked",mKeyStore.importKey(TEST_KEYNAME,PRIVKEY_BYTES,KeyStore.UID_SELF,KeyStore.FLAG_ENCRYPTED));
  assertEquals("-1 should be returned for non-existent key",-1L,mKeyStore.getmtime(TEST_KEYNAME2));
}
