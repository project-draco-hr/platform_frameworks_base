{
  assertTrue("Password should work for keystore",mKeyStore.password(TEST_PASSWD));
  assertTrue("Should be able to generate key for testcase",mKeyStore.generate(TEST_KEYNAME,KeyStore.UID_SELF,KeyStore.FLAG_ENCRYPTED));
  assertFalse("Should not be able to revoke not existent grant",mKeyStore.ungrant(TEST_KEYNAME,0));
}
