{
  assertTrue(mKeyStore.password(TEST_PASSWD));
  assertTrue("Should be able to generate key when unlocked",mKeyStore.generate(TEST_KEYNAME,Process.WIFI_UID,NativeCrypto.EVP_PKEY_RSA,RSA_KEY_SIZE,KeyStore.FLAG_ENCRYPTED,null));
  assertTrue(mKeyStore.contains(TEST_KEYNAME,Process.WIFI_UID));
  assertFalse(mKeyStore.contains(TEST_KEYNAME));
}
