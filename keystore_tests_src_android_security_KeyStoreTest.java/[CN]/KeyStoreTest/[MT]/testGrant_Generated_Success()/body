{
  assertTrue("Password should work for keystore",mKeyStore.password(TEST_PASSWD));
  assertTrue("Should be able to generate key for testcase",mKeyStore.generate(TEST_KEYNAME,KeyStore.UID_SELF,NativeCrypto.EVP_PKEY_RSA,RSA_KEY_SIZE,KeyStore.FLAG_ENCRYPTED,null));
  assertTrue("Should be able to grant key to other user",mKeyStore.grant(TEST_KEYNAME,0));
}
