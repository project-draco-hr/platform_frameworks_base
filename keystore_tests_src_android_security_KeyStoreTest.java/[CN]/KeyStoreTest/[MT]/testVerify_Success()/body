{
  mKeyStore.password(TEST_PASSWD);
  assertTrue(mKeyStore.generate(TEST_KEYNAME,KeyStore.UID_SELF,NativeCrypto.EVP_PKEY_RSA,RSA_KEY_SIZE,KeyStore.FLAG_ENCRYPTED,null));
  assertTrue(mKeyStore.contains(TEST_KEYNAME));
  final byte[] signature=mKeyStore.sign(TEST_KEYNAME,TEST_DATA);
  assertNotNull("Signature should not be null",signature);
  assertTrue("Signature should verify with same data",mKeyStore.verify(TEST_KEYNAME,TEST_DATA,signature));
}
