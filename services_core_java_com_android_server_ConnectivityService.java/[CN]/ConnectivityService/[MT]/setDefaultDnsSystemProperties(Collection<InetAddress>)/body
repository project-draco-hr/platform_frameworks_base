{
  int last=0;
  for (  InetAddress dns : dnses) {
    ++last;
    String key="net.dns" + last;
    String value=dns.getHostAddress();
    SystemProperties.set(key,value);
  }
  for (int i=last + 1; i <= mNumDnsEntries; ++i) {
    String key="net.dns" + i;
    SystemProperties.set(key,"");
  }
  mNumDnsEntries=last;
}
