{
synchronized (mVpns) {
    Vpn userVpn=mVpns.get(userId);
    if (userVpn != null) {
      loge("Starting user already has a VPN");
      return;
    }
    userVpn=new Vpn(mContext,mVpnCallback,mNetd,this,userId);
    mVpns.put(userId,userVpn);
    userVpn.startMonitoring(mContext,mTrackerHandler);
  }
}
