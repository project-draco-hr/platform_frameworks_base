{
  enforceConnectivityInternalPermission();
  NetworkAgentInfo nai=new NetworkAgentInfo(messenger,new AsyncChannel(),nextNetId(),new NetworkInfo(networkInfo),new LinkProperties(linkProperties),new NetworkCapabilities(networkCapabilities),currentScore,mContext,mTrackerHandler);
  if (VDBG)   log("registerNetworkAgent " + nai);
  mHandler.sendMessage(mHandler.obtainMessage(EVENT_REGISTER_NETWORK_AGENT,nai));
}
