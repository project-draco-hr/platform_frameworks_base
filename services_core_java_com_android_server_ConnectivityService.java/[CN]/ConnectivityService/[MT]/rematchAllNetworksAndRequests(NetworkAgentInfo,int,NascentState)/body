{
  if (changed != null && (oldScore < changed.getCurrentScore() || nascent == NascentState.JUST_VALIDATED)) {
    rematchNetworkAndRequests(changed,nascent,ReapUnvalidatedNetworks.REAP);
  }
 else {
    for (Iterator i=mNetworkAgentInfos.values().iterator(); i.hasNext(); ) {
      rematchNetworkAndRequests((NetworkAgentInfo)i.next(),NascentState.NOT_JUST_VALIDATED,i.hasNext() ? ReapUnvalidatedNetworks.DONT_REAP : ReapUnvalidatedNetworks.REAP);
    }
  }
}
