{
  enforceConnectivityInternalPermission();
  NetworkAgentInfo nai=new NetworkAgentInfo(messenger,new AsyncChannel(),new Network(reserveNetId()),new NetworkInfo(networkInfo),new LinkProperties(linkProperties),new NetworkCapabilities(networkCapabilities),currentScore,mContext,mTrackerHandler,new NetworkMisc(networkMisc),mDefaultRequest);
synchronized (this) {
    nai.networkMonitor.systemReady=mSystemReady;
  }
  addValidationLogs(nai.networkMonitor.getValidationLogs(),nai.network);
  if (DBG)   log("registerNetworkAgent " + nai);
  mHandler.sendMessage(mHandler.obtainMessage(EVENT_REGISTER_NETWORK_AGENT,nai));
  return nai.network.netId;
}
