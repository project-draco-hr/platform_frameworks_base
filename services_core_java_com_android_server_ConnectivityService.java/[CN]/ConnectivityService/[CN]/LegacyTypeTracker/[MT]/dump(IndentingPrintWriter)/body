{
  for (int type=0; type < mTypeLists.length; type++) {
    if (mTypeLists[type] == null)     continue;
    pw.print(type + " ");
    pw.increaseIndent();
    if (mTypeLists[type].size() == 0)     pw.println("none");
    for (    NetworkAgentInfo nai : mTypeLists[type]) {
      pw.println(naiToString(nai));
    }
    pw.decreaseIndent();
  }
}
