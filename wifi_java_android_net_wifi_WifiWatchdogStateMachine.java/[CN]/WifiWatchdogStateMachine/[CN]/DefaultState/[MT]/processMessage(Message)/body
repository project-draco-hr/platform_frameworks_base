{
switch (msg.what) {
case EVENT_WATCHDOG_SETTINGS_CHANGE:
    updateSettings();
  if (DBG) {
    log("Updating wifi-watchdog secure settings");
  }
break;
case EVENT_RSSI_CHANGE:
mCurrentSignalLevel=WifiManager.calculateSignalLevel(msg.arg1,WifiManager.RSSI_LEVELS);
break;
case EVENT_WIFI_RADIO_STATE_CHANGE:
case EVENT_NETWORK_STATE_CHANGE:
case CMD_ARP_CHECK:
case CMD_DELAYED_WALLED_GARDEN_CHECK:
break;
default :
log("Unhandled message " + msg + " in state "+ getCurrentState().getName());
break;
}
return HANDLED;
}
