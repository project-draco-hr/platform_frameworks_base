{
switch (msg.what) {
case EVENT_WATCHDOG_SETTINGS_CHANGE:
    updateSettings();
  if (VDBG) {
    Slog.d(WWSM_TAG,"Updating wifi-watchdog secure settings");
  }
return HANDLED;
}
if (VDBG) {
Slog.v(WWSM_TAG,"Caught message " + msg.what + " in state "+ getCurrentState().getName());
}
return HANDLED;
}
