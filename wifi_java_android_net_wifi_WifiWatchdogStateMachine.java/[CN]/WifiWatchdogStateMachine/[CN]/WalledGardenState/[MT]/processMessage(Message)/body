{
  if (msg.what != MESSAGE_HANDLE_WALLED_GARDEN) {
    return NOT_HANDLED;
  }
  if (msg.arg1 != mNetEventCounter) {
    if (DBG) {
      log("WalledGardenState::Msg out of sync, ignoring...");
    }
    return HANDLED;
  }
  setWalledGardenNotificationVisible(true);
  transitionTo(mOnlineWatchState);
  return HANDLED;
}
