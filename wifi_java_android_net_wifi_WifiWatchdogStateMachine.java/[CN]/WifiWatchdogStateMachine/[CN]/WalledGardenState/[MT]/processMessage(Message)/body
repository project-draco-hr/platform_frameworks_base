{
  if (msg.what != MESSAGE_HANDLE_WALLED_GARDEN) {
    return NOT_HANDLED;
  }
  if (msg.arg1 != mNetEventCounter) {
    if (VDBG) {
      Slog.v(WWSM_TAG,"WalledGardenState::Msg out of sync, ignoring...");
    }
    return HANDLED;
  }
  popUpBrowser();
  transitionTo(mOnlineWatchState);
  return HANDLED;
}
