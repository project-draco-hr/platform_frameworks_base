{
  mNumFullDNSchecks++;
  dnsCheckSuccesses=0;
  dnsCheckTries=0;
  if (DBG) {
    Slog.d(WWSM_TAG,"Starting DNS pings at " + SystemClock.elapsedRealtime());
    dnsCheckLogStr=String.format("Dns Check %d.  Pinging %s on ssid [%s]: ",mNumFullDNSchecks,mDnsPinger.getDns(),mInitialConnInfo.getSSID());
  }
  sendCheckStepMessage(0);
}
