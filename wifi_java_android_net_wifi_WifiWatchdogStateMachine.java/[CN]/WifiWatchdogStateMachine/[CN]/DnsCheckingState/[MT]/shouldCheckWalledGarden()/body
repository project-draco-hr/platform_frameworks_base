{
  if (!mWalledGardenTestEnabled) {
    if (VDBG)     Slog.v(WWSM_TAG,"Skipping walled garden check - disabled");
    return false;
  }
  long waitTime=waitTime(mWalledGardenIntervalMs,mLastWalledGardenCheckTime);
  if (waitTime > 0) {
    if (DBG) {
      Slog.d(WWSM_TAG,"Skipping walled garden check - wait " + waitTime + " ms.");
    }
    return false;
  }
  return true;
}
