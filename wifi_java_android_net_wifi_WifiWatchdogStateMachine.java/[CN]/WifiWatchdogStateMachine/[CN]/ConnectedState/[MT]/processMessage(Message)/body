{
switch (msg.what) {
case EVENT_WATCHDOG_SETTINGS_CHANGE:
    updateSettings();
  DBG=true;
if (DBG) log("Updated secure settings and turned debug on");
if (mPoorNetworkDetectionEnabled) {
transitionTo(mOnlineWatchState);
}
 else {
transitionTo(mOnlineState);
}
return HANDLED;
}
return NOT_HANDLED;
}
