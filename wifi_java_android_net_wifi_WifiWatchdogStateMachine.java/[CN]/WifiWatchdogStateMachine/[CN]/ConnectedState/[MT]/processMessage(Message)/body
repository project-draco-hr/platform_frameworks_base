{
switch (msg.what) {
case EVENT_SCAN_RESULTS_AVAILABLE:
    if (mPoorNetworkDetectionEnabled) {
      updateBssids();
    }
  return HANDLED;
case EVENT_WATCHDOG_SETTINGS_CHANGE:
updateSettings();
if (mPoorNetworkDetectionEnabled) {
transitionTo(mOnlineWatchState);
}
 else {
transitionTo(mOnlineState);
}
return HANDLED;
}
return NOT_HANDLED;
}
