{
  while (accumulator <= -mConfigTickDistance) {
    sendKeyDownOrRepeat(time,negativeKeyCode,metaState);
    accumulator+=mConfigTickDistance;
  }
  while (accumulator >= mConfigTickDistance) {
    sendKeyDownOrRepeat(time,positiveKeyCode,metaState);
    accumulator-=mConfigTickDistance;
  }
  return accumulator;
}
