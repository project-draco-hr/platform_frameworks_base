{
  ViewRootImpl viewRootImpl=mViewRootImpl.get();
  if (viewRootImpl != null && viewRootImpl.mView != null) {
    viewRootImpl.getAccessibilityInteractionController().findAccessibilityNodeInfoByAccessibilityIdClientThread(accessibilityNodeId,interactionId,callback,interrogatingPid,interrogatingTid);
  }
}
