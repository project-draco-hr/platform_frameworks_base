{
  final int displayState=mAttachInfo.mDisplayState;
  if (mView != null && mAdded && mTraversalScheduled && (displayState == Display.STATE_DOZE || displayState == Display.STATE_DOZE_SUSPEND)) {
    try {
      mWindowSession.pokeDrawLock(mWindow);
    }
 catch (    RemoteException ex) {
    }
  }
}
