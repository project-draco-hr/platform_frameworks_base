{
  if (mLastWasImTarget) {
    InputMethodManager imm=InputMethodManager.peekInstance();
    if (imm != null) {
      final InputEvent event=q.mEvent;
      final int seq=event.getSequenceNumber();
      if (DEBUG_IMF)       Log.v(TAG,"Sending input event to IME: seq=" + seq + " event="+ event);
      return imm.dispatchInputEvent(mView.getContext(),seq,event,mInputMethodCallback);
    }
  }
  return EVENT_POST_IME;
}
