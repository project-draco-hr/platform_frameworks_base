{
  QueuedInputEvent q=mQueuedInputEventPool;
  if (q != null) {
    mQueuedInputEventPoolSize-=1;
    mQueuedInputEventPool=q.mNext;
    q.mNext=null;
  }
 else {
    q=new QueuedInputEvent();
  }
  q.mEvent=event;
  q.mFinishedCallback=finishedCallback;
  q.mFlags=flags;
  q.mSeq=mQueuedInputEventNextSeq++;
  return q;
}
