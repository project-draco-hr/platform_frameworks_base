{
  if (child != mView) {
    return 0;
  }
  final WindowManager.LayoutParams lp=(WindowManager.LayoutParams)child.getLayoutParams();
  final int horizontal=(lp.width == WindowManager.LayoutParams.WRAP_CONTENT || lp.horizontalWeight != 0) ? FLAG_LAYOUT_AXIS_HORIZONTAL : 0;
  final int vertical=(lp.height == WindowManager.LayoutParams.WRAP_CONTENT || lp.verticalWeight != 0) ? FLAG_LAYOUT_AXIS_VERTICAL : 0;
  return (horizontal | vertical) & axisFilter;
}
