{
  if (mSendWindowContentChangedAccessibilityEvent == null) {
    mSendWindowContentChangedAccessibilityEvent=new SendWindowContentChangedAccessibilityEvent();
  }
  View oldSource=mSendWindowContentChangedAccessibilityEvent.mSource;
  if (oldSource == null) {
    mSendWindowContentChangedAccessibilityEvent.mSource=source;
    mHandler.postDelayed(mSendWindowContentChangedAccessibilityEvent,ViewConfiguration.getSendRecurringAccessibilityEventsInterval());
  }
 else {
    mSendWindowContentChangedAccessibilityEvent.mSource=getCommonPredecessor(oldSource,source);
  }
}
