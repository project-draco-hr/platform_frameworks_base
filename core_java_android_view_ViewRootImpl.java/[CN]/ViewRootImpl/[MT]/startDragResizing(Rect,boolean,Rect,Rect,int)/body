{
  if (!mDragResizing) {
    mDragResizing=true;
synchronized (mWindowCallbacks) {
      for (int i=mWindowCallbacks.size() - 1; i >= 0; i--) {
        mWindowCallbacks.get(i).onWindowDragResizeStart(initialBounds,fullscreen,systemInsets,stableInsets,resizeMode);
      }
    }
    mFullRedrawNeeded=true;
  }
}
