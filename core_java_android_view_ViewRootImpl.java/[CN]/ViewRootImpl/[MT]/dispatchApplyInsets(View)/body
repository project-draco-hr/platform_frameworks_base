{
  mDispatchContentInsets.set(mAttachInfo.mContentInsets);
  mDispatchStableInsets.set(mAttachInfo.mStableInsets);
  boolean isRound=false;
  if ((mWindowAttributes.flags & WindowManager.LayoutParams.FLAG_LAYOUT_IN_OVERSCAN) != 0 && mDisplay.getDisplayId() == 0) {
    isRound=(mIsEmulator && SystemProperties.getBoolean(PROPERTY_EMULATOR_CIRCULAR,false)) || mContext.getResources().getBoolean(com.android.internal.R.bool.config_windowIsRound);
  }
  host.dispatchApplyWindowInsets(new WindowInsets(mDispatchContentInsets,null,mDispatchStableInsets,isRound));
}
