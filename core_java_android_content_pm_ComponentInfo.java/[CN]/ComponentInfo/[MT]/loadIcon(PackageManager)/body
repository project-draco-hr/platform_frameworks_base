{
  ApplicationInfo ai=applicationInfo;
  Drawable dr;
  if (icon != 0) {
    dr=pm.getDrawable(packageName,icon,ai);
    if (dr != null) {
      return dr;
    }
  }
  if (ai.icon != 0) {
    dr=pm.getDrawable(packageName,ai.icon,ai);
    if (dr != null) {
      return dr;
    }
  }
  return pm.getDefaultActivityIcon();
}
