{
synchronized (mLock) {
    throwIfCalledByNotTrustedUidLocked();
    throwIfShutdownLocked();
    if (!isConnectedLocked()) {
      throw new IllegalStateException("Already disconnected.");
    }
    mOwningUid=-1;
    unregisterUiTestAutomationServiceLocked();
    restoreRotationStateLocked();
  }
}
