{
  final LongSparseLongArray array=new LongSparseLongArray(5);
  for (int i=0; i < 48; i++) {
    final long value=1 << i;
    array.put(value,value);
  }
  for (int i=0; i < 48; i++) {
    final long value=1 << i;
    assertEquals(value,array.get(value,-1));
    assertEquals(-1,array.get(-value,-1));
  }
}
