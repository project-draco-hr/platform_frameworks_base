{
  if (!mDrawNeeded) {
    return;
  }
  mDrawNeeded=false;
  Rect dirty=new Rect(0,0,mLastDW,mLastDH);
  Canvas c=null;
  try {
    c=mSurface.lockCanvas(dirty);
  }
 catch (  IllegalArgumentException e) {
  }
catch (  Surface.OutOfResourcesException e) {
  }
  if (c == null) {
    return;
  }
  c.drawCircle(160,160,160,mPaint);
  mSurface.unlockCanvasAndPost(c);
}
