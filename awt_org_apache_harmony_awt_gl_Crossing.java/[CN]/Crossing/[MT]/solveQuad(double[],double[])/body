{
  double a=eqn[2];
  double b=eqn[1];
  double c=eqn[0];
  int rc=0;
  if (a == 0.0) {
    if (b == 0.0) {
      return -1;
    }
    res[rc++]=-c / b;
  }
 else {
    double d=b * b - 4.0 * a * c;
    if (d < 0.0) {
      return 0;
    }
    d=Math.sqrt(d);
    res[rc++]=(-b + d) / (a * 2.0);
    if (d != 0.0) {
      res[rc++]=(-b - d) / (a * 2.0);
    }
  }
  return fixRoots(res,rc);
}
