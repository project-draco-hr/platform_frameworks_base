{
  if ((x < x1 && x < x2) || (x > x1 && x > x2) || (y > y1 && y > y2)|| (x1 == x2)) {
    return 0;
  }
  if (y < y1 && y < y2) {
  }
 else {
    if ((y2 - y1) * (x - x1) / (x2 - x1) <= y - y1) {
      return 0;
    }
  }
  if (x == x1) {
    return x1 < x2 ? 0 : -1;
  }
  if (x == x2) {
    return x1 < x2 ? 1 : 0;
  }
  return x1 < x2 ? 1 : -1;
}
