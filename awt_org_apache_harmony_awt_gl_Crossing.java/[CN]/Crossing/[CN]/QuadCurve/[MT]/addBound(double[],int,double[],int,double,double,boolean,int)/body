{
  for (int i=0; i < rc; i++) {
    double t=res[i];
    if (t > -DELTA && t < 1 + DELTA) {
      double rx=t * (t * Ax + Bx);
      if (minX <= rx && rx <= maxX) {
        bound[bc++]=t;
        bound[bc++]=rx;
        bound[bc++]=t * (t * Ay + By);
        bound[bc++]=id;
        if (changeId) {
          id++;
        }
      }
    }
  }
  return bc;
}
