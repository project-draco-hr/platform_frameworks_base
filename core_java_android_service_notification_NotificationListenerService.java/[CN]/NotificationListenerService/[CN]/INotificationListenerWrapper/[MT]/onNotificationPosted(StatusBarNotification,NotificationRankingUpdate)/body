{
  Notification.Builder.rebuild(getContext(),sbn.getNotification());
synchronized (mWrapper) {
    applyUpdate(update);
    try {
      NotificationListenerService.this.onNotificationPosted(sbn,mRankingMap);
    }
 catch (    Throwable t) {
      Log.w(TAG,"Error running onNotificationPosted",t);
    }
  }
}
