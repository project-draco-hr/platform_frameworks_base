{
  mVerifier.initForExportTest(VCardConfig.VCARD_TYPE_DOCOMO,"Shift_JIS");
  ContactEntry entry=mVerifier.addInputEntry();
  entry.addContentValues(Note.CONTENT_ITEM_TYPE).put(Note.NOTE,"note1");
  entry.addContentValues(Note.CONTENT_ITEM_TYPE).put(Note.NOTE,"note2");
  entry.addContentValues(Note.CONTENT_ITEM_TYPE).put(Note.NOTE,"note3");
  mVerifier.addPropertyNodesVerifierElemWithEmptyName().addExpectedNode("TEL","",new TypeSet("HOME")).addExpectedNode("EMAIL","",new TypeSet("HOME")).addExpectedNode("X-CLASS","PUBLIC").addExpectedNode("X-REDUCTION","").addExpectedNode("X-NO","").addExpectedNode("X-DCM-HMN-MODE","").addExpectedNode("ADR","",new TypeSet("HOME")).addExpectedNode("NOTE","note1\nnote2\nnote3",mContentValuesForQP);
}
