{
  if ((filename == null) || (filename.length() == 0))   throw new IllegalArgumentException("Filename may not be null or empty string.");
  VoicetagItemImpl voicetag=null;
  long nativeVoicetag=createVoicetagProxy(filename,listener);
  if (nativeVoicetag != 0) {
    voicetag=new VoicetagItemImpl(nativeVoicetag,true);
  }
  return voicetag;
}
