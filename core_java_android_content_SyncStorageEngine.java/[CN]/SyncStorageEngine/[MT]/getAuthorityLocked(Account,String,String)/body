{
  AccountInfo account=mAccounts.get(accountName);
  if (account == null) {
    if (tag != null) {
      Log.w(TAG,tag + ": unknown account " + accountName);
    }
    return null;
  }
  AuthorityInfo authority=account.authorities.get(authorityName);
  if (authority == null) {
    if (tag != null) {
      Log.w(TAG,tag + ": unknown authority " + authorityName);
    }
    return null;
  }
  return authority;
}
