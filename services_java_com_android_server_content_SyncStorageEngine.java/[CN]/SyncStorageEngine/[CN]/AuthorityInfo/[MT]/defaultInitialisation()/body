{
  syncable=-1;
  backoffTime=-1;
  backoffDelay=-1;
  PeriodicSync defaultSync;
  if (target.target_provider) {
    defaultSync=new PeriodicSync(target.account,target.provider,new Bundle(),DEFAULT_POLL_FREQUENCY_SECONDS,calculateDefaultFlexTime(DEFAULT_POLL_FREQUENCY_SECONDS));
    periodicSyncs.add(defaultSync);
  }
}
