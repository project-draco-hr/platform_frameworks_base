{
  if (DBG)   log("disconnectPce()");
  if (mService != null) {
    try {
      mService.disconnectPce();
      return true;
    }
 catch (    RemoteException e) {
      Log.e(TAG,e.toString());
    }
  }
 else {
    Log.w(TAG,"Proxy not attached to service");
    if (DBG)     log(Log.getStackTraceString(new Throwable()));
  }
  return false;
}
