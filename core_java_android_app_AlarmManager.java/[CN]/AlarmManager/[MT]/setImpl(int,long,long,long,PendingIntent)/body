{
  if (triggerAtMillis < 0) {
    if (mAlwaysExact) {
      throw new IllegalArgumentException("Invalid alarm trigger time " + triggerAtMillis);
    }
    triggerAtMillis=0;
  }
  try {
    mService.set(type,triggerAtMillis,windowMillis,intervalMillis,operation);
  }
 catch (  RemoteException ex) {
  }
}
