{
  if (obj == null) {
    return false;
  }
  if (this == obj) {
    return true;
  }
  if (obj instanceof StreamConfigurationMap) {
    final StreamConfigurationMap other=(StreamConfigurationMap)obj;
    return Arrays.equals(mConfigurations,other.mConfigurations) && Arrays.equals(mDurations,other.mDurations);
  }
  return false;
}
