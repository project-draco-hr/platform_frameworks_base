{
  if (mArray.size() > 0) {
    byte[] temp=mArray.toByteArray();
    mArray.reset();
    byte[] result=new byte[size];
    System.arraycopy(temp,0,result,0,size);
    if (temp.length != size) {
      mArray.write(temp,size,temp.length - size);
    }
    return result;
  }
 else {
    return null;
  }
}
