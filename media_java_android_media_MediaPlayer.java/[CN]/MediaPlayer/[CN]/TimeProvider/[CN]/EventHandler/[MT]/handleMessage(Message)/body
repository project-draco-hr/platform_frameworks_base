{
  if (msg.what == NOTIFY) {
switch (msg.arg1) {
case NOTIFY_TIME:
      notifyTimedEvent(false);
    break;
case REFRESH_AND_NOTIFY_TIME:
  notifyTimedEvent(true);
break;
case NOTIFY_STOP:
notifyStop();
break;
case NOTIFY_SEEK:
notifySeek();
break;
case NOTIFY_TRACK_DATA:
notifyTrackData((Pair<SubtitleTrack,byte[]>)msg.obj);
break;
}
}
}
