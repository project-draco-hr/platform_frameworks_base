{
  mEditIndex=-1;
  mTileDividerIndex=mTiles.size();
  for (int i=0; i < mTiles.size(); i++) {
    if (mTiles.get(i) == null) {
      if (mEditIndex == -1) {
        mEditIndex=i;
      }
 else {
        mTileDividerIndex=i;
      }
    }
  }
  if (mTiles.get(mTiles.size() - 1) == null) {
    mTiles.remove(mTiles.size() - 1);
  }
}
