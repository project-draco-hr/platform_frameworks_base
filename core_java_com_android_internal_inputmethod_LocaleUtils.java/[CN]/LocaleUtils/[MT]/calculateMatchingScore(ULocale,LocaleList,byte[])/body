{
  if (desired.isEmpty()) {
    return false;
  }
  boolean allZeros=true;
  for (int i=0; i < desired.size(); ++i) {
    final Locale loc=desired.get(i);
    if (!loc.getLanguage().equals(supported.getLanguage())) {
      out[i]=0;
    }
 else {
      out[i]=calculateMatchingSubScore(supported,ULocale.addLikelySubtags(ULocale.forLocale(loc)));
      if (allZeros && out[i] != 0) {
        allZeros=false;
      }
    }
  }
  return !allZeros;
}
