{
  super.setUp();
  dbDir=getContext().getDir(this.getClass().getName(),Context.MODE_PRIVATE);
  mDatabaseFile=new File(dbDir,DB_NAME);
  if (mDatabaseFile.exists()) {
    mDatabaseFile.delete();
  }
  mDatabase=SQLiteDatabase.openOrCreateDatabase(mDatabaseFile.getPath(),null,new MyDatabaseCorruptionHandler());
  assertNotNull(mDatabase);
}
