{
  if (mDrawable != null) {
    mDrawable.setCallback(null);
  }
  mDrawable=dr;
  if (dr != null) {
    dr.setCallback(this);
    dr.setVisible(isVisible(),true);
    dr.setState(getState());
    dr.setLevel(getLevel());
    dr.setBounds(getBounds());
    dr.setLayoutDirection(getLayoutDirection());
  }
}
