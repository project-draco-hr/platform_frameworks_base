{
  if (!pkgList.containsKey(pkg)) {
    if (baseProcessTracker != null) {
      ProcessStats.ProcessState state=tracker.getProcessStateLocked(pkg,info.uid,processName);
      pkgList.put(pkg,state);
      if (state != baseProcessTracker) {
        state.makeActive();
      }
    }
 else {
      pkgList.put(pkg,null);
    }
    return true;
  }
  return false;
}
