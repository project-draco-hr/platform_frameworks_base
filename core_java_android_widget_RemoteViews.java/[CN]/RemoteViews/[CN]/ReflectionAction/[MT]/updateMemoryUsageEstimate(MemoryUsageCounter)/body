{
switch (this.type) {
case BITMAP:
    if (this.value != null) {
      final Bitmap b=(Bitmap)this.value;
      final Bitmap.Config c=b.getConfig();
      int bpp=4;
      if (c != null) {
switch (c) {
case ALPHA_8:
          bpp=1;
        break;
case RGB_565:
case ARGB_4444:
      bpp=2;
    break;
case ARGB_8888:
  bpp=4;
break;
}
}
counter.bitmapIncrement(b.getWidth() * b.getHeight() * bpp);
}
break;
default :
break;
}
}
