{
  Context c;
  String packageName=mPackage;
  if (packageName != null) {
    try {
      c=context.createPackageContextAsUser(packageName,Context.CONTEXT_RESTRICTED,mUser);
    }
 catch (    NameNotFoundException e) {
      Log.e(LOG_TAG,"Package name " + packageName + " not found");
      c=context;
    }
  }
 else {
    c=context;
  }
  return c;
}
