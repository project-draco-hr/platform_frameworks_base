{
  Context c=null;
  String packageName=mPackage;
  if (packageName != null) {
    try {
      c=context.createPackageContext(packageName,0);
    }
 catch (    NameNotFoundException e) {
      Log.e(LOG_TAG,"Package name " + packageName + " not found");
      c=context;
    }
  }
 else {
    c=context;
  }
  mContext=c;
  return c;
}
