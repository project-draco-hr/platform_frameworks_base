{
  if (ph == null) {
    Log.e(TAG,"setPanelHolder: null PanelHolder",new Throwable());
    return;
  }
  ph.setBar(this);
  mPanelHolder=ph;
  final int N=ph.getChildCount();
  for (int i=0; i < N; i++) {
    final View v=ph.getChildAt(i);
    if (v != null && v instanceof PanelView) {
      addPanel((PanelView)v);
    }
  }
}
