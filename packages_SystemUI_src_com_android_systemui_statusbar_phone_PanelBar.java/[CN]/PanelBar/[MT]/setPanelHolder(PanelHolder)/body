{
  if (ph == null) {
    Slog.e(TAG,"setPanelHolder: null PanelHolder",new Throwable());
    return;
  }
  ph.setBar(this);
  mPanelHolder=ph;
  final int N=ph.getChildCount();
  for (int i=0; i < N; i++) {
    final PanelView v=(PanelView)ph.getChildAt(i);
    if (v != null) {
      addPanel(v);
    }
  }
}
