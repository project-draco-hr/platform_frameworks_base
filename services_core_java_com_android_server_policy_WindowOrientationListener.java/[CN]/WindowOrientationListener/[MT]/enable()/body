{
synchronized (mLock) {
    if (mSensor == null) {
      Slog.w(TAG,"Cannot detect sensors. Not enabled");
      return;
    }
    if (mEnabled == false) {
      if (LOG) {
        Slog.d(TAG,"WindowOrientationListener enabled");
      }
      mOrientationJudge.resetLocked();
      mSensorManager.registerListener(mOrientationJudge,mSensor,mRate,mHandler);
      mEnabled=true;
    }
  }
}
