{
  if (!checkConnection()) {
    ic.imageComplete(ImageConsumer.IMAGEERROR);
    return;
  }
  ImageConsumer cons=findConsumer(consumers,ic);
  if (cons == null) {
    ImageDecoder d=null;
    for (Iterator<ImageDecoder> i=decoders.iterator(); i.hasNext(); ) {
      d=i.next();
      cons=findConsumer(d.consumers,ic);
      if (cons != null) {
        break;
      }
    }
  }
  if (cons == null) {
    consumers.add(ic);
  }
}
