{
synchronized (this) {
    if (consumers.size() == 0) {
      loading=false;
      return;
    }
  }
  ImageDecoder d=createDecoder();
  if (d != null) {
    try {
      decoder.decodeImage();
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
 finally {
      removeDecoder(d);
      abortAllConsumers(d.consumers);
    }
  }
}
