{
  if (receiver == null) {
    return null;
  }
  final SynchronousResultReceiver.Result result=receiver.awaitResult(EXTERNAL_STATS_SYNC_TIMEOUT_MILLIS);
  if (result.bundle != null) {
    result.bundle.setDefusable(true);
    final T data=result.bundle.getParcelable(BatteryStats.RESULT_RECEIVER_CONTROLLER_KEY);
    if (data != null) {
      return data;
    }
  }
  Slog.e(TAG,"no controller energy info supplied");
  return null;
}
