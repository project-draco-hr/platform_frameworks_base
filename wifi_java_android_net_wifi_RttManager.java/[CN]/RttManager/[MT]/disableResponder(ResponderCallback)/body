{
  if (callback == null) {
    throw new IllegalArgumentException("callback cannot be null");
  }
  validateChannel();
  int key=removeListener(callback);
  if (key == INVALID_KEY) {
    Log.e(TAG,"responder not enabled yet");
    return;
  }
  mAsyncChannel.sendMessage(CMD_OP_DISABLE_RESPONDER,0,key);
}
