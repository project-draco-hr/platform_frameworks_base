{
  if (listener == null)   return INVALID_KEY;
synchronized (sListenerMapLock) {
    int key=getListenerKey(listener);
    if (key != INVALID_KEY) {
      return key;
    }
    do {
      key=sListenerKey++;
    }
 while (key == INVALID_KEY);
    sListenerMap.put(key,listener);
    return key;
  }
}
