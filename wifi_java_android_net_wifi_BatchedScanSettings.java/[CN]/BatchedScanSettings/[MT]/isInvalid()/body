{
  if (maxScansPerBatch != UNSPECIFIED && (maxScansPerBatch < MIN_SCANS_PER_BATCH || maxScansPerBatch > MAX_SCANS_PER_BATCH))   return true;
  if (maxApPerScan != UNSPECIFIED && (maxApPerScan < MIN_AP_PER_SCAN || maxApPerScan > MAX_AP_PER_SCAN))   return true;
  if (channelSetIsValid() == false)   return true;
  if (scanIntervalSec != UNSPECIFIED && (scanIntervalSec < MIN_INTERVAL_SEC || scanIntervalSec > MAX_INTERVAL_SEC))   return true;
  if (maxApForDistance != UNSPECIFIED && (maxApForDistance < MIN_AP_FOR_DISTANCE || maxApForDistance > MAX_AP_FOR_DISTANCE))   return true;
  return false;
}
