{
  super.makeNewLayout(w,hintWidth,boring,hintBoring,ellipsisWidth,bringIntoView);
  if (DebugFlags.DRAW_WEBTEXTVIEW || willNotDraw()) {
    float lineHeight=-1;
    if (mWebView != null) {
      float height=mWebView.nativeFocusCandidateLineHeight();
      if (height != -1) {
        lineHeight=height * mWebView.getScale();
      }
    }
    CharSequence text=getText();
    mLayout=new WebTextViewLayout(text,text,getPaint(),w,mLayout.getAlignment(),mLayout.getSpacingMultiplier(),mLayout.getSpacingAdd(),false,null,ellipsisWidth,lineHeight);
  }
}
