{
  super.setBounds(left,top,right,bottom);
  mLines[0]=left;
  mLines[1]=top + 1;
  mLines[2]=right;
  mLines[3]=top + 1;
  mLines[4]=right;
  mLines[5]=top;
  mLines[6]=right;
  mLines[7]=bottom;
  mLines[8]=left;
  mLines[9]=bottom;
  mLines[10]=right;
  mLines[11]=bottom;
  mLines[12]=left + 1;
  mLines[13]=top;
  mLines[14]=left + 1;
  mLines[15]=bottom;
}
