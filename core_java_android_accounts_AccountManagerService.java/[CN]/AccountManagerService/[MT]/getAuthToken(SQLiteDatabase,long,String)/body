{
  Cursor cursor=db.query(TABLE_AUTHTOKENS,new String[]{AUTHTOKENS_AUTHTOKEN},AUTHTOKENS_ACCOUNTS_ID + "=" + accountId+ " AND "+ AUTHTOKENS_TYPE+ "=?",new String[]{authTokenType},null,null,null);
  try {
    if (cursor.moveToNext()) {
      return cursor.getString(0);
    }
    return null;
  }
  finally {
    cursor.close();
  }
}
