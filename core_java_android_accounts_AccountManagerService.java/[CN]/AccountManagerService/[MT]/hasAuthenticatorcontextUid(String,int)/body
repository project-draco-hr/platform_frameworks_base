{
  for (  RegisteredServicesCache.ServiceInfo<AuthenticatorDescription> serviceInfo : mAuthenticatorCache.getAllServices()) {
    if (serviceInfo.type.type.equals(accountType)) {
      return (serviceInfo.uid == callingUid) || (mContext.getPackageManager().checkSignatures(serviceInfo.uid,callingUid) == PackageManager.SIGNATURE_MATCH);
    }
  }
  return false;
}
