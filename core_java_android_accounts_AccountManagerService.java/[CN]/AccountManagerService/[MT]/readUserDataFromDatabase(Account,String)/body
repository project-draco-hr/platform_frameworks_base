{
  SQLiteDatabase db=mOpenHelper.getReadableDatabase();
  Cursor cursor=db.query(TABLE_EXTRAS,new String[]{EXTRAS_VALUE},EXTRAS_ACCOUNTS_ID + "=(select _id FROM accounts WHERE name=? AND type=?) AND " + EXTRAS_KEY+ "=?",new String[]{account.name,account.type,key},null,null,null);
  try {
    if (cursor.moveToNext()) {
      return cursor.getString(0);
    }
    return null;
  }
  finally {
    cursor.close();
  }
}
