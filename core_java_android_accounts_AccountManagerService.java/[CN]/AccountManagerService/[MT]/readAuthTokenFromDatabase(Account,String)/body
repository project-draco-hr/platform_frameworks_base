{
  SQLiteDatabase db=mOpenHelper.getReadableDatabase();
  db.beginTransaction();
  try {
    long accountId=getAccountId(db,account);
    if (accountId < 0) {
      return null;
    }
    return getAuthToken(db,accountId,authTokenType);
  }
  finally {
    db.setTransactionSuccessful();
    db.endTransaction();
  }
}
