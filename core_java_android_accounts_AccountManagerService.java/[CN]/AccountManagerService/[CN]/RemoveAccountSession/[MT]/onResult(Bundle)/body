{
  if (result != null && result.containsKey(Constants.BOOLEAN_RESULT_KEY) && !result.containsKey(Constants.INTENT_KEY)) {
    final boolean removalAllowed=result.getBoolean(Constants.BOOLEAN_RESULT_KEY);
    if (removalAllowed) {
      removeAccount(mAccount);
    }
    IAccountManagerResponse response=getResponseAndClose();
    if (response != null) {
      Bundle result2=new Bundle();
      result2.putBoolean(Constants.BOOLEAN_RESULT_KEY,removalAllowed);
      try {
        response.onResult(result2);
      }
 catch (      RemoteException e) {
      }
    }
  }
  super.onResult(result);
}
