{
  Log.e(TAG,"upgrade from version " + oldVersion + " to version "+ newVersion);
  if (oldVersion == 1) {
    oldVersion++;
  }
  if (oldVersion == 2) {
    createGrantsTable(db);
    db.execSQL("DROP TRIGGER " + TABLE_ACCOUNTS + "Delete");
    createAccountsDeletionTrigger(db);
    oldVersion++;
  }
}
