{
  if (Environment.isExternalStorageEmulated()) {
    return;
  }
  IMountService ms=getMs();
  assertEquals(StorageResultCode.OperationSucceeded,createContainer("testUnmountBusyContainer",4,"none",FS_FAT,true));
  String path=ms.getSecureContainerPath(SECURE_CONTAINER_PREFIX + "testUnmountBusyContainer");
  File f=new File(path,"reference");
  FileOutputStream fos=new FileOutputStream(f);
  assertEquals(StorageResultCode.OperationFailedStorageBusy,unmountContainer("testUnmountBusyContainer",false));
  fos.close();
  assertEquals(StorageResultCode.OperationSucceeded,unmountContainer("testUnmountBusyContainer",false));
}
