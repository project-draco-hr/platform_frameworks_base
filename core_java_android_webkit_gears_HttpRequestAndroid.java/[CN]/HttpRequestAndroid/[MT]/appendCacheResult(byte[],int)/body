{
  if (cacheResult == null) {
    log("appendCacheResult() called without a CacheResult initialized");
    return false;
  }
  try {
    cacheResult.getOutputStream().write(data,0,bytes);
  }
 catch (  IOException ex) {
    log("Got IOException writing cache data: " + ex);
    return false;
  }
  return true;
}
