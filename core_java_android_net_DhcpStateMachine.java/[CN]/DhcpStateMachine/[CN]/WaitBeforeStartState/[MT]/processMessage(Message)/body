{
  boolean retValue=HANDLED;
  if (DBG)   Log.d(TAG,getName() + message.toString() + "\n");
switch (message.what) {
case CMD_PRE_DHCP_ACTION_COMPLETE:
    if (runDhcpStart()) {
      transitionTo(mRunningState);
    }
 else {
      transitionTo(mPollingState);
    }
  break;
case CMD_STOP_DHCP:
transitionTo(mStoppedState);
break;
case CMD_START_DHCP:
break;
default :
retValue=NOT_HANDLED;
break;
}
return retValue;
}
