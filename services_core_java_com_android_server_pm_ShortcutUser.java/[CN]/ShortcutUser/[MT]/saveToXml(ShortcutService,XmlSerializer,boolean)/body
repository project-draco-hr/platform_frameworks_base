{
  out.startTag(null,TAG_ROOT);
  ShortcutService.writeAttr(out,ATTR_KNOWN_LOCALE_CHANGE_SEQUENCE_NUMBER,mKnownLocaleChangeSequenceNumber);
  ShortcutService.writeTagValue(out,TAG_LAUNCHER,mLauncherComponent);
{
    final int size=mLaunchers.size();
    for (int i=0; i < size; i++) {
      saveShortcutPackageItem(s,out,mLaunchers.valueAt(i),forBackup);
    }
  }
{
    final int size=mPackages.size();
    for (int i=0; i < size; i++) {
      saveShortcutPackageItem(s,out,mPackages.valueAt(i),forBackup);
    }
  }
  out.endTag(null,TAG_ROOT);
}
