{
  String newSetting=Settings.Secure.getString(mContext.getContentResolver(),COARSE_ACCURACY_CONFIG_NAME);
  if (D) {
    Log.d(TAG,"loadCoarseAccuracy: newSetting = \"" + newSetting + "\"");
  }
  if (newSetting == null) {
    return DEFAULT_ACCURACY_IN_METERS;
  }
  try {
    return Float.parseFloat(newSetting);
  }
 catch (  NumberFormatException e) {
    return DEFAULT_ACCURACY_IN_METERS;
  }
}
