{
  if (mVerb != VERB_BINDING) {
    Slog.e(TAG,"Sending onStartTask for a task that isn't pending. " + VERB_STRINGS[mVerb]);
    closeAndCleanupTaskH(false);
    return;
  }
  if (mCancelled.get()) {
    if (DEBUG) {
      Slog.d(TAG,"Task cancelled while waiting for bind to complete. " + mRunningTask);
    }
    closeAndCleanupTaskH(true);
    return;
  }
  try {
    mVerb=VERB_STARTING;
    scheduleOpTimeOut();
    service.startTask(mParams);
  }
 catch (  RemoteException e) {
    Log.e(TAG,"Error sending onStart message to '" + mRunningTask.getServiceComponent().getShortClassName() + "' ",e);
  }
}
