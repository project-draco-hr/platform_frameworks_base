{
switch (started.verb) {
case VERB_STARTING:
    started.verb=VERB_EXECUTING;
  if (!workOngoing) {
    handleFinishedH(started,false);
    return;
  }
 else   if (started.cancelled.get()) {
    handleCancelH(started);
    return;
  }
 else {
    scheduleOpTimeOut(started);
  }
break;
default :
Log.e(TAG,"Handling started task but task wasn't starting! " + started);
return;
}
}
