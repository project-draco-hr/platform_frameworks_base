{
  if (Log.isLoggable(TaskManagerService.TAG,Log.DEBUG)) {
    Log.d(TAG,"MSG_TIMEOUT of " + mRunningTask.getServiceComponent().getShortClassName() + " : "+ mParams.getTaskId());
  }
  final int taskId=mParams.getTaskId();
switch (mVerb) {
case VERB_STARTING:
    Log.e(TAG,"No response from client for onStartTask '" + mRunningTask.getServiceComponent().getShortClassName() + "' tId: "+ taskId);
  closeAndCleanupTaskH(false);
break;
case VERB_STOPPING:
Log.e(TAG,"No response from client for onStopTask, '" + mRunningTask.getServiceComponent().getShortClassName() + "' tId: "+ taskId);
closeAndCleanupTaskH(true);
break;
case VERB_EXECUTING:
Log.i(TAG,"Client timed out while executing (no taskFinished received)." + " Reporting failure and asking for reschedule. " + mRunningTask.getServiceComponent().getShortClassName() + "' tId: "+ taskId);
sendStopMessageH();
break;
default :
Log.e(TAG,"Handling timeout for an unknown active task state: " + mRunningTask);
return;
}
}
