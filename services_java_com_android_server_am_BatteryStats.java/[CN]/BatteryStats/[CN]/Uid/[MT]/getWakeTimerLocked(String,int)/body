{
  Wakelock wl=wakelockStats.get(name);
  if (wl == null) {
    wl=new Wakelock();
    wakelockStats.put(name,wl);
  }
  Timer t=null;
switch (type) {
case WAKE_TYPE_PARTIAL:
    t=wl.wakeTimePartial;
  if (t == null) {
    t=new Timer();
    wl.wakeTimePartial=t;
  }
return t;
case WAKE_TYPE_FULL:
t=wl.wakeTimeFull;
if (t == null) {
t=new Timer();
wl.wakeTimeFull=t;
}
return t;
case WAKE_TYPE_WINDOW:
t=wl.wakeTimeWindow;
if (t == null) {
t=new Timer();
wl.wakeTimeWindow=t;
}
return t;
}
return t;
}
