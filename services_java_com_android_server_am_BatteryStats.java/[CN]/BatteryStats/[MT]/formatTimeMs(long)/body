{
  long sec=time / 1000;
  StringBuilder sb=new StringBuilder();
  sb.append(sec);
  if (time != 0) {
    sb.append('.');
    sb.append((char)(((time / 100) % 10) + '0'));
    sb.append((char)(((time / 10) % 10) + '0'));
    sb.append((char)((time % 10) + '0'));
  }
  sb.append(" sec");
  return sb.toString();
}
