{
  String arg=nextArg();
  if (arg.indexOf('.') >= 0) {
    doRestorePackage(arg);
  }
 else {
    try {
      long token=Long.parseLong(nextArg(),16);
      doRestoreAll(token);
    }
 catch (    NumberFormatException e) {
      showUsage();
      return;
    }
  }
  System.out.println("done");
}
