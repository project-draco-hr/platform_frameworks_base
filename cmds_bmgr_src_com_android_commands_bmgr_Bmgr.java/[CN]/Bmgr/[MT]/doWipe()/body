{
  String pkg=nextArg();
  if (pkg == null) {
    showUsage();
    return;
  }
  try {
    mBmgr.clearBackupData(pkg);
    System.out.println("Wiped backup data for " + pkg);
  }
 catch (  RemoteException e) {
    System.err.println(e.toString());
    System.err.println(BMGR_NOT_RUNNING_ERR);
  }
}
