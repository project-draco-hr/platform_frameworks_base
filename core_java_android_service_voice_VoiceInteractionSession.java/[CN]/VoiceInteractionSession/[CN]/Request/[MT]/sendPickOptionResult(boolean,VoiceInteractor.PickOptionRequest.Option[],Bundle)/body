{
  try {
    if (DEBUG)     Log.d(TAG,"sendPickOptionResult: req=" + mInterface + " finished="+ finished+ " selections="+ selections+ " result="+ result);
    if (finished) {
      finishRequest();
    }
    mCallback.deliverPickOptionResult(mInterface,finished,selections,result);
  }
 catch (  RemoteException e) {
  }
}
