{
synchronized (this) {
    Request req=mActiveRequests.get(callback.asBinder());
    if (req != null) {
      if (newRequest) {
        throw new IllegalArgumentException("Given request callback " + callback + " is already active");
      }
      return req;
    }
    req=new Request(callback,mHandlerCaller);
    mActiveRequests.put(callback.asBinder(),req);
    return req;
  }
}
