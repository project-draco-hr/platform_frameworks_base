{
  final DragEvent ev;
synchronized (gRecyclerLock) {
    if (gRecyclerTop == null) {
      return new DragEvent();
    }
    ev=gRecyclerTop;
    gRecyclerTop=ev.mNext;
    gRecyclerUsed-=1;
  }
  ev.mRecycledLocation=null;
  ev.mRecycled=false;
  ev.mNext=null;
  ev.mAction=action;
  ev.mX=x;
  ev.mY=y;
  ev.mClipDescription=description;
  ev.mClipData=data;
  return ev;
}
