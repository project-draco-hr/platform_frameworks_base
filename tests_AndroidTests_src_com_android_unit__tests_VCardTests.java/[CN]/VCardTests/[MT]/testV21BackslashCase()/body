{
  VCardParser_V21 parser=new VCardParser_V21();
  VDataBuilder builder=new VDataBuilder();
  InputStream is=getContext().getResources().openRawResource(R.raw.v21_backslash);
  assertEquals(true,parser.parse(is,"ISO-8859-1",builder));
  is.close();
  assertEquals(1,builder.vNodeList.size());
  PropertyNodesVerifier verifier=new PropertyNodesVerifier(new PropertyNode("VERSION","2.1",null,null,null,null,null),new PropertyNode("N",";A;B\\;C\\;;D;:E;\\\\;",Arrays.asList("","A;B\\","C\\;","D",":E","\\\\",""),null,null,null,null),new PropertyNode("FN","A;B\\C\\;D:E\\\\",null,null,null,null,null));
  verifier.verify(builder.vNodeList.get(0));
}
