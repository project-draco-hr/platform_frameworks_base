{
  Thread.sleep(10000);
  mEndPid=getMediaserverPid();
  int memDiff=mEndMemory - startMemory;
  if (memDiff < 0) {
    memDiff=0;
  }
  output.write("The total diff = " + memDiff);
  output.write("\n\n");
  if (startPid != mEndPid) {
    output.write("mediaserver died. Test failed\n");
    return false;
  }
  if (memDiff > limit)   return false;
  return true;
}
