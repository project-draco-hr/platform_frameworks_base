{
switch (sMatcher.match(uri)) {
case URI_DOCS_ID:
{
      final String guid=uri.getPathSegments().get(1);
      final File file=guidToFile(guid);
      final File newFile=new File(file.getParentFile(),values.getAsString(DocumentColumns.DISPLAY_NAME));
      return file.renameTo(newFile) ? 1 : 0;
    }
default :
{
    throw new UnsupportedOperationException("Unsupported Uri " + uri);
  }
}
}
