{
  if (DEBUG)   Log.d(TAG,"moveTo " + from + " "+ to);
  int fromIndex=mTiles.indexOf(from);
  if (fromIndex < 0) {
    Log.e(TAG,"Unknown from tile " + from);
    return;
  }
  int index=mTiles.indexOf(to);
  if (index < 0) {
    Log.e(TAG,"Unknown to tile " + to);
    return;
  }
  mTiles.remove(fromIndex);
  mTiles.add(index,from);
  setTilesInternal();
}
