{
  for (  String suffix : fontFileSuffix) {
    String name=ttf + suffix + FONT_EXT;
    File f=new File(name);
    if (f.isFile()) {
      try {
        Font font=Font.createFont(Font.TRUETYPE_FONT,f);
        if (font != null) {
          return font;
        }
      }
 catch (      FontFormatException e) {
      }
catch (      IOException e) {
      }
    }
  }
  return null;
}
