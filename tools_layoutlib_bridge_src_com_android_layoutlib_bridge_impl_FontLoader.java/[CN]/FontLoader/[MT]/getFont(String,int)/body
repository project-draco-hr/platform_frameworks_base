{
  List<Font> result=new ArrayList<Font>();
  if (family == null) {
    return result;
  }
  for (  FontInfo info : mMainFonts) {
    if (info.families.contains(family)) {
      result.add(info.font[style]);
      break;
    }
  }
  for (  FontInfo info : mFallbackFonts) {
    result.add(info.font[style]);
  }
  return result;
}
