{
  Preconditions.checkNotNull(url,"url");
  beforeRemote();
  try {
    return mContentProvider.canonicalize(mPackageName,url);
  }
 catch (  DeadObjectException e) {
    if (!mStable) {
      mContentResolver.unstableProviderDied(mContentProvider);
    }
    throw e;
  }
 finally {
    afterRemote();
  }
}
