{
synchronized (this) {
    if (mReleased) {
      throw new IllegalStateException("Already released");
    }
    mReleased=true;
    mGuard.close();
    if (mStable) {
      return mContentResolver.releaseProvider(mContentProvider);
    }
 else {
      return mContentResolver.releaseUnstableProvider(mContentProvider);
    }
  }
}
