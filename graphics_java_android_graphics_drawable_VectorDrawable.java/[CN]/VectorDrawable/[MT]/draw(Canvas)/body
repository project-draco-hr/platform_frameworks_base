{
  copyBounds(mTmpBounds);
  if (mTmpBounds.width() <= 0 || mTmpBounds.height() <= 0) {
    return;
  }
  final ColorFilter colorFilter=(mColorFilter == null ? mTintFilter : mColorFilter);
  final long colorFilterNativeInstance=colorFilter == null ? 0 : colorFilter.native_instance;
  boolean canReuseCache=mVectorState.canReuseCache();
  nDraw(mVectorState.getNativeRenderer(),canvas.getNativeCanvasWrapper(),colorFilterNativeInstance,mTmpBounds,needMirroring(),canReuseCache);
}
