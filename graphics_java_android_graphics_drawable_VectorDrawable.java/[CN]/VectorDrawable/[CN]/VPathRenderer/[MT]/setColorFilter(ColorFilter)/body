{
  if (colorFilter != mColorFilter || (colorFilter != null && !colorFilter.equals(mColorFilter))) {
    mColorFilter=colorFilter;
    if (mFillPaint != null) {
      mFillPaint.setColorFilter(colorFilter);
    }
    if (mStrokePaint != null) {
      mStrokePaint.setColorFilter(colorFilter);
    }
    return true;
  }
  return false;
}
