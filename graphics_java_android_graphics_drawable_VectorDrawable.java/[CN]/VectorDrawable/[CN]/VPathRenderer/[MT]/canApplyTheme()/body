{
  final ArrayList<VGroup> groups=mGroupList;
  for (int i=groups.size() - 1; i >= 0; i--) {
    final ArrayList<VPath> paths=groups.get(i).mVGList;
    for (int j=paths.size() - 1; j >= 0; j--) {
      final VPath path=paths.get(j);
      if (path.canApplyTheme()) {
        return true;
      }
    }
  }
  return false;
}
