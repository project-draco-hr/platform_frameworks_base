{
  mTotalDuration=0;
  int size=mCurrentAnimList.size();
  for (int i=0; i < size; i++) {
    VAnimation vAnimation=mCurrentAnimList.get(i);
    long t=vAnimation.getTotalDuration();
    if (t == -1) {
      mTotalDuration=-1;
      return -1;
    }
    mTotalDuration=Math.max(mTotalDuration,t);
  }
  return mTotalDuration;
}
