{
switch (state) {
case 0:
    return UNKNOWN;
case 1:
  return ABSENT;
case 2:
return PIN_REQUIRED;
case 3:
return PUK_REQUIRED;
case 4:
return NETWORK_LOCKED;
case 5:
return READY;
case 6:
return NOT_READY;
case 7:
return PERM_DISABLED;
case 8:
return CARD_IO_ERROR;
default :
throw new IllegalArgumentException();
}
}
