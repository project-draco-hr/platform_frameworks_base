{
  if (isTabletUi() == false) {
    boolean windowFullscreen=getBooleanThemeValue(resources,"windowFullscreen",false);
    if (windowFullscreen == false) {
      mStatusBarSize=DEFAULT_STATUS_BAR_HEIGHT;
      ResourceValue value=resources.getFrameworkResource(ResourceType.DIMEN,"status_bar_height");
      if (value != null) {
        TypedValue typedValue=ResourceHelper.getValue(value.getValue());
        if (typedValue != null) {
          mStatusBarSize=(int)typedValue.getDimension(metrics);
        }
      }
    }
  }
}
