{
  RenderResources resources=mContext.getRenderResources();
  if (mParams.getTargetSdkVersion() < 11) {
    return false;
  }
  StyleResourceValue currentTheme=resources.getCurrentTheme();
  StyleResourceValue holoTheme=resources.getTheme("Theme.Holo",true);
  if (currentTheme == holoTheme || resources.themeIsParentOf(holoTheme,currentTheme)) {
    return true;
  }
  StyleResourceValue holoLightTheme=resources.getTheme("Theme.Holo.Light",true);
  if (currentTheme == holoLightTheme || resources.themeIsParentOf(holoLightTheme,currentTheme)) {
    return true;
  }
  return false;
}
