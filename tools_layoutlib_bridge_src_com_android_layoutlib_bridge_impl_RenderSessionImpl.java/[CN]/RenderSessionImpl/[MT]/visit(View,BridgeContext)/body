{
  if (view == null) {
    return null;
  }
  ViewInfo result=new ViewInfo(view.getClass().getName(),context.getViewKey(view),view.getLeft(),view.getTop(),view.getRight(),view.getBottom(),view,view.getLayoutParams());
  if (view instanceof ViewGroup) {
    ViewGroup group=((ViewGroup)view);
    result.setChildren(visitAllChildren(group,context));
  }
  return result;
}
