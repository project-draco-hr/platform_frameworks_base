{
  if (isTabletUi() && getBooleanThemeValue(resources,"windowIsFloating",true) == false) {
    mSystemBarSize=56;
    ResourceValue value=resources.getFrameworkResource(RenderResources.RES_DIMEN,"status_bar_height");
    if (value != null) {
      TypedValue typedValue=ResourceHelper.getValue(value.getValue());
      if (typedValue != null) {
        mSystemBarSize=(int)typedValue.getDimension(metrics);
      }
    }
  }
}
