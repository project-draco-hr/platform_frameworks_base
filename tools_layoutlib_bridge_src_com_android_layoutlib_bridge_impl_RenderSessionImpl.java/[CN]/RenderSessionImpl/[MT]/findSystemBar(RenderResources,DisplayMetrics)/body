{
  if (isTabletUi() && mWindowIsFloating == false) {
    mSystemBarSize=48;
    ResourceValue value=resources.getFrameworkResource(ResourceType.DIMEN,"status_bar_height");
    if (value != null) {
      TypedValue typedValue=ResourceHelper.getValue("status_bar_height",value.getValue(),true);
      if (typedValue != null) {
        mSystemBarSize=(int)typedValue.getDimension(metrics);
      }
    }
  }
}
