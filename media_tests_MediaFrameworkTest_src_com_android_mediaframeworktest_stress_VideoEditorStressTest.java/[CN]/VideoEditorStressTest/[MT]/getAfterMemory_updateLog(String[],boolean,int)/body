{
  System.gc();
  Thread.sleep(2500);
  AfterNativeMemory=Debug.getNativeHeapAllocatedSize();
  if (when == false) {
    loggingInfo[0]="\n Before Remove: iteration No.= " + iteration + "\t "+ (AfterNativeMemory - BeginNativeMemory);
  }
 else {
    loggingInfo[0]="\n After Remove: iteration No.= " + iteration + "\t "+ (AfterNativeMemory - BeginNativeMemory);
  }
  writeTimingInfo(loggingInfo);
  mMediaTestUtil.getMemoryLog();
}
