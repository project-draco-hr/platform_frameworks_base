{
  if (mSinglePane) {
    setResult(resultCode,resultData);
    finish();
  }
 else {
    if (caller != null) {
      if (caller.getTargetFragment() != null) {
        caller.getTargetFragment().onActivityResult(caller.getTargetRequestCode(),resultCode,resultData);
      }
    }
    onBackPressed();
  }
}
