{
  Fragment f;
  try {
    f=Fragment.instantiate(this,pref.getFragment());
  }
 catch (  Exception e) {
    Log.w(TAG,"Failure instantiating fragment " + pref.getFragment(),e);
    return false;
  }
  openFragmentTransaction().replace(com.android.internal.R.id.prefs,f).addToBackStack(BACK_STACK_PREFS).commit();
  return true;
}
