{
  String directionStr="??";
switch (direction) {
case View.FOCUS_UP:
    directionStr="FOCUS_UP";
  break;
case View.FOCUS_DOWN:
directionStr="FOCUS_DOWN";
break;
case View.FOCUS_LEFT:
directionStr="FOCUS_LEFT";
break;
case View.FOCUS_RIGHT:
directionStr="FOCUS_RIGHT";
break;
default :
fail("passed in unknown direction, ya blewit!");
}
return directionStr;
}
