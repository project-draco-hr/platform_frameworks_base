{
  assertDirectionIsCandidate(View.FOCUS_DOWN,new Rect(0,0,0,0),new Rect(0,0,20,1));
  assertDirectionIsCandidate(View.FOCUS_UP,new Rect(0,0,0,0),new Rect(0,-1,20,0));
  assertDirectionIsCandidate(View.FOCUS_LEFT,new Rect(0,0,0,0),new Rect(-1,0,0,20));
  assertDirectionIsCandidate(View.FOCUS_RIGHT,new Rect(0,0,0,0),new Rect(0,0,1,20));
}
