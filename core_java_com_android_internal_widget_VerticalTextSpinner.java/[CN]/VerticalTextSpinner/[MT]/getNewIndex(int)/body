{
  int index=mCurrentSelectedPos + offset;
  if (index < 0) {
    if (mWrapAround) {
      index+=mTextList.length;
    }
 else {
      return -1;
    }
  }
 else   if (index >= mTextList.length) {
    if (mWrapAround) {
      index-=mTextList.length;
    }
 else {
      return -1;
    }
  }
  return index;
}
