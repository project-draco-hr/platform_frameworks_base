{
  ArrayList<F> res=null;
  if (array != null) {
    for (int i=0; i < array.length; i++) {
      F cur=array[i];
      if (cur == null) {
        break;
      }
      if (filterEquals(cur,matching)) {
        if (res == null) {
          res=new ArrayList<F>();
        }
        res.add(cur);
      }
    }
  }
  return res;
}
