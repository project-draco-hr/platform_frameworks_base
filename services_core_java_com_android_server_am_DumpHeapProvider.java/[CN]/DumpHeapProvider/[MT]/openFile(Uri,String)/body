{
synchronized (sLock) {
    String path=uri.getEncodedPath();
    final String tag=Uri.decode(path);
    if (tag.equals("/java")) {
      return ParcelFileDescriptor.open(sHeapDumpJavaFile,ParcelFileDescriptor.MODE_READ_ONLY);
    }
 else {
      throw new FileNotFoundException("Invalid path for " + uri);
    }
  }
}
