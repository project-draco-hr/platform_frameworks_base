break;
case 13:
mTest=new Mandelbrot();
break;
}
mTest.createBaseTest(this,mBitmapIn,mBitmapIn2);
setupBars();
mTest.runTest();
updateDisplay();
mBenchmarkResult.setText("Result: not run");
}
