{
  if (mWin != null) {
    pw.print(prefix);
    pw.println(mTag);
    pw.print("  ");
    pw.print("mState");
    pw.print('=');
    pw.println(StatusBarManager.windowStateToString(mState));
    pw.print("  ");
    pw.print("mTransientBar");
    pw.print('=');
    pw.println(transientBarStateToString(mTransientBarState));
  }
}
