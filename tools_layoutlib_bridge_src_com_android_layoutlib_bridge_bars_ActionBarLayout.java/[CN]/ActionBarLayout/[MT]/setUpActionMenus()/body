{
  if (mMenuIds == null) {
    return;
  }
  ActionBarView actionBarView=(ActionBarView)findViewById(Bridge.getResourceId(ResourceType.ID,"action_bar"));
  final MenuInflater inflater=new MenuInflater(mActionBar.getThemedContext());
  for (  int id : mMenuIds) {
    inflater.inflate(id,mMenuBuilder);
  }
  actionBarView.setMenu(mMenuBuilder,null);
}
