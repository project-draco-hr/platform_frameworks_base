{
  if (systemCarrierAppsDisabledUntilUsed == null || systemCarrierAppsDisabledUntilUsed.length == 0) {
    return null;
  }
  List<ApplicationInfo> apps=null;
  try {
    apps=new ArrayList<>(systemCarrierAppsDisabledUntilUsed.length);
    for (    String packageName : systemCarrierAppsDisabledUntilUsed) {
      ApplicationInfo ai=packageManager.getApplicationInfo(packageName,PackageManager.GET_DISABLED_UNTIL_USED_COMPONENTS,userId);
      if (ai == null) {
        continue;
      }
      if (!ai.isSystemApp()) {
        continue;
      }
      apps.add(ai);
    }
  }
 catch (  RemoteException e) {
    Slog.w(TAG,"Could not reach PackageManager",e);
  }
  return apps;
}
