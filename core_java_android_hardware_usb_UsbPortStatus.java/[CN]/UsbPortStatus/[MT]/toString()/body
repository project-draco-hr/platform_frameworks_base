{
  return "UsbPortStatus{connected=" + isConnected() + ", currentMode="+ UsbPort.modeToString(mCurrentMode)+ ", currentPowerRole="+ UsbPort.powerRoleToString(mCurrentPowerRole)+ ", currentDataRole="+ UsbPort.dataRoleToString(mCurrentDataRole)+ ", supportedRoleCombinations="+ UsbPort.roleCombinationsToString(mSupportedRoleCombinations)+ "}";
}
