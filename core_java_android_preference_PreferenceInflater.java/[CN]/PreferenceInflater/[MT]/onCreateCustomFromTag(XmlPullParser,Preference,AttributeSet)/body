{
  final String tag=parser.getName();
  if (tag.equals(INTENT_TAG_NAME)) {
    Intent intent=null;
    try {
      intent=Intent.parseIntent(getContext().getResources(),parser,attrs);
    }
 catch (    IOException e) {
      Log.w(TAG,"Could not parse Intent.");
      Log.w(TAG,e);
    }
    if (intent != null) {
      parentPreference.setIntent(intent);
    }
    return true;
  }
  return false;
}
