{
  if (!TextUtils.isEmpty(mTransientIndication)) {
    return mTransientIndication;
  }
  if (mPowerPluggedIn) {
    String indication=computePowerIndication();
    if (DEBUG_CHARGING_SPEED) {
      indication+=",  " + (mChargingWattage / 1000) + " mW";
    }
    return indication;
  }
  return mRestingIndication;
}
