{
  new AsyncTask<List<PrinterRecord>,Void,Void>(){
    @Override protected Void doInBackground(    List<PrinterRecord>... printers){
      doWriteState(printers[0]);
      return null;
    }
    @Override protected void onPostExecute(    Void result){
      notifyChanged();
    }
  }
.executeOnExecutor(AsyncTask.SERIAL_EXECUTOR,new ArrayList<PrinterRecord>(mHistoricalPrinters));
}
