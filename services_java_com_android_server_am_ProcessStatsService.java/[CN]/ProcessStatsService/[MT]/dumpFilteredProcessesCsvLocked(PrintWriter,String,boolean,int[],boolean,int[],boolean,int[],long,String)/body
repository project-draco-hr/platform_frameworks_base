{
  ArrayList<ProcessStats.ProcessState> procs=mProcessStats.collectProcessesLocked(screenStates,memStates,procStates,procStates,now,reqPackage,false);
  if (procs.size() > 0) {
    if (header != null) {
      pw.println(header);
    }
    ProcessStats.dumpProcessListCsv(pw,procs,sepScreenStates,screenStates,sepMemStates,memStates,sepProcStates,procStates,now);
    return true;
  }
  return false;
}
