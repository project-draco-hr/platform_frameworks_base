{
  mOps=new int[bse.mNumOp * 4];
  BackStackEntry.Op op=bse.mHead;
  int pos=0;
  while (op != null) {
    mOps[pos++]=op.cmd;
    mOps[pos++]=op.fragment.mIndex;
    mOps[pos++]=op.enterAnim;
    mOps[pos++]=op.exitAnim;
    op=op.next;
  }
  mTransition=bse.mTransition;
  mTransitionStyle=bse.mTransitionStyle;
  mName=bse.mName;
}
