{
  if ((mType.mElement.mType == Element.DataType.SIGNED_16) || (mType.mElement.mType == Element.DataType.UNSIGNED_16) || (mType.mElement.mType == Element.DataType.FLOAT_16)) {
    return;
  }
  throw new RSIllegalArgumentException("16 bit integer source does not match allocation type " + mType.mElement.mType);
}
