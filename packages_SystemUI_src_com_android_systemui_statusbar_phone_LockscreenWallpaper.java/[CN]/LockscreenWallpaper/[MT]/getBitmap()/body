{
  if (mCached) {
    return mCache;
  }
  if (!mWallpaperManager.isWallpaperSupported()) {
    mCached=true;
    mCache=null;
    return null;
  }
  LoaderResult result=loadBitmap(mCurrentUserId,mSelectedUser);
  if (result.success) {
    mCached=true;
    mCache=result.bitmap;
  }
  return mCache;
}
