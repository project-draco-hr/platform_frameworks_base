{
  ConnectionRequest newRequest=new ConnectionRequest(request.getAccountHandle(),UUID.randomUUID().toString(),request.getHandle(),request.getHandlePresentation(),request.getExtras(),request.getVideoState());
  try {
    mConnectionService.createConnection(connectionManagerPhoneAccount,newRequest,isIncoming);
    RemoteConnection connection=new RemoteConnection(mConnectionService,request);
    mPendingConnections.add(connection);
    mConnectionById.put(newRequest.getCallId(),connection);
    return connection;
  }
 catch (  RemoteException e) {
    return RemoteConnection.failure(DisconnectCause.ERROR_UNSPECIFIED,e.toString());
  }
}
