{
  View child=getChildAt(index);
  if (child instanceof ImageView) {
    ImageView imageView=(ImageView)child;
    StringBuilder[] pathOut=new StringBuilder[1];
    Density[] densityInOut=new Density[]{density};
    LayoutDirection dir=isRtl ? LayoutDirection.RTL : null;
    InputStream stream=getIcon(iconName,densityInOut,dir,pathOut,true);
    density=densityInOut[0];
    String path=pathOut[0].toString();
    if (stream != null) {
      Bitmap bitmap=Bridge.getCachedBitmap(path,true);
      if (bitmap == null) {
        try {
          bitmap=Bitmap_Delegate.createBitmap(stream,false,density);
          Bridge.setCachedBitmap(path,bitmap,true);
        }
 catch (        IOException e) {
          return;
        }
      }
      if (bitmap != null) {
        BitmapDrawable drawable=new BitmapDrawable(getContext().getResources(),bitmap);
        imageView.setImageDrawable(drawable);
      }
    }
  }
}
