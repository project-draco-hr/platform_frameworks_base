{
  Density density=densityInOut[0];
  if (direction != null) {
    pathOut[0]="/bars/" + direction.getResourceValue() + "-"+ density.getResourceValue()+ "/"+ iconName;
  }
 else {
    pathOut[0]="/bars/" + density.getResourceValue() + "/"+ iconName;
  }
  InputStream stream=getClass().getResourceAsStream(pathOut[0]);
  if (stream == null && tryOtherDensities) {
    for (    Density d : Density.values()) {
      if (d != density) {
        densityInOut[0]=d;
        stream=getIcon(iconName,densityInOut,direction,pathOut,false);
        if (stream != null) {
          return stream;
        }
      }
    }
    if (direction != null) {
      return getIcon(iconName,densityInOut,null,pathOut,true);
    }
  }
  return stream;
}
