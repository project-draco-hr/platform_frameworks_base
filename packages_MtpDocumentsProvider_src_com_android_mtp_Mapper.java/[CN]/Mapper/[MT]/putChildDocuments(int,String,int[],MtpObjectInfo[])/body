{
  final ContentValues[] valuesList=new ContentValues[documents.length];
  for (int i=0; i < documents.length; i++) {
    valuesList[i]=new ContentValues();
    MtpDatabase.getObjectDocumentValues(valuesList[i],deviceId,parentId,operationsSupported,documents[i]);
  }
  putDocuments(parentId,valuesList,null,COLUMN_PARENT_DOCUMENT_ID + " = ?",strings(parentId),strings(COLUMN_OBJECT_HANDLE,Document.COLUMN_DISPLAY_NAME));
}
