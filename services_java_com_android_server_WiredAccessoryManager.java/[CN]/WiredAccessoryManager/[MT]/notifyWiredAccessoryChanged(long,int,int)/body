{
  if (LOG)   Slog.v(TAG,"notifyWiredAccessoryChanged: when=" + whenNanos + " bits="+ switchCodeToString(switchValues,switchMask)+ " mask="+ Integer.toHexString(switchMask));
synchronized (mLock) {
    int headset;
    mSwitchValues=(mSwitchValues & ~switchMask) | switchValues;
switch (mSwitchValues & (SW_HEADPHONE_INSERT_BIT | SW_MICROPHONE_INSERT_BIT)) {
case 0:
      headset=0;
    break;
case SW_HEADPHONE_INSERT_BIT:
  headset=BIT_HEADSET_NO_MIC;
break;
case SW_HEADPHONE_INSERT_BIT | SW_MICROPHONE_INSERT_BIT:
headset=BIT_HEADSET;
break;
case SW_MICROPHONE_INSERT_BIT:
headset=BIT_HEADSET;
break;
default :
headset=0;
break;
}
updateLocked(NAME_H2W,headset);
}
}
