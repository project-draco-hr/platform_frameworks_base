{
  final Pair<String,Drawable> p=arg != null ? (Pair<String,Drawable>)arg : mLastUpdate;
  if (p != null) {
    state.label=p.first;
    state.contentDescription=p.first;
    state.icon=new Icon(){
      @Override public Drawable getDrawable(      Context context){
        return p.second;
      }
    }
;
  }
 else {
  }
}
