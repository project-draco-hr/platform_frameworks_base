{
  final int N=mSignalExtractors.length;
  for (int i=0; i < N; i++) {
    NotificationSignalExtractor extractor=mSignalExtractors[i];
    try {
      RankingReconsideration recon=extractor.process(r);
      if (recon != null) {
        Message m=Message.obtain(mRankingHandler,NotificationManagerService.MESSAGE_RECONSIDER_RANKING,recon);
        long delay=recon.getDelay(TimeUnit.MILLISECONDS);
        mRankingHandler.sendMessageDelayed(m,delay);
      }
    }
 catch (    Throwable t) {
      Slog.w(TAG,"NotificationSignalExtractor failed.",t);
    }
  }
}
