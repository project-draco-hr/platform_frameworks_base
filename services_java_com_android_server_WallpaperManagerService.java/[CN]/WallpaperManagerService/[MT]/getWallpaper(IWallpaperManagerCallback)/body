{
synchronized (mLock) {
    try {
      mCallbacks.register(cb);
      File f=WALLPAPER_FILE;
      if (!f.exists()) {
        return null;
      }
      return ParcelFileDescriptor.open(f,MODE_READ_ONLY);
    }
 catch (    FileNotFoundException e) {
      Log.w(TAG,"Error getting wallpaper",e);
    }
    return null;
  }
}
