{
  boolean success=false;
synchronized (mLock) {
    loadSettingsLocked();
    if ("".equals(mName)) {
      success=true;
    }
 else {
      success=restoreNamedResourceLocked();
    }
  }
  if (!success) {
    Log.e(TAG,"Failed to restore wallpaper: '" + mName + "'");
    mName="";
    WALLPAPER_FILE.delete();
  }
  saveSettingsLocked();
}
