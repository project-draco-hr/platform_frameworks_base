{
  if (mClosed) {
    throw new IllegalArgumentException("Cannot write to an object that has already been closed.");
  }
  if (values != null) {
    try {
      mWriter.name(key.toLowerCase(Locale.US));
      mWriter.beginArray();
      for (      String value : values) {
        mWriter.value(value.toUpperCase(Locale.US));
      }
      mWriter.endArray();
    }
 catch (    IOException e) {
      throw new AssertionError("Unreachable exception.");
    }
  }
}
