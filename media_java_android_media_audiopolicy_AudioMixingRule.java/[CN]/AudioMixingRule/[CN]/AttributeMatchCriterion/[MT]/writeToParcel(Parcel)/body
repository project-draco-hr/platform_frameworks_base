{
  dest.writeInt(mRule);
  final int match_rule=mRule & ~RULE_EXCLUSION_MASK;
switch (match_rule) {
case RULE_MATCH_ATTRIBUTE_USAGE:
    dest.writeInt(mAttr.getUsage());
  break;
case RULE_MATCH_ATTRIBUTE_CAPTURE_PRESET:
dest.writeInt(mAttr.getCapturePreset());
break;
case RULE_MATCH_UID:
dest.writeInt(mIntProp.intValue());
break;
default :
Log.e("AttributeMatchCriterion","Unknown match rule" + match_rule + " when writing to Parcel");
dest.writeInt(-1);
}
}
