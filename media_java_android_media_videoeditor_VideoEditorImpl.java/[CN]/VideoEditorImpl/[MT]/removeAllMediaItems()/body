{
  mMANativeHelper.setGeneratePreview(true);
  mMediaItems.clear();
  for (  Transition transition : mTransitions) {
    transition.invalidate();
  }
  mTransitions.clear();
  mDurationMs=0;
  if ((new File(mProjectPath + "/" + THUMBNAIL_FILENAME)).exists()) {
    (new File(mProjectPath + "/" + THUMBNAIL_FILENAME)).delete();
  }
}
