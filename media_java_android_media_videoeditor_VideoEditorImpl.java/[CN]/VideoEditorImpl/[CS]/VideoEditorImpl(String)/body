{
  mProjectPath=projectPath;
  final File projectXml=new File(projectPath,PROJECT_FILENAME);
  if (projectXml.exists()) {
    try {
      load();
    }
 catch (    Exception ex) {
      throw new IOException(ex);
    }
  }
 else {
    mAspectRatio=MediaProperties.ASPECT_RATIO_16_9;
    mDurationMs=0;
  }
}
