{
  if (jpegThumbnail == null)   return false;
  Uri uri=getImageThumbnailUri(cr,origId,width,height);
  if (uri == null) {
    return false;
  }
  try {
    OutputStream thumbOut=cr.openOutputStream(uri);
    thumbOut.write(jpegThumbnail);
    thumbOut.close();
    return true;
  }
 catch (  Throwable t) {
    Log.e(TAG,"Unable to store thumbnail",t);
    return false;
  }
}
