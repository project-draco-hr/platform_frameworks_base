{
  StringBuffer sb=new StringBuffer();
  sb.append("SSID: ").append(ssid);
  if (friendlyName != null)   sb.append(" friendlyName: ").append(friendlyName);
  if (serverUri != null)   sb.append(" serverUri: ").append(serverUri);
  sb.append(" osuMethod: ").append(osuMethod);
  if (iconFileName != null) {
    sb.append(" icon: [").append(iconWidth).append("x").append(iconHeight).append(" ").append(iconType).append(" ").append(iconFileName);
  }
  if (osuNai != null)   sb.append(" osuNai: ").append(osuNai);
  if (osuService != null)   sb.append(" osuService: ").append(osuService);
  return sb.toString();
}
