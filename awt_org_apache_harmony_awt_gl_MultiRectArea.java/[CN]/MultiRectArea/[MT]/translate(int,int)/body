{
  for (int i=1; i < rect[0]; ) {
    rect[i++]+=x;
    rect[i++]+=y;
    rect[i++]+=x;
    rect[i++]+=y;
  }
  if (bounds != null && !bounds.isEmpty()) {
    bounds.translate(x,y);
  }
  if (rectangles != null) {
    for (    Rectangle element : rectangles) {
      element.translate(x,y);
    }
  }
}
