{
  int cnt=getRectCount();
  StringBuffer sb=new StringBuffer((cnt << 5) + 128);
  sb.append(getClass().getName()).append(" [");
  for (int i=1; i < rect[0]; i+=4) {
    sb.append(i > 1 ? ", [" : "[").append(rect[i]).append(", ").append(rect[i + 1]).append(", ").append(rect[i + 2] - rect[i] + 1).append(", ").append(rect[i + 3] - rect[i + 1] + 1).append("]");
  }
  return sb.append("]").toString();
}
