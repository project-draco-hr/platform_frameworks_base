{
  if (JobSchedulerService.DEBUG) {
    Slog.d(TAG,"Handling cancel for: " + mRunningJob.getJobId() + " "+ VERB_STRINGS[mVerb]);
  }
switch (mVerb) {
case VERB_BINDING:
case VERB_STARTING:
    mCancelled.set(true);
  break;
case VERB_EXECUTING:
if (hasMessages(MSG_CALLBACK)) {
  return;
}
sendStopMessageH();
break;
case VERB_STOPPING:
break;
default :
Slog.e(TAG,"Cancelling a job without a valid verb: " + mVerb);
break;
}
}
