{
switch (mVerb) {
case VERB_STARTING:
    mVerb=VERB_EXECUTING;
  if (!workOngoing) {
    handleFinishedH(false);
    return;
  }
if (mCancelled.get()) {
  if (DEBUG) {
    Slog.d(TAG,"Job cancelled while waiting for onStartJob to complete.");
  }
  handleCancelH();
  return;
}
scheduleOpTimeOut();
break;
default :
Slog.e(TAG,"Handling started job but job wasn't starting! Was " + VERB_STRINGS[mVerb] + ".");
return;
}
}
