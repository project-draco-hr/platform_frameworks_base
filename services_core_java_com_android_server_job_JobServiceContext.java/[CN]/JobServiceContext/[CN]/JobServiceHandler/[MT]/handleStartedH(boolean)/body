{
switch (mVerb) {
case VERB_STARTING:
    mVerb=VERB_EXECUTING;
  if (!workOngoing) {
    handleFinishedH(false);
    return;
  }
if (mCancelled.get()) {
  handleCancelH();
  return;
}
scheduleOpTimeOut();
break;
default :
Log.e(TAG,"Handling started job but job wasn't starting! Was " + VERB_STRINGS[mVerb] + ".");
return;
}
}
