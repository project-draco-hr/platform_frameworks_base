{
  removeMessages(MSG_TIMEOUT);
synchronized (mLock) {
    mWakeLock.release();
    mContext.unbindService(JobServiceContext.this);
    mCompletedListener.onJobCompleted(mRunningJob,reschedule);
    mWakeLock=null;
    mRunningJob=null;
    mParams=null;
    mVerb=-1;
    mCancelled.set(false);
    service=null;
    mAvailable=true;
  }
}
