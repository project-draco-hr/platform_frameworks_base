{
  for (int testStrength=SignalStrength.SIGNAL_STRENGTH_NONE_OR_UNKNOWN; testStrength <= SignalStrength.SIGNAL_STRENGTH_GREAT; testStrength++) {
    setupDefaultSignal();
    setCdma();
    setCdmaRoaming(true);
    setLevel(testStrength);
    verifyLastMobileDataIndicators(true,TelephonyIcons.TELEPHONY_SIGNAL_STRENGTH_ROAMING[1][testStrength],TelephonyIcons.ROAMING_ICON);
  }
}
