{
  byte unpacked[];
  unpacked=IccUtils.hexStringToBytes("61626320646566FF");
  assertEquals("abc def",GsmAlphabet.gsm8BitUnpackedToString(unpacked,0,unpacked.length,"euc-kr"));
  unpacked=IccUtils.hexStringToBytes("C5D7BDBAC6AEFF");
  assertEquals("\uD14C\uC2A4\uD2B8",GsmAlphabet.gsm8BitUnpackedToString(unpacked,0,unpacked.length,"euc-kr"));
  unpacked=GsmAlphabet.stringToGsm8BitPacked(sGsmExtendedChars);
  assertEquals(sGsmExtendedChars,GsmAlphabet.gsm8BitUnpackedToString(unpacked,0,unpacked.length,"euc-kr"));
}
