{
  InputStream inputStream=null;
  try {
    inputStream=new FileInputStream(srcFile);
    return copyToFile(inputStream,out);
  }
 catch (  IOException e) {
    return false;
  }
 finally {
    try {
      if (inputStream != null)       inputStream.close();
    }
 catch (    IOException e) {
    }
  }
}
