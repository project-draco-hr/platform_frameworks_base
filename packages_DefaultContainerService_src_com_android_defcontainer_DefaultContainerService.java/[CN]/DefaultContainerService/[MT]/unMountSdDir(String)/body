{
  try {
    getMountService().unmountSecureContainer(containerId);
    return true;
  }
 catch (  IllegalStateException e) {
    Log.e(TAG,"Failed to unmount id:  " + containerId + " with exception "+ e);
  }
catch (  RemoteException e) {
    Log.e(TAG,"MounteService not running?");
  }
  return false;
}
