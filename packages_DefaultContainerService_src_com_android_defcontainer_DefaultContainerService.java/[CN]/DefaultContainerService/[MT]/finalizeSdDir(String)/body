{
  try {
    getMountService().finalizeSecureContainer(containerId);
    return true;
  }
 catch (  IllegalStateException e) {
    Log.i(TAG,"Failed to finalize container for pkg : " + containerId);
  }
catch (  RemoteException e) {
    Log.e(TAG,"MountService not running?");
  }
  return false;
}
