{
  mCallback.userActivity(DIGIT_PRESS_WAKE_MILLIS);
  if (event.getAction() == MotionEvent.ACTION_DOWN) {
    if (actionId == EditorInfo.IME_NULL || actionId == EditorInfo.IME_ACTION_DONE || actionId == EditorInfo.IME_ACTION_NEXT) {
      mStateMachine.next();
      return true;
    }
  }
  return false;
}
