{
  if (view == mPrintButton) {
    PrinterInfo currentPrinter=getCurrentPrinter();
    if (currentPrinter != null) {
      confirmPrint();
    }
 else {
      cancelPrint();
    }
  }
 else   if (view == mMoreOptionsButton) {
    PrinterInfo currentPrinter=getCurrentPrinter();
    if (currentPrinter != null) {
      startAdvancedPrintOptionsActivity(currentPrinter);
    }
  }
}
