{
  if (view.mTitle == null) {
    SimpleMonthView_Delegate delegate=getDelegate(view);
    if (delegate.mTitleFormatter == null) {
      delegate.mTitleFormatter=new SimpleDateFormat(DateFormat.getBestDateTimePattern(getLocale(delegate,view),DEFAULT_TITLE_FORMAT));
    }
    view.mTitle=delegate.mTitleFormatter.format(view.mCalendar.getTime());
  }
  return view.mTitle;
}
