{
  if (getWindowToken() != null) {
    mUniverseTransform.getMatrix().getValues(mTmpFloats);
    try {
      mSession.setUniverseTransform(getWindowToken(),mUniverseTransform.getAlpha(),mTmpFloats[Matrix.MTRANS_X],mTmpFloats[Matrix.MTRANS_Y],mTmpFloats[Matrix.MSCALE_X],mTmpFloats[Matrix.MSKEW_Y],mTmpFloats[Matrix.MSKEW_X],mTmpFloats[Matrix.MSCALE_Y]);
    }
 catch (    RemoteException e) {
    }
  }
}
