{
  final long deltaNanos=Math.max(frameTimeNanos - mAnimLastTimeNanos,0);
  final float t=deltaNanos * 0.000000001f;
  final float y=mAnimY;
  final float v=mAnimVel;
  final float a=mAnimAccel;
  mAnimY=y + (v * t) + (0.5f * a * t* t);
  mAnimVel=v + (a * t);
  mAnimLastTimeNanos=frameTimeNanos;
}
