{
  long ident=Binder.clearCallingIdentity();
  try {
    mAm.revokeUriPermissionFromOwner(mPermissionOwner,ContentProvider.getUriWithoutUserId(uri),Intent.FLAG_GRANT_READ_URI_PERMISSION | Intent.FLAG_GRANT_WRITE_URI_PERMISSION,resolveUserId(uri,Process.myUid()));
  }
 catch (  RemoteException e) {
  }
 finally {
    Binder.restoreCallingIdentity(ident);
  }
}
