{
  long ident=Binder.clearCallingIdentity();
  try {
    mAm.grantUriPermissionFromOwner(mPermissionOwner,Process.myUid(),pkg,ContentProvider.getUriWithoutUserId(uri),Intent.FLAG_GRANT_READ_URI_PERMISSION,resolveUserId(uri,Process.myUid()));
  }
 catch (  RemoteException e) {
  }
 finally {
    Binder.restoreCallingIdentity(ident);
  }
}
