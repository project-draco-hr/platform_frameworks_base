{
  if (!"content".equals(uri.getScheme())) {
    return;
  }
  long ident=Binder.clearCallingIdentity();
  try {
    mAm.checkGrantUriPermission(uid,null,ContentProvider.getUriWithoutUserId(uri),Intent.FLAG_GRANT_READ_URI_PERMISSION,ContentProvider.getUserIdFromUri(uri,UserHandle.getUserId(uid)));
  }
 catch (  RemoteException e) {
  }
 finally {
    Binder.restoreCallingIdentity(ident);
  }
}
