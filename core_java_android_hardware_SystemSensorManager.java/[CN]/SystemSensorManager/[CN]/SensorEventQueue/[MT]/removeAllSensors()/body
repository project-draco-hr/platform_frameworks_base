{
  for (int i=0; i < mActiveSensors.size(); i++) {
    if (mActiveSensors.valueAt(i) == true) {
      int handle=mActiveSensors.keyAt(i);
      Sensor sensor=sHandleToSensor.get(handle);
      if (sensor != null) {
        disableSensor(sensor);
        mActiveSensors.put(handle,false);
      }
 else {
      }
    }
  }
}
