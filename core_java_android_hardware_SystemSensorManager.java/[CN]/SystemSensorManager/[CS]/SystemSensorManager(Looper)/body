{
  mMainLooper=mainLooper;
synchronized (sSensorModuleLock) {
    if (!sSensorModuleInitialized) {
      sSensorModuleInitialized=true;
      nativeClassInit();
      final ArrayList<Sensor> fullList=sFullSensorsList;
      int i=0;
      do {
        Sensor sensor=new Sensor();
        i=nativeGetNextSensor(sensor,i);
        if (i >= 0) {
          fullList.add(sensor);
          sHandleToSensor.append(sensor.getHandle(),sensor);
        }
      }
 while (i > 0);
      sSensorEventPool=new Pools.SynchronizedPool<SensorEvent>(Math.max(sFullSensorsList.size() * 2,1));
      sTriggerEventPool=new Pools.SynchronizedPool<TriggerEvent>(Math.max(sFullSensorsList.size() * 2,1));
    }
  }
}
