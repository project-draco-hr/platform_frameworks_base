{
  if (isNullOrEmpty(mVpnDns1) && isNullOrEmpty(mVpnDns2)) {
    return;
  }
  Log.d(TAG,String.format("restore vpn dns prop: %s --> %s",SystemProperties.get(DNS1),mVpnDns1));
  Log.d(TAG,String.format("restore vpn dns prop: %s --> %s",SystemProperties.get(DNS2),mVpnDns2));
  SystemProperties.set(DNS1,mVpnDns1);
  SystemProperties.set(DNS2,mVpnDns2);
}
