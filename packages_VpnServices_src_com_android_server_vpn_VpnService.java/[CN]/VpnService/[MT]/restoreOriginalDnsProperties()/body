{
  if (mVpnDns1.equals(SystemProperties.get(DNS1))) {
    Log.d(TAG,String.format("restore original dns prop: %s --> %s",SystemProperties.get(DNS1),mOriginalDns1));
    Log.d(TAG,String.format("restore original dns prop: %s --> %s",SystemProperties.get(DNS2),mOriginalDns2));
    SystemProperties.set(DNS1,mOriginalDns1);
    SystemProperties.set(DNS2,mOriginalDns2);
  }
}
