{
  String path=getProfilePath() + subpath;
  if (new File(path).exists()) {
    return path;
  }
 else {
    Log.w(TAG,"Profile subpath does not exist: " + path + ", use default one");
    String path2=getDefaultConfigPath() + subpath;
    if (!new File(path2).exists()) {
      throw new IOException("Profile subpath does not exist at " + path + " or "+ path2);
    }
    return path2;
  }
}
