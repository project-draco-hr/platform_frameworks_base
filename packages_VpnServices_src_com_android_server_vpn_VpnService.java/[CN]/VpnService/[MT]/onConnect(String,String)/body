{
  try {
    mState=VpnState.CONNECTING;
    broadcastConnectivity(VpnState.CONNECTING);
    String serverIp=getIp(getProfile().getServerName());
    onBeforeConnect();
    connect(serverIp,username,password);
    waitUntilConnectedOrTimedout();
    return true;
  }
 catch (  Throwable e) {
    Log.e(TAG,"onConnect()",e);
    onError(e);
    return false;
  }
}
