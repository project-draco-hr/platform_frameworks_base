{
  this.mGuessedNumCameras=0;
  if (CameraBinderTestUtils.isFeatureAvailable(mContext,PackageManager.FEATURE_CAMERA_FRONT)) {
    this.mGuessedNumCameras=getGuessedNumCameras() + 1;
  }
  if (CameraBinderTestUtils.isFeatureAvailable(mContext,PackageManager.FEATURE_CAMERA)) {
    this.mGuessedNumCameras=getGuessedNumCameras() + 1;
  }
  if (getGuessedNumCameras() == 0 && CameraBinderTestUtils.isFeatureAvailable(mContext,PackageManager.FEATURE_CAMERA_ANY)) {
    this.mGuessedNumCameras=getGuessedNumCameras() + 1;
  }
  Log.v(CameraBinderTest.TAG,"Guessing there are at least " + getGuessedNumCameras() + " cameras");
}
