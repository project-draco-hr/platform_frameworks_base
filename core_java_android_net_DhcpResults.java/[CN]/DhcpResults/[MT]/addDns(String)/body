{
  if (TextUtils.isEmpty(addrString) == false) {
    try {
      linkProperties.addDnsServer(NetworkUtils.numericToInetAddress(addrString));
    }
 catch (    IllegalArgumentException e) {
      Log.e(TAG,"addDns failed with addrString " + addrString);
      return true;
    }
  }
  return false;
}
