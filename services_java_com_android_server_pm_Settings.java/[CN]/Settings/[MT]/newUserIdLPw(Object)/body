{
  final int N=mUserIds.size();
  for (int i=0; i < N; i++) {
    if (mUserIds.get(i) == null) {
      mUserIds.set(i,obj);
      return Process.FIRST_APPLICATION_UID + i;
    }
  }
  if (N > (Process.LAST_APPLICATION_UID - Process.FIRST_APPLICATION_UID)) {
    return -1;
  }
  mUserIds.add(obj);
  return Process.FIRST_APPLICATION_UID + N;
}
