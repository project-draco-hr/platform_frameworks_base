{
  final int N=mUserIds.size();
  for (int i=0; i < N; i++) {
    if (mUserIds.get(i) == null) {
      mUserIds.set(i,obj);
      return PackageManagerService.FIRST_APPLICATION_UID + i;
    }
  }
  if (N >= PackageManagerService.MAX_APPLICATION_UIDS) {
    return -1;
  }
  mUserIds.add(obj);
  return PackageManagerService.FIRST_APPLICATION_UID + N;
}
