{
  super(context,attrs);
  final TypedArray a=mContext.obtainStyledAttributes(attrs,R.styleable.DatePicker,defStyleAttr,defStyleRes);
  final int resId=a.getResourceId(R.styleable.DatePicker_yearListItemTextAppearance,-1);
  if (resId != -1) {
    setTextAppearance(context,resId);
  }
  final Resources res=context.getResources();
  mItemIsSelectedText=res.getString(R.string.item_is_selected);
  a.recycle();
  init();
}
