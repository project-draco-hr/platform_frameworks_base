{
  boolean firstReading=true;
  while (true) {
    if (mCanceled) {
      break;
    }
    if (!parseOneVCard(firstReading)) {
      break;
    }
    firstReading=false;
  }
  if (mNestCount > 0) {
    boolean useCache=true;
    for (int i=0; i < mNestCount; i++) {
      readEndVCard(useCache,true);
      useCache=false;
    }
  }
}
