{
  FrameType throughputType=FrameType.single(Throughput.class);
  return new Signature().addInputPort("frame",Signature.PORT_REQUIRED,FrameType.any()).addOutputPort("throughput",Signature.PORT_REQUIRED,throughputType).addOutputPort("frame",Signature.PORT_REQUIRED,FrameType.any()).addInputPort("period",Signature.PORT_OPTIONAL,FrameType.single(int.class)).disallowOtherPorts();
}
