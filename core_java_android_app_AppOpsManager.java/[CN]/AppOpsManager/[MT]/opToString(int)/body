{
switch (op) {
case OP_COARSE_LOCATION:
    return "COARSE_LOCATION";
case OP_FINE_LOCATION:
  return "FINE_LOCATION";
case OP_GPS:
return "GPS";
case OP_VIBRATE:
return "VIBRATE";
case OP_READ_CONTACTS:
return "READ_CONTACTS";
case OP_WRITE_CONTACTS:
return "WRITE_CONTACTS";
case OP_READ_CALL_LOG:
return "READ_CALL_LOG";
case OP_WRITE_CALL_LOG:
return "WRITE_CALL_LOG";
default :
return "Unknown(" + op + ")";
}
}
