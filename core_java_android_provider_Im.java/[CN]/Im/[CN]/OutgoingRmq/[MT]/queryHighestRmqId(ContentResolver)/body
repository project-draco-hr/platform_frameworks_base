{
  Cursor cursor=resolver.query(Im.OutgoingRmq.CONTENT_URI_FOR_HIGHEST_RMQ_ID,RMQ_ID_PROJECTION,null,null,null);
  long retVal=0;
  try {
    if (cursor.moveToFirst()) {
      retVal=cursor.getLong(cursor.getColumnIndexOrThrow(RMQ_ID));
    }
  }
  finally {
    cursor.close();
  }
  return retVal;
}
