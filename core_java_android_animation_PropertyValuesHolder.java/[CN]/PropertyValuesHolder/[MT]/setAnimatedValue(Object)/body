{
  if (mSetter != null) {
    try {
      mTmpValueArray[0]=mAnimatedValue;
      mSetter.invoke(target,mTmpValueArray);
    }
 catch (    InvocationTargetException e) {
      Log.e("PropertyValuesHolder",e.toString());
    }
catch (    IllegalAccessException e) {
      Log.e("PropertyValuesHolder",e.toString());
    }
  }
}
