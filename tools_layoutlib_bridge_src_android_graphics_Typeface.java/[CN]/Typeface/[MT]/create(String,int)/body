{
  styleBuffer[0]=style;
  Font font=mFontLoader.getFont(familyName,styleBuffer);
  if (font != null) {
    ArrayList<Font> list=new ArrayList<Font>();
    list.add(font);
    list.addAll(mFontLoader.getFallBackFonts());
    return new Typeface(familyName,styleBuffer[0],list);
  }
  return null;
}
