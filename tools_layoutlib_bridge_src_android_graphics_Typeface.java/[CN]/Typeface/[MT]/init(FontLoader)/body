{
  mFontLoader=fontLoader;
  DEFAULT=create(DEFAULT_FAMILY,NORMAL);
  DEFAULT_BOLD=create(DEFAULT_FAMILY,BOLD);
  SANS_SERIF=create("sans-serif",NORMAL);
  SERIF=create("serif",NORMAL);
