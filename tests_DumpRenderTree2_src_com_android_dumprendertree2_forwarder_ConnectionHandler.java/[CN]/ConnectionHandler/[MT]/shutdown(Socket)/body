{
synchronized (mFromToPipe) {
synchronized (mToFromPipe) {
      try {
        socket.shutdownInput();
      }
 catch (      IOException e) {
        Log.e(LOG_TAG,"mFromToPipe=" + mFromToPipe + " mToFromPipe="+ mToFromPipe,e);
      }
      try {
        socket.shutdownOutput();
      }
 catch (      IOException e) {
        Log.e(LOG_TAG,"mFromToPipe=" + mFromToPipe + " mToFromPipe="+ mToFromPipe,e);
      }
      try {
        socket.close();
      }
 catch (      IOException e) {
        Log.e(LOG_TAG,"mFromToPipe=" + mFromToPipe + " mToFromPipe="+ mToFromPipe,e);
      }
    }
  }
}
