{
  assertEquals("going up from mMidShort2Bottom",mMidShort1Top,mFocusFinder.findNextFocus(mLayout,mMidShort2Bottom,View.FOCUS_UP));
  assertNull("going down from mMidShort2Bottom",mFocusFinder.findNextFocus(mLayout,mMidShort2Bottom,View.FOCUS_DOWN));
  assertEquals("going left from mMidShort2Bottom",mMidShort1Top,mFocusFinder.findNextFocus(mLayout,mMidShort2Bottom,View.FOCUS_LEFT));
  assertEquals("goind right from mMidShort2Bottom",mRightTall,mFocusFinder.findNextFocus(mLayout,mMidShort2Bottom,View.FOCUS_RIGHT));
}
