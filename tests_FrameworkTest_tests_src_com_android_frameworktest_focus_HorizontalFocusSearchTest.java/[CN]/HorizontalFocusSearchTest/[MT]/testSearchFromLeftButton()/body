{
  assertNull("going up from mLeftTall",mFocusFinder.findNextFocus(mLayout,mLeftTall,View.FOCUS_UP));
  assertNull("going down from mLeftTall",mFocusFinder.findNextFocus(mLayout,mLeftTall,View.FOCUS_DOWN));
  assertNull("going left from mLeftTall",mFocusFinder.findNextFocus(mLayout,mLeftTall,View.FOCUS_LEFT));
  assertEquals("going right from mLeftTall",mMidShort1Top,mFocusFinder.findNextFocus(mLayout,mLeftTall,View.FOCUS_RIGHT));
}
