{
  ArrayMap<Integer,String> map1=new ArrayMap<Integer,String>();
  ArrayMap<Integer,String> map2=new ArrayMap<Integer,String>();
  HashMap<Integer,String> map3=new HashMap<Integer,String>();
  if (!compare(map1,map2) || !compare(map1,map3) || !compare(map3,map2)) {
    Log.e("test","ArrayMap equals failure for empty maps " + map1 + ", "+ map2+ ", "+ map3);
    return false;
  }
  for (int i=0; i < 10; ++i) {
    String value=String.valueOf(i);
    map1.put(i,value);
    map2.put(i,value);
    map3.put(i,value);
  }
  if (!compare(map1,map2) || !compare(map1,map3) || !compare(map3,map2)) {
    Log.e("test","ArrayMap equals failure for populated maps " + map1 + ", "+ map2+ ", "+ map3);
    return false;
  }
  map1.remove(0);
  if (compare(map1,map2) || compare(map1,map3) || compare(map3,map1)) {
    Log.e("test","ArrayMap equals failure for map size " + map1 + ", "+ map2+ ", "+ map3);
    return false;
  }
  map1.put(0,"-1");
  if (compare(map1,map2) || compare(map1,map3) || compare(map3,map1)) {
    Log.e("test","ArrayMap equals failure for map contents " + map1 + ", "+ map2+ ", "+ map3);
    return false;
  }
  return true;
}
