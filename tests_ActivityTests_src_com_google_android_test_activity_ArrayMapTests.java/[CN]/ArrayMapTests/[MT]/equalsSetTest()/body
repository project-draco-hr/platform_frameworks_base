{
  ArraySet<Integer> set1=new ArraySet<Integer>();
  ArraySet<Integer> set2=new ArraySet<Integer>();
  HashSet<Integer> set3=new HashSet<Integer>();
  if (!compare(set1,set2) || !compare(set1,set3) || !compare(set3,set2)) {
    Log.e("test","ArraySet equals failure for empty sets " + set1 + ", "+ set2+ ", "+ set3);
    return false;
  }
  for (int i=0; i < 10; ++i) {
    set1.add(i);
    set2.add(i);
    set3.add(i);
  }
  if (!compare(set1,set2) || !compare(set1,set3) || !compare(set3,set2)) {
    Log.e("test","ArraySet equals failure for populated sets " + set1 + ", "+ set2+ ", "+ set3);
    return false;
  }
  set1.remove(0);
  if (compare(set1,set2) || compare(set1,set3) || compare(set3,set1)) {
    Log.e("test","ArraSet equals failure for set size " + set1 + ", "+ set2+ ", "+ set3);
    return false;
  }
  set1.add(-1);
  if (compare(set1,set2) || compare(set1,set3) || compare(set3,set1)) {
    Log.e("test","ArraySet equals failure for set contents " + set1 + ", "+ set2+ ", "+ set3);
    return false;
  }
  return true;
}
