{
  if (mExpectAutoPop && (mTargetActivity.getWindow().getAttributes().softInputMode & WindowManager.LayoutParams.SOFT_INPUT_MASK_ADJUST) == WindowManager.LayoutParams.SOFT_INPUT_ADJUST_RESIZE) {
    assertTrue(destructiveCheckImeUp(rootView,servedView));
  }
 else {
    assertFalse(destructiveCheckImeUp(rootView,servedView));
  }
}
