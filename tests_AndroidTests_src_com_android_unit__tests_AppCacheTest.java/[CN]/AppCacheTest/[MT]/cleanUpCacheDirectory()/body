{
  File testDir=mContext.getCacheDir();
  if (!testDir.exists()) {
    return;
  }
  String fList[]=testDir.list();
  if (fList == null) {
    testDir.delete();
    return;
  }
  for (int i=0; i < fList.length; i++) {
    File file=new File(testDir,fList[i]);
    if (file.isDirectory()) {
      cleanUpDirectory(testDir,fList[i]);
    }
 else {
      file.delete();
    }
  }
}
