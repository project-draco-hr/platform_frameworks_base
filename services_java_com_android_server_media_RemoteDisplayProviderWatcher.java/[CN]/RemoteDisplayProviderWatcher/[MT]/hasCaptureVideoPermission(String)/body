{
  if (mPackageManager.checkPermission(Manifest.permission.CAPTURE_VIDEO_OUTPUT,packageName) == PackageManager.PERMISSION_GRANTED) {
    return true;
  }
  if (mPackageManager.checkPermission(Manifest.permission.CAPTURE_SECURE_VIDEO_OUTPUT,packageName) == PackageManager.PERMISSION_GRANTED) {
    return true;
  }
  return false;
}
