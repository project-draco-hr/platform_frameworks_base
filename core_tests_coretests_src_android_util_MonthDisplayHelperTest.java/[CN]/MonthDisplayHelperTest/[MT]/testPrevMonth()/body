{
  MonthDisplayHelper helper=new MonthDisplayHelper(2007,Calendar.SEPTEMBER,Calendar.SUNDAY);
  assertArraysEqual(new int[]{26,27,28,29,30,31,1},helper.getDigitsForRow(0));
  helper.previousMonth();
  assertEquals(Calendar.AUGUST,helper.getMonth());
  assertArraysEqual(new int[]{29,30,31,1,2,3,4},helper.getDigitsForRow(0));
}
