{
  KeyguardHostView hostView=mThis.get();
  if (hostView == null) {
    return false;
  }
  if (pendingIntent.isActivity()) {
    hostView.setOnDismissAction(new OnDismissAction(){
      public boolean onDismiss(){
        try {
          Context context=view.getContext();
          ActivityOptions opts=ActivityOptions.makeScaleUpAnimation(view,0,0,view.getMeasuredWidth(),view.getMeasuredHeight());
          context.startIntentSender(pendingIntent.getIntentSender(),fillInIntent,Intent.FLAG_ACTIVITY_NEW_TASK,Intent.FLAG_ACTIVITY_NEW_TASK,0,opts.toBundle());
        }
 catch (        IntentSender.SendIntentException e) {
          android.util.Log.e(TAG,"Cannot send pending intent: ",e);
        }
catch (        Exception e) {
          android.util.Log.e(TAG,"Cannot send pending intent due to " + "unknown exception: ",e);
        }
        return false;
      }
    }
);
    if (hostView.mViewStateManager.isChallengeShowing()) {
      hostView.mViewStateManager.showBouncer(true);
    }
 else {
      hostView.mCallback.dismiss(false);
    }
    return true;
  }
 else {
    return super.onClickHandler(view,pendingIntent,fillInIntent);
  }
}
