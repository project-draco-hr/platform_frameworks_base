{
  ParameterBlock replica;
  try {
    replica=(ParameterBlock)super.clone();
  }
 catch (  Exception e) {
    return null;
  }
  if (sources != null) {
    replica.setSources((Vector<Object>)(sources.clone()));
  }
  if (parameters != null) {
    replica.setParameters((Vector<Object>)(parameters.clone()));
  }
  return replica;
}
