{
  if (mNotificationPanel.isShowing()) {
    return;
  }
  if (hasTicker(n.notification) && mStatusBarView.getWindowToken() != null) {
    if (0 == (mDisabled & (StatusBarManager.DISABLE_NOTIFICATION_ICONS | StatusBarManager.DISABLE_NOTIFICATION_TICKER))) {
      mTicker.add(key,n);
      mNotificationArea.setVisibility(View.GONE);
    }
  }
}
