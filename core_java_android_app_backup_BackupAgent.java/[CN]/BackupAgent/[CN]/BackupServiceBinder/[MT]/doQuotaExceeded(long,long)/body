{
  long ident=Binder.clearCallingIdentity();
  try {
    BackupAgent.this.onQuotaExceeded(backupDataBytes,quotaBytes);
  }
 catch (  Exception e) {
    Log.d(TAG,"onQuotaExceeded(" + BackupAgent.this.getClass().getName() + ") threw",e);
    throw e;
  }
 finally {
    waitForSharedPrefs();
    Binder.restoreCallingIdentity(ident);
  }
}
