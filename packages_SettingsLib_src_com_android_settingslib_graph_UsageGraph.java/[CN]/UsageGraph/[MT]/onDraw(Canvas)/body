{
  drawDivider(0,canvas);
  drawDivider((canvas.getHeight() - mDividerSize) / 2,canvas);
  drawDivider(canvas.getHeight() - mDividerSize,canvas);
  if (mLocalPaths.size() == 0) {
    return;
  }
  if (mShowProjection) {
    drawProjection(canvas);
  }
  drawFilledPath(canvas);
  drawLinePath(canvas);
}
