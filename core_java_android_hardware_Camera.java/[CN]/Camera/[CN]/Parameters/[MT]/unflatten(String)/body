{
  mMap.clear();
  StringTokenizer tokenizer=new StringTokenizer(flattened,";");
  while (tokenizer.hasMoreElements()) {
    String kv=tokenizer.nextToken();
    int pos=kv.indexOf('=');
    if (pos == -1) {
      continue;
    }
    String k=kv.substring(0,pos);
    String v=kv.substring(pos + 1);
    mMap.put(k,v);
  }
}
