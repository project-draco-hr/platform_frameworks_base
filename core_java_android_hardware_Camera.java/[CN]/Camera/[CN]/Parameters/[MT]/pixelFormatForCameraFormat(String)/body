{
  if (format == null)   return PixelFormat.UNKNOWN;
  if (format.equals(PIXEL_FORMAT_YUV422SP))   return PixelFormat.YCbCr_422_SP;
  if (format.equals(PIXEL_FORMAT_YUV420SP))   return PixelFormat.YCbCr_420_SP;
  if (format.equals(PIXEL_FORMAT_RGB565))   return PixelFormat.RGB_565;
  if (format.equals(PIXEL_FORMAT_JPEG))   return PixelFormat.JPEG;
  return PixelFormat.UNKNOWN;
}
