{
  if (format == null)   return PixelFormat.UNKNOWN;
  if (format.equals("yuv422sp"))   return PixelFormat.YCbCr_422_SP;
  if (format.equals("yuv420sp"))   return PixelFormat.YCbCr_420_SP;
  if (format.equals("rgb565"))   return PixelFormat.RGB_565;
  if (format.equals("jpeg"))   return PixelFormat.JPEG;
  return PixelFormat.UNKNOWN;
}
