{
  StringBuilder result=new StringBuilder("[");
  boolean firstValue=true;
  for (  @BlockModeEnum int mode : getSetFlags(modes)) {
    if (firstValue) {
      firstValue=false;
    }
 else {
      result.append(", ");
    }
    result.append(toString(mode));
  }
  result.append(']');
  return result.toString();
}
