{
switch (digest) {
case NONE:
    return "NONE";
case MD5:
  return "MD5";
case SHA1:
return "SHA-1";
case SHA224:
return "SHA-224";
case SHA256:
return "SHA-256";
case SHA384:
return "SHA-384";
case SHA512:
return "SHA-512";
default :
throw new IllegalArgumentException("Unknown digest: " + digest);
}
}
