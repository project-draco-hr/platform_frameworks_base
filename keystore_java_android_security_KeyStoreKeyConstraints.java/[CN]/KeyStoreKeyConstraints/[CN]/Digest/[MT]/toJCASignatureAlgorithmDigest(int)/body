{
switch (digest) {
case NONE:
    return "NONE";
case MD5:
  return "MD5";
case SHA1:
return "SHA1";
case SHA224:
return "SHA224";
case SHA256:
return "SHA256";
case SHA384:
return "SHA384";
case SHA512:
return "SHA512";
default :
throw new IllegalArgumentException("Unknown digest: " + digest);
}
}
