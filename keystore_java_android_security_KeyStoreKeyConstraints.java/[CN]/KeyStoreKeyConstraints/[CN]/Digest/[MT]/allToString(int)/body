{
  StringBuilder result=new StringBuilder("[");
  boolean firstValue=true;
  for (  @DigestEnum int digest : getSetFlags(digests)) {
    if (firstValue) {
      firstValue=false;
    }
 else {
      result.append(", ");
    }
    result.append(toString(digest));
  }
  result.append(']');
  return result.toString();
}
