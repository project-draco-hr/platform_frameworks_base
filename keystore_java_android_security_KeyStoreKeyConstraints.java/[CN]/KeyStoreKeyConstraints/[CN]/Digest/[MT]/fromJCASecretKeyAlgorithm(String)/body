{
  String algorithmLower=algorithm.toLowerCase(Locale.US);
  if (algorithmLower.startsWith("hmac")) {
    if ("hmacsha256".equals(algorithmLower)) {
      return SHA256;
    }
 else {
      throw new IllegalArgumentException("Unsupported digest: " + algorithmLower.substring("hmac".length()));
    }
  }
 else {
    return null;
  }
}
