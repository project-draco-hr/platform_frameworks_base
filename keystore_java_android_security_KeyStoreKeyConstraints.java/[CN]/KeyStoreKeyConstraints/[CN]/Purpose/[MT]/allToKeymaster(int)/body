{
  int[] result=new int[VALUE_COUNT];
  int resultCount=0;
  int purpose=1;
  for (int i=0; i < 32; i++) {
    if ((purposes & 1) != 0) {
      result[resultCount]=toKeymaster(purpose);
      resultCount++;
    }
    purposes>>>=1;
    purpose<<=1;
    if (purposes == 0) {
      break;
    }
  }
  return Arrays.copyOf(result,resultCount);
}
