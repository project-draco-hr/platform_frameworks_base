{
  if (name.isDirectory()) {
    String[] files=name.list();
    for (int i=0; files != null && i < files.length; i++) {
      File f=new File(name,files[i]);
      recursiveDelete(f);
    }
  }
  if (!name.delete()) {
    Log.e(TAG,"Can't delete: " + name);
  }
 else {
    Log.i(TAG,"Deleted: " + name);
  }
}
