{
  if (mRunning.containsKey(id)) {
    Log.w(TAG,"Duplicate job id: " + id + ". Ignoring job request for srcs: "+ srcs+ ", stack: "+ stack+ ".");
    return null;
  }
  Job job=null;
switch (operationType) {
case OPERATION_COPY:
    job=jobFactory.createCopy(this,getApplicationContext(),this,id,stack,srcs);
  break;
case OPERATION_MOVE:
job=jobFactory.createMove(this,getApplicationContext(),this,id,stack,srcs,srcParent);
break;
case OPERATION_DELETE:
job=jobFactory.createDelete(this,getApplicationContext(),this,id,stack,srcs,srcParent);
break;
default :
throw new UnsupportedOperationException();
}
assert(job != null);
return job;
}
