{
  boolean oldMClosed=mClosed;
  if (mClosed) {
    mClosed=false;
    compile(mQuery,false);
  }
  if (mBindArgs != null) {
    int len=mBindArgs.length;
    try {
      for (int i=0; i < len; i++) {
        super.bindString(i + 1,mBindArgs[i]);
      }
    }
 catch (    SQLiteMisuseException e) {
      StringBuilder errMsg=new StringBuilder("old mClosed " + oldMClosed + " mQuery "+ mQuery);
      for (int i=0; i < len; i++) {
        errMsg.append(" ");
        errMsg.append(mBindArgs[i]);
      }
      errMsg.append(" ");
      IllegalStateException leakProgram=new IllegalStateException(errMsg.toString(),e);
      throw leakProgram;
    }
  }
}
