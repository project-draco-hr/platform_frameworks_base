{
  if (mVersion == null) {
    int versionIdx=vcalStr.indexOf("\nVERSION:");
    mVersion=VERSION_VCALENDAR10;
    if (versionIdx != -1) {
      String versionStr=vcalStr.substring(versionIdx,vcalStr.indexOf("\n",versionIdx + 1));
      if (versionStr.indexOf("2.0") > 0)       mVersion=VERSION_VCALENDAR20;
    }
  }
  if (mVersion.equals(VERSION_VCALENDAR10))   mParser=new VCalParser_V10();
  if (mVersion.equals(VERSION_VCALENDAR20))   mParser=new VCalParser_V20();
}
