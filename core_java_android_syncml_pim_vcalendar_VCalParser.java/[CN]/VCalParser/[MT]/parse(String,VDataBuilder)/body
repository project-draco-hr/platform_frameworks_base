{
  vcalendarStr=verifyVCal(vcalendarStr);
  try {
    boolean isSuccess=mParser.parse(new ByteArrayInputStream(vcalendarStr.getBytes()),"US-ASCII",builder);
    if (!isSuccess) {
      if (mVersion.equals(VERSION_VCALENDAR10)) {
        if (Config.LOGD)         Log.d(TAG,"Parse failed for vCal 1.0 parser." + " Try to use 2.0 parser.");
        mVersion=VERSION_VCALENDAR20;
        return parse(vcalendarStr,builder);
      }
 else       throw new VCalException("parse failed.(even use 2.0 parser)");
    }
  }
 catch (  IOException e) {
    throw new VCalException(e.getMessage());
  }
  return true;
}
