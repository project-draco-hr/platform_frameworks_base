{
  if (language.length() == 3 && language.equalsIgnoreCase(locale.getISO3Language()) || language.length() == 2 && language.equalsIgnoreCase(locale.getLanguage())) {
    return 4096;
  }
 else   if (language.equalsIgnoreCase(GenericLocale)) {
    return 3072;
  }
 else   if (language.equalsIgnoreCase("eng")) {
    return 2048;
  }
 else {
    return 1024;
  }
}
