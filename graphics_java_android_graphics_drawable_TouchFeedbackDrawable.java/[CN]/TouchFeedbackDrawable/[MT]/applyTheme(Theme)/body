{
  super.applyTheme(t);
  final TouchFeedbackState state=mState;
  if (state == null) {
    throw new RuntimeException("Can't apply theme to <touch-feedback> with no constant state");
  }
  final int[] themeAttrs=state.mTouchThemeAttrs;
  if (themeAttrs != null) {
    final TypedArray a=t.resolveAttributes(themeAttrs,R.styleable.TouchFeedbackDrawable,0,0);
    updateStateFromTypedArray(a);
    a.recycle();
  }
}
