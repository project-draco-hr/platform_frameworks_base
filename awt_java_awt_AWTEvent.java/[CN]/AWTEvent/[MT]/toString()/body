{
  String name="";
  if (source instanceof Component && (source != null)) {
    Component comp=(Component)getSource();
    name=comp.getName();
    if (name == null) {
      name="";
    }
  }
  return (getClass().getName() + "[" + paramString()+ "]"+ " on "+ (name.length() > 0 ? name : source));
}
