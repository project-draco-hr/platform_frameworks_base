{
  if (!mCheckedConstantState) {
    mCanConstantState=true;
    final int N=mNumChildren;
    for (int i=0; i < N; i++) {
      if (mDrawables[i].getConstantState() == null) {
        mCanConstantState=false;
        break;
      }
    }
    mCheckedConstantState=true;
  }
  return mCanConstantState;
}
