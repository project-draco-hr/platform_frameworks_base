{
  Map<String,ContentValues> rows=activemap.getRows();
  for (  ContentValues values : rows.values()) {
    if (values.getAsString("account").equals(account.mName) && values.getAsString("account_type").equals(account.mType) && values.getAsString("authority").equals(authority)) {
      return true;
    }
  }
  return false;
}
