{
  double cos=Math.cos(angle);
  double sin=Math.sin(angle);
  int size=points.length;
  for (int i=0; i < size; i+=2) {
    float x=(float)(points[i] * cos - points[i + 1] * sin);
    float y=(float)(points[i] * sin + points[i + 1] * cos);
    points[i]=x;
    points[i + 1]=y;
  }
  return points;
}
