{
  if (!areItemsCoveredBySelection(mRelativePointer,mRelativeOrigin)) {
    return null;
  }
  RelativeCoordinate minXLocation=xLocation.compareTo(other.xLocation) < 0 ? xLocation : other.xLocation;
  RelativeCoordinate maxXLocation=minXLocation == xLocation ? other.xLocation : xLocation;
  RelativeCoordinate minYLocation=yLocation.compareTo(other.yLocation) < 0 ? yLocation : other.yLocation;
  RelativeCoordinate maxYLocation=minYLocation == yLocation ? other.yLocation : yLocation;
  Rect rect=new Rect();
  rect.left=getCoordinateValue(minXLocation,mXLimitsList,true);
  rect.right=getCoordinateValue(maxXLocation,mXLimitsList,false);
  rect.top=getCoordinateValue(minYLocation,mYLimitsList,true);
  rect.bottom=getCoordinateValue(maxYLocation,mYLimitsList,false);
  return rect;
}
