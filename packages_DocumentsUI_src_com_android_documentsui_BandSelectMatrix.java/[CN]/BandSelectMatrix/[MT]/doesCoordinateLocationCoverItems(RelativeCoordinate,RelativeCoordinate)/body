{
  if (pointerCoordinate.type == RelativeCoordinate.BEFORE_FIRST_ITEM && originCoordinate.type == RelativeCoordinate.BEFORE_FIRST_ITEM) {
    return false;
  }
  if (pointerCoordinate.type == RelativeCoordinate.AFTER_LAST_ITEM && originCoordinate.type == RelativeCoordinate.AFTER_LAST_ITEM) {
    return false;
  }
  if (pointerCoordinate.type == RelativeCoordinate.BETWEEN_TWO_ITEMS && originCoordinate.type == RelativeCoordinate.BETWEEN_TWO_ITEMS && pointerCoordinate.limitsBeforeCoordinate.equals(originCoordinate) && pointerCoordinate.limitsAfterCoordinate.equals(originCoordinate)) {
    return false;
  }
  return true;
}
