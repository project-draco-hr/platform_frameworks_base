{
  mSelectionForListeners.clear();
  if (rect == null) {
    return;
  }
  int columnIndex=Collections.binarySearch(mXLimitsList,new Limits(rect.left,rect.left));
  Preconditions.checkState(columnIndex >= 0);
  for (; columnIndex < mXLimitsList.size() && mXLimitsList.get(columnIndex).lowerLimit <= rect.right; columnIndex++) {
    List<ItemData> positions=mColumns.get(mXLimitsList.get(columnIndex).lowerLimit);
    int rowIndex=Collections.binarySearch(positions,new ItemData(0,rect.top));
    if (rowIndex < 0) {
      continue;
    }
    for (; rowIndex < positions.size() && positions.get(rowIndex).offset <= rect.bottom; rowIndex++) {
      mSelectionForListeners.append(positions.get(rowIndex).position,true);
    }
  }
}
