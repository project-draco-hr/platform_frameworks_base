{
  if (mXLimitsList.size() != mHelper.getNumColumns()) {
    recordLimits(mXLimitsList,new Limits(absoluteChildRect.left,absoluteChildRect.right));
  }
  if (mYLimitsList.size() != mHelper.getNumRows()) {
    recordLimits(mYLimitsList,new Limits(absoluteChildRect.top,absoluteChildRect.bottom));
  }
  List<ItemData> columnList=mColumns.get(absoluteChildRect.left);
  if (columnList == null) {
    columnList=new ArrayList<ItemData>();
    mColumns.put(absoluteChildRect.left,columnList);
  }
  recordItemData(columnList,new ItemData(adapterPosition,absoluteChildRect.top));
}
