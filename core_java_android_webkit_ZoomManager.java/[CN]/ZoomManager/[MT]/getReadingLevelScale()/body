{
  final float MIN_SCALE_DIFF=0.5f;
  final float zoomOverviewScale=getZoomOverviewScale();
  if (zoomOverviewScale > DEFAULT_READING_LEVEL_SCALE) {
    return Math.min(DEFAULT_READING_LEVEL_SCALE,zoomOverviewScale - MIN_SCALE_DIFF);
  }
  return Math.max(zoomOverviewScale + MIN_SCALE_DIFF,DEFAULT_READING_LEVEL_SCALE);
}
