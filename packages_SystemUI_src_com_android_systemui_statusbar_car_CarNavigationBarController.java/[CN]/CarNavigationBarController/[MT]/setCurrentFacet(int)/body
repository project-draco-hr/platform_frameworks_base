{
  if (index == mCurrentFacetIndex) {
    return;
  }
  if (mNavButtons.get(mCurrentFacetIndex) != null) {
    mNavButtons.get(mCurrentFacetIndex).setSelected(false,false);
  }
  if (mNavButtons.get(index) != null) {
    mNavButtons.get(index).setSelected(true,mFacetHasMultipleAppsCache.get(index));
  }
  mCurrentFacetIndex=index;
}
