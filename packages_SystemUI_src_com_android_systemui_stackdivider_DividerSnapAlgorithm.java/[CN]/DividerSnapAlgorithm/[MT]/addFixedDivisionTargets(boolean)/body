{
  int start=isHorizontalDivision ? mInsets.top : mInsets.left;
  int end=isHorizontalDivision ? mDisplayHeight - mInsets.bottom : mDisplayWidth - mInsets.right;
  mTargets.add(new SnapTarget((int)(start + mFixedRatio * (end - start)) - mDividerSize / 2,SnapTarget.FLAG_NONE));
  addMiddleTarget(isHorizontalDivision);
  mTargets.add(new SnapTarget((int)(start + (1 - mFixedRatio) * (end - start)) - mDividerSize / 2,SnapTarget.FLAG_NONE));
}
