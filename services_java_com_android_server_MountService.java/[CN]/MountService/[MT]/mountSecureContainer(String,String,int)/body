{
  if (mContext.checkCallingOrSelfPermission(android.Manifest.permission.ASEC_MOUNT_UNMOUNT) != PackageManager.PERMISSION_GRANTED) {
    throw new SecurityException("Requires ASEC_MOUNT_UNMOUNT permission");
  }
  String cmd=String.format("mount_asec %s %s %d",id,key,ownerUid);
  mConnector.doCommand(cmd);
  return getSecureContainerPath(id);
}
