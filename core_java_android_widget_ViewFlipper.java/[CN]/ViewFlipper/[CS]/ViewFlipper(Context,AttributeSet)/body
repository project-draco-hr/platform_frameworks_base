{
  super(context,attrs);
  TypedArray a=context.obtainStyledAttributes(attrs,com.android.internal.R.styleable.ViewFlipper);
  mFlipInterval=a.getInt(com.android.internal.R.styleable.ViewFlipper_flipInterval,DEFAULT_INTERVAL);
  mAutoStart=a.getBoolean(com.android.internal.R.styleable.ViewFlipper_autoStart,false);
  a.recycle();
}
