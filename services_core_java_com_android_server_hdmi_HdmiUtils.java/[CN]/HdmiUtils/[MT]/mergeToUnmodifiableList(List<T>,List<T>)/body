{
  if (a.isEmpty() && b.isEmpty()) {
    return Collections.emptyList();
  }
  if (a.isEmpty()) {
    return Collections.unmodifiableList(b);
  }
  if (b.isEmpty()) {
    return Collections.unmodifiableList(a);
  }
  List<T> newList=new ArrayList<>();
  newList.addAll(a);
  newList.addAll(b);
  return Collections.unmodifiableList(newList);
}
