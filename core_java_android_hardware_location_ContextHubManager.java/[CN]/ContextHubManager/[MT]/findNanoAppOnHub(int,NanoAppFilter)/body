{
  int[] temp;
  Integer[] retVal=null;
  try {
    temp=getBinder().findNanoAppOnHub(hubHandle,filter);
    retVal=new Integer[temp.length];
    for (int i=0; i < temp.length; i++) {
      retVal[i]=temp[i];
    }
  }
 catch (  RemoteException e) {
    Log.e(TAG,"Could not query nanoApp instance :" + e.toString());
  }
  return retVal;
}
