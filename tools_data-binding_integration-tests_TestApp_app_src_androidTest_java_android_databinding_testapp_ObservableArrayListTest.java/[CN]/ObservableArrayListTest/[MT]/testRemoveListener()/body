{
  mObservable.removeOnListChangedListener(mListener);
  mObservable.addOnListChangedListener(mListener);
  mObservable.add("Hello");
  mNotifications.clear();
  mObservable.removeOnListChangedListener(mListener);
  mObservable.add("World");
  assertTrue(mNotifications.isEmpty());
  mObservable.removeOnListChangedListener(mListener);
}
