{
  if (domain != null && 0 < domain.length()) {
    if (!mDomain.equals(domain)) {
      return false;
    }
  }
  if (secureHash != null) {
    int hashLength=secureHash.length;
    if (0 < hashLength) {
      if (hashLength == mHash.length) {
        for (int i=0; i < hashLength; ++i) {
          if (secureHash[i] != mHash[i]) {
            return false;
          }
        }
        return true;
      }
    }
  }
  return false;
}
