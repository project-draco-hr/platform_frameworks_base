{
  try {
    final Bundle result=AbstractAccountAuthenticator.this.getAuthToken(new AccountAuthenticatorResponse(response),account,authTokenType,loginOptions);
    if (result != null) {
      response.onResult(result);
    }
  }
 catch (  UnsupportedOperationException e) {
    response.onError(Constants.ERROR_CODE_UNSUPPORTED_OPERATION,"getAuthToken not supported");
  }
catch (  NetworkErrorException e) {
    response.onError(Constants.ERROR_CODE_NETWORK_ERROR,e.getMessage());
  }
}
