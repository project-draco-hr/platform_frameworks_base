{
  checkBinderPermission();
  final Bundle result;
  try {
    result=AbstractAccountAuthenticator.this.confirmCredentials(new AccountAuthenticatorResponse(response),account);
  }
 catch (  UnsupportedOperationException e) {
    response.onError(Constants.ERROR_CODE_UNSUPPORTED_OPERATION,"confirmCredentials not supported");
    return;
  }
  if (result != null) {
    response.onResult(result);
  }
}
