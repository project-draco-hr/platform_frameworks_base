{
  checkBinderPermission();
  try {
    final Bundle result=AbstractAccountAuthenticator.this.isCredentialsUpdateSuggested(new AccountAuthenticatorResponse(response),account,statusToken);
    if (result != null) {
      response.onResult(result);
    }
  }
 catch (  Exception e) {
    handleException(response,"isCredentialsUpdateSuggested",account.toString(),e);
  }
}
