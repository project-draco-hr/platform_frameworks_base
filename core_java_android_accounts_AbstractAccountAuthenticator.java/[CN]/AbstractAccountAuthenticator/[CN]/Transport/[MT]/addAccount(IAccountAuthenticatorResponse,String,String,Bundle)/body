{
  final Bundle result;
  try {
    result=AbstractAccountAuthenticator.this.addAccount(new AccountAuthenticatorResponse(response),accountType,authTokenType,options);
  }
 catch (  NetworkErrorException e) {
    response.onError(Constants.ERROR_CODE_NETWORK_ERROR,e.getMessage());
    return;
  }
catch (  UnsupportedOperationException e) {
    response.onError(Constants.ERROR_CODE_UNSUPPORTED_OPERATION,"addAccount not supported");
    return;
  }
  if (result != null) {
    response.onResult(result);
  }
}
