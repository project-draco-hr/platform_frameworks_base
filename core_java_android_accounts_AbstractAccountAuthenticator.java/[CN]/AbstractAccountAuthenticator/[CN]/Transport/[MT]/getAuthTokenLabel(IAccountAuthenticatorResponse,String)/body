{
  checkBinderPermission();
  try {
    Bundle result=new Bundle();
    result.putString(Constants.AUTH_TOKEN_LABEL_KEY,AbstractAccountAuthenticator.this.getAuthTokenLabel(authTokenType));
    response.onResult(result);
  }
 catch (  IllegalArgumentException e) {
    response.onError(Constants.ERROR_CODE_BAD_ARGUMENTS,"unknown authTokenType");
  }
catch (  UnsupportedOperationException e) {
    response.onError(Constants.ERROR_CODE_UNSUPPORTED_OPERATION,"getAuthTokenTypeLabel not supported");
  }
}
