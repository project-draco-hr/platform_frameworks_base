{
  if ("int".equals(className)) {
    return "0";
  }
  if ("short".equals(className)) {
    return "0";
  }
  if ("long".equals(className)) {
    return "0L";
  }
  if ("float".equals(className)) {
    return "0fL";
  }
  if ("double".equals(className)) {
    return "0.0";
  }
  if ("boolean".equals(className)) {
    return "false";
  }
  return "null";
}
