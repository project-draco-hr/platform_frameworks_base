{
  mAcculumalatedThumbDrag+=deltaAngle;
  if (mAcculumalatedThumbDrag > mCallbackThreshold || mAcculumalatedThumbDrag < -mCallbackThreshold) {
    if (mCallback != null) {
      boolean canStillZoom=mCallback.onZoomRingThumbDragged(mAcculumalatedThumbDrag / mCallbackThreshold,mAcculumalatedThumbDrag,mThumbDragStartAngle,touchAngle);
      mDisabler.setEnabling(canStillZoom);
    }
    mAcculumalatedThumbDrag=0;
  }
  setThumbAngle(touchAngle);
  mIsThumbAngleValid=true;
}
