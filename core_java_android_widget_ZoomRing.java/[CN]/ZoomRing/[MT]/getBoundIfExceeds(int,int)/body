{
  if (deltaAngle > 0) {
    if (mThumbCcwBound != Integer.MIN_VALUE && getDelta(startAngle,mThumbCcwBound,true) < deltaAngle) {
      return mThumbCcwBound;
    }
  }
 else   if (deltaAngle < 0) {
    int deltaThumbAndBound=getDelta(startAngle,mThumbCwBound,false);
    if (mThumbCwBound != Integer.MIN_VALUE && deltaThumbAndBound > deltaAngle) {
      return mThumbCwBound;
    }
  }
  return Integer.MIN_VALUE;
}
