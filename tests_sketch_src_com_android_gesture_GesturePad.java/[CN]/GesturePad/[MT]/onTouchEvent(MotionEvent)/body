{
  if (this.isEnabled() == false) {
    return true;
  }
switch (event.getAction()) {
case MotionEvent.ACTION_DOWN:
    touch_start(event);
  invalidate();
break;
case MotionEvent.ACTION_MOVE:
touch_move(event);
invalidate();
break;
case MotionEvent.ACTION_UP:
touch_up(event);
invalidate();
break;
}
return true;
}
