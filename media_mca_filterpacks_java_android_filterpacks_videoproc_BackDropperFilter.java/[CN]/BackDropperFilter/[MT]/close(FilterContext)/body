{
  if (mMemoryFormat == null) {
    return;
  }
  if (mLogVerbose)   Log.v(TAG,"Filter Closing!");
  for (int i=0; i < 2; i++) {
    mBgMean[i].release();
    mBgVariance[i].release();
    mMaskVerify[i].release();
  }
  mDistance.release();
  mMask.release();
  mAutoWB.release();
  mVideoInput.release();
  mBgInput.release();
  mMaskAverage.release();
  mMemoryFormat=null;
}
