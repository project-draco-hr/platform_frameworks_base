{
  if (scanResult == null) {
    return false;
  }
  BluetoothDevice device=scanResult.getDevice();
  if (mMacAddress != null && (device == null || !mMacAddress.equals(device.getAddress()))) {
    return false;
  }
  int rssi=scanResult.getRssi();
  if (rssi < mMinRssi || rssi > mMaxRssi) {
    return false;
  }
  byte[] scanRecordBytes=scanResult.getScanRecord();
  ScanRecord scanRecord=ScanRecord.parseFromBytes(scanRecordBytes);
  if (scanRecord == null && (mLocalName != null || mServiceUuid != null || mManufacturerData != null || mServiceData != null)) {
    return false;
  }
  if (mLocalName != null && !mLocalName.equals(scanRecord.getLocalName())) {
    return false;
  }
  if (mServiceUuid != null && !matchesServiceUuids(mServiceUuid,mServiceUuidMask,scanRecord.getServiceUuids())) {
    return false;
  }
  if (mServiceData != null && !matchesPartialData(mServiceData,mServiceDataMask,scanRecord.getServiceData())) {
    return false;
  }
  if (mManufacturerData != null && !matchesPartialData(mManufacturerData,mManufacturerDataMask,scanRecord.getManufacturerSpecificData())) {
    return false;
  }
  return true;
}
