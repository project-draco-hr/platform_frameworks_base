{
  Looper.prepare();
  if (!createHandler()) {
    return;
  }
synchronized (WebCoreThreadWatchdog.class) {
    if (!mPaused) {
      mWebCoreThreadHandler.obtainMessage(EventHub.HEARTBEAT,mHandler.obtainMessage(IS_ALIVE)).sendToTarget();
      mHandler.sendMessageDelayed(mHandler.obtainMessage(TIMED_OUT),TIMEOUT_PERIOD);
    }
  }
  Looper.loop();
}
