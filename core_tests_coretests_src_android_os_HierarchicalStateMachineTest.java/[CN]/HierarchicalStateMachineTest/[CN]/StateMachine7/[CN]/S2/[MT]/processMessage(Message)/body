{
  if (message.what == TEST_CMD_2) {
    mMsgCount+=1;
    mArrivalTimeMsg2=SystemClock.elapsedRealtime();
  }
 else   if (message.what == TEST_CMD_3) {
    mMsgCount+=1;
    mArrivalTimeMsg3=SystemClock.elapsedRealtime();
  }
  if (mMsgCount == 2) {
    transitionToHaltingState();
  }
  return HANDLED;
}
