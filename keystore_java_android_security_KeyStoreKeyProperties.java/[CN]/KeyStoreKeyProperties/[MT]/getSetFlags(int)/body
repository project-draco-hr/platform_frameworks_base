{
  if (flags == 0) {
    return EmptyArray.INT;
  }
  int result[]=new int[getSetBitCount(flags)];
  int resultOffset=0;
  int flag=1;
  while (flags != 0) {
    if ((flags & 1) != 0) {
      result[resultOffset]=flag;
      resultOffset++;
    }
    flags>>>=1;
    flag<<=1;
  }
  return result;
}
