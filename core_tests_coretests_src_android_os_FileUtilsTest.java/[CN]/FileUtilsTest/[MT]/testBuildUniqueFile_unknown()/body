{
  assertNameEquals("test",FileUtils.buildUniqueFile(mTarget,"application/octet-stream","test"));
  assertNameEquals("test.jpg",FileUtils.buildUniqueFile(mTarget,"application/octet-stream","test.jpg"));
  assertNameEquals(".test",FileUtils.buildUniqueFile(mTarget,"application/octet-stream",".test"));
  assertNameEquals("test",FileUtils.buildUniqueFile(mTarget,"lolz/lolz","test"));
  assertNameEquals("test.lolz",FileUtils.buildUniqueFile(mTarget,"lolz/lolz","test.lolz"));
}
