{
  touch("file1",-HOUR_IN_MILLIS);
  touch("file2",HOUR_IN_MILLIS);
  touch("file3",WEEK_IN_MILLIS);
  FileUtils.deleteOlderFiles(mDir,0,DAY_IN_MILLIS);
  assertDirContents("file1","file2");
  touch("file1",-HOUR_IN_MILLIS);
  touch("file2",HOUR_IN_MILLIS);
  touch("file3",WEEK_IN_MILLIS);
  FileUtils.deleteOlderFiles(mDir,0,DAY_IN_MILLIS);
  assertDirContents("file1","file2");
}
