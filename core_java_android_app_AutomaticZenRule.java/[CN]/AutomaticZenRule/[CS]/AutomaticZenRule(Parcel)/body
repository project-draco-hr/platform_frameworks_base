{
  enabled=source.readInt() == 1;
  if (source.readInt() == 1) {
    name=source.readString();
  }
  interruptionFilter=source.readInt();
  conditionId=source.readParcelable(null);
  owner=source.readParcelable(null);
  if (source.readInt() == 1) {
    id=source.readString();
  }
  creationTime=source.readLong();
}
