{
  String oldPackageName=Settings.Secure.getString(context.getContentResolver(),Settings.Secure.DIALER_DEFAULT_APPLICATION);
  if (packageName != null && oldPackageName != null && packageName.equals(oldPackageName)) {
    return;
  }
  List<String> packageNames=getInstalledDialerApplications(context);
  if (packageNames.contains(packageName)) {
    Settings.Secure.putString(context.getContentResolver(),Settings.Secure.DIALER_DEFAULT_APPLICATION,packageName);
  }
}
