{
  mDatabase.execSQL("CREATE TABLE test (_id INTEGER PRIMARY KEY, data INT);");
  final int count=505;
  String sql="INSERT INTO test (data) VALUES (?);";
  SQLiteStatement s=mDatabase.compileStatement(sql);
  for (int i=0; i < count; i++) {
    s.bindLong(1,i);
    s.execute();
  }
  int maxRead=500;
  int initialRead=5;
  SQLiteCursor c=(SQLiteCursor)mDatabase.rawQuery("select * from test;",null,initialRead,maxRead);
  TestObserver observer=new TestObserver(count,c);
  c.getCount();
  c.registerDataSetObserver(observer);
  if (!observer.quit) {
    Looper.loop();
  }
  c.close();
}
