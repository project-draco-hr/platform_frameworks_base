{
  WeakReferencedListener listener=mChangeListeners[localFieldId];
  if (listener == null) {
    if (observable == null) {
      return false;
    }
 else {
      listener=new WeakReferencedListener(this,localFieldId);
      mChangeListeners[localFieldId]=listener;
    }
  }
  return listener.register(observable);
}
