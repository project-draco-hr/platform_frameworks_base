{
synchronized (mAnchorsLock) {
    if (mAnchors != null) {
      return mAnchors;
    }
    Set<TrustAnchor> anchors=new ArraySet<TrustAnchor>();
    for (    CertificatesEntryRef ref : mCertificatesEntryRefs) {
      anchors.addAll(ref.getTrustAnchors());
    }
    mAnchors=anchors;
    return anchors;
  }
}
