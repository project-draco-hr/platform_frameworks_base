{
  if (obj instanceof NetworkIdentity) {
    final NetworkIdentity ident=(NetworkIdentity)obj;
    return mType == ident.mType && mSubType == ident.mSubType && mRoaming == ident.mRoaming && Objects.equals(mSubscriberId,ident.mSubscriberId) && Objects.equals(mNetworkId,ident.mNetworkId);
  }
  return false;
}
