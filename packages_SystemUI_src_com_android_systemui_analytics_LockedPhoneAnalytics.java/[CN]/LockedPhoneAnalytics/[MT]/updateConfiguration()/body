{
  mEnableAnalytics=Build.IS_DEBUGGABLE && 0 != Settings.Secure.getInt(mContext.getContentResolver(),ANALYTICS_ENABLE,0);
  mEnforceBouncer=mEnableAnalytics && 0 != Settings.Secure.getInt(mContext.getContentResolver(),ENFORCE_BOUNCER,0);
  mCollectBadTouches=mEnableAnalytics && 0 != Settings.Secure.getInt(mContext.getContentResolver(),COLLECT_BAD_TOCUHES,0);
}
