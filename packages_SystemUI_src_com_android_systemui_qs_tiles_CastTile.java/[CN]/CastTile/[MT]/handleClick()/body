{
  mHost.collapsePanels();
  final Dialog[] dialog=new Dialog[1];
  dialog[0]=MediaRouteDialogPresenter.createDialog(mContext,MediaRouter.ROUTE_TYPE_REMOTE_DISPLAY,new View.OnClickListener(){
    @Override public void onClick(    View v){
      dialog[0].dismiss();
      mHost.startSettingsActivity(WIFI_DISPLAY_SETTINGS);
    }
  }
);
  dialog[0].getWindow().setType(WindowManager.LayoutParams.TYPE_VOLUME_OVERLAY);
  dialog[0].show();
}
