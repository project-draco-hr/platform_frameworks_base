{
  rewriteActive(new Rewriter(){
    /** 
 * {@inheritDoc} 
 */
    public void reset(){
    }
    /** 
 * {@inheritDoc} 
 */
    public void read(    InputStream in) throws IOException {
      reader.read(in);
    }
    /** 
 * {@inheritDoc} 
 */
    public boolean shouldWrite(){
      return true;
    }
    /** 
 * {@inheritDoc} 
 */
    public void write(    OutputStream out) throws IOException {
      writer.write(out);
    }
  }
,currentTimeMillis);
}
