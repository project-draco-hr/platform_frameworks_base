{
switch (dcState) {
case ACTIVE:
    return State.ACTIVE;
case HOLDING:
  return State.HOLDING;
case DIALING:
return State.DIALING;
case ALERTING:
return State.ALERTING;
case INCOMING:
return State.INCOMING;
case WAITING:
return State.WAITING;
default :
throw new RuntimeException("illegal call state:" + dcState);
}
}
