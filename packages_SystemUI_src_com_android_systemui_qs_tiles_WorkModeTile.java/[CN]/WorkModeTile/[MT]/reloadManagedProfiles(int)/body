{
synchronized (mProfiles) {
    mProfiles.clear();
    if (userHandle == UserHandle.USER_CURRENT) {
      userHandle=ActivityManager.getCurrentUser();
    }
    for (    UserInfo ui : mUserManager.getEnabledProfiles(userHandle)) {
      if (ui.isManagedProfile()) {
        mProfiles.add(ui);
      }
    }
  }
}
