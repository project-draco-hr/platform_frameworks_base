{
  log("testGetRadioState E");
  Msg.send(mMockRilChannel,1,9876,0,null);
  Msg resp=Msg.recv(mMockRilChannel);
  assertTrue(String.format("expected cmd == 1 was %d",resp.getCmd()),resp.getCmd() == 1);
  assertTrue(String.format("expected token == 9876 was %d",resp.getToken()),resp.getToken() == 9876);
  assertTrue(String.format("expected status == 0 was %d",resp.getStatus()),resp.getStatus() == 0);
  RilCtrlCmds.CtrlRspRadioState rsp=resp.getDataAs(RilCtrlCmds.CtrlRspRadioState.class);
  int state=rsp.getState();
  log("testGetRadioState state=" + state);
  assertTrue(String.format("expected RadioState >= 0 && RadioState <= 9 was %d",state),((state >= 0) && (state <= 9)));
  log("testGetRadioState X");
}
