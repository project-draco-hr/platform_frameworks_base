{
  if (mPowerSaveEnabled) {
    return mColors[mColors.length - 1];
  }
  int thresh, color=0;
  for (int i=0; i < mColors.length; i+=2) {
    thresh=mColors[i];
    color=mColors[i + 1];
    if (percent <= thresh) {
      if (i == mColors.length - 2) {
        return mIconTint;
      }
 else {
        return color;
      }
    }
  }
  return color;
}
