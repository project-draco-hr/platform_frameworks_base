{
  if (mCurFilterName == null) {
    throw new SAXException("Found 'input' element outside of 'filter' " + "element!");
  }
  String inputName=getRequiredAttribute(attributes,"name");
  Object inputValue=getAssignmentValue(attributes);
  if (inputValue == null) {
    throw new SAXException("No value specified for input '" + inputName + "' "+ "of filter '"+ mCurFilterName+ "'!");
  }
  mCommandStack.append(new SetFilterInputCommand(mCurFilterName,inputName,inputValue));
}
