{
  mLastAccessibilityEvent=null;
  getActivity();
  final long startTimeMillis=SystemClock.uptimeMillis();
  while (true) {
    if (mLastAccessibilityEvent != null) {
      final int eventType=mLastAccessibilityEvent.getEventType();
      if (eventType == AccessibilityEvent.TYPE_WINDOW_STATE_CHANGED) {
        return;
      }
    }
    final long remainingTimeMillis=TIMEOUT_ACCESSIBLITY_STATE_INITIALIZED_MILLIS - (SystemClock.uptimeMillis() - startTimeMillis);
    if (remainingTimeMillis <= 0) {
      return;
    }
synchronized (this) {
      try {
        wait(remainingTimeMillis);
      }
 catch (      InterruptedException e) {
      }
    }
  }
}
