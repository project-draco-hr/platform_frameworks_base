{
  SharedUserSetting s=mSharedUsers.get(name);
  if (s == null) {
    if (!create) {
      return null;
    }
    s=new SharedUserSetting(name,pkgFlags);
    s.userId=newUserIdLPw(s);
    Log.i(PackageManagerService.TAG,"New shared user " + name + ": id="+ s.userId);
    if (s.userId >= 0) {
      mSharedUsers.put(name,s);
    }
  }
  return s;
}
