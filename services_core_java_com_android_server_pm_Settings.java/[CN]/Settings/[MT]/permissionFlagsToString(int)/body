{
  StringBuilder flagsString=new StringBuilder();
  flagsString.append("[ ");
  while (flags != 0) {
    final int flag=1 << Integer.numberOfTrailingZeros(flags);
    flags&=~flag;
    flagsString.append(PackageManager.permissionFlagToString(flag));
    flagsString.append(' ');
  }
  flagsString.append(']');
  return flagsString.toString();
}
