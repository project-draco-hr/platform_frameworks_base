{
  Log.i(LOG_TAG,mCurrentTestRelativePath + ": setMockDeviceOrientation(" + canProvideAlpha+ ", "+ alpha+ ", "+ canProvideBeta+ ", "+ beta+ ", "+ canProvideGamma+ ", "+ gamma+ ")");
  WebViewClassic.fromWebView(mCurrentWebView).setMockDeviceOrientation(canProvideAlpha,alpha,canProvideBeta,beta,canProvideGamma,gamma);
}
