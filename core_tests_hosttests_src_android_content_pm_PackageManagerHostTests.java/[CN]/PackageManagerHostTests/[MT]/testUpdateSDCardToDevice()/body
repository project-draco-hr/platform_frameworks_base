{
  Log.i(LOG_TAG,"Test updating an app on the SD card to the Device through manifest change");
  try {
    installAppAndVerifyExistsOnSDCard(UPDATE_EXT_TO_INT_LOC_V1_EXT_APK,UPDATE_EXT_TO_INT_LOC_PKG,false);
    installAppAndVerifyExistsOnDevice(UPDATE_EXT_TO_INT_LOC_V2_INT_APK,UPDATE_EXT_TO_INT_LOC_PKG,true);
  }
  finally {
    uninstallApp(UPDATE_EXT_TO_INT_LOC_PKG);
  }
}
