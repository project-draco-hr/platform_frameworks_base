{
  super(id,rs);
  if ((dt != DataType.UNSIGNED_5_6_5) && (dt != DataType.UNSIGNED_4_4_4_4) && (dt != DataType.UNSIGNED_5_5_5_1)) {
    if (size == 3) {
      mSize=dt.mSize * 4;
    }
 else {
      mSize=dt.mSize * size;
    }
  }
 else {
    mSize=dt.mSize;
  }
  mType=dt;
  mKind=dk;
  mNormalized=norm;
  mVectorSize=size;
}
