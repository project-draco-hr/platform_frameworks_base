{
  String result=getString(columnIndex);
  if (result != null) {
    char[] data=buffer.data;
    if (data == null || data.length < result.length()) {
      buffer.data=result.toCharArray();
    }
 else {
      result.getChars(0,result.length(),data,0);
    }
    buffer.sizeCopied=result.length();
  }
}
