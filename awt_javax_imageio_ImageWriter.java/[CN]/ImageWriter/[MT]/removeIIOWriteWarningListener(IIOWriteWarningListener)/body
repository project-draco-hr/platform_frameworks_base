{
  if (warningListeners == null || listener == null) {
    return;
  }
  int idx=warningListeners.indexOf(listener);
  if (idx > -1) {
    warningListeners.remove(idx);
    warningLocales.remove(idx);
    if (warningListeners.isEmpty()) {
      warningListeners=null;
      warningLocales=null;
    }
  }
}
