{
  final StringBuilder sb=new StringBuilder();
  try {
    throw new RuntimeException();
  }
 catch (  RuntimeException e) {
    final StackTraceElement[] frames=e.getStackTrace();
    for (int j=1; j < frames.length; ++j) {
      sb.append(frames[j].toString() + "\n");
    }
  }
  return sb.toString();
}
