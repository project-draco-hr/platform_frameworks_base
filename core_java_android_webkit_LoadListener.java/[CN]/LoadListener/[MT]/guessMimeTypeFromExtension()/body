{
  if (WebView.LOGV_ENABLED) {
    Log.v(LOGTAG,"guessMimeTypeFromExtension: mURL = " + mUrl);
  }
  String mimeType=MimeTypeMap.getSingleton().getMimeTypeFromExtension(MimeTypeMap.getFileExtensionFromUrl(mUrl));
  if (mimeType != null) {
    if (mimeType.equals("application/xhtml+xml")) {
      mimeType="text/html";
    }
  }
  return mimeType;
}
