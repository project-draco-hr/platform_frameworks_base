{
  if (url != null) {
    if (URLUtil.isDataUrl(url)) {
      mUrl=url;
    }
 else {
      mUrl=URLUtil.stripAnchor(url);
    }
    mUri=null;
    if (URLUtil.isNetworkUrl(mUrl)) {
      try {
        mUri=new WebAddress(mUrl);
      }
 catch (      ParseException e) {
        e.printStackTrace();
      }
    }
  }
}
