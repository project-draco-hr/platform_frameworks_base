{
  final int sx=32;
  final int sy=32;
  final int sz=16;
  Type.Builder tb=new Type.Builder(mRS,Element.U8_4(mRS));
  tb.setX(sx);
  tb.setY(sy);
  tb.setZ(sz);
  Type t=tb.create();
  mCube=Allocation.createTyped(mRS,t);
  int dat[]=new int[sx * sy * sz];
  for (int z=0; z < sz; z++) {
    for (int y=0; y < sy; y++) {
      for (int x=0; x < sx; x++) {
        dat[z * sy * sx + y * sx + x]=0xff000000 | ((x | (x << 2)) << 16) | ((y | (y << 2)) << 8)| ((z | (z << 2)) << 0);
      }
    }
  }
  mCube.copyFromUnchecked(dat);
}
