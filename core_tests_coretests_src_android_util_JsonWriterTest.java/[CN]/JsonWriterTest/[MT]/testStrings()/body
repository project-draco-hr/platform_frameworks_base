{
  StringWriter stringWriter=new StringWriter();
  JsonWriter jsonWriter=new JsonWriter(stringWriter);
  jsonWriter.beginArray();
  jsonWriter.value("a");
  jsonWriter.value("a\"");
  jsonWriter.value("\"");
  jsonWriter.value(":");
  jsonWriter.value(",");
  jsonWriter.value("\b");
  jsonWriter.value("\f");
  jsonWriter.value("\n");
  jsonWriter.value("\r");
  jsonWriter.value("\t");
  jsonWriter.value(" ");
  jsonWriter.value("\\");
  jsonWriter.value("{");
  jsonWriter.value("}");
  jsonWriter.value("[");
  jsonWriter.value("]");
  jsonWriter.value("\0");
  jsonWriter.value("\u0019");
  jsonWriter.endArray();
  assertEquals("[\"a\"," + "\"a\\\"\"," + "\"\\\"\","+ "\":\","+ "\",\","+ "\"\\b\","+ "\"\\f\","+ "\"\\n\","+ "\"\\r\","+ "\"\\t\","+ "\" \","+ "\"\\\\\","+ "\"{\","+ "\"}\","+ "\"[\","+ "\"]\","+ "\"\\u0000\","+ "\"\\u0019\"]",stringWriter.toString());
}
