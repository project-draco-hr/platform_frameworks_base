{
  super.processMessage(message);
switch (message.what) {
case EVENT_LINKADDRESS_CONFIGURED:
    if (mDhcpLease.serverAddress != null && !connectUdpSock(mDhcpLease.serverAddress)) {
      notifyFailure();
      transitionTo(mStoppedState);
    }
 else {
      transitionTo(mDhcpBoundState);
    }
  return HANDLED;
default :
return NOT_HANDLED;
}
}
