{
  if (location == null) {
    return otherLocation;
  }
  if (otherLocation == null) {
    return location;
  }
  final long currentTime=System.currentTimeMillis();
  float gpsPotentialMove=MAX_VELOCITY_M_MS * (currentTime - location.getTime()) + location.getAccuracy();
  float otherPotentialMove=MAX_VELOCITY_M_MS * (currentTime - otherLocation.getTime()) + otherLocation.getAccuracy();
  if (gpsPotentialMove < otherPotentialMove) {
    return location;
  }
 else {
    return otherLocation;
  }
}
