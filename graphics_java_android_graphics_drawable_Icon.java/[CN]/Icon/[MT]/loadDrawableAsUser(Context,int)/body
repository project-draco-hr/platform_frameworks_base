{
  if (mType == TYPE_RESOURCE) {
    if (getResources() == null && getResPackage() != null && !(getResPackage().equals("android"))) {
      final PackageManager pm=context.getPackageManager();
      try {
        mObj1=pm.getResourcesForApplicationAsUser(getResPackage(),userId);
      }
 catch (      PackageManager.NameNotFoundException e) {
        Log.e(TAG,String.format("Unable to find pkg=%s user=%d",getResPackage(),userId),e);
      }
    }
  }
  return loadDrawable(context);
}
