{
  if (param.length > 2 || value.length > 2) {
    return BAD_VALUE;
  }
  byte[] p=intToByteArray(param[0]);
  if (param.length > 1) {
    byte[] p2=intToByteArray(param[1]);
    p=concatArrays(p,p2);
  }
  byte[] v=new byte[value.length * 4];
  int status=getParameter(p,v);
  value[0]=byteArrayToInt(v);
  if (v.length > 4) {
    value[1]=byteArrayToInt(v,4);
  }
  return status;
}
