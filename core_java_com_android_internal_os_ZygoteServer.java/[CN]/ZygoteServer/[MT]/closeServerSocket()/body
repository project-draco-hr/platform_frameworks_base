{
  try {
    if (mServerSocket != null) {
      FileDescriptor fd=mServerSocket.getFileDescriptor();
      mServerSocket.close();
      if (fd != null) {
        Os.close(fd);
      }
    }
  }
 catch (  IOException ex) {
    Log.e(TAG,"Zygote:  error closing sockets",ex);
  }
catch (  ErrnoException ex) {
    Log.e(TAG,"Zygote:  error closing descriptor",ex);
  }
  mServerSocket=null;
}
