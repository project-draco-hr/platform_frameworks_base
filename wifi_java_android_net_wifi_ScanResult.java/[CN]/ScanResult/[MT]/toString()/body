{
  StringBuffer sb=new StringBuffer();
  String none="<none>";
  sb.append("SSID: ").append(SSID == null ? none : SSID).append(", BSSID: ").append(BSSID == null ? none : BSSID).append(", capabilities: ").append(capabilities == null ? none : capabilities).append(", level: ").append(level).append(", frequency: ").append(frequency);
  return sb.toString();
}
