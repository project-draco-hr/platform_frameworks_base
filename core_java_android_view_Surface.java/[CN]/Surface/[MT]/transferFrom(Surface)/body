{
  if (other == null) {
    throw new IllegalArgumentException("other must not be null");
  }
  if (other != this) {
    final int newPtr;
synchronized (other.mLock) {
      newPtr=other.mNativeSurface;
      other.setNativeObjectLocked(0);
    }
synchronized (mLock) {
      if (mNativeSurface != 0) {
        nativeRelease(mNativeSurface);
      }
      setNativeObjectLocked(newPtr);
    }
  }
}
