{
switch (rotation) {
case Surface.ROTATION_0:
{
      return "ROTATION_0";
    }
case Surface.ROTATION_90:
{
    return "ROATATION_90";
  }
case Surface.ROTATION_180:
{
  return "ROATATION_180";
}
case Surface.ROTATION_270:
{
return "ROATATION_270";
}
default :
{
throw new IllegalArgumentException("Invalid rotation: " + rotation);
}
}
}
