{
  if (mSurface != 0 || mSurfaceControl != 0) {
    if (DEBUG_RELEASE) {
      Log.w(LOG_TAG,"Surface.finalize() has work. You should have called release() (" + mSurface + ", "+ mSurfaceControl+ ")",mCreationStack);
    }
 else {
      Log.w(LOG_TAG,"Surface.finalize() has work. You should have called release() (" + mSurface + ", "+ mSurfaceControl+ ")");
    }
  }
  release();
}
