{
  mContext.enforceCallingPermission("android.permission.BACKUP","getAvailableRestoreSets");
  try {
synchronized (this) {
      if (mRestoreSets == null) {
        mRestoreSets=mRestoreTransport.getAvailableRestoreSets();
      }
      return mRestoreSets;
    }
  }
 catch (  RuntimeException e) {
    Log.d(TAG,"getAvailableRestoreSets exception");
    e.printStackTrace();
    throw e;
  }
}
