{
  mContext.enforceCallingOrSelfPermission(android.Manifest.permission.BACKUP,"getAvailableRestoreSets");
  try {
synchronized (this) {
      if (mRestoreTransport == null) {
        Log.w(TAG,"Null transport getting restore sets");
      }
 else       if (mRestoreSets == null) {
        mRestoreSets=mRestoreTransport.getAvailableRestoreSets();
        if (mRestoreSets == null)         EventLog.writeEvent(RESTORE_TRANSPORT_FAILURE_EVENT);
      }
      return mRestoreSets;
    }
  }
 catch (  RuntimeException e) {
    Log.d(TAG,"getAvailableRestoreSets exception");
    e.printStackTrace();
    throw e;
  }
}
