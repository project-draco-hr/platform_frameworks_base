{
  try {
    File stateDir=new File(mBaseStateDir,mTransport.transportDirName());
    File stateFile=new File(stateDir,mPackage.packageName);
    stateFile.delete();
    mTransport.clearBackupData(mPackage);
  }
 catch (  RemoteException e) {
  }
 finally {
    try {
      mTransport.finishBackup();
    }
 catch (    RemoteException e) {
    }
    mWakelock.release();
  }
}
