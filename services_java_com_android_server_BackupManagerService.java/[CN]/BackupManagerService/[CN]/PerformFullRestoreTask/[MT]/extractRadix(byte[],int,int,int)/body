{
  long value=0;
  final int end=offset + maxChars;
  for (int i=offset; i < end; i++) {
    final byte b=data[i];
    if (b == 0 || b == ' ')     break;
    if (b < '0' || b > ('0' + radix - 1)) {
      throw new IOException("Invalid number in header");
    }
    value=radix * value + (b - '0');
  }
  return value;
}
