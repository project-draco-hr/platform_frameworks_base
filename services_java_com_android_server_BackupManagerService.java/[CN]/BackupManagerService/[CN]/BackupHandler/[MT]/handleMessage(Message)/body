{
switch (msg.what) {
case MSG_RUN_BACKUP:
synchronized (mQueueLock) {
      mBackupQueue=new ArrayList();
      for (      BackupRequest b : mPendingBackups.values()) {
        mBackupQueue.add(b);
      }
      mPendingBackups=new HashMap<ComponentName,BackupRequest>();
    }
  startOneService();
break;
}
}
