{
  if (MORE_DEBUG)   Slog.v(TAG,"*** Executing restore step: " + mCurrentState);
switch (mCurrentState) {
case INITIAL:
    beginRestore();
  break;
case DOWNLOAD_DATA:
downloadRestoreData();
break;
case PM_METADATA:
restorePmMetadata();
break;
case RUNNING_QUEUE:
restoreNextAgent();
break;
case FINAL:
if (!mFinished) finalizeRestore();
 else {
Slog.e(TAG,"Duplicate finish");
}
mFinished=true;
break;
}
}
