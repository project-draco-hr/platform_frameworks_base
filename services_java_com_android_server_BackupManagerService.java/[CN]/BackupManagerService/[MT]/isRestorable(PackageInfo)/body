{
  if (packageInfo.packageName != null) {
    try {
      PackageInfo app=mPackageManager.getPackageInfo(packageInfo.packageName,PackageManager.GET_SIGNATURES);
      if ((app.applicationInfo.flags & ApplicationInfo.FLAG_ALLOW_BACKUP) != 0) {
        return app;
      }
    }
 catch (    Exception e) {
    }
  }
  return null;
}
