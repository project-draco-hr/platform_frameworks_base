{
  mContext.enforceCallingOrSelfPermission(android.Manifest.permission.BACKUP,"setBackupEnabled");
  Log.i(TAG,"Auto restore => " + doAutoRestore);
synchronized (this) {
    Settings.Secure.putInt(mContext.getContentResolver(),Settings.Secure.BACKUP_AUTO_RESTORE,doAutoRestore ? 1 : 0);
    mAutoRestore=doAutoRestore;
  }
}
