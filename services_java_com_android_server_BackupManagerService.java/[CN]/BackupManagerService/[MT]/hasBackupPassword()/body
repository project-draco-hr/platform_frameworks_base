{
  mContext.enforceCallingOrSelfPermission(android.Manifest.permission.BACKUP,"hasBackupPassword");
  try {
    return (mMountService.getEncryptionState() != IMountService.ENCRYPTION_STATE_NONE) || (mPasswordHash != null && mPasswordHash.length() > 0);
  }
 catch (  Exception e) {
    return true;
  }
}
