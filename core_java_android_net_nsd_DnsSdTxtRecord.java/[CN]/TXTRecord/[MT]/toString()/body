{
  String a, result=null;
  for (int i=0; null != (a=this.getKey(i)); i++) {
    String av=String.valueOf(i) + "={" + a;
    String val=this.getValueAsString(i);
    if (val != null)     av+="=" + val + "}";
 else     av+="}";
    if (result == null)     result=av;
 else     result=result + ", " + av;
  }
  return result != null ? result : "";
}
