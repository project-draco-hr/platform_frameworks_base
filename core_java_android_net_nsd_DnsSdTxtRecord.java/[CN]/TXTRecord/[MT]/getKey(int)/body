{
  int avStart=0;
  for (int i=0; i < index && avStart < fBytes.length; i++)   avStart+=fBytes[avStart] + 1;
  if (avStart < fBytes.length) {
    int avLen=fBytes[avStart];
    int aLen=0;
    for (aLen=0; aLen < avLen; aLen++)     if (fBytes[avStart + aLen + 1] == kAttrSep)     break;
    return new String(fBytes,avStart + 1,aLen);
  }
  return null;
}
