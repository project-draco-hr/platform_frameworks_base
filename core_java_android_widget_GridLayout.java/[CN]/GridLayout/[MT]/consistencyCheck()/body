{
  if (lastLayoutParamsHashCode == UNINITIALIZED_HASH) {
    validateLayoutParams();
    lastLayoutParamsHashCode=computeLayoutParamsHashCode();
  }
 else   if (lastLayoutParamsHashCode != computeLayoutParamsHashCode()) {
    printer.println("The fields of some layout parameters were modified in between " + "layout operations. Check the javadoc for GridLayout.LayoutParams#rowSpec.");
    invalidateStructure();
    consistencyCheck();
  }
}
