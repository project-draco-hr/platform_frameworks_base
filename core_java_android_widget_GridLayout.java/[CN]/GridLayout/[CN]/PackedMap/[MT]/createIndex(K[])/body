{
  int size=keys.length;
  int[] result=new int[size];
  Map<K,Integer> keyToIndex=new HashMap<K,Integer>();
  for (int i=0; i < size; i++) {
    K key=keys[i];
    Integer index=keyToIndex.get(key);
    if (index == null) {
      index=keyToIndex.size();
      keyToIndex.put(key,index);
    }
    result[i]=index;
  }
  return result;
}
