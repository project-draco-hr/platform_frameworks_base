{
  int N=getCount() + 1;
  Arc[][] result=new Arc[N][];
  int[] sizes=new int[N];
  for (  Arc arc : arcs) {
    sizes[arc.span.min]++;
  }
  for (int i=0; i < sizes.length; i++) {
    result[i]=new Arc[sizes[i]];
  }
  Arrays.fill(sizes,0);
  for (  Arc arc : arcs) {
    int i=arc.span.min;
    result[i][sizes[i]++]=arc;
  }
  return result;
}
