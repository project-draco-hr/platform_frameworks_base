{
  if (((String)convertView.getTag()).equals("double")) {
    TextView t1=(TextView)((LinearLayout)convertView).getChildAt(0);
    TextView t2=(TextView)((LinearLayout)convertView).getChildAt(1);
    t1.setText(text);
    t2.setText(text);
    convertView.setId(position);
    return convertView;
  }
 else {
    return null;
  }
}
