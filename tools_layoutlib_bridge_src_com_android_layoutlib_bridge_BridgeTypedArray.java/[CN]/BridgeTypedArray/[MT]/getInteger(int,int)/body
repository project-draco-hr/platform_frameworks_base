{
  if (mData[index] == null) {
    return defValue;
  }
  String s=mData[index].getValue();
  if (s != null) {
    try {
      return Integer.parseInt(s);
    }
 catch (    NumberFormatException e) {
      mContext.getLogger().warning(String.format("Unable to convert \"%s\" into a integer in attribute \"%2$s\"",s,mNames[index]));
    }
  }
  return defValue;
}
