{
  if (mData[index] == null) {
    return defValue;
  }
  String s=mData[index].getValue();
  try {
    return ResourceHelper.getColor(s);
  }
 catch (  NumberFormatException e) {
    mContext.getLogger().warning(String.format("Unable to convert \"%s\" into a color in attribute \"%2$s\"",s,mNames[index]));
  }
  return defValue;
}
