{
  if (mData[index] == null) {
    return null;
  }
  String value=mData[index].getValue();
  if (value == null || BridgeConstants.REFERENCE_NULL.equals(value)) {
    return null;
  }
  Drawable d=ResourceHelper.getDrawable(value,mContext,mData[index].isFramework());
  if (d != null) {
    return d;
  }
  mContext.getLogger().warning(String.format("Unable to resolve drawable \"%1$s\" in attribute \"%2$s\"",value,mNames[index]));
  return null;
}
