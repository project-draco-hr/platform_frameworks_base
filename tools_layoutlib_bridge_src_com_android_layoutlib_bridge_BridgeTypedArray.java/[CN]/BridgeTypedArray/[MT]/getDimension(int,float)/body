{
  if (mData[index] == null) {
    return defValue;
  }
  String s=mData[index].getValue();
  if (s == null) {
    return defValue;
  }
 else   if (s.equals(BridgeConstants.FILL_PARENT)) {
    return LayoutParams.FILL_PARENT;
  }
 else   if (s.equals(BridgeConstants.WRAP_CONTENT)) {
    return LayoutParams.WRAP_CONTENT;
  }
  if (ResourceHelper.stringToFloat(s,mValue)) {
    return mValue.getDimension(mResources.mMetrics);
  }
  mContext.getLogger().warning(String.format("Unable to resolve dimension value \"%1$s\" in attribute \"%2$s\"",s,mNames[index]));
  return defValue;
}
