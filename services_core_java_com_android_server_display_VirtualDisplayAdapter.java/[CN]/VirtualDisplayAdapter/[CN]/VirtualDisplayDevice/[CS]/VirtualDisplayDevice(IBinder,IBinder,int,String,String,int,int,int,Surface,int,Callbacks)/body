{
  super(VirtualDisplayAdapter.this,displayToken);
  mAppToken=appToken;
  mOwnerUid=ownerUid;
  mOwnerPackageName=ownerPackageName;
  mName=name;
  mWidth=width;
  mHeight=height;
  mDensityDpi=densityDpi;
  mSurface=surface;
  mFlags=flags;
  mCallbacks=callbacks;
  mState=Display.STATE_UNKNOWN;
}
