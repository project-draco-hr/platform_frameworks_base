{
  if (FalsingLog.ENABLED) {
    FalsingLog.i("setStatusBarState",new StringBuilder().append("from=").append(StatusBarState.toShortString(mState)).append(" to=").append(StatusBarState.toShortString(state)).toString());
  }
  mState=state;
  if (shouldSessionBeActive()) {
    sessionEntrypoint();
  }
 else {
    sessionExitpoint(false);
  }
}
