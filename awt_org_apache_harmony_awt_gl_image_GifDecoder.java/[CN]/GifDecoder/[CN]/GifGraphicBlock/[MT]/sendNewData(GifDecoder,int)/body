{
  if (transparentColor != IMPOSSIBLE_VALUE) {
    replaceTransparentPixels(numLines);
  }
  if (forceRGB) {
    decoder.setPixels(imageLeft,imageTop + currY,imageWidth,numLines,ColorModel.getRGBdefault(),getRgbImageData(),currY * imageWidth,imageWidth);
  }
 else {
    decoder.setPixels(imageLeft,imageTop + currY,imageWidth,numLines,null,imageData,currY * imageWidth,imageWidth);
  }
  currY+=numLines;
}
