{
  if (rgbImageData == null) {
    rgbImageData=toRGB(imageData,gifDataStream.logicalScreen.globalColorTable.colors,transparentColor);
    if (transparentColor != IMPOSSIBLE_VALUE) {
      transparentColor=gifDataStream.logicalScreen.globalColorTable.cm.getRGB(transparentColor);
      transparentColor&=0x00FFFFFF;
    }
  }
  return rgbImageData;
}
