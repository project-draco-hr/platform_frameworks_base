{
  int dataInt[];
  byte dataByte[];
  int width=gifDataStream.logicalScreen.logicalScreenWidth;
  if (forceRGB) {
    dataInt=(int[])data;
    if (imageWidth == width) {
      System.arraycopy(dataInt,0,getScreenRGBBuffer(),imageLeft + imageTop * width,dataInt.length);
    }
 else {
      copyScanlines(dataInt,getScreenRGBBuffer(),width);
    }
  }
 else {
    dataByte=(byte[])data;
    if (imageWidth == width) {
      System.arraycopy(dataByte,0,screenBuffer,imageLeft + imageTop * width,dataByte.length);
    }
 else {
      copyScanlines(dataByte,screenBuffer,width);
    }
  }
}
