{
synchronized (mAuthorities) {
    if (Log.isLoggable(TAG,Log.VERBOSE)) {
      Log.v(TAG,"removeActiveSync: account=" + syncInfo.account + " user="+ userId+ " auth="+ syncInfo.authority+ " service="+ syncInfo.service);
    }
    getCurrentSyncs(userId).remove(syncInfo);
  }
  reportActiveChange();
}
