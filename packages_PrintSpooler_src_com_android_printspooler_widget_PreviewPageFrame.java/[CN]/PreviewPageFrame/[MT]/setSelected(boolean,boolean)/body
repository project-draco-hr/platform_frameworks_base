{
  if (isSelected() == selected) {
    return;
  }
  setSelected(selected);
  if (selected) {
    if (animate) {
      animate().translationZ(mSelectedElevation).alpha(mSelectedPageAlpha);
    }
 else {
      setTranslationZ(mSelectedElevation);
      setAlpha(mSelectedPageAlpha);
    }
  }
 else {
    if (animate) {
      animate().translationZ(mNotSelectedElevation).alpha(mNotSelectedAlpha);
    }
 else {
      setTranslationZ(mNotSelectedElevation);
      setAlpha(mNotSelectedAlpha);
    }
  }
}
