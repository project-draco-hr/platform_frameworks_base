{
  try {
    FileInputStream in=new FileInputStream(source);
    FileOutputStream out=new FileOutputStream(dest.getFileDescriptor());
    byte[] buffer=new byte[4096];
    int bytesRead;
    while ((bytesRead=in.read(buffer)) >= 0) {
      out.write(buffer,0,bytesRead);
    }
  }
 catch (  IOException e) {
    return -1;
  }
  return 0;
}
