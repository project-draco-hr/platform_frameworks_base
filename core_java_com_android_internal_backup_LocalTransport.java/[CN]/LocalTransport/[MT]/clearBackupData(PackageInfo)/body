{
  if (DEBUG)   Log.v(TAG,"clearBackupData() pkg=" + packageInfo.packageName);
  File packageDir=new File(mCurrentSetDir,packageInfo.packageName);
  final File[] fileset=packageDir.listFiles();
  if (fileset != null) {
    for (    File f : fileset) {
      f.delete();
    }
    packageDir.delete();
  }
  return BackupTransport.TRANSPORT_OK;
}
