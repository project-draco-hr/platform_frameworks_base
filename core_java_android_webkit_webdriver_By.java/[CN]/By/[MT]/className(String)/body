{
  throwIfNull(className);
  return new By(){
    @Override public WebElement findElement(    WebElement element){
      return element.findElementByClassName(className);
    }
    @Override public List<WebElement> findElements(    WebElement element){
      return element.findElementsByClassName(className);
    }
    @Override public String toString(){
      return "By.className: " + className;
    }
  }
;
}
