{
  throwIfNull(linkText);
  return new By(){
    @Override public WebElement findElement(    WebElement element){
      return element.findElementByPartialLinkText(linkText);
    }
    @Override public List<WebElement> findElements(    WebElement element){
      return element.findElementsByPartialLinkText(linkText);
    }
    @Override public String toString(){
      return "By.partialLinkText: " + linkText;
    }
  }
;
}
