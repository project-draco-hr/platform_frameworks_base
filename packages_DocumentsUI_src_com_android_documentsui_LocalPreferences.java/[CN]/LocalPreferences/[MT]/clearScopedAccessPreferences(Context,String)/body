{
  final String keySubstring="|" + packageName + "|";
  final SharedPreferences prefs=getPrefs(context);
  Editor editor=null;
  for (  final String key : prefs.getAll().keySet()) {
    if (key.contains(keySubstring)) {
      if (editor == null) {
        editor=prefs.edit();
      }
      editor.remove(key);
    }
  }
  if (editor != null) {
    editor.apply();
  }
}
