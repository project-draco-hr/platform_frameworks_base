{
  String value=getAttribute(TAG_WHITE_BALANCE);
  if (value == null)   return "";
  int whitebalance;
  try {
    whitebalance=Integer.parseInt(value);
  }
 catch (  NumberFormatException ex) {
    return "";
  }
switch (whitebalance) {
case WHITEBALANCE_AUTO:
    return "Auto";
case WHITEBALANCE_MANUAL:
  return "Manual";
default :
return "";
}
}
