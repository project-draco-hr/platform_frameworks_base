{
  StringBuilder bob=new StringBuilder();
  bob.append("RouteInfo: id=").append(mId).append(", name=").append(mName).append(", provider=").append(mProviderId).append(", options={");
  for (int i=0; i < mOptions.size(); i++) {
    if (i != 0) {
      bob.append(", ");
    }
    bob.append(mOptions.get(i).toString());
  }
  bob.append("}");
  return bob.toString();
}
