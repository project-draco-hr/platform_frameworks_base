{
  Preconditions.checkNotNull(listener);
synchronized (mListeners) {
    boolean removed=mListeners.remove(listener);
    boolean isLastRemoved=removed && mListeners.isEmpty();
    if (isLastRemoved) {
      try {
        unregisterFromServer();
      }
 catch (      RemoteException e) {
      }
    }
  }
}
