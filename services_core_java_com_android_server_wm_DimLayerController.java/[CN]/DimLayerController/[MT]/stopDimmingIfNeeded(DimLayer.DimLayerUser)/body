{
  DimLayerState state=mState.get(dimLayerUser);
  if (DEBUG_DIM_LAYER)   Slog.v(TAG,"stopDimmingIfNeeded," + " dimLayerUser=" + dimLayerUser.toShortString() + " state.continueDimming="+ state.continueDimming+ " state.dimLayer.isDimming="+ state.dimLayer.isDimming());
  if (!state.continueDimming && state.dimLayer.isDimming()) {
    state.animator=null;
    dimLayerUser.getBounds(mTmpBounds);
    state.dimLayer.setBounds(mTmpBounds);
  }
}
