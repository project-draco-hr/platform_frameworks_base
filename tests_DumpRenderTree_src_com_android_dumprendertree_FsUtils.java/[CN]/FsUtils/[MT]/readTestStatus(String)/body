{
  String status=null;
  File testStatusFile=new File(statusFile);
  if (testStatusFile.exists()) {
    try {
      BufferedReader inReader=new BufferedReader(new FileReader(testStatusFile));
      status=inReader.readLine();
      inReader.close();
    }
 catch (    IOException e) {
      Log.e(LOGTAG,"Error reading test status.",e);
    }
  }
  return status;
}
