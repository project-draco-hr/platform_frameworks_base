{
  Cursor sortedCursor=getContentResolver().query(Settings.System.CONTENT_URI,PROJECTION,null,null,Settings.NameValueTable.NAME);
  if (sortedSystemKeys == null) {
    sortedSystemKeys=copyAndSort(Settings.System.SETTINGS_TO_BACKUP);
  }
  byte[] result=extractRelevantValues(sortedCursor,sortedSystemKeys);
  sortedCursor.close();
  return result;
}
