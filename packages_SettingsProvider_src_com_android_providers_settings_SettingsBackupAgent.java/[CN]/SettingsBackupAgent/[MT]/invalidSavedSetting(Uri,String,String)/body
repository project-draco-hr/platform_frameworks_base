{
  if (contentUri.equals(Settings.Secure.CONTENT_URI)) {
    if (settingName.equals(Settings.Secure.PREFERRED_NETWORK_MODE) || settingName.equals(Settings.Secure.PREFERRED_TTY_MODE) || settingName.equals(Settings.Secure.CDMA_CELL_BROADCAST_SMS)|| settingName.equals(Settings.Secure.PREFERRED_CDMA_SUBSCRIPTION)|| settingName.equals(Settings.Secure.ENHANCED_VOICE_PRIVACY_ENABLED)) {
      if (DEBUG)       Log.v(TAG,"Ignoring restore datum: " + settingName);
      return true;
    }
  }
  return false;
}
