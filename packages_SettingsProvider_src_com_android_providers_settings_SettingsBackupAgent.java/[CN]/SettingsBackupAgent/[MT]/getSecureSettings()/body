{
  Cursor sortedCursor=getContentResolver().query(Settings.Secure.CONTENT_URI,PROJECTION,null,null,Settings.NameValueTable.NAME);
  if (sortedSecureKeys == null) {
    sortedSecureKeys=copyAndSort(Settings.Secure.SETTINGS_TO_BACKUP);
  }
  byte[] result=extractRelevantValues(sortedCursor,sortedSecureKeys);
  sortedCursor.close();
  return result;
}
