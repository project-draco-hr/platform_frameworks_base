{
  boolean noneSet=true;
  if (restrictions != null) {
    for (    String key : restrictions.keySet()) {
      if (restrictions.getBoolean(key,false)) {
        pw.println(prefix + key);
        noneSet=false;
      }
    }
    if (noneSet) {
      pw.println(prefix + "none");
    }
  }
 else {
    pw.println(prefix + "null");
  }
}
