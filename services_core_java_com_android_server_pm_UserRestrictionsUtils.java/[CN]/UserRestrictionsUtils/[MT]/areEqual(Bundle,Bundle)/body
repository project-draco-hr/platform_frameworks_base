{
  if (a == b) {
    return true;
  }
  if (isEmpty(a)) {
    return isEmpty(b);
  }
  if (isEmpty(b)) {
    return false;
  }
  for (  String key : a.keySet()) {
    if (a.getBoolean(key) != b.getBoolean(key)) {
      return false;
    }
  }
  for (  String key : b.keySet()) {
    if (a.getBoolean(key) != b.getBoolean(key)) {
      return false;
    }
  }
  return true;
}
