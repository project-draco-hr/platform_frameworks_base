{
  volume=Math.max(0,Math.min(volume,getVolumeMax()));
  if (mVolume != volume) {
    mVolume=volume;
    if (USE_SESSIONS) {
      if (mSvp != null) {
        mSvp.notifyVolumeChanged();
      }
    }
 else {
      setPlaybackInfoOnRcc(RemoteControlClient.PLAYBACKINFO_VOLUME,volume);
    }
    dispatchRouteVolumeChanged(this);
    if (mGroup != null) {
      mGroup.memberVolumeChanged(this);
    }
  }
}
