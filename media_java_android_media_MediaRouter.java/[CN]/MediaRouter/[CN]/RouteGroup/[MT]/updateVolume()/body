{
  final int routeCount=getRouteCount();
  int volume=0;
  for (int i=0; i < routeCount; i++) {
    final int routeVol=getRouteAt(i).getVolume();
    if (routeVol > volume) {
      volume=routeVol;
    }
  }
  if (volume != mVolume) {
    mVolume=volume;
    dispatchRouteVolumeChanged(this);
  }
}
