{
  if (mPlaybackType == PLAYBACK_TYPE_LOCAL) {
    int volMax=0;
    try {
      volMax=sStatic.mAudioService.getStreamMaxVolume(mPlaybackStream);
    }
 catch (    RemoteException e) {
      Log.e(TAG,"Error getting local stream volume",e);
    }
    return volMax;
  }
 else {
    return mVolumeMax;
  }
}
