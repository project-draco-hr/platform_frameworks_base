{
  if (mSimFdnEnabledState != SimFdnState.REQUIRE_PUK2) {
    Log.i(LOG_TAG,"[SimCmd] supplySimPuk2: wrong state, state=" + mSimLockedState);
    CommandException ex=new CommandException(CommandException.Error.PASSWORD_INCORRECT);
    AsyncResult.forMessage(result,null,ex);
    result.sendToTarget();
    return;
  }
  if (puk2 != null && puk2.equals(SIM_PUK2_CODE)) {
    Log.i(LOG_TAG,"[SimCmd] supplySimPuk2: success!");
    mSimFdnEnabledState=SimFdnState.NONE;
    mPuk2UnlockAttempts=0;
    if (result != null) {
      AsyncResult.forMessage(result,null,null);
      result.sendToTarget();
    }
    return;
  }
  if (result != null) {
    mPuk2UnlockAttempts++;
    Log.i(LOG_TAG,"[SimCmd] supplySimPuk2: failed! attempt=" + mPuk2UnlockAttempts);
    if (mPuk2UnlockAttempts >= 10) {
      Log.i(LOG_TAG,"[SimCmd] supplySimPuk2: set state to SIM_PERM_LOCKED");
      mSimFdnEnabledState=SimFdnState.SIM_PERM_LOCKED;
    }
    CommandException ex=new CommandException(CommandException.Error.PASSWORD_INCORRECT);
    AsyncResult.forMessage(result,null,ex);
    result.sendToTarget();
  }
}
