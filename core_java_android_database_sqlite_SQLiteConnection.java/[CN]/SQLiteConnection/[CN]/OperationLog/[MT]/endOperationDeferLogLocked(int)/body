{
  final Operation operation=getOperationLocked(cookie);
  if (operation != null) {
    if (Trace.isTagEnabled(Trace.TRACE_TAG_DATABASE)) {
      Trace.asyncTraceEnd(Trace.TRACE_TAG_DATABASE,operation.getTraceMethodName(),operation.mCookie);
    }
    operation.mEndTime=SystemClock.uptimeMillis();
    operation.mFinished=true;
    return SQLiteDebug.DEBUG_LOG_SLOW_QUERIES && SQLiteDebug.shouldLogSlowQuery(operation.mEndTime - operation.mStartTime);
  }
  return false;
}
