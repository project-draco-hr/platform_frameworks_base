{
  if (!mConfiguration.isInMemoryDb()) {
    String result=executeForString("PRAGMA journal_mode=" + mConfiguration.journalMode,null,null);
    if (!result.equalsIgnoreCase(mConfiguration.journalMode)) {
      Log.e(TAG,"setting journal_mode to " + mConfiguration.journalMode + " failed for db: "+ mConfiguration.label+ " (on pragma set journal_mode, sqlite returned:"+ result);
    }
  }
}
