{
  if (pin == null) {
    return null;
  }
  byte[] pinBytes;
  try {
    pinBytes=pin.getBytes("UTF8");
  }
 catch (  UnsupportedEncodingException uee) {
    Log.e(TAG,"UTF8 not supported?!?");
    return null;
  }
  if (pinBytes.length <= 0 || pinBytes.length > 16) {
    return null;
  }
  return pinBytes;
}
