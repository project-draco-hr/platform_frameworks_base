{
  if (sService == null) {
    Log.e(TAG,"BT not enabled. Cannot remove Remote Device bond");
    return false;
  }
  try {
    Log.i(TAG,"removeBond() for device " + getAddress() + " called by pid: "+ Process.myPid()+ " tid: "+ Process.myTid());
    return sService.removeBond(this);
  }
 catch (  RemoteException e) {
    Log.e(TAG,"",e);
  }
  return false;
}
