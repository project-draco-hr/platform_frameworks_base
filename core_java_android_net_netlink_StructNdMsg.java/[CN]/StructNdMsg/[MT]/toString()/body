{
  final String stateStr="" + ndm_state + " ("+ stringForNudState(ndm_state)+ ")";
  final String flagsStr="" + ndm_flags + " ("+ stringForNudFlags(ndm_flags)+ ")";
  return "StructNdMsg{ " + "family{" + NetlinkConstants.stringForAddressFamily((int)ndm_family) + "}, "+ "ifindex{"+ ndm_ifindex+ "}, "+ "state{"+ stateStr+ "}, "+ "flags{"+ flagsStr+ "}, "+ "type{"+ ndm_type+ "} "+ "}";
}
