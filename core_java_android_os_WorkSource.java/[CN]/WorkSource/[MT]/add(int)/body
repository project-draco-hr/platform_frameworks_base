{
  if (mNum <= 0) {
    mNames=null;
    insert(0,uid);
    return true;
  }
  if (mNames != null) {
    throw new IllegalArgumentException("Adding without name to named " + this);
  }
  int i=Arrays.binarySearch(mUids,0,mNum,uid);
  if (DEBUG)   Log.d(TAG,"Adding uid " + uid + " to "+ this+ ": binsearch res = "+ i);
  if (i >= 0) {
    return false;
  }
  insert(-i - 1,uid);
  return true;
}
