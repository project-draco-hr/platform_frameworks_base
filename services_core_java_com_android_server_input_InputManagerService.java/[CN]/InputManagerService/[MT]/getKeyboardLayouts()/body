{
  final ArrayList<KeyboardLayout> list=new ArrayList<KeyboardLayout>();
  visitAllKeyboardLayouts(new KeyboardLayoutVisitor(){
    @Override public void visitKeyboardLayout(    Resources resources,    String descriptor,    String label,    String collection,    int keyboardLayoutResId,    int priority){
      list.add(new KeyboardLayout(descriptor,label,collection,priority));
    }
  }
);
  return list.toArray(new KeyboardLayout[list.size()]);
}
