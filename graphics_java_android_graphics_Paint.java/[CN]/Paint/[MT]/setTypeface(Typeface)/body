{
  if (mNativePaint == 0)   throw new NullPointerException("Already finalized!");
  long typefaceNative=0;
  if (typeface != null) {
    typefaceNative=typeface.native_instance;
  }
  nSetTypeface(mNativePaint,typefaceNative);
  mTypeface=typeface;
  mNativeTypeface=typefaceNative;
  return typeface;
}
