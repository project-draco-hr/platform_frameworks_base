{
  maybeLogMessage(this,message.what);
  boolean retValue=true;
switch (message.what) {
case CMD_TETHER_UNREQUESTED:
    mTetherController.notifyInterfaceTetheringReadiness(false,TetherInterfaceSM.this);
  if (mUsb) {
    if (!configureUsbIface(false,mIfaceName)) {
      setLastErrorAndTransitionToInitialState(ConnectivityManager.TETHER_ERROR_IFACE_CFG_ERROR);
      break;
    }
  }
transitionTo(mInitialState);
break;
case CMD_CELL_DUN_ERROR:
case CMD_IP_FORWARDING_ENABLE_ERROR:
case CMD_IP_FORWARDING_DISABLE_ERROR:
case CMD_START_TETHERING_ERROR:
case CMD_STOP_TETHERING_ERROR:
case CMD_SET_DNS_FORWARDERS_ERROR:
setLastErrorAndTransitionToInitialState(ConnectivityManager.TETHER_ERROR_MASTER_ERROR);
break;
case CMD_INTERFACE_DOWN:
mTetherController.notifyInterfaceTetheringReadiness(false,TetherInterfaceSM.this);
transitionTo(mUnavailableState);
break;
default :
retValue=false;
}
return retValue;
}
