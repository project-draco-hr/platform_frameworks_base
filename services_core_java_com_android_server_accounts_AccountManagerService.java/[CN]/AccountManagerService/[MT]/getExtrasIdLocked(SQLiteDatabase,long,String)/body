{
  Cursor cursor=db.query(CE_TABLE_EXTRAS,new String[]{EXTRAS_ID},EXTRAS_ACCOUNTS_ID + "=" + accountId+ " AND "+ EXTRAS_KEY+ "=?",new String[]{key},null,null,null);
  try {
    if (cursor.moveToNext()) {
      return cursor.getLong(0);
    }
    return -1;
  }
  finally {
    cursor.close();
  }
}
