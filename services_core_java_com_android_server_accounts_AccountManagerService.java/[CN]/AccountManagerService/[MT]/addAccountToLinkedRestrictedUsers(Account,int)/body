{
  List<UserInfo> users=getUserManager().getUsers();
  for (  UserInfo user : users) {
    if (user.isRestricted() && (parentUserId == user.restrictedProfileParentId)) {
      addSharedAccountAsUser(account,user.id);
      try {
        if (ActivityManagerNative.getDefault().isUserRunning(user.id,false)) {
          mMessageHandler.sendMessage(mMessageHandler.obtainMessage(MESSAGE_COPY_SHARED_ACCOUNT,parentUserId,user.id,account));
        }
      }
 catch (      RemoteException re) {
      }
    }
  }
}
