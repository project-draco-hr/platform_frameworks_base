{
  Cursor cursor=db.query(CE_TABLE_ACCOUNTS,new String[]{ACCOUNTS_PASSWORD},ACCOUNTS_NAME + "=? AND " + ACCOUNTS_TYPE+ "=?",new String[]{name,type},null,null,null);
  try {
    if (cursor.moveToNext()) {
      return cursor.getString(0);
    }
    return null;
  }
  finally {
    cursor.close();
  }
}
