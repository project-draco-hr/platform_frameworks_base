{
  Log.i(TAG,"Moving pre-N DB " + oldDbFile + " to CE "+ ceDbFile);
  try {
    FileUtils.copyFileOrThrow(oldDbFile,ceDbFile);
  }
 catch (  IOException e) {
    Log.e(TAG,"Cannot copy file to " + ceDbFile + " from "+ oldDbFile,e);
    deleteDbFileWarnIfFailed(ceDbFile);
    return false;
  }
  return true;
}
