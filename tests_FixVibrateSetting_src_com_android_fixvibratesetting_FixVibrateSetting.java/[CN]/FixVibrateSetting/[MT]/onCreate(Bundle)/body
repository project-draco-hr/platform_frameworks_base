{
  super.onCreate(icicle);
  setContentView(R.layout.fix_vibrate);
  mAudioManager=(AudioManager)getSystemService(AUDIO_SERVICE);
  mNotificationManager=(NotificationManager)getSystemService(NOTIFICATION_SERVICE);
