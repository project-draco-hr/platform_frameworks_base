  mFix=findViewById(R.id.fix);
  mFix.setOnClickListener(this);
  mUnfix=findViewById(R.id.unfix);
  mUnfix.setOnClickListener(this);
  mTest=findViewById(R.id.test);
  mTest.setOnClickListener(this);
}
