{
  if (icon.getType() == Icon.TYPE_RESOURCE && TextUtils.isEmpty(icon.getResPackage())) {
    icon=Icon.createWithResource(resPackage,icon.getResId());
  }
  this.pkg=resPackage;
  this.user=user;
  this.icon=icon;
  this.iconLevel=iconLevel;
  this.number=number;
  this.contentDescription=contentDescription;
}
