{
  if (!mQuitAllowed) {
    throw new IllegalStateException("Main thread not allowed to quit.");
  }
synchronized (this) {
    if (mQuitting) {
      return;
    }
    mQuitting=true;
    if (safe) {
      removeAllFutureMessagesLocked();
    }
 else {
      removeAllMessagesLocked();
    }
    nativeWake(mPtr);
  }
}
