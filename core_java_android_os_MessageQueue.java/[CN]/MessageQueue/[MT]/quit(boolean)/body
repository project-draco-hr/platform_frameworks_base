{
  if (!mQuitAllowed) {
    throw new RuntimeException("Main thread not allowed to quit.");
  }
synchronized (this) {
    if (mQuiting) {
      return;
    }
    mQuiting=true;
    if (safe) {
      removeAllFutureMessagesLocked();
    }
 else {
      removeAllMessagesLocked();
    }
  }
  nativeWake(mPtr);
}
