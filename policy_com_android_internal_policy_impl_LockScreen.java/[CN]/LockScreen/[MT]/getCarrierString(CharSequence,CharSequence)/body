{
  if (telephonyPlmn != null && telephonySpn == null) {
    return telephonyPlmn;
  }
 else   if (telephonyPlmn != null && telephonySpn != null) {
    return telephonyPlmn + "\n" + telephonySpn;
  }
 else   if (telephonyPlmn == null && telephonySpn != null) {
    return telephonySpn;
  }
 else {
    return "";
  }
}
