{
  assertEquals(mBindingObject.getSrc(),mView.getDrawable());
  assertEquals(mBindingObject.getTint(),mView.getImageTintList().getDefaultColor());
  assertEquals(mBindingObject.getTintMode(),mView.getImageTintMode());
  changeValues();
  assertEquals(mBindingObject.getSrc(),mView.getDrawable());
  assertEquals(mBindingObject.getTint(),mView.getImageTintList().getDefaultColor());
  assertEquals(mBindingObject.getTintMode(),mView.getImageTintMode());
}
