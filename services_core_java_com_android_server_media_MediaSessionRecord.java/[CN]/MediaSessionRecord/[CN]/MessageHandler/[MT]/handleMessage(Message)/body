{
switch (msg.what) {
case MSG_UPDATE_METADATA:
    pushMetadataUpdate();
  break;
case MSG_UPDATE_PLAYBACK_STATE:
pushPlaybackStateUpdate();
break;
case MSG_UPDATE_ROUTE:
pushRouteUpdate();
break;
case MSG_SEND_EVENT:
pushEvent((String)msg.obj,msg.getData());
break;
case MSG_SEND_COMMAND:
Pair<RouteCommand,ResultReceiver> cmd=(Pair<RouteCommand,ResultReceiver>)msg.obj;
pushRouteCommand(cmd.first,cmd.second);
break;
}
}
