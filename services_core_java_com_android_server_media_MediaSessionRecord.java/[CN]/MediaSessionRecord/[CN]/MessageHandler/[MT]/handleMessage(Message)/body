{
switch (msg.what) {
case MSG_UPDATE_METADATA:
    pushMetadataUpdate();
  break;
case MSG_UPDATE_PLAYBACK_STATE:
pushPlaybackStateUpdate();
break;
case MSG_SEND_EVENT:
pushEvent((String)msg.obj,msg.getData());
break;
case MSG_UPDATE_SESSION_STATE:
break;
case MSG_UPDATE_VOLUME:
pushVolumeUpdate();
break;
}
}
