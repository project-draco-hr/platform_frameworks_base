{
synchronized (mLock) {
    if (mDestroyed) {
      return;
    }
    if (mRoute == null || !TextUtils.equals(command.getRouteInfo(),mRoute.getId())) {
      if (cb != null) {
        cb.send(RouteInterface.RESULT_ROUTE_IS_STALE,null);
        return;
      }
    }
    if (mConnection != null) {
      mConnection.sendCommand(command,cb);
    }
 else     if (cb != null) {
      cb.send(RouteInterface.RESULT_NOT_CONNECTED,null);
    }
  }
}
