{
  if (!isSystemReady) {
    return false;
  }
  if (!isSystemIme(imi)) {
    return false;
  }
  if (imi.getIsDefaultResourceId() != 0) {
    try {
      if (imi.isDefault(context) && containsSubtypeOf(imi,context.getResources().getConfiguration().locale.getLanguage(),null)) {
        return true;
      }
    }
 catch (    Resources.NotFoundException ex) {
    }
  }
  if (imi.getSubtypeCount() == 0) {
    Slog.w(TAG,"Found no subtypes in a system IME: " + imi.getPackageName());
  }
  return false;
}
