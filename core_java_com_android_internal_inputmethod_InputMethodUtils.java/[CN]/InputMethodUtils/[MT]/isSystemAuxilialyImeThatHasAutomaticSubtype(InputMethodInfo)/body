{
  if (!isSystemIme(imi)) {
    return false;
  }
  if (!imi.isAuxiliaryIme()) {
    return false;
  }
  final int subtypeCount=imi.getSubtypeCount();
  for (int i=0; i < subtypeCount; ++i) {
    final InputMethodSubtype s=imi.getSubtypeAt(i);
    if (s.overridesImplicitlyEnabledSubtype()) {
      return true;
    }
  }
  return false;
}
