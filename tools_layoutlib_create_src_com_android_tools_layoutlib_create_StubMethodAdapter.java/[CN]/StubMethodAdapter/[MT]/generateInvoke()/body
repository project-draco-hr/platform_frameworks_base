{
  mParentVisitor.visitLdcInsn(mInvokeSignature);
  if (mIsStatic) {
    mParentVisitor.visitInsn(Opcodes.ACONST_NULL);
  }
 else {
    mParentVisitor.visitVarInsn(Opcodes.ALOAD,0);
  }
  mParentVisitor.visitMethodInsn(Opcodes.INVOKESTATIC,"com/android/tools/layoutlib/create/OverrideMethod","invoke","(Ljava/lang/String;Ljava/lang/Object;)V");
}
