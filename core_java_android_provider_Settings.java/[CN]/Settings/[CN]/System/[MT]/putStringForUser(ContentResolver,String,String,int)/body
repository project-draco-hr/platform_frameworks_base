{
  if (MOVED_TO_SECURE.contains(name)) {
    Log.w(TAG,"Setting " + name + " has moved from android.provider.Settings.System"+ " to android.provider.Settings.Secure, value is unchanged.");
    return false;
  }
  if (MOVED_TO_GLOBAL.contains(name)) {
    Log.w(TAG,"Setting " + name + " has moved from android.provider.Settings.System"+ " to android.provider.Settings.Global, value is unchanged.");
    return false;
  }
  lazyInitCache();
  return sNameValueCache.putStringForUser(resolver,name,value,userHandle);
}
