{
  float fraction=duration / (float)period;
  int percent=(int)((fraction * 100) + .5f);
  if (percent > 0) {
    pw.print(" ");
    pw.print(percent);
    pw.print("% ");
    pw.print(count);
    pw.print("x ");
    pw.print(suffix);
  }
 else   if (count > 0) {
    pw.print(" ");
    pw.print(count);
    pw.print("x ");
    pw.print(suffix);
  }
}
