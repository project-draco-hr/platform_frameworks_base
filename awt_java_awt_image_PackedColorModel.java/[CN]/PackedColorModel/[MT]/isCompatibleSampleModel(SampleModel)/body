{
  if (sm == null) {
    return false;
  }
  if (!(sm instanceof SinglePixelPackedSampleModel)) {
    return false;
  }
  SinglePixelPackedSampleModel esm=(SinglePixelPackedSampleModel)sm;
  return ((esm.getNumBands() == numComponents) && (esm.getTransferType() == transferType) && Arrays.equals(esm.getBitMasks(),componentMasks));
}
