{
  ImportVerifier verifier=new ImportVerifier();
  verifier.addImportVerifierElem().addExpected(StructuredName.CONTENT_ITEM_TYPE).put(StructuredName.GIVEN_NAME,"A;B\\").put(StructuredName.MIDDLE_NAME,"C\\;").put(StructuredName.PREFIX,"D").put(StructuredName.SUFFIX,":E").put(StructuredName.DISPLAY_NAME,"A;B\\C\\;D:E\\\\");
  verifier.verify(R.raw.v21_backslash,VCardConfig.VCARD_TYPE_V21_GENERIC_UTF8);
}
