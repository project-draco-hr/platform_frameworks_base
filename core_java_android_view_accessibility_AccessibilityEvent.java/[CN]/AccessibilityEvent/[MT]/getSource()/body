{
  enforceSealed();
  if (mSourceAccessibilityWindowId == View.NO_ID || mSourceAccessibilityViewId == View.NO_ID) {
    return null;
  }
  try {
    return mConnection.findAccessibilityNodeInfoByAccessibilityId(mSourceAccessibilityWindowId,mSourceAccessibilityViewId);
  }
 catch (  RemoteException e) {
    return null;
  }
}
