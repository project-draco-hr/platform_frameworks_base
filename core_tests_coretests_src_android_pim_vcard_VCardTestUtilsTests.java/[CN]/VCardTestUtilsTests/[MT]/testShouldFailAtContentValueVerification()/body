{
  boolean failureDetected=false;
  try {
    final VCardVerifier verifier=new VCardVerifier(this);
    verifier.initForImportTest(VCardConfig.VCARD_TYPE_V21_GENERIC,R.raw.v21_backslash);
    verifier.addContentValuesVerifierElem().addExpected(StructuredName.CONTENT_ITEM_TYPE).put(StructuredName.GIVEN_NAME,"A;B\\").put(StructuredName.MIDDLE_NAME,"C\\;").put(StructuredName.PREFIX,"D").put(StructuredName.SUFFIX,":E");
    verifier.verify();
  }
 catch (  AssertionFailedError e) {
    failureDetected=true;
  }
  if (!failureDetected) {
    TestCase.fail("Test case that should fail actually succeeded.");
  }
}
