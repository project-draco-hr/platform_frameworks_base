{
  if (index < 0 || index >= mResourceData.length) {
    return null;
  }
  if (mResourceData[index] == null) {
    return null;
  }
  String value=mResourceData[index].getValue();
  if (value != null) {
    if (RenderResources.REFERENCE_NULL.equals(value)) {
      return null;
    }
    return new CharSequence[]{value};
  }
  Bridge.getLog().warning(LayoutLog.TAG_RESOURCES_FORMAT,String.format(String.format("Unknown value for getTextArray(%d) => %s",index,mResourceData[index].getName())),null);
  return null;
}
