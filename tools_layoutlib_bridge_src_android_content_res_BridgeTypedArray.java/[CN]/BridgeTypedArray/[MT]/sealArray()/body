{
  int count=0;
  for (  ResourceValue data : mResourceData) {
    if (data != null && !RenderResources.REFERENCE_NULL.equals(data.getValue())) {
      count++;
    }
  }
  mIndices=new int[count + 1];
  mIndices[0]=count;
  int index=1;
  for (int i=0; i < mResourceData.length; i++) {
    if (mResourceData[i] != null && !RenderResources.REFERENCE_NULL.equals(mResourceData[i].getValue())) {
      mIndices[index++]=i;
    }
  }
}
