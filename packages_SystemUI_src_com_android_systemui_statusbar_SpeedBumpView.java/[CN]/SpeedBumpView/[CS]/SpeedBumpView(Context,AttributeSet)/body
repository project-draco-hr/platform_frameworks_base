{
  super(context,attrs);
  mCollapsedHeight=getResources().getDimensionPixelSize(R.dimen.speed_bump_height_collapsed);
  mTextPaddingInset=getResources().getDimensionPixelSize(R.dimen.speed_bump_text_padding_inset);
  mDotsHeight=getResources().getDimensionPixelSize(R.dimen.speed_bump_dots_height);
  setOnClickListener(this);
  mFastOutSlowInInterpolator=AnimationUtils.loadInterpolator(getContext(),android.R.interpolator.fast_out_slow_in);
}
