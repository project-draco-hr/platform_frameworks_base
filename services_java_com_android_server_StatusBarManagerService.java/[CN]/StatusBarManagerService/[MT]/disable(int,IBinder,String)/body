{
  enforceStatusBar();
synchronized (mDisableRecords) {
    manageDisableListLocked(what,token,pkg);
    final int net=gatherDisableActionsLocked();
    if (net != mDisabled) {
      mDisabled=net;
      mHandler.post(new Runnable(){
        public void run(){
          mNotificationCallbacks.onSetDisabled(net);
        }
      }
);
      if (mBar != null) {
        try {
          mBar.disable(net);
        }
 catch (        RemoteException ex) {
        }
      }
    }
  }
}
