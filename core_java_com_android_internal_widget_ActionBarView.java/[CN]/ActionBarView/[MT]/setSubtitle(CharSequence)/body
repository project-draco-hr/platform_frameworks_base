{
  mSubtitle=subtitle;
  if (mSubtitleView != null) {
    mSubtitleView.setText(subtitle);
    mSubtitleView.setVisibility(subtitle != null ? VISIBLE : GONE);
    mTitleLayout.setVisibility(mExpandedActionView != null && (mDisplayOptions & ActionBar.DISPLAY_SHOW_TITLE) != 0 && (!TextUtils.isEmpty(mTitle) || !TextUtils.isEmpty(mSubtitle)) ? VISIBLE : GONE);
  }
}
