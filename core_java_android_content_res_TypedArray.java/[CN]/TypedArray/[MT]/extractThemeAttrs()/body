{
  if (mRecycled) {
    throw new RuntimeException("Cannot make calls to a recycled instance!");
  }
  int[] attrs=null;
  final int N=length();
  for (int i=0; i < N; i++) {
    final int attrId=getThemeAttributeId(i,0);
    if (attrId != 0) {
      if (attrs == null) {
        attrs=new int[N];
      }
      attrs[i]=attrId;
    }
  }
  return attrs;
}
