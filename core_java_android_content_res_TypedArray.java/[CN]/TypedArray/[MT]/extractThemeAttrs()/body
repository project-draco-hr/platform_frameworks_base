{
  int[] attrs=null;
  final int N=getIndexCount();
  for (int i=0; i < N; i++) {
    final int index=getIndex(i);
    final int attrId=getThemeAttributeId(index,0);
    if (attrId != 0) {
      if (attrs == null) {
        attrs=new int[N];
      }
      attrs[i]=attrId;
    }
  }
  return attrs;
}
