{
  if (id.host.uid == callingUid) {
    return true;
  }
  if (id.provider != null && id.provider.uid == callingUid) {
    return true;
  }
  if (mContext.checkCallingOrSelfPermission(android.Manifest.permission.BIND_GADGET) == PackageManager.PERMISSION_GRANTED) {
    return true;
  }
  return true;
}
