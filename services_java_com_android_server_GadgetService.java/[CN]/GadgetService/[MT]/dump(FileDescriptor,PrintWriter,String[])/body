{
  if (mContext.checkCallingPermission(android.Manifest.permission.DUMP) != PackageManager.PERMISSION_GRANTED) {
    pw.println("Permission Denial: can't dump from from pid=" + Binder.getCallingPid() + ", uid="+ Binder.getCallingUid());
    return;
  }
synchronized (mGadgetIds) {
    int N=mInstalledProviders.size();
    pw.println("Providers: (size=" + N + ")");
    for (int i=0; i < N; i++) {
      GadgetInfo info=mInstalledProviders.get(i);
      pw.println("  [" + i + "] provder="+ info.provider+ " min=("+ info.minWidth+ "x"+ info.minHeight+ ")"+ " updatePeriodMillis="+ info.updatePeriodMillis+ " initialLayout="+ info.initialLayout);
    }
    N=mGadgetIds.size();
    pw.println("GadgetIds: (size=" + N + ")");
    for (int i=0; i < N; i++) {
      GadgetId id=mGadgetIds.get(i);
      pw.println("  [" + i + "] gadgetId="+ id.gadgetId+ " host="+ id.hostPackage+ " provider="+ (id.info == null ? "null" : id.info.provider));
    }
  }
}
