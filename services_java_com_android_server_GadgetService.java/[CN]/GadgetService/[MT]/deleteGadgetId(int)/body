{
synchronized (mGadgetIds) {
    int callingUid=getCallingUid();
    final int N=mGadgetIds.size();
    for (int i=0; i < N; i++) {
      GadgetId id=mGadgetIds.get(i);
      if (id.provider != null && canAccessGadgetId(id,callingUid)) {
        deleteGadgetLocked(id);
        saveStateLocked();
        return;
      }
    }
  }
}
