{
  if (mAccessibilityController == null) {
    return;
  }
  mLockIcon.setClickable(mUnlockMethodCache.isTrustManaged() || mAccessibilityController.isTouchExplorationEnabled());
  mLockIcon.setLongClickable(mAccessibilityController.isTouchExplorationEnabled() && mUnlockMethodCache.isTrustManaged());
  mLockIcon.setFocusable(mAccessibilityController.isAccessibilityEnabled());
}
