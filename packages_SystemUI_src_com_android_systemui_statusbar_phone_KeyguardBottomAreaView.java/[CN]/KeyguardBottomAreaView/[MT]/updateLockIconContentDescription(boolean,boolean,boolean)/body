{
  mLockIcon.setContentDescription(getResources().getString(faceUnlockRunning ? R.string.accessibility_unlock_button_face_unlock_running : insecure && !trustManaged ? R.string.accessibility_unlock_button_not_secured : insecure ? R.string.accessibility_unlock_button_not_secured_trust_managed : !trustManaged ? R.string.accessibility_unlock_button_secured : R.string.accessibility_unlock_button_secured_trust_managed));
}
