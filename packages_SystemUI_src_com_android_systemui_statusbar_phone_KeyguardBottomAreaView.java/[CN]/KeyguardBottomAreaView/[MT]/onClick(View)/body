{
  if (v == mCameraImageView) {
    launchCamera();
  }
 else   if (v == mPhoneImageView) {
    launchPhone();
  }
  if (v == mLockIcon) {
    mIndicationController.showTransientIndication(R.string.keyguard_indication_trust_disabled);
    mLockPatternUtils.requireCredentialEntry(mLockPatternUtils.getCurrentUser());
  }
}
