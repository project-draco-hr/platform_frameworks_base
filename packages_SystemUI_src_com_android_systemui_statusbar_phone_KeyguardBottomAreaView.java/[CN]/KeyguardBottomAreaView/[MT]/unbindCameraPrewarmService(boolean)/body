{
  if (mPrewarmBound) {
    if (launched) {
      try {
        mPrewarmMessenger.send(Message.obtain(null,CameraPrewarmService.MSG_CAMERA_FIRED));
      }
 catch (      RemoteException e) {
        Log.w(TAG,"Error sending camera fired message",e);
      }
    }
    mContext.unbindService(mPrewarmConnection);
    mPrewarmBound=false;
  }
}
