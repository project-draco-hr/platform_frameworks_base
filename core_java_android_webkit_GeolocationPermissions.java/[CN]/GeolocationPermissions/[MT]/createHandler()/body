{
  if (mHandler == null) {
    mHandler=new Handler(){
      @Override public void handleMessage(      Message msg){
switch (msg.what) {
case GET_ORIGINS:
          getOriginsImpl();
        break;
case GET_ALLOWED:
      getAllowedImpl((String)msg.obj);
    break;
case CLEAR:
  nativeClear((String)msg.obj);
break;
case CLEAR_ALL:
nativeClearAll();
break;
}
}
}
;
}
}
