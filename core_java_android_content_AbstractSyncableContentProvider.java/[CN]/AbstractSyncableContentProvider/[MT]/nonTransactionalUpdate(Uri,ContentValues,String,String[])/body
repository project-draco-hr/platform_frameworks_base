{
  if (isTemporary() && mSyncState.matches(uri)) {
    int numRows=mSyncState.asContentProvider().update(uri,values,selection,selectionArgs);
    return numRows;
  }
  int result=updateInternal(uri,values,selection,selectionArgs);
  if (!isTemporary() && result > 0) {
    getContext().getContentResolver().notifyChange(uri,null,changeRequiresLocalSync(uri));
  }
  return result;
}
