{
  if (mDevice != null) {
    dest.writeInt(1);
    mDevice.writeToParcel(dest,flags);
  }
 else {
    dest.writeInt(0);
  }
  if (mScanRecord != null) {
    dest.writeInt(mScanRecord.getBytes().length);
    dest.writeByteArray(mScanRecord.getBytes());
  }
 else {
    dest.writeInt(0);
  }
  dest.writeInt(mRssi);
  dest.writeLong(mTimestampNanos);
}
