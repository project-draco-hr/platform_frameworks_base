{
  Notification.Builder builder=new Notification.Builder(mContext).setSmallIcon(android.R.drawable.ic_secure).setContentTitle(mContext.getString(R.string.queued_notification_title_template,printJob.getLabel())).addAction(android.R.drawable.ic_secure,mContext.getString(R.string.cancel),createCancelIntent(printJob.getId())).setContentText(printJob.getPrinterId().getPrinterName()).setOngoing(true).setWhen(System.currentTimeMillis()).setShowWhen(true);
  mNotificationManager.notify(printJob.getId(),builder.build());
}
