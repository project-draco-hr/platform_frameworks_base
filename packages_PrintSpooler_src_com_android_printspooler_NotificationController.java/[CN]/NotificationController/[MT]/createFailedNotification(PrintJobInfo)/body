{
  Notification.Builder builder=new Notification.Builder(mContext).setContentIntent(createContentIntent(printJob.getId())).setSmallIcon(computeNotificationIcon(printJob)).setContentTitle(computeNotificationTitle(printJob)).addAction(R.drawable.stat_notify_cancelling,mContext.getString(R.string.cancel),createCancelIntent(printJob)).addAction(R.drawable.ic_restart,mContext.getString(R.string.restart),createRestartIntent(printJob.getId())).setContentText(printJob.getPrinterName()).setWhen(System.currentTimeMillis()).setOngoing(true).setShowWhen(true);
  mNotificationManager.notify(0,builder.build());
}
