{
  Notification.Builder builder=new Notification.Builder(mContext).setContentIntent(createContentIntent(printJob.getId())).setSmallIcon(com.android.internal.R.drawable.ic_print_error).setContentTitle(mContext.getString(R.string.failed_notification_title_template,printJob.getLabel())).addAction(R.drawable.stat_notify_cancelling,mContext.getString(R.string.cancel),createCancelIntent(printJob)).addAction(R.drawable.ic_restart,mContext.getString(R.string.restart),createRestartIntent(printJob.getId())).setContentText(printJob.getPrinterName()).setWhen(System.currentTimeMillis()).setOngoing(true).setShowWhen(true);
  mNotificationManager.notify(printJob.getId().flattenToString(),0,builder.build());
}
