{
  Notification.Builder builder=new Notification.Builder(mContext).setContentIntent(createContentIntent(printJob.getId())).setSmallIcon(computeNotificationIcon(printJob)).setContentTitle(computeNotificationTitle(printJob)).setContentText(printJob.getPrinterName()).setWhen(System.currentTimeMillis()).setOngoing(true).setShowWhen(true);
  mNotificationManager.notify(0,builder.build());
}
