{
  Notification.Builder builder=new Notification.Builder(mContext).setSmallIcon(android.R.drawable.ic_secure).setContentTitle(mContext.getString(R.string.printing_notification_title_template,printJob.getLabel())).addAction(android.R.drawable.ic_secure,mContext.getString(R.string.cancel),createCancelIntent(printJob)).setContentText(printJob.getPrinterId().getPrinterName()).setWhen(System.currentTimeMillis()).setOngoing(true).setShowWhen(true);
  mNotificationManager.notify(printJob.getId(),builder.build());
}
