{
  if (DEBUG) {
    Log.i(LOG_TAG,"onPrintJobStateChanged() printJobId: " + printJob.getId() + " oldState: "+ PrintJobInfo.stateToString(oldState)+ " newState:"+ PrintJobInfo.stateToString(printJob.getState()));
  }
switch (printJob.getState()) {
case PrintJobInfo.STATE_QUEUED:
{
      createQueuingNotificaiton(printJob);
    }
  break;
case PrintJobInfo.STATE_STARTED:
{
  createPrintingNotificaiton(printJob);
}
break;
case PrintJobInfo.STATE_FAILED:
{
createFailedNotificaiton(printJob);
}
break;
case PrintJobInfo.STATE_COMPLETED:
case PrintJobInfo.STATE_CANCELED:
{
if (oldState > PrintJobInfo.STATE_CREATED) {
removeNotification(printJob.getId());
}
}
break;
}
}
