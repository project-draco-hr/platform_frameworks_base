{
  Notification.Builder builder=new Notification.Builder(mContext).setContentIntent(createContentIntent(printJob.getId())).setSmallIcon(com.android.internal.R.drawable.ic_print).setContentTitle(mContext.getString(R.string.printing_notification_title_template,printJob.getLabel())).addAction(R.drawable.stat_notify_cancelling,mContext.getString(R.string.cancel),createCancelIntent(printJob)).setContentText(printJob.getPrinterName()).setWhen(System.currentTimeMillis()).setOngoing(true).setShowWhen(true);
  mNotificationManager.notify(printJob.getId().flattenToString(),0,builder.build());
}
