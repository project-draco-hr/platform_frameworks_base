{
  int act=ev.getAction();
  if (act == ev.ACTION_UP) {
    mRender.newTouchPosition(0,0,0,0);
    return false;
  }
  int count=ev.getHistorySize();
  int pcount=ev.getPointerCount();
  for (int p=0; p < pcount; p++) {
    int id=ev.getPointerId(p);
    mRender.newTouchPosition(ev.getX(p),ev.getY(p),ev.getPressure(p),id);
    for (int i=0; i < count; i++) {
      mRender.newTouchPosition(ev.getHistoricalX(p,i),ev.getHistoricalY(p,i),ev.getHistoricalPressure(p,i),id);
    }
  }
  return true;
}
