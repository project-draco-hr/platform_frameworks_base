{
  int[] list;
switch (abbrev) {
case LENGTH_LONG:
    list=sMonthsLong;
  break;
case LENGTH_MEDIUM:
list=sMonthsMedium;
break;
case LENGTH_SHORT:
list=sMonthsMedium;
break;
case LENGTH_SHORTER:
list=sMonthsMedium;
break;
case LENGTH_SHORTEST:
list=sMonthsShortest;
break;
default :
list=sMonthsMedium;
break;
}
Resources r=Resources.getSystem();
return r.getString(list[month - Calendar.JANUARY]);
}
