{
  if (Build.VERSION.SDK_INT >= Build.VERSION_CODES.JELLY_BEAN) {
    assertEquals(mBindingObject.getPopupBackground(),((ColorDrawable)mView.getPopupBackground()).getColor());
    changeValues();
    assertEquals(mBindingObject.getPopupBackground(),((ColorDrawable)mView.getPopupBackground()).getColor());
  }
}
