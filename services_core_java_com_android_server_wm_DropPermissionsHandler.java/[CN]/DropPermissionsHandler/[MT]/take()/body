{
  if (mPermissionOwner != null) {
    return;
  }
  mPermissionOwner=ActivityManagerNative.getDefault().newUriPermissionOwner("drop");
  long origId=Binder.clearCallingIdentity();
  try {
    for (int i=0; i < mUris.size(); i++) {
      ActivityManagerNative.getDefault().grantUriPermissionFromOwner(mPermissionOwner,mSourceUid,mTargetPackage,mUris.get(i),mMode,mSourceUserId,mTargetUserId);
    }
  }
  finally {
    Binder.restoreCallingIdentity(origId);
  }
}
