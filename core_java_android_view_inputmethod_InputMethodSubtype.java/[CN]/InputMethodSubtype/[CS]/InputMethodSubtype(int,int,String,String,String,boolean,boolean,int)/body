{
  mSubtypeNameResId=nameId;
  mSubtypeIconResId=iconId;
  mSubtypeLocale=locale != null ? locale : "";
  mSubtypeMode=mode != null ? mode : "";
  mSubtypeExtraValue=extraValue != null ? extraValue : "";
  mIsAuxiliary=isAuxiliary;
  mOverridesImplicitlyEnabledSubtype=overridesImplicitlyEnabledSubtype;
  mSubtypeHashCode=id != 0 ? id : hashCodeInternal(mSubtypeLocale,mSubtypeMode,mSubtypeExtraValue,mIsAuxiliary,mOverridesImplicitlyEnabledSubtype);
  mSubtypeId=id;
}
