{
  final boolean isEvent=conditionId != null && conditionId.getScheme().equals(Condition.SCHEME) && conditionId.getAuthority().equals(ZenModeConfig.SYSTEM_AUTHORITY) && conditionId.getPathSegments().size() == 1 && conditionId.getPathSegments().get(0).equals(EVENT_PATH);
  if (!isEvent)   return null;
  final EventInfo rt=new EventInfo();
  rt.userId=tryParseInt(conditionId.getQueryParameter("userId"),UserHandle.USER_NULL);
  rt.calendar=conditionId.getQueryParameter("calendar");
  if (TextUtils.isEmpty(rt.calendar) || tryParseLong(rt.calendar,-1L) != -1L) {
    rt.calendar=null;
  }
  rt.reply=tryParseInt(conditionId.getQueryParameter("reply"),0);
  return rt;
}
