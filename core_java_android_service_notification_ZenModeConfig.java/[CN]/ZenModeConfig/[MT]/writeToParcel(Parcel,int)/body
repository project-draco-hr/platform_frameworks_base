{
  dest.writeInt(allowCalls ? 1 : 0);
  dest.writeInt(allowMessages ? 1 : 0);
  if (sleepMode != null) {
    dest.writeInt(1);
    dest.writeString(sleepMode);
  }
 else {
    dest.writeInt(0);
  }
  dest.writeInt(sleepStartHour);
  dest.writeInt(sleepStartMinute);
  dest.writeInt(sleepEndHour);
  dest.writeInt(sleepEndMinute);
}
