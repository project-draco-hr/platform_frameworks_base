{
  return new StringBuilder(ZenModeConfig.class.getSimpleName()).append('[').append("allowCalls=").append(allowCalls).append(",allowMessages=").append(allowMessages).append(",allowFrom=").append(sourceToString(allowFrom)).append(",allowReminders=").append(allowReminders).append(",allowEvents=").append(allowEvents).append(",automaticRules=").append(automaticRules).append(",manualRule=").append(manualRule).append(']').toString();
}
