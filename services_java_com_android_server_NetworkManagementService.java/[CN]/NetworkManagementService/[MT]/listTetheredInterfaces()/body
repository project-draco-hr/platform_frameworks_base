{
  mContext.enforceCallingOrSelfPermission(android.Manifest.permission.ACCESS_NETWORK_STATE,"NetworkManagementService");
  try {
    return mConnector.doListCommand("tether interface list",NetdResponseCode.TetherInterfaceListResult);
  }
 catch (  NativeDaemonConnectorException e) {
    throw new IllegalStateException("Unable to communicate to native daemon for listing tether interfaces");
  }
}
