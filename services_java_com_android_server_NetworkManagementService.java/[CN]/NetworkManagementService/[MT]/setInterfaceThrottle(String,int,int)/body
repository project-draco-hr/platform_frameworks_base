{
  mContext.enforceCallingOrSelfPermission(android.Manifest.permission.CHANGE_NETWORK_STATE,"NetworkManagementService");
  try {
    mConnector.doCommand(String.format("interface setthrottle %s %d %d",iface,rxKbps,txKbps));
  }
 catch (  NativeDaemonConnectorException e) {
    Slog.e(TAG,"Error communicating with native daemon to set throttle",e);
  }
}
