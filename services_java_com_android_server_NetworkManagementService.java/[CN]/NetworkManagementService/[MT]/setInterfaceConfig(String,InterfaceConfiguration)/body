{
  String cmd=String.format("interface setcfg %s %s %s %s",iface,intToIpString(cfg.ipAddr),intToIpString(cfg.netmask),cfg.interfaceFlags);
  try {
    mConnector.doCommand(cmd);
  }
 catch (  NativeDaemonConnectorException e) {
    throw new IllegalStateException("Unable to communicate with native daemon to interface setcfg");
  }
}
