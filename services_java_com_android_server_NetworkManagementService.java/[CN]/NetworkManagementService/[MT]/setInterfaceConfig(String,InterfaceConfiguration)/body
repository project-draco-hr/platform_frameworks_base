{
  String cmd=String.format("interface setcfg %s %s %s %s",iface,cfg.addr.getHostAddress(),cfg.mask.getHostAddress(),cfg.interfaceFlags);
  try {
    mConnector.doCommand(cmd);
  }
 catch (  NativeDaemonConnectorException e) {
    throw new IllegalStateException("Unable to communicate with native daemon to interface setcfg - " + e);
  }
}
