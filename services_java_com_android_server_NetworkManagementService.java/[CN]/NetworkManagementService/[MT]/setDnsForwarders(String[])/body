{
  mContext.enforceCallingOrSelfPermission(android.Manifest.permission.CHANGE_NETWORK_STATE,"NetworkManagementService");
  try {
    String cmd="tether dns set ";
    for (    String s : dns) {
      cmd+=InetAddress.getByName(s).toString() + " ";
    }
    mConnector.doCommand(cmd);
  }
 catch (  UnknownHostException e) {
    throw new IllegalStateException("Error resolving dns name",e);
  }
}
