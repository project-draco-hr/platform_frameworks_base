{
  mContext.enforceCallingOrSelfPermission(android.Manifest.permission.CHANGE_NETWORK_STATE,"NetworkManagementService");
  if (DBG)   Log.d(TAG,"enableNat(" + internalInterface + ", "+ externalInterface+ ")");
  try {
    modifyNat("enable",internalInterface,externalInterface);
  }
 catch (  Exception e) {
    Log.e(TAG,"enableNat got Exception " + e.toString());
    throw new IllegalStateException("Unable to communicate to native daemon for enabling NAT interface");
  }
}
