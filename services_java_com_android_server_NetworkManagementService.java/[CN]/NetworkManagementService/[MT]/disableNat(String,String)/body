{
  mContext.enforceCallingOrSelfPermission(android.Manifest.permission.CHANGE_NETWORK_STATE,"NetworkManagementService");
  if (DBG)   Log.d(TAG,"disableNat(" + internalInterface + ", "+ externalInterface+ ")");
  try {
    modifyNat("disable",internalInterface,externalInterface);
  }
 catch (  Exception e) {
    Log.e(TAG,"disableNat got Exception " + e.toString());
    throw new IllegalStateException("Unable to communicate to native daemon for disabling NAT interface");
  }
}
