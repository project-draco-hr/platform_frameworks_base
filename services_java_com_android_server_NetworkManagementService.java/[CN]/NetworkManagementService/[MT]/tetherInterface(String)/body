{
  mContext.enforceCallingOrSelfPermission(android.Manifest.permission.CHANGE_NETWORK_STATE,"NetworkManagementService");
  try {
    mConnector.doCommand("tether interface add " + iface);
  }
 catch (  NativeDaemonConnectorException e) {
    throw new IllegalStateException("Unable to communicate to native daemon for adding tether interface");
  }
}
