{
  mContext.enforceCallingOrSelfPermission(android.Manifest.permission.ACCESS_NETWORK_STATE,"NetworkManagementService");
  ArrayList<String> rsp;
  try {
    rsp=mConnector.doCommand("ipfwd status");
  }
 catch (  NativeDaemonConnectorException e) {
    throw new IllegalStateException("Unable to communicate with native daemon to ipfwd status");
  }
  for (  String line : rsp) {
    String[] tok=line.split(" ");
    if (tok.length < 3) {
      Slog.e(TAG,"Malformed response from native daemon: " + line);
      return false;
    }
    int code=Integer.parseInt(tok[0]);
    if (code == NetdResponseCode.IpFwdStatusResult) {
      return "enabled".equals(tok[2]);
    }
 else {
      throw new IllegalStateException(String.format("Unexpected response code %d",code));
    }
  }
  throw new IllegalStateException("Got an empty response");
}
