{
  mContext.enforceCallingOrSelfPermission(android.Manifest.permission.ACCESS_NETWORK_STATE,"NetworkManagementService");
  try {
    return mConnector.doListCommand("list_ttys",NetdResponseCode.TtyListResult);
  }
 catch (  NativeDaemonConnectorException e) {
    throw new IllegalStateException("Unable to communicate to native daemon for listing TTYs");
  }
}
