{
  mContext.enforceCallingOrSelfPermission(android.Manifest.permission.CHANGE_NETWORK_STATE,"NetworkManagementService");
  mContext.enforceCallingOrSelfPermission(android.Manifest.permission.CHANGE_WIFI_STATE,"NetworkManagementService");
  mConnector.doCommand(String.format("softap stop " + intf));
  mConnector.doCommand(String.format("softap fwreload " + intf + " AP"));
  mConnector.doCommand(String.format("softap start " + intf));
  if (wifiConfig == null) {
    mConnector.doCommand(String.format("softap set " + intf + " wl0.1"));
  }
 else {
    String str=String.format("softap set " + intf + " wl0.1 %s %s %s",wifiConfig.SSID,wifiConfig.allowedKeyManagement.get(KeyMgmt.WPA_PSK) ? "wpa2-psk" : "open",wifiConfig.preSharedKey);
    mConnector.doCommand(str);
  }
  mConnector.doCommand(String.format("softap startap"));
}
