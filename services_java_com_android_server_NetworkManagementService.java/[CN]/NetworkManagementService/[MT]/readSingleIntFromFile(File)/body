{
  RandomAccessFile f=null;
  try {
    f=new RandomAccessFile(file,"r");
    byte[] buffer=new byte[(int)f.length()];
    f.readFully(buffer);
    return Integer.parseInt(new String(buffer).trim());
  }
 catch (  NumberFormatException e) {
    return -1;
  }
catch (  IOException e) {
    return -1;
  }
 finally {
    IoUtils.closeQuietly(f);
  }
}
