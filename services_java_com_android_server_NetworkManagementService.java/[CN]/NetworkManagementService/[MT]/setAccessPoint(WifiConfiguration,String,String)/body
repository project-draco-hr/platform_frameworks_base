{
  mContext.enforceCallingOrSelfPermission(CHANGE_NETWORK_STATE,TAG);
  mContext.enforceCallingOrSelfPermission(CHANGE_WIFI_STATE,TAG);
  try {
    if (wifiConfig == null) {
      mConnector.doCommand(String.format("softap set " + wlanIface + " "+ softapIface));
    }
 else {
      String str=String.format("softap set " + wlanIface + " "+ softapIface+ " %s %s %s",convertQuotedString(wifiConfig.SSID),getSecurityType(wifiConfig),convertQuotedString(wifiConfig.preSharedKey));
      mConnector.doCommand(str);
    }
  }
 catch (  NativeDaemonConnectorException e) {
    throw e.rethrowAsParcelableException();
  }
}
