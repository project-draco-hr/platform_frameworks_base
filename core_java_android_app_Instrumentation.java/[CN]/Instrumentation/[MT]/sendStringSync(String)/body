{
  if (text == null) {
    return;
  }
  KeyCharacterMap keyCharacterMap=KeyCharacterMap.load(KeyCharacterMap.VIRTUAL_KEYBOARD);
  KeyEvent[] events=keyCharacterMap.getEvents(text.toCharArray());
  if (events != null) {
    for (int i=0; i < events.length; i++) {
      sendKeySync(events[i]);
    }
  }
}
