{
  mStart=0;
  if (!isProfiling()) {
    Bundle binderCounts=getBinderCounts();
    for (    String key : binderCounts.keySet()) {
      addPerfMetricLong("pre_" + key,binderCounts.getLong(key));
    }
    startAllocCounting();
    mStart=SystemClock.uptimeMillis();
    mPreCpuTime=Process.getElapsedCpuTime();
  }
}
