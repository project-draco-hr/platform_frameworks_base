{
  if ((iface.asBinder()) instanceof Binder) {
    try {
      caller.go(iface,out,prefix,args);
    }
 catch (    RemoteException e) {
    }
    return;
  }
  TransferPipe tp=new TransferPipe();
  try {
    caller.go(iface,tp.getWriteFd().getFileDescriptor(),prefix,args);
    tp.go(out,timeout);
  }
  finally {
    tp.kill();
  }
}
