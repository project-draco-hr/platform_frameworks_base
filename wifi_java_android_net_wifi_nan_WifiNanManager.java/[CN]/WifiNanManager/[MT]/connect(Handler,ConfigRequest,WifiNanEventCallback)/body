{
  if (VDBG) {
    Log.v(TAG,"connect(): handler=" + handler + ", callback="+ callback+ ", configRequest="+ configRequest);
  }
synchronized (mLock) {
    mLooper=(handler == null) ? Looper.getMainLooper() : handler.getLooper();
    try {
      mClientId=mService.connect(mBinder,mContext.getOpPackageName(),new WifiNanEventCallbackProxy(this,mLooper,callback),configRequest);
    }
 catch (    RemoteException e) {
      mClientId=INVALID_CLIENT_ID;
      mLooper=null;
      throw e.rethrowFromSystemServer();
    }
  }
  mCloseGuard.open("disconnect");
}
