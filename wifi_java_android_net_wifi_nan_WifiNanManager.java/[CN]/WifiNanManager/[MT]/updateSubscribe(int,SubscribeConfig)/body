{
  if (VDBG) {
    Log.v(TAG,"subscribe(): config=" + subscribeConfig);
  }
  int clientId;
synchronized (mLock) {
    if (mClientId == INVALID_CLIENT_ID) {
      Log.e(TAG,"updateSubscribe(): called with invalid client ID - not connected first?");
      return;
    }
    clientId=mClientId;
  }
  try {
    mService.updateSubscribe(clientId,sessionId,subscribeConfig);
  }
 catch (  RemoteException e) {
    throw e.rethrowFromSystemServer();
  }
}
