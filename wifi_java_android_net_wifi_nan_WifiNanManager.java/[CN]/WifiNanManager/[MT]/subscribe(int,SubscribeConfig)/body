{
  if (VDBG) {
    Log.v(TAG,"subscribe(): config=" + subscribeConfig);
  }
  if (subscribeConfig.mSubscribeType == SubscribeConfig.SUBSCRIBE_TYPE_ACTIVE && subscribeConfig.mRxFilterLength != 0) {
    throw new IllegalArgumentException("Invalid subscribe config: ACTIVE subscribes can't have an Rx filter");
  }
  if (subscribeConfig.mSubscribeType == SubscribeConfig.SUBSCRIBE_TYPE_PASSIVE && subscribeConfig.mTxFilterLength != 0) {
    throw new IllegalArgumentException("Invalid subscribe config: PASSIVE subscribes can't have a Tx filter");
  }
  try {
    mService.subscribe(mClientId,sessionId,subscribeConfig);
  }
 catch (  RemoteException e) {
    throw e.rethrowFromSystemServer();
  }
}
