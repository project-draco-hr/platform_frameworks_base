{
  if (VDBG) {
    Log.v(TAG,"connect(): looper=" + looper + ", callback="+ callback+ ", configRequest="+ configRequest);
  }
synchronized (mLock) {
    if (mState != STATE_UNCONNECTED) {
      Log.e(TAG,"connect(): Calling connect() when state != UNCONNECTED!");
      return;
    }
    mLooper=looper;
    mBinder=new Binder();
    mState=STATE_CONNECTING;
    try {
      mClientId=mService.connect(mBinder,new WifiNanEventCallbackProxy(this,looper,callback),configRequest);
    }
 catch (    RemoteException e) {
      mLooper=null;
      mBinder=null;
      mState=STATE_UNCONNECTED;
      throw e.rethrowFromSystemServer();
    }
  }
}
