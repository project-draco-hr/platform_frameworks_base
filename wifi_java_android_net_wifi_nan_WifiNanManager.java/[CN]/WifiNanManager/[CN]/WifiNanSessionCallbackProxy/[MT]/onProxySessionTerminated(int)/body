{
  if (VDBG)   Log.v(TAG,"Proxy: onSessionTerminated: reason=" + reason);
  if (mSession != null) {
    mSession.setTerminated();
    mSession=null;
  }
 else {
    Log.w(TAG,"Proxy: onSessionTerminated called but mSession is null!?");
  }
  mNanManager.clear();
  mOriginalCallback.onSessionTerminated(reason);
}
