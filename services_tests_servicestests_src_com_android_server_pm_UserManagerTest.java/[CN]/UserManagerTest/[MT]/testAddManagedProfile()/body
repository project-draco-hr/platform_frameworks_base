{
  UserInfo userInfo1=createProfileForUser("Managed 1",UserInfo.FLAG_MANAGED_PROFILE,UserHandle.USER_OWNER);
  UserInfo userInfo2=createProfileForUser("Managed 2",UserInfo.FLAG_MANAGED_PROFILE,UserHandle.USER_OWNER);
  assertNotNull(userInfo1);
  assertNull(userInfo2);
  assertFalse(mUserManager.isManagedProfile());
}
