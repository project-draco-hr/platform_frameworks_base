{
  if (resp.getDnsType() == WifiP2pDnsSdServiceInfo.DNS_TYPE_PTR) {
    if (mDnsSdServRspListener != null) {
      mDnsSdServRspListener.onDnsSdServiceAvailable(resp.getInstanceName(),resp.getDnsQueryName(),resp.getSrcDevice());
    }
  }
 else   if (resp.getDnsType() == WifiP2pDnsSdServiceInfo.DNS_TYPE_TXT) {
    if (mDnsSdTxtListener != null) {
      mDnsSdTxtListener.onDnsSdTxtRecordAvailable(resp.getDnsQueryName(),resp.getTxtRecord(),resp.getSrcDevice());
    }
  }
 else {
    Log.e(TAG,"Unhandled resp " + resp);
  }
}
