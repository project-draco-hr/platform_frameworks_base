{
  try {
    if (which == DialogInterface.BUTTON_POSITIVE) {
      mConfig.configureIntent.send();
    }
 else     if (which == DialogInterface.BUTTON_NEUTRAL) {
      mService.prepareVpn(mConfig.user,VpnConfig.LEGACY_VPN);
    }
  }
 catch (  Exception e) {
    Log.e(TAG,"onClick",e);
    finish();
  }
}
