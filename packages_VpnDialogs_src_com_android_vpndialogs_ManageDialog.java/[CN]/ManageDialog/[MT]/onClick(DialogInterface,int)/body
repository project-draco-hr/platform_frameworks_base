{
  try {
    if (which == AlertDialog.BUTTON_POSITIVE) {
      Intent intent=new Intent();
      intent.setClassName(mConfig.packageName,mConfig.configureActivity);
      startActivity(intent);
    }
 else     if (which == AlertDialog.BUTTON_NEUTRAL) {
      mService.prepareVpn("");
    }
  }
 catch (  Exception e) {
    Log.e(TAG,"onClick",e);
    finish();
  }
}
