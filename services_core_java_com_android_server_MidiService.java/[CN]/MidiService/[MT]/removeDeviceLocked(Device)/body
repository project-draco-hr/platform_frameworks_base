{
  if (mDevicesByInfo.remove(device.getDeviceInfo()) != null) {
    IMidiDeviceServer server=device.getDeviceServer();
    if (server != null) {
      mDevicesByServer.remove(server);
    }
synchronized (mClients) {
      for (      Client c : mClients.values()) {
        c.deviceRemoved(device);
      }
    }
  }
}
