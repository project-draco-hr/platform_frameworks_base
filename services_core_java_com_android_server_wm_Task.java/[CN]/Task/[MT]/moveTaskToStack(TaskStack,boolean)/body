{
  if (stack == mStack) {
    return;
  }
  if (DEBUG_STACK)   Slog.i(TAG,"moveTaskToStack: removing taskId=" + mTaskId + " from stack="+ mStack);
  EventLog.writeEvent(EventLogTags.WM_TASK_REMOVED,mTaskId,"removeTask");
  if (mStack != null) {
    mStack.removeTask(this);
  }
  stack.addTask(this,toTop);
}
