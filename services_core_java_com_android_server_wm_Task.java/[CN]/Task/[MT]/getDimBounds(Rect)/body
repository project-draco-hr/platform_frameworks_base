{
  if (useCurrentBounds()) {
    if (inFreeformWorkspace() && getMaxVisibleBounds(out)) {
      return;
    }
    if (!mFullscreen) {
      mStack.getBounds(mTmpRect);
      mTmpRect.intersect(mBounds);
      out.set(mTmpRect);
    }
 else {
      out.set(mBounds);
    }
    return;
  }
  mStack.getDisplayContent().getLogicalDisplayRect(out);
}
