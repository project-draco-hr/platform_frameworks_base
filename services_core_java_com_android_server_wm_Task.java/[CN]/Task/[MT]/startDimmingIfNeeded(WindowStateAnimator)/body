{
  if (newWinAnimator.mSurfaceShown && (mDimWinAnimator == null || !mDimWinAnimator.mSurfaceShown || mDimWinAnimator.mAnimLayer < newWinAnimator.mAnimLayer)) {
    mDimWinAnimator=newWinAnimator;
    if (mDimWinAnimator.mWin.mAppToken == null && !mFullscreen && mStack.getDisplayContent() != null) {
      mStack.getDisplayContent().getLogicalDisplayRect(mTmpRect);
      mDimLayer.setBounds(mTmpRect);
    }
  }
}
