{
synchronized (mGuard) {
    ParcelFileDescriptor pfd;
synchronized (mBuffer) {
      pfd=mParcelFileDescriptor;
      if (pfd == null)       return null;
      IoUtils.closeQuietly(mOutputStream);
      mParcelFileDescriptor=null;
      mOutputStream=null;
    }
    mIsClosed=true;
    return pfd;
  }
}
