{
synchronized (mGuard) {
    if (mIsClosed)     return;
    mGuard.close();
    mOutputStream.close();
    if (mDeviceServer != null) {
      try {
        mDeviceServer.closePort(mToken);
      }
 catch (      RemoteException e) {
        Log.e(TAG,"RemoteException in MidiInputPort.close()");
      }
    }
    mIsClosed=true;
  }
}
