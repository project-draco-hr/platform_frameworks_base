{
  android.util.Log.v("RSR","validate");
  Node n=mFirstNode;
  while (n != null) {
    n.mSeen=false;
    n.mDepth=0;
    n=n.mNext;
  }
  n=mFirstNode;
  while (n != null) {
    android.util.Log.v("RSR","validate n= " + n);
    if ((n.mSeen == false) && (n.mInputCount == 0)) {
      android.util.Log.v("RSR"," recursing " + n);
      validateRecurse(n,0);
    }
    n=n.mNext;
  }
}
