{
  if (mTo == null) {
    mTo=new Node[1];
    mToName=new String[1];
  }
 else {
    Node nt[]=new Node[mTo.length + 1];
    String ns[]=new String[mTo.length + 1];
    System.arraycopy(mTo,0,nt,0,mTo.length);
    System.arraycopy(mToName,0,ns,0,mTo.length);
    mTo=nt;
    mToName=ns;
  }
  mTo[mTo.length - 1]=n;
  mToName[mTo.length - 1]=name;
}
