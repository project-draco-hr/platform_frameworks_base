{
  if (s1 == null) {
    return s2 == null;
  }
  if (s2 == null)   return false;
  ArraySet<Signature> set1=new ArraySet<>();
  for (  Signature signature : s1) {
    set1.add(signature);
  }
  ArraySet<Signature> set2=new ArraySet<>();
  for (  Signature signature : s2) {
    set2.add(signature);
  }
  return set1.equals(set2);
}
