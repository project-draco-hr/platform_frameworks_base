{
  try {
    Context webViewContext=AppGlobals.getInitialApplication().createPackageContext(getWebViewPackageName(),Context.CONTEXT_INCLUDE_CODE | Context.CONTEXT_IGNORE_SECURITY);
    ClassLoader clazzLoader=webViewContext.getClassLoader();
    return (Class<WebViewFactoryProvider>)Class.forName(CHROMIUM_WEBVIEW_FACTORY,true,clazzLoader);
  }
 catch (  PackageManager.NameNotFoundException e) {
    throw new ClassNotFoundException("Chromium WebView package does not exist",e);
  }
}
