{
  if (w.mAttrs.type == TYPE_DOCK_DIVIDER) {
    mDockDivider=w;
  }
 else {
    final TaskStack stack=w.getStack();
    if (stack == null) {
      return;
    }
    if (stack.mStackId == StackId.PINNED_STACK_ID) {
      mPinnedWindow=w;
    }
 else     if (stack.mStackId == StackId.DOCKED_STACK_ID) {
      mDockedWindow=w;
    }
  }
}
