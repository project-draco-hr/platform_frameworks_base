{
  final int oldValue=(Integer)mState.value;
  final int newValue=oldValue == AudioManager.RINGER_MODE_NORMAL ? AudioManager.RINGER_MODE_VIBRATE : oldValue == AudioManager.RINGER_MODE_VIBRATE ? AudioManager.RINGER_MODE_SILENT : AudioManager.RINGER_MODE_NORMAL;
  mAudioManager.setRingerMode(newValue);
}
