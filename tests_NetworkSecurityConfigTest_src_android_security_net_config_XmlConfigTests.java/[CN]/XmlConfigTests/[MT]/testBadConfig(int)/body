{
  try {
    XmlConfigSource source=new XmlConfigSource(getContext(),configId);
    ApplicationConfig appConfig=new ApplicationConfig(source);
    appConfig.getConfigForHostname("android.com");
    fail("Bad config " + getContext().getResources().getResourceName(configId) + " did not fail to parse");
  }
 catch (  RuntimeException e) {
    MoreAsserts.assertAssignableFrom(XmlConfigSource.ParserException.class,e.getCause());
  }
}
