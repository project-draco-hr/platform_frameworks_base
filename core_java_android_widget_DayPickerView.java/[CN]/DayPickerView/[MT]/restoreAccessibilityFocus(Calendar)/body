{
  if (day == null) {
    return false;
  }
  final int childCount=getChildCount();
  for (int i=0; i < childCount; i++) {
    final View child=getChildAt(i);
    if (child instanceof SimpleMonthView) {
      if (((SimpleMonthView)child).restoreAccessibilityFocus(day)) {
        return true;
      }
    }
  }
  return false;
}
