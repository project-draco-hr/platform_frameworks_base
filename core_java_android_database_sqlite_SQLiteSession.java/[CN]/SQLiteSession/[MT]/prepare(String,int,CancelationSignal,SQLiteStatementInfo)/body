{
  if (sql == null) {
    throw new IllegalArgumentException("sql must not be null.");
  }
  if (cancelationSignal != null) {
    cancelationSignal.throwIfCanceled();
  }
  acquireConnection(sql,connectionFlags,cancelationSignal);
  try {
    mConnection.prepare(sql,outStatementInfo);
  }
  finally {
    releaseConnection();
  }
}
