{
  if (cancelationSignal != null) {
    cancelationSignal.throwIfCanceled();
  }
  if (mTransactionStack == null) {
    acquireConnection(null,connectionFlags,cancelationSignal);
  }
  try {
    if (mTransactionStack == null) {
switch (transactionMode) {
case TRANSACTION_MODE_IMMEDIATE:
        mConnection.execute("BEGIN IMMEDIATE;",null,cancelationSignal);
      break;
case TRANSACTION_MODE_EXCLUSIVE:
    mConnection.execute("BEGIN EXCLUSIVE;",null,cancelationSignal);
  break;
default :
mConnection.execute("BEGIN;",null,cancelationSignal);
break;
}
}
if (transactionListener != null) {
try {
transactionListener.onBegin();
}
 catch (RuntimeException ex) {
if (mTransactionStack == null) {
mConnection.execute("ROLLBACK;",null,cancelationSignal);
}
throw ex;
}
}
Transaction transaction=obtainTransaction(transactionMode,transactionListener);
transaction.mParent=mTransactionStack;
mTransactionStack=transaction;
}
  finally {
if (mTransactionStack == null) {
releaseConnection();
}
}
}
