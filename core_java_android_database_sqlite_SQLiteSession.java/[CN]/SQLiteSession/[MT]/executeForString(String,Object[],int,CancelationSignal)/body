{
  if (sql == null) {
    throw new IllegalArgumentException("sql must not be null.");
  }
  if (executeSpecial(sql,bindArgs,connectionFlags,cancelationSignal)) {
    return null;
  }
  acquireConnection(sql,connectionFlags,cancelationSignal);
  try {
    return mConnection.executeForString(sql,bindArgs,cancelationSignal);
  }
  finally {
    releaseConnection();
  }
}
