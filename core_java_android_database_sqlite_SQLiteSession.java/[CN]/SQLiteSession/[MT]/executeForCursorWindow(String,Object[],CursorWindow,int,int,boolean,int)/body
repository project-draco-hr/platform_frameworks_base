{
  if (sql == null) {
    throw new IllegalArgumentException("sql must not be null.");
  }
  if (window == null) {
    throw new IllegalArgumentException("window must not be null.");
  }
  if (executeSpecial(sql,bindArgs,connectionFlags)) {
    window.clear();
    return 0;
  }
  acquireConnectionIfNoTransaction(sql,connectionFlags);
  try {
    return mConnection.executeForCursorWindow(sql,bindArgs,window,startPos,requiredPos,countAllRows);
  }
  finally {
    releaseConnectionIfNoTransaction();
  }
}
