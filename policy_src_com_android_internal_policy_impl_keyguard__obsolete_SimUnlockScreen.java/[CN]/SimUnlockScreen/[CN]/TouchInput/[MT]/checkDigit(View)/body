{
  int digit=-1;
  if (v == mZero) {
    digit=0;
  }
 else   if (v == mOne) {
    digit=1;
  }
 else   if (v == mTwo) {
    digit=2;
  }
 else   if (v == mThree) {
    digit=3;
  }
 else   if (v == mFour) {
    digit=4;
  }
 else   if (v == mFive) {
    digit=5;
  }
 else   if (v == mSix) {
    digit=6;
  }
 else   if (v == mSeven) {
    digit=7;
  }
 else   if (v == mEight) {
    digit=8;
  }
 else   if (v == mNine) {
    digit=9;
  }
  return digit;
}
