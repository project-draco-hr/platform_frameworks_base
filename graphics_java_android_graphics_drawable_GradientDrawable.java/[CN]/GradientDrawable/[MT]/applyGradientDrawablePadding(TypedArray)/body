{
  mPadding=new Rect(a.getDimensionPixelOffset(R.styleable.GradientDrawablePadding_left,0),a.getDimensionPixelOffset(R.styleable.GradientDrawablePadding_top,0),a.getDimensionPixelOffset(R.styleable.GradientDrawablePadding_right,0),a.getDimensionPixelOffset(R.styleable.GradientDrawablePadding_bottom,0));
  mGradientState.mPadding=mPadding;
  mGradientState.mAttrPadding=a.extractThemeAttrs();
}
