{
  if (mShape != RECTANGLE) {
    mOpaque=false;
    return;
  }
  if (mStrokeWidth > 0 && !isOpaque(mStrokeColor)) {
    mOpaque=false;
    return;
  }
  if (mHasSolidColor) {
    mOpaque=isOpaque(mSolidColor);
    return;
  }
  if (mColors != null) {
    for (int i=0; i < mColors.length; i++) {
      if (!isOpaque(mColors[i])) {
        mOpaque=false;
        return;
      }
    }
  }
  mOpaque=true;
}
