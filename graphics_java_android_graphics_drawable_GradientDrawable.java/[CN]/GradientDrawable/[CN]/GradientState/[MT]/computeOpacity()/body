{
  if (mShape != RECTANGLE) {
    mOpaque=false;
    return;
  }
  if (mRadius > 0 || mRadiusArray != null) {
    mOpaque=false;
    return;
  }
  if (mStrokeWidth > 0) {
    if (mStrokeColorStateList != null) {
      if (!mStrokeColorStateList.isOpaque()) {
        mOpaque=false;
        return;
      }
    }
 else     if (!isOpaque(mStrokeColor)) {
      mOpaque=false;
      return;
    }
  }
  if (mColorStateList != null && !mColorStateList.isOpaque()) {
    mOpaque=false;
    return;
  }
  if (mHasSolidColor) {
    mOpaque=isOpaque(mSolidColor);
    return;
  }
  if (mColors != null) {
    for (int i=0; i < mColors.length; i++) {
      if (!isOpaque(mColors[i])) {
        mOpaque=false;
        return;
      }
    }
  }
  mOpaque=true;
}
