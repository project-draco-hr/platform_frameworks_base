{
  dest.writeString(mTypeface);
  dest.writeInt(mStyle);
  dest.writeInt(mTextSize);
  if (mTextColor != null) {
    dest.writeInt(1);
    mTextColor.writeToParcel(dest,flags);
  }
 else {
    dest.writeInt(0);
  }
  if (mTextColorLink != null) {
    dest.writeInt(1);
    mTextColorLink.writeToParcel(dest,flags);
  }
 else {
    dest.writeInt(0);
  }
}
