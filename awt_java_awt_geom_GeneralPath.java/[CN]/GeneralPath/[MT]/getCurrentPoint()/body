{
  if (typeSize == 0) {
    return null;
  }
  int j=pointSize - 2;
  if (types[typeSize - 1] == PathIterator.SEG_CLOSE) {
    for (int i=typeSize - 2; i > 0; i--) {
      int type=types[i];
      if (type == PathIterator.SEG_MOVETO) {
        break;
      }
      j-=pointShift[type];
    }
  }
  return new Point2D.Float(points[j],points[j + 1]);
}
