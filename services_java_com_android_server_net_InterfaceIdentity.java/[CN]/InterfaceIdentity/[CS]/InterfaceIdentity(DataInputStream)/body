{
  final int version=in.readInt();
switch (version) {
case VERSION_CURRENT:
{
      final int size=in.readInt();
      for (int i=0; i < size; i++) {
        add(new NetworkIdentity(in));
      }
      break;
    }
default :
{
    throw new ProtocolException("unexpected version: " + version);
  }
}
}
