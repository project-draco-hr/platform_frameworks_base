{
  mMtpManager.addValidDevice(0);
  mProvider.openDevice(0);
  mMtpManager.setObjectInfo(0,new MtpObjectInfo.Builder().setObjectHandle(2).build());
  try {
    mProvider.deleteDocument("0_0_1");
    fail();
  }
 catch (  Throwable e) {
    assertTrue(e instanceof IOException);
  }
  assertEquals(0,mResolver.getChangeCount(DocumentsContract.buildChildDocumentsUri(MtpDocumentsProvider.AUTHORITY,"0_0_2")));
}
