{
  setupProvider(MtpDatabaseConstants.FLAG_DATABASE_IN_MEMORY);
  setupRoots(0,new MtpRoot[]{new MtpRoot(0,0,"Device","Storage",0,0,"")});
  setupDocuments(0,0,MtpManager.OBJECT_HANDLE_ROOT_CHILDREN,"1",new MtpObjectInfo[]{new MtpObjectInfo.Builder().setName("test.txt").setObjectHandle(1).setParent(-1).build()});
  try {
    mProvider.deleteDocument("4");
    fail();
  }
 catch (  Throwable e) {
    assertTrue(e instanceof IOException);
  }
  assertEquals(0,mResolver.getChangeCount(DocumentsContract.buildChildDocumentsUri(MtpDocumentsProvider.AUTHORITY,"1")));
}
