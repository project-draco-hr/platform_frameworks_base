{
  mMtpManager.setObjectInfo(0,new MtpObjectInfo.Builder().setObjectHandle(1).setFormat(0x3801).setName("image.jpg").setDateModified(1422716400000L).setCompressedSize(1024 * 1024 * 5).setThumbCompressedSize(1024 * 50).build());
  mMtpManager.setParent(0,1,2);
  mProvider.deleteDocument("0_0_1");
  assertEquals(1,mResolver.getChangeCount(DocumentsContract.buildChildDocumentsUri(MtpDocumentsProvider.AUTHORITY,"0_0_2")));
}
