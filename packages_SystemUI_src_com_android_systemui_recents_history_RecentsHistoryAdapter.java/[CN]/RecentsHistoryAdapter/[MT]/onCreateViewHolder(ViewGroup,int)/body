{
switch (viewType) {
case DATE_ROW_VIEW_TYPE:
    return new ViewHolder(mLayoutInflater.inflate(R.layout.recents_history_date,parent,false));
case TASK_ROW_VIEW_TYPE:
  return new ViewHolder(mLayoutInflater.inflate(R.layout.recents_history_task,parent,false));
default :
return new ViewHolder(null);
}
}
