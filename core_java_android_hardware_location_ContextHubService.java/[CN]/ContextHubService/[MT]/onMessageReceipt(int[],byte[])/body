{
  if (mCallback != null) {
    ContextHubMessage msg=new ContextHubMessage(header[0],header[1],data);
    try {
      mCallback.onMessageReceipt(0,0,msg);
    }
 catch (    Exception e) {
      Log.e(TAG,"Exception " + e + " when calling remote callback");
      return -1;
    }
  }
 else {
    Log.d(TAG,"Message Callback is NULL");
  }
  return 0;
}
