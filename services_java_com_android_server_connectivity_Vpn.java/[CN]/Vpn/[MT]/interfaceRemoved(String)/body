{
  if (interfaze.equals(mInterface) && jniCheck(interfaze) == 0) {
    long identity=Binder.clearCallingIdentity();
    mCallback.restore();
    hideNotification();
    Binder.restoreCallingIdentity(identity);
    mInterface=null;
    if (mConnection != null) {
      mContext.unbindService(mConnection);
      mConnection=null;
    }
  }
}
