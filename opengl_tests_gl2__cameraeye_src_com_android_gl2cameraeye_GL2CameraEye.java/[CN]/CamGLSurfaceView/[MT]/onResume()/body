{
  mCamera=Camera.open();
  Camera.Parameters p=mCamera.getParameters();
  mCamera.setParameters(p);
  queueEvent(new Runnable(){
    public void run(){
      mRenderer.setCamera(mCamera);
    }
  }
);
  mSensorManager.registerListener(this,mAcceleration,SensorManager.SENSOR_DELAY_GAME);
  super.onResume();
}
