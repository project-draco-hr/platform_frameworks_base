{
  IpPrefix p;
  int oldCode=-1;
  Random random=new Random();
  for (int i=0; i < 100; i++) {
    if (random.nextBoolean()) {
      byte[] b=new byte[4];
      random.nextBytes(b);
      p=new IpPrefix(b,random.nextInt(33));
      assertNotEqual(oldCode,p.hashCode());
      oldCode=p.hashCode();
    }
 else {
      byte[] b=new byte[16];
      random.nextBytes(b);
      p=new IpPrefix(b,random.nextInt(129));
      assertNotEqual(oldCode,p.hashCode());
      oldCode=p.hashCode();
    }
  }
}
