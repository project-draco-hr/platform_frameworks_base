{
  if (approx) {
    if (relaxed)     mScript_approx_relaxed.invoke_init_vignette(mInPixelsAllocation.getType().getX(),mInPixelsAllocation.getType().getY(),center_x,center_y,scale,shade,slope);
 else     mScript_approx_full.invoke_init_vignette(mInPixelsAllocation.getType().getX(),mInPixelsAllocation.getType().getY(),center_x,center_y,scale,shade,slope);
  }
 else   if (relaxed)   mScript_relaxed.invoke_init_vignette(mInPixelsAllocation.getType().getX(),mInPixelsAllocation.getType().getY(),center_x,center_y,scale,shade,slope);
 else   mScript_full.invoke_init_vignette(mInPixelsAllocation.getType().getX(),mInPixelsAllocation.getType().getY(),center_x,center_y,scale,shade,slope);
}
