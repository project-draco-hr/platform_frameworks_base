{
  if (minTime < 0L) {
    minTime=0L;
  }
  if (minDistance < 0.0f) {
    minDistance=0.0f;
  }
  try {
    mService.requestLocationUpdatesPI(provider,criteria,minTime,minDistance,singleShot,intent,mContext.getPackageName());
  }
 catch (  RemoteException ex) {
    Log.e(TAG,"requestLocationUpdates: RemoteException",ex);
  }
}
