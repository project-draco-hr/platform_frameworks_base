{
  if (mCompiledQueries.containsKey(sql)) {
    return false;
  }
  mCompiledQueries.put(sql,compiledStatement);
  if (!isCacheFullWarningLogged() && mCompiledQueries.size() == mMaxSqlCacheSize) {
    Log.w(TAG,"Reached MAX size for compiled-sql statement cache for database " + mDatabase.getPath() + ". Use setMaxSqlCacheSize() in SQLiteDatabase to increase cachesize. ");
    setCacheFullWarningLogged();
  }
  return true;
}
