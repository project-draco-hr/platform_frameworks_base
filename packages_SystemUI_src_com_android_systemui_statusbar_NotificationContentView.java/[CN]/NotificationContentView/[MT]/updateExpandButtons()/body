{
  if (mExpandedChild != null) {
    mExpandedWrapper.updateExpandability(mContainingNotification.isExpandable(),mExpandClickListener);
  }
  if (mContractedChild != null) {
    mContractedWrapper.updateExpandability(mContainingNotification.isExpandable(),mExpandClickListener);
  }
  if (mHeadsUpChild != null) {
    mHeadsUpWrapper.updateExpandability(mContainingNotification.isExpandable(),mExpandClickListener);
  }
}
