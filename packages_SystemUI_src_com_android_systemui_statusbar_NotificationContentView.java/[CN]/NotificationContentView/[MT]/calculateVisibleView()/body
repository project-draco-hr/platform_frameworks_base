{
  boolean noExpandedChild=mExpandedChild == null;
  if (mIsHeadsUp && mHeadsUpChild != null) {
    if (mContentHeight <= mHeadsUpChild.getHeight() || noExpandedChild) {
      return HEADSUP;
    }
 else {
      return EXPANDED;
    }
  }
 else {
    if (mContentHeight <= mSmallHeight || noExpandedChild) {
      return CONTRACTED;
    }
 else {
      return EXPANDED;
    }
  }
}
