{
  StringBuilder b=new StringBuilder();
  b.append("{ what=");
  b.append(what);
  b.append(" when=");
  b.append(when);
  if (arg1 != 0) {
    b.append(" arg1=");
    b.append(arg1);
  }
  if (arg2 != 0) {
    b.append(" arg2=");
    b.append(arg2);
  }
  if (obj != null) {
    b.append(" obj=");
    b.append(obj);
  }
  b.append(" }");
  return b.toString();
}
