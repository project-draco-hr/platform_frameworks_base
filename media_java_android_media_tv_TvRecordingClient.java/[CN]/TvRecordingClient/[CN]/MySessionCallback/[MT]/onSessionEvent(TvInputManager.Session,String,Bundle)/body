{
  if (DEBUG) {
    Log.d(TAG,"onSessionEvent(eventType=" + eventType + ", eventArgs="+ eventArgs+ ")");
  }
  if (this != mSessionCallback) {
    Log.w(TAG,"onSessionEvent - session not created");
    return;
  }
  if (mCallback != null) {
    mCallback.onEvent(mInputId,eventType,eventArgs);
  }
}
