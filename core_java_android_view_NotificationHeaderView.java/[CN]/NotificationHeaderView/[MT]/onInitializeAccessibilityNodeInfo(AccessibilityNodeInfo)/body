{
  super.onInitializeAccessibilityNodeInfo(info);
  if (mExpandClickListener != null) {
    AccessibilityNodeInfo.AccessibilityAction expand=new AccessibilityNodeInfo.AccessibilityAction(AccessibilityNodeInfo.ACTION_CLICK,getContext().getString(com.android.internal.R.string.expand_action_accessibility));
    info.addAction(expand);
  }
}
