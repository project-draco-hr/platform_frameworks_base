{
  StringBuilder sb=new StringBuilder();
  sb.append("{");
  boolean firstTime=true;
  for (  Entry<Integer,String> entry : mCapabilities.entrySet()) {
    if (firstTime) {
      firstTime=false;
    }
 else {
      sb.append(",");
    }
    sb.append(entry.getKey());
    sb.append(":\"");
    sb.append(entry.getValue());
    sb.append("\"");
  }
  sb.append("}");
  return sb.toString();
}
