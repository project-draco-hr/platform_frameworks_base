{
  setParameter(String.format("time-lapse-enable=%d",(enableTimeLapse) ? 1 : 0));
  setParameter(String.format("use-still-camera-for-time-lapse=%d",(useStillCameraForTimeLapse) ? 1 : 0));
  setParameter(String.format("time-between-time-lapse-frame-capture=%d",timeBetweenTimeLapseFrameCaptureMs));
  setVideoEncoderLevel(encoderLevel);
}
