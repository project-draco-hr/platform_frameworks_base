{
  int initialCharIndex=charIndex;
  while (charIndex > 0) {
    lastChar=text.charAt(--charIndex);
    if (lastChar == '&') {
      return Character.DIRECTIONALITY_WHITESPACE;
    }
    if (lastChar == ';') {
      break;
    }
  }
  charIndex=initialCharIndex;
  lastChar=';';
  return Character.DIRECTIONALITY_OTHER_NEUTRALS;
}
