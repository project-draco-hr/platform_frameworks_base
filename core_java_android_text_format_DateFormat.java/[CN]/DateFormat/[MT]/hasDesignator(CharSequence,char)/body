{
  if (inFormat == null)   return false;
  final int length=inFormat.length();
  int c;
  int count;
  for (int i=0; i < length; i+=count) {
    count=1;
    c=inFormat.charAt(i);
    if (c == QUOTE) {
      count=skipQuotedText(inFormat,i,length);
    }
 else     if (c == designator) {
      return true;
    }
  }
  return false;
}
