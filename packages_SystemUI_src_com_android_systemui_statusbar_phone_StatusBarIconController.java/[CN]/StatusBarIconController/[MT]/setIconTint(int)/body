{
  if (mTransitionPending) {
    deferIconTintChange(tint);
  }
 else   if (mTransitionDeferring) {
    animateIconTint(tint,Math.max(0,mTransitionDeferringStartTime - SystemClock.uptimeMillis()),mTransitionDeferringDuration);
  }
 else {
    animateIconTint(tint,0,DEFAULT_TINT_ANIMATION_DURATION);
  }
}
