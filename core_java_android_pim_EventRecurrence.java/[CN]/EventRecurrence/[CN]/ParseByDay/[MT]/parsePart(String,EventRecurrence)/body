{
  int[] byday;
  int[] bydayNum;
  int bydayCount;
  if (value.indexOf(",") < 0) {
    bydayCount=1;
    byday=new int[1];
    bydayNum=new int[1];
    parseWday(value,byday,bydayNum,0);
  }
 else {
    String[] wdays=value.split(",");
    int len=wdays.length;
    bydayCount=len;
    byday=new int[len];
    bydayNum=new int[len];
    for (int i=0; i < len; i++) {
      parseWday(wdays[i],byday,bydayNum,i);
    }
  }
  er.byday=byday;
  er.bydayNum=bydayNum;
  er.bydayCount=bydayCount;
  return PARSED_BYDAY;
}
