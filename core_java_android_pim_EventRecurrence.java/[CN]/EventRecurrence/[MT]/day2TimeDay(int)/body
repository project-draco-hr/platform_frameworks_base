{
switch (day) {
case SU:
    return Time.SUNDAY;
case MO:
  return Time.MONDAY;
case TU:
return Time.TUESDAY;
case WE:
return Time.WEDNESDAY;
case TH:
return Time.THURSDAY;
case FR:
return Time.FRIDAY;
case SA:
return Time.SATURDAY;
default :
throw new RuntimeException("bad day of week: " + day);
}
}
