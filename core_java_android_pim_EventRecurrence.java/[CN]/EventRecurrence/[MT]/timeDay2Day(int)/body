{
switch (day) {
case Time.SUNDAY:
    return SU;
case Time.MONDAY:
  return MO;
case Time.TUESDAY:
return TU;
case Time.WEDNESDAY:
return WE;
case Time.THURSDAY:
return TH;
case Time.FRIDAY:
return FR;
case Time.SATURDAY:
return SA;
default :
throw new RuntimeException("bad day of week: " + day);
}
}
