{
  if (!(o instanceof Condition))   return false;
  if (o == this)   return true;
  final Condition other=(Condition)o;
  return Objects.equals(other.id,id) && Objects.equals(other.summary,summary) && Objects.equals(other.line1,line1)&& Objects.equals(other.line2,line2)&& other.icon == icon && other.state == state && other.flags == flags;
}
