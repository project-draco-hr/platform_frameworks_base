{
  if (url.equals(mCurrentUrl)) {
    if (!mFirstLayoutDone) {
      mFirstLayoutDone=true;
      mWebViewCore.contentInvalidate();
    }
  }
 else   if (url.equals(mCompletedUrl)) {
    if (!mFirstLayoutDone) {
      mFirstLayoutDone=true;
      mWebViewCore.contentInvalidate();
    }
  }
  mWebViewCore.mEndScaleZoom=true;
}
