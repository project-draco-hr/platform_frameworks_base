{
  mIsMainFrame=isMainFrame;
  if (isMainFrame || loadType == FRAME_LOADTYPE_STANDARD) {
    mLoadType=loadType;
    if (isMainFrame) {
      mCallbackProxy.onPageStarted(url,favicon);
      mFirstLayoutDone=false;
      mCommitted=false;
      mWebViewCore.removeMessages(WebViewCore.EventHub.WEBKIT_DRAW);
    }
  }
}
