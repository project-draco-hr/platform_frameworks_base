{
  mLoadInitFromJava=true;
  if (failUrl == null) {
    failUrl="";
  }
  if (data == null) {
    data="";
  }
  if (baseUrl == null || baseUrl.length() == 0) {
    baseUrl="about:blank";
  }
  if (mimeType == null || mimeType.length() == 0) {
    mimeType="text/html";
  }
  nativeLoadData(baseUrl,data,mimeType,encoding,failUrl);
  mLoadInitFromJava=false;
}
