{
  mLoadInitFromJava=true;
  if (URLUtil.isJavaScriptUrl(url)) {
    stringByEvaluatingJavaScriptFromString(url.substring("javascript:".length()));
  }
 else {
    if (!nativeLoadUrl(url)) {
      reportError(android.net.http.EventHandler.ERROR_BAD_URL,mContext.getString(com.android.internal.R.string.httpErrorBadUrl),url);
    }
  }
  mLoadInitFromJava=false;
}
