{
  boolean allowed=(mLastUserActivityElapsedTimeMillis != LAST_USER_ACTIVITY_TIME_INVALID && SystemClock.elapsedRealtime() - mLastUserActivityElapsedTimeMillis > MIN_USER_INACTIVITY_IDLE_MAINTENANCE_START);
  if (!allowed && noisy) {
    Slog.i("IdleMaintenance","Idle maintenance not allowed due to last user activity");
  }
  return allowed;
}
