{
  final int originalPos=position;
  final int size=mMarkedForDeletion.size();
  for (int i=0; i < size; ++i) {
    if (mMarkedForDeletion.keyAt(i) <= position && mMarkedForDeletion.valueAt(i)) {
      ++position;
    }
  }
  if (DEBUG) {
    Log.d(TAG,"Item position adjusted for deletion.  Original: " + originalPos + "  Adjusted: "+ position);
  }
  if (position >= mCursorCount) {
    throw new IndexOutOfBoundsException("Attempt to retrieve " + position + " of "+ mCursorCount+ " items");
  }
  mCursor.moveToPosition(position);
  return mCursor;
}
