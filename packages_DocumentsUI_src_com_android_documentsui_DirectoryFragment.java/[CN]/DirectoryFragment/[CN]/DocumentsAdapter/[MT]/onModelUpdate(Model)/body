{
  if (model.info != null || model.error != null) {
    mMessageBar.setInfo(model.info);
    mMessageBar.setError(model.error);
    mMessageBar.show();
  }
  if (model.isEmpty()) {
    mEmptyView.setVisibility(View.VISIBLE);
  }
 else {
    mEmptyView.setVisibility(View.GONE);
  }
  notifyDataSetChanged();
}
