{
  new GetDocumentsTask(){
    @Override void onDocumentsReady(    List<DocumentInfo> docs){
      mClipper.clipDocuments(docs);
      Activity activity=getActivity();
      Shared.makeSnackbar(activity,activity.getResources().getQuantityString(R.plurals.clipboard_files_clipped,docs.size(),docs.size()),Snackbar.LENGTH_SHORT).show();
    }
  }
.execute(items);
}
