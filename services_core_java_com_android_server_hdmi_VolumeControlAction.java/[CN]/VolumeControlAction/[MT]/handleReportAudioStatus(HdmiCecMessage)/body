{
  byte[] params=cmd.getParams();
  int volume=params[0] & 0x7F;
  tv().setAudioStatus(false,volume);
  if (mIsVolumeUp) {
    if (mTargetVolume <= volume) {
      finishWithVolumeChangeRelease();
      return;
    }
  }
 else {
    if (mTargetVolume >= volume) {
      finishWithVolumeChangeRelease();
      return;
    }
  }
  clear();
  startVolumeChange();
}
