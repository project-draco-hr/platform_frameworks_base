{
  pw.println("AppIdle");
  pw.println("Parole On: " + mAppIdleParoleOn);
synchronized (mTrackedTasks) {
    for (    JobStatus task : mTrackedTasks) {
      pw.print(task.job.getService().getPackageName());
      pw.print(":idle=" + !task.appNotIdleConstraintSatisfied.get());
      pw.print(", ");
    }
    pw.println();
  }
}
