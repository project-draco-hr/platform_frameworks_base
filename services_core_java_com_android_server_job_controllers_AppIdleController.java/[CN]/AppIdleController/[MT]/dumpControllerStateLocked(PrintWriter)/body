{
  pw.println("AppIdle");
  pw.println("Parole On: " + mAppIdleParoleOn);
  for (  JobStatus task : mTrackedTasks) {
    pw.print(task.getSourcePackageName());
    pw.print(":runnable=" + ((task.satisfiedConstraints & JobStatus.CONSTRAINT_APP_NOT_IDLE) != 0));
    pw.print(", ");
  }
  pw.println();
}
