{
  final boolean hasPermission=(mContext.checkCallingOrSelfPermission(android.Manifest.permission.READ_INSTALL_SESSIONS) == PackageManager.PERMISSION_GRANTED);
  final boolean isHomeApp=mPm.checkCallerIsHomeApp();
  if (hasPermission || isHomeApp) {
    return;
  }
 else {
    throw new SecurityException("Caller must be current home app to read install sessions");
  }
}
