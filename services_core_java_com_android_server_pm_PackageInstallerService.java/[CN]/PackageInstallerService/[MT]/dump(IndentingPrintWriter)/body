{
  pw.println("Active install sessions:");
  pw.increaseIndent();
synchronized (mSessions) {
    final int N=mSessions.size();
    for (int i=0; i < N; i++) {
      final PackageInstallerSession session=mSessions.valueAt(i);
      session.dump(pw);
      pw.println();
    }
  }
  pw.println();
  pw.decreaseIndent();
}
