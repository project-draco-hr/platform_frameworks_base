{
  mContext.enforceCallingOrSelfPermission(BLUETOOTH_ADMIN_PERM,"Need BLUETOOTH ADMIN permissicacheNameAndAddresson");
  if ((Binder.getCallingUid() != Process.SYSTEM_UID) && (!checkIfCallerIsForegroundUser())) {
    Log.w(TAG,"disable(): not allowed for non-active and non system user");
    return false;
  }
  if (DBG) {
    Log.d(TAG,"disable(): mBluetooth = " + mBluetooth + " mBinding = "+ mBinding);
  }
synchronized (mReceiver) {
    if (persist) {
      long callingIdentity=Binder.clearCallingIdentity();
      persistBluetoothSetting(BLUETOOTH_OFF);
      Binder.restoreCallingIdentity(callingIdentity);
    }
    mEnableExternal=false;
    sendDisableMsg();
  }
  return true;
}
