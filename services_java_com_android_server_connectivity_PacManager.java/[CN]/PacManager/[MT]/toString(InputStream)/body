{
  StringBuffer buffer=new StringBuffer();
  String line;
  BufferedReader bufferedReader=new BufferedReader(new InputStreamReader(content));
  while ((line=bufferedReader.readLine()) != null) {
    if (buffer.length() != 0) {
      buffer.append('\n');
    }
    buffer.append(line);
  }
  return buffer.toString();
}
