{
  if ((uri == null) || ((folder != MESSAGE_TYPE_INBOX) && (folder != MESSAGE_TYPE_OUTBOX) && (folder != MESSAGE_TYPE_SENT)&& (folder != MESSAGE_TYPE_DRAFT)&& (folder != MESSAGE_TYPE_FAILED)&& (folder != MESSAGE_TYPE_QUEUED))) {
    return false;
  }
  ContentValues values=new ContentValues(1);
  values.put(TYPE,folder);
  return 1 == SqliteWrapper.update(context,context.getContentResolver(),uri,values,null,null);
}
