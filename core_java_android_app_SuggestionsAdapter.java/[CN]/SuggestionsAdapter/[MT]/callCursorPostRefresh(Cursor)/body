{
  if (!mGlobalSearchMode)   return;
  final Bundle request=new Bundle();
  request.putInt(DialogCursorProtocol.METHOD,DialogCursorProtocol.POST_REFRESH);
  final Bundle response=cursor.respond(request);
  mSearchDialog.setWorking(response.getBoolean(DialogCursorProtocol.POST_REFRESH_RECEIVE_ISPENDING,false));
  mDisplayNotifyPos=response.getInt(DialogCursorProtocol.POST_REFRESH_RECEIVE_DISPLAY_NOTIFY,-1);
}
