{
  boolean isPressedNow=isPressed();
  boolean isSelectedNow=isSelected();
  ChildViewCache views=(ChildViewCache)getTag();
  views.mText1.setText((isPressedNow ? mText1Strings.pressed() : (isSelectedNow ? mText1Strings.selected() : mText1Strings.normal())));
  views.mText2.setText((isPressedNow ? mText2Strings.pressed() : (isSelectedNow ? mText2Strings.selected() : mText2Strings.normal())));
}
