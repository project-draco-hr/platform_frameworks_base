{
synchronized (this) {
    if (mDiskWritesInFlight > 0) {
      if (DEBUG)       Log.d(TAG,"disk write in flight, not unexpected.");
      return false;
    }
  }
  FileStatus stat=new FileStatus();
  if (!FileUtils.getFileStatus(mFile.getPath(),stat)) {
    return true;
  }
synchronized (this) {
    return mTimestamp != stat.mtime;
  }
}
