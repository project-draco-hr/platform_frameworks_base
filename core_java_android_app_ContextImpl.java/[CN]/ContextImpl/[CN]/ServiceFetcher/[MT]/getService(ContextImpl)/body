{
  ArrayList<Object> cache=ctx.mServiceCache;
  Object service;
synchronized (cache) {
    if (cache.size() == 0) {
      for (int i=0; i < sNextPerContextServiceCacheIndex; i++) {
        cache.add(null);
      }
    }
 else {
      service=cache.get(mContextCacheIndex);
      if (service != null) {
        return service;
      }
    }
    service=createService(ctx);
    cache.set(mContextCacheIndex,service);
    return service;
  }
}
