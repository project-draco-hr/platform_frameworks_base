{
synchronized (mSync) {
    if (mDatabasesDir == null) {
      if ("android".equals(getPackageName())) {
        mDatabasesDir=new File("/data/system");
      }
 else {
        mDatabasesDir=new File(getDataDirFile(),"databases");
      }
    }
    return ensurePrivateDirExists(mDatabasesDir);
  }
}
