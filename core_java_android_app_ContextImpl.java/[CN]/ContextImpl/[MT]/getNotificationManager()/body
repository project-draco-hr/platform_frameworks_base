{
synchronized (mSync) {
    if (mNotificationManager == null) {
      final Context outerContext=getOuterContext();
      mNotificationManager=new NotificationManager(new ContextThemeWrapper(outerContext,outerContext.getApplicationInfo().targetSdkVersion >= Build.VERSION_CODES.HONEYCOMB ? com.android.internal.R.style.Theme_Holo_Dialog : com.android.internal.R.style.Theme_Dialog),mMainThread.getHandler());
    }
  }
  return mNotificationManager;
}
