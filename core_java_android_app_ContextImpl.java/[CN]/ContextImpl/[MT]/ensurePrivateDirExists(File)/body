{
  if (!file.exists()) {
    try {
      Os.mkdir(file.getAbsolutePath(),0771);
      Os.chmod(file.getAbsolutePath(),0771);
    }
 catch (    ErrnoException e) {
      if (e.errno == OsConstants.EEXIST) {
      }
 else {
        Log.w(TAG,"Failed to ensure " + file + ": "+ e.getMessage());
      }
    }
  }
  return file;
}
