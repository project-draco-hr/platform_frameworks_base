{
  if (mLastTouchOffset < 0) {
    return null;
  }
  long wordLimits=getWordLimitsAt(mLastTouchOffset);
  if (wordLimits >= 0) {
    int start=(int)(wordLimits >>> 32);
    int end=(int)(wordLimits & 0x00000000FFFFFFFFL);
    return mTransformed.subSequence(start,end).toString();
  }
 else {
    return null;
  }
}
