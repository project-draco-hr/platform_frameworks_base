{
  if (who == null)   return false;
  if (mDrawables != null) {
    final Drawables drawables=mDrawables;
    if (who == drawables.mDrawableLeft || who == drawables.mDrawableRight || who == drawables.mDrawableTop || who == drawables.mDrawableBottom) {
      return isLayoutRtl();
    }
  }
  return super.isLayoutRtl(who);
}
