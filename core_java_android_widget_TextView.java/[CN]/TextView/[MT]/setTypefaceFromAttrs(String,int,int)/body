{
  Typeface tf=null;
  if (familyName != null) {
    tf=Typeface.create(familyName,styleIndex);
    if (tf != null) {
      setTypeface(tf);
      return;
    }
  }
switch (typefaceIndex) {
case SANS:
    tf=Typeface.SANS_SERIF;
  break;
case SERIF:
tf=Typeface.SERIF;
break;
case MONOSPACE:
tf=Typeface.MONOSPACE;
break;
}
setTypeface(tf,styleIndex);
}
