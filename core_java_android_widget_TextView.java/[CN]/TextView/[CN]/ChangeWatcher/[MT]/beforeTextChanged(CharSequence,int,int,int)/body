{
  if (DEBUG_EXTRACT)   Log.v(LOG_TAG,"beforeTextChanged start=" + start + " before="+ before+ " after="+ after+ ": "+ buffer);
  if (AccessibilityManager.getInstance(mContext).isEnabled() && !isPasswordInputType(mInputType) && !hasPasswordTransformationMethod()) {
    mBeforeText=buffer.toString();
  }
  TextView.this.sendBeforeTextChanged(buffer,start,before,after);
}
