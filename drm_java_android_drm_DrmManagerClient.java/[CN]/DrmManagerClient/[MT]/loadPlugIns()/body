{
  int result=ERROR_UNKNOWN;
  if (STATE_UNINITIALIZED == getState()) {
    if (null != mEventHandler) {
      Message msg=mEventHandler.obtainMessage(ACTION_INITIALIZE,new WeakReference<DrmManagerClient>(this));
      result=(mEventHandler.sendMessage(msg)) ? ERROR_NONE : result;
    }
  }
 else {
    result=ERROR_NONE;
  }
  return result;
}
