{
  int result=ERROR_UNKNOWN;
  if (STATE_INITIALIZED == getState()) {
    if (null != mEventHandler) {
      Message msg=mEventHandler.obtainMessage(ACTION_FINALIZE);
      result=(mEventHandler.sendMessage(msg)) ? ERROR_NONE : result;
    }
  }
 else {
    result=ERROR_NONE;
  }
  return result;
}
