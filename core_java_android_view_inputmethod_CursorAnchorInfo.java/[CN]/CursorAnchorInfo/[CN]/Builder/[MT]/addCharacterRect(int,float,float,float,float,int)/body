{
  if (index < 0) {
    throw new IllegalArgumentException("index must not be a negative integer.");
  }
  final int type=flags & CHARACTER_RECT_TYPE_MASK;
  if (type == CHARACTER_RECT_TYPE_UNSPECIFIED) {
    throw new IllegalArgumentException("Type except for " + "CHARACTER_RECT_TYPE_UNSPECIFIED must be specified.");
  }
  if (mCharacterRectBuilder == null) {
    mCharacterRectBuilder=new SparseRectFArrayBuilder();
  }
  mCharacterRectBuilder.append(index,leadingEdgeX,leadingEdgeY,trailingEdgeX,trailingEdgeY,flags);
  return this;
}
