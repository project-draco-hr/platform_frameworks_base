{
  final float floatHash=mSelectionStart + mSelectionEnd + mCandidatesStart+ mCandidatesEnd+ mInsertionMarkerHorizontal+ mInsertionMarkerTop+ mInsertionMarkerBaseline+ mInsertionMarkerBottom;
  int hash=floatHash > 0 ? (int)floatHash : (int)(-floatHash);
  if (mCharacterRects != null) {
    hash+=mCharacterRects.hashCode();
  }
  hash+=mMatrix.hashCode();
  return hash;
}
