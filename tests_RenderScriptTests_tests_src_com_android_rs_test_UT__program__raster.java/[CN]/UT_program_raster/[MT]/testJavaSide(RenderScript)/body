{
  _RS_ASSERT("pointSpriteEnabled.isPointSpriteEnabled() == true",pointSpriteEnabled.isPointSpriteEnabled() == true);
  _RS_ASSERT("pointSpriteEnabled.getCullMode() == ProgramRaster.CullMode.BACK",pointSpriteEnabled.getCullMode() == ProgramRaster.CullMode.BACK);
  _RS_ASSERT("cullMode.isPointSpriteEnabled() == false",cullMode.isPointSpriteEnabled() == false);
  _RS_ASSERT("cullMode.getCullMode() == ProgramRaster.CullMode.FRONT",cullMode.getCullMode() == ProgramRaster.CullMode.FRONT);
}
