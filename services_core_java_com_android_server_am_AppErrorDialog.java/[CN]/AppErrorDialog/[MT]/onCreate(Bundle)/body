{
  super.onCreate(savedInstanceState);
  if (!ActivityManagerService.IS_USER_BUILD) {
    FrameLayout frame=(FrameLayout)findViewById(android.R.id.custom);
    Context context=getContext();
    LayoutInflater.from(context).inflate(com.android.internal.R.layout.app_error_dialog_dont_show_again,frame,true);
    ((TextView)frame.findViewById(com.android.internal.R.id.text)).setText(context.getResources().getString(com.android.internal.R.string.aerr_process_silence,mName.toString()));
    findViewById(com.android.internal.R.id.customPanel).setVisibility(View.VISIBLE);
  }
}
