{
  Parcel in=getParcelForFile(file);
  int vers=in.readInt();
  if (vers != VERSION) {
    Slog.w(TAG,"Usage stats version of " + file + " changed from "+ vers+ " to "+ VERSION+ "; dropping");
    return;
  }
  LocalUsageStats stats=new LocalUsageStats(in,true);
synchronized (mStatsLock) {
    mStats=stats;
  }
}
