{
  if (info == null)   return null;
  final String type=playbackInfoTypeToString(info.getPlaybackType());
  final String vc=volumeProviderControlToString(info.getVolumeControl());
  return String.format("PlaybackInfo[vol=%s,max=%s,type=%s,vc=%s],atts=%s",info.getCurrentVolume(),info.getMaxVolume(),type,vc,info.getAudioAttributes());
}
