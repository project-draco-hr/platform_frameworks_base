{
  if (!mRunning)   return;
  mRunning=false;
  mMyWakeLock.release(mSession);
  if (mSession != null) {
    mSession.setListener(null);
    if (mNetworkType != -1 && mRegistered)     mSession.unregister();
  }
  mTimer.cancel(this);
  stopKeepAliveProcess();
  mRegistered=false;
  setListener(mProxy.getListener());
}
