{
synchronized (SipService.this) {
    if (mSession == null)     return;
    if (SKAI_DBG)     log("restart: interval=" + mInterval);
    try {
      mSession.stopKeepAliveProcess();
      mPassCount=0;
      mSession.startKeepAliveProcess(mInterval,this);
    }
 catch (    SipException e) {
      loge("restart",e);
    }
  }
}
