{
  int count=0;
  for (  MyEvent event : mEventQueue) {
    Log.d(TAG,"     " + event + ": scheduled at "+ showTime(event.mTriggerTime)+ ": last at "+ showTime(event.mLastTriggerTime));
    if (++count >= 5)     break;
  }
  if (mEventQueue.size() > count) {
    Log.d(TAG,"     .....");
  }
 else   if (count == 0) {
    Log.d(TAG,"     <empty>");
  }
}
