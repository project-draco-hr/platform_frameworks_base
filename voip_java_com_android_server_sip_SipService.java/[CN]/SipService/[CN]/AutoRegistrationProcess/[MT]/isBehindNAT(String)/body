{
  try {
    byte[] d=InetAddress.getByName(address).getAddress();
    if ((d[0] == 10) || (((0x000000FF & ((int)d[0])) == 172) && ((0x000000F0 & ((int)d[1])) == 16)) || (((0x000000FF & ((int)d[0])) == 192) && ((0x000000FF & ((int)d[1])) == 168))) {
      return true;
    }
  }
 catch (  UnknownHostException e) {
    Log.e(TAG,"isBehindAT()" + address,e);
  }
  return false;
}
