{
  if (!mRunning) {
    mRunning=true;
    mBackoff=1;
    mSession=(SipSessionGroup.SipSessionImpl)group.createSession(this);
    if (mSession == null)     return;
    mMyWakeLock.acquire(mSession);
    mSession.unregister();
    TAG="SipAutoReg:" + mSession.getLocalProfile().getUriString();
  }
}
