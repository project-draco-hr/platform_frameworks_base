{
synchronized (SipService.this) {
    if (mTask != this) {
      Log.w(TAG,"  unexpected task: " + mNetworkType + (mConnected ? " CONNECTED" : "DISCONNECTED"));
      return;
    }
    mTask=null;
    if (DEBUG)     Log.d(TAG," deliver change for " + mNetworkType + (mConnected ? " CONNECTED" : "DISCONNECTED"));
    onConnectivityChanged(mNetworkType,mConnected);
    mMyWakeLock.release(this);
  }
}
