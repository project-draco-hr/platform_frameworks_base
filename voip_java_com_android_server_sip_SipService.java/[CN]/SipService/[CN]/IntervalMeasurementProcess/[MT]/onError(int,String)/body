{
synchronized (SipService.this) {
    Log.w(TAG,"interval measurement error: " + description);
    if (++mErrorCount < MAX_RETRY_COUNT) {
      Log.w(TAG,"  retry count = " + mErrorCount);
      mPassCount=0;
      restart();
    }
 else {
      Log.w(TAG,"  max retry count reached; measurement aborted");
    }
  }
}
