{
  for (  SipSessionGroupExt group : mSipGroups.values()) {
    if (group.isOpenedToReceiveCalls()) {
      if (mNetworkType == ConnectivityManager.TYPE_WIFI || mNetworkType == -1) {
        mWifiLock.acquire();
      }
 else {
        mWifiLock.release();
      }
      return;
    }
  }
  mWifiLock.release();
  mMyWakeLock.reset();
}
