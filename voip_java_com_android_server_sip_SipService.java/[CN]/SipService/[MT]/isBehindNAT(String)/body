{
  try {
    byte[] d=InetAddress.getByName(address).getAddress();
    if ((d[0] == 10) || (((0x000000FF & d[0]) == 172) && ((0x000000F0 & d[1]) == 16)) || (((0x000000FF & d[0]) == 192) && ((0x000000FF & d[1]) == 168))) {
      return true;
    }
  }
 catch (  UnknownHostException e) {
    loge("isBehindAT()" + address,e);
  }
  return false;
}
