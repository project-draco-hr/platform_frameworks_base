{
  mContext.enforceCallingOrSelfPermission(android.Manifest.permission.USE_SIP,null);
  localProfile.setCallingUid(Binder.getCallingUid());
  if (incomingCallPendingIntent == null) {
    if (DBG)     log("open3: incomingCallPendingIntent cannot be null; " + "the profile is not opened");
    return;
  }
  if (DBG)   log("open3: " + localProfile.getUriString() + ": "+ incomingCallPendingIntent+ ": "+ listener);
  try {
    SipSessionGroupExt group=createGroup(localProfile,incomingCallPendingIntent,listener);
    if (localProfile.getAutoRegistration()) {
      group.openToReceiveCalls();
      updateWakeLocks();
    }
  }
 catch (  SipException e) {
    loge("open3:",e);
  }
}
