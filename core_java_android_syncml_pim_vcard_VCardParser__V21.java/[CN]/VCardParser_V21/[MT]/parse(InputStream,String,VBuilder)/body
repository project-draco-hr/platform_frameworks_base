{
  mReader=new CustomBufferedReader(new InputStreamReader(is,charset));
  mBuilder=builder;
  long start=System.currentTimeMillis();
  if (mBuilder != null) {
    mBuilder.start();
  }
  parseVCardFile();
  if (mBuilder != null) {
    mBuilder.end();
  }
  mTimeTotal+=System.currentTimeMillis() - start;
  return true;
}
