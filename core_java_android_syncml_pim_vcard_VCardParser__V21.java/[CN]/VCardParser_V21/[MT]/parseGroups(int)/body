{
  int ret=0, sum=0;
  ret=parseWord(offset);
  if (ret == PARSE_ERROR) {
    return PARSE_ERROR;
  }
  offset+=ret;
  sum+=ret;
  for (; ; ) {
    ret=parseString(offset,".",false);
    if (ret == PARSE_ERROR) {
      break;
    }
    int ret1=parseWord(offset);
    if (ret1 == PARSE_ERROR) {
      break;
    }
    offset+=ret + ret1;
    sum+=ret + ret1;
  }
  return sum;
}
