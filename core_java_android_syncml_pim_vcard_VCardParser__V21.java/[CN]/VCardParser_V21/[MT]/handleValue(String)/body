{
  if (sKnownValueSet.contains(pvalueval.toUpperCase()) || pvalueval.startsWith("X-")) {
    if (mBuilder != null) {
      mBuilder.propertyParamType("VALUE");
      mBuilder.propertyParamValue(pvalueval);
    }
  }
 else {
    throw new VCardException("Unknown value \"" + pvalueval + "\"");
  }
}
