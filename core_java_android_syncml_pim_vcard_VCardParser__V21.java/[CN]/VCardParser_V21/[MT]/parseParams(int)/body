{
  int ret=0, sum=0;
  ret=parseString(offset,";",false);
  if (ret == PARSE_ERROR) {
    return PARSE_ERROR;
  }
  offset+=ret;
  sum+=ret;
  ret=removeWs(offset);
  offset+=ret;
  sum+=ret;
  ret=parseParamList(offset);
  if (ret == PARSE_ERROR) {
    return PARSE_ERROR;
  }
  sum+=ret;
  return sum;
}
