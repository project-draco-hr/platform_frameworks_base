{
  if (sKnownTypeSet.contains(ptypeval.toUpperCase()) || ptypeval.startsWith("X-")) {
    if (mBuilder != null) {
      mBuilder.propertyParamType("TYPE");
      mBuilder.propertyParamValue(ptypeval.toUpperCase());
    }
  }
 else {
    throw new VCardException("Unknown type: \"" + ptypeval + "\"");
  }
}
