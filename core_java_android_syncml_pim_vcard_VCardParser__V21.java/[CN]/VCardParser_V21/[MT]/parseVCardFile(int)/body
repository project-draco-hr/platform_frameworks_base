{
  int ret=0, sum=0;
  while ((ret=parseWsls(offset)) != PARSE_ERROR) {
    offset+=ret;
    sum+=ret;
  }
  ret=parseVCard(offset);
  if (ret != PARSE_ERROR) {
    offset+=ret;
    sum+=ret;
  }
 else {
    return PARSE_ERROR;
  }
  while ((ret=parseWsls(offset)) != PARSE_ERROR) {
    offset+=ret;
    sum+=ret;
  }
  return sum;
}
