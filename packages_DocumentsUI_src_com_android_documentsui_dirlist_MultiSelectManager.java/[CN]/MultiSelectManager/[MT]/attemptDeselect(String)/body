{
  assert(id != null);
  if (notifyBeforeItemStateChange(id,false)) {
    mSelection.remove(id);
    notifyItemStateChanged(id,false);
    if (DEBUG)     Log.d(TAG,"Selection after deselect: " + mSelection);
    return true;
  }
 else {
    if (DEBUG)     Log.d(TAG,"Select cancelled by listener.");
    return false;
  }
}
