{
  int columnStart=Collections.binarySearch(mColumnBounds,new Limits(rect.left,rect.left));
  assert(columnStart >= 0);
  int columnEnd=columnStart;
  for (int i=columnStart; i < mColumnBounds.size() && mColumnBounds.get(i).lowerLimit <= rect.right; i++) {
    columnEnd=i;
  }
  int rowStart=Collections.binarySearch(mRowBounds,new Limits(rect.top,rect.top));
  if (rowStart < 0) {
    mPositionNearestOrigin=NOT_SET;
    return;
  }
  int rowEnd=rowStart;
  for (int i=rowStart; i < mRowBounds.size() && mRowBounds.get(i).lowerLimit <= rect.bottom; i++) {
    rowEnd=i;
  }
  updateSelection(columnStart,columnEnd,rowStart,rowEnd);
}
