{
  if (size() <= 0) {
    return "size=0, items=[]";
  }
  StringBuilder buffer=new StringBuilder(size() * 28);
  buffer.append("Selection{").append("applied{size=" + mSelection.size()).append(", entries=" + mSelection).append("}, provisional{size=" + mProvisionalSelection.size()).append(", entries=" + mProvisionalSelection).append("}}");
  return buffer.toString();
}
