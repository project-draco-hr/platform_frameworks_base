{
  final InternalSelectionView isvLeft=new InternalSelectionView(parent.getContext(),5,"isv left");
  isvLeft.setDesiredHeight(mItemHeight);
  final InternalSelectionView isvRight=new InternalSelectionView(parent.getContext(),5,"isv right");
  isvRight.setDesiredHeight(mItemHeight);
  return combineAdjacent(isvLeft,isvRight);
}
