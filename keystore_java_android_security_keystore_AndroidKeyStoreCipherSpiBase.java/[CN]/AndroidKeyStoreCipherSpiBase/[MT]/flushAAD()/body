{
  if ((mAdditionalAuthenticationDataStreamer != null) && (!mAdditionalAuthenticationDataStreamerClosed)) {
    byte[] output;
    try {
      output=mAdditionalAuthenticationDataStreamer.doFinal(EmptyArray.BYTE,0,0,null);
    }
  finally {
      mAdditionalAuthenticationDataStreamerClosed=true;
    }
    if ((output != null) && (output.length > 0)) {
      throw new ProviderException("AAD update unexpectedly returned data: " + output.length + " bytes");
    }
  }
}
