{
synchronized (MSGS) {
    final int start=(sNext - sSize + SIZE) % SIZE;
    for (int i=0; i < sSize; i++) {
      final int j=(start + i) % SIZE;
      pw.print(prefix);
      pw.print(FORMAT.format(new Date(TIMES[j])));
      pw.print(' ');
      pw.print(typeToString(TYPES[j]));
      pw.print(": ");
      pw.println(MSGS[j]);
    }
  }
}
