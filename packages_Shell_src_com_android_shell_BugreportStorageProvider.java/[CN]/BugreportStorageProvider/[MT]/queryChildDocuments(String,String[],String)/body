{
  final MatrixCursor result=new MatrixCursor(resolveDocumentProjection(projection));
  if (DOC_ID_ROOT.equals(parentDocumentId)) {
    final File[] files=mRoot.listFiles();
    if (files != null) {
      for (      File file : files) {
        addFileRow(result,file);
      }
    }
  }
  return result;
}
