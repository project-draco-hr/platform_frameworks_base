{
  if (sGlobals.mService == null) {
    Log.w(TAG,"WallpaperService not running");
  }
 else {
    try {
      return sGlobals.mService.isWallpaperSettingAllowed(mContext.getOpPackageName());
    }
 catch (    RemoteException e) {
      throw e.rethrowFromSystemServer();
    }
  }
  return false;
}
