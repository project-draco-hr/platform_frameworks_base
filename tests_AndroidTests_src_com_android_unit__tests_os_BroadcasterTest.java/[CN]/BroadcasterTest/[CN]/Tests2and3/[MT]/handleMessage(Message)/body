{
  int index=msg.what - MESSAGE_B;
  if (index < 0 || index >= N) {
    failure();
  }
 else {
    if (msg.getTarget() == mHandlers[index]) {
      mSuccess[index]=true;
    }
  }
  boolean winner=true;
  for (int i=0; i < N; i++) {
    if (!mSuccess[i]) {
      winner=false;
    }
  }
  if (winner) {
    success();
  }
}
