{
  if (src.authority.equals(dstDirInfo.authority)) {
    if ((src.flags & Document.FLAG_SUPPORTS_COPY) != 0) {
      if (DocumentsContract.copyDocument(getClient(src),src.derivedUri,dstDirInfo.derivedUri) == null) {
        onFileFailed(src,"Provider side copy failed for documents: " + src.derivedUri + ".");
        return false;
      }
      return true;
    }
  }
  return byteCopyDocument(src,dstDirInfo);
}
