{
  if (parent.isDirectory() && doc.authority.equals(parent.authority)) {
    try {
      return isChildDocument(getClient(doc),doc.derivedUri,parent.derivedUri);
    }
 catch (    RemoteException|RuntimeException e) {
      throw new ResourceException("Failed to check if %s is a child of %s due to an exception.",doc.derivedUri,parent.derivedUri,e);
    }
  }
  return false;
}
