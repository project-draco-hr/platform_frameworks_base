{
  try {
    final ApplicationInfo applicationInfo=mContext.getPackageManager().getApplicationInfoAsUser(caller,0,UserHandle.getUserId(Binder.getCallingUid()));
    if (applicationInfo.targetSdkVersion >= Build.VERSION_CODES.N) {
      return true;
    }
    return false;
  }
 catch (  PackageManager.NameNotFoundException e) {
    return true;
  }
}
