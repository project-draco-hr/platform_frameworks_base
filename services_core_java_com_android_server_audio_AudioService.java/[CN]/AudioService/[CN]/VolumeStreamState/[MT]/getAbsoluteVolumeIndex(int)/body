{
  if (index == 0) {
    index=0;
  }
 else   if (index == 1) {
    index=(int)(mIndexMax * 0.5) / 10;
  }
 else   if (index == 2) {
    index=(int)(mIndexMax * 0.70) / 10;
  }
 else   if (index == 3) {
    index=(int)(mIndexMax * 0.85) / 10;
  }
 else {
    index=(mIndexMax + 5) / 10;
  }
  return index;
}
