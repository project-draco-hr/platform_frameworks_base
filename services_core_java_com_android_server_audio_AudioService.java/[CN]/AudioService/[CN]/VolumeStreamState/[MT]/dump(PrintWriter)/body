{
  pw.print("   Muted: ");
  pw.println(mIsMuted);
  pw.print("   Min: ");
  pw.println((mIndexMin + 5) / 10);
  pw.print("   Max: ");
  pw.println((mIndexMax + 5) / 10);
  pw.print("   Current: ");
  for (int i=0; i < mIndexMap.size(); i++) {
    if (i > 0) {
      pw.print(", ");
    }
    final int device=mIndexMap.keyAt(i);
    pw.print(Integer.toHexString(device));
    final String deviceName=device == AudioSystem.DEVICE_OUT_DEFAULT ? "default" : AudioSystem.getOutputDeviceName(device);
    if (!deviceName.isEmpty()) {
      pw.print(" (");
      pw.print(deviceName);
      pw.print(")");
    }
    pw.print(": ");
    final int index=(mIndexMap.valueAt(i) + 5) / 10;
    pw.print(index);
  }
  pw.println();
  pw.print("   Devices: ");
  final int devices=AudioSystem.getDevicesForStream(mStreamType);
  int device, i=0, n=0;
  while ((device=1 << i) != AudioSystem.DEVICE_OUT_DEFAULT) {
    if ((devices & device) != 0) {
      if (n++ > 0) {
        pw.print(", ");
      }
      pw.print(AudioSystem.getOutputDeviceName(device));
    }
    i++;
  }
}
