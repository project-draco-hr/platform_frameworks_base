{
  if (DEBUG_WAKEUP)   Slog.d(TAG,"finishScreenTurningOn: mKeyguardDrawComplete=" + mKeyguardDrawComplete + " mWindowManagerDrawComplete="+ mWindowManagerDrawComplete);
  if (!mKeyguardDrawComplete || !mWindowManagerDrawComplete) {
    return;
  }
  ScreenOnListener screenOnListener;
synchronized (mLock) {
    mScreenOnFully=true;
    screenOnListener=mScreenOnListener;
    mScreenOnListener=null;
  }
  try {
    mWindowManager.setEventDispatching(true);
  }
 catch (  RemoteException unhandled) {
  }
  if (screenOnListener != null) {
    screenOnListener.onScreenOn();
  }
  setKeyguardDrawn();
  if (mBootMessageNeedsHiding) {
    handleHideBootMessage();
    mBootMessageNeedsHiding=false;
  }
}
