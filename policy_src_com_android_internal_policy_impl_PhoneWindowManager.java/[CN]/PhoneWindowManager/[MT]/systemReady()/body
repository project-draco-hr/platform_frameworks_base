{
  if (mKeyguardMediator != null) {
    mKeyguardMediator.onSystemReady();
  }
  android.os.SystemProperties.set("dev.bootcomplete","1");
synchronized (mLock) {
    updateOrientationListenerLp();
    mSystemReady=true;
    mHandler.post(new Runnable(){
      public void run(){
        updateSettings();
      }
    }
);
  }
}
