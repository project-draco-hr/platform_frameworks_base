{
  if (mKeyguardDelegate != null && mKeyguardDelegate.isShowing()) {
    mHandler.post(new Runnable(){
      public void run(){
        if (mKeyguardDelegate.isDismissable()) {
          mKeyguardDelegate.keyguardDone(false,true);
        }
 else {
          mKeyguardDelegate.dismiss();
        }
      }
    }
);
  }
}
