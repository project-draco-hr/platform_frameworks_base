{
  int result=0;
  final boolean isWakeMotion=(policyFlags & (WindowManagerPolicy.FLAG_WAKE | WindowManagerPolicy.FLAG_WAKE_DROPPED)) != 0;
  if (isWakeMotion) {
    if (mKeyguardMediator.isShowing()) {
      mKeyguardMediator.onWakeMotionWhenKeyguardShowingTq();
    }
 else {
      result|=ACTION_POKE_USER_ACTIVITY;
    }
  }
  return result;
}
