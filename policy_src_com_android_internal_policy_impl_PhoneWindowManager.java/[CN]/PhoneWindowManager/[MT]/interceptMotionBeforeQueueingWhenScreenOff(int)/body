{
  int result=0;
  final boolean isWakeMotion=(policyFlags & (WindowManagerPolicy.FLAG_WAKE | WindowManagerPolicy.FLAG_WAKE_DROPPED)) != 0;
  if (isWakeMotion) {
    if (mKeyguardDelegate != null && mKeyguardDelegate.isShowing()) {
      mKeyguardDelegate.onWakeMotionWhenKeyguardShowingTq();
    }
 else {
      result|=ACTION_WAKE_UP;
    }
  }
  return result;
}
