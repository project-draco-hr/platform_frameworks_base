{
  final int N=mSize * 2;
  final Object[] array=mArray;
  if (value == null) {
    for (int i=1; i < N; i+=2) {
      if (array[i] == null) {
        return i >> 1;
      }
    }
  }
 else {
    for (int i=1; i < N; i+=2) {
      if (value.equals(array[i])) {
        return i >> 1;
      }
    }
  }
  return -1;
}
