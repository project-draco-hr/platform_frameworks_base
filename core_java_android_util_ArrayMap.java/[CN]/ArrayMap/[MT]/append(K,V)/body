{
  int index=mSize;
  final int hash=key.hashCode();
  if (index >= mHashes.length) {
    throw new IllegalStateException("Array is full");
  }
  if (index > 0 && mHashes[index - 1] > hash) {
    throw new IllegalArgumentException("New hash " + hash + " is before end of array hash "+ mHashes[index - 1]);
  }
  mSize=index + 1;
  mHashes[index]=hash;
  index<<=1;
  mArray[index]=key;
  mArray[index + 1]=value;
}
