{
  try {
    return sService.getNfcFServices(UserHandle.myUserId());
  }
 catch (  RemoteException e) {
    recoverService();
    if (sService == null) {
      Log.e(TAG,"Failed to recover CardEmulationService.");
      return null;
    }
    try {
      return sService.getNfcFServices(UserHandle.myUserId());
    }
 catch (    RemoteException ee) {
      Log.e(TAG,"Failed to reach CardEmulationService.");
      return null;
    }
  }
}
