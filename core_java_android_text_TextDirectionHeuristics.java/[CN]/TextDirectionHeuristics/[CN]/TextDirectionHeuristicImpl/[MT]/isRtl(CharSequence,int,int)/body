{
  if (text == null || start < 0 || end < start || text.length() < end) {
    throw new IllegalArgumentException();
  }
  if (mAlgorithm == null) {
    return defaultIsRtl();
  }
  text=text.subSequence(start,end);
  char[] chars=text.toString().toCharArray();
  return doCheck(chars,0,chars.length);
}
