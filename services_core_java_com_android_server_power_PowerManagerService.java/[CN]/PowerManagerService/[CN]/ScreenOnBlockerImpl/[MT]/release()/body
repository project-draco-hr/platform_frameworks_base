{
synchronized (this) {
    mNestCount-=1;
    if (mNestCount < 0) {
      Log.wtf(TAG,"Screen on blocker was released without being acquired!",new Throwable());
      mNestCount=0;
    }
    if (mNestCount == 0) {
      mHandler.sendEmptyMessage(MSG_SCREEN_ON_BLOCKER_RELEASED);
    }
    if (DEBUG) {
      Slog.d(TAG,"Screen on unblocked: mNestCount=" + mNestCount);
    }
  }
}
