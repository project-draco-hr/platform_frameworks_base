{
  int card=getDefaultUsbCard();
  if (DEBUG) {
    Slog.d(TAG,"getDefaultCard() default usb card:" + card);
  }
  if (card < 0 && getNumCardRecords() > 0) {
    card=getCardRecordAt(getNumCardRecords() - 1).mCardNum;
  }
  if (DEBUG) {
    Slog.d(TAG,"  returns card:" + card);
  }
  return card;
}
