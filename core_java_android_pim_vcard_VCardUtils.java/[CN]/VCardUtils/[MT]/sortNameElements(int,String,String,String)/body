{
  String[] list=new String[3];
switch (VCardConfig.getNameOrderType(vcardType)) {
case VCardConfig.NAME_ORDER_JAPANESE:
    list[0]=familyName;
  list[1]=middleName;
list[2]=givenName;
break;
case VCardConfig.NAME_ORDER_EUROPE:
list[0]=middleName;
list[1]=givenName;
list[2]=familyName;
break;
default :
list[0]=givenName;
list[1]=middleName;
list[2]=familyName;
break;
}
return list;
}
