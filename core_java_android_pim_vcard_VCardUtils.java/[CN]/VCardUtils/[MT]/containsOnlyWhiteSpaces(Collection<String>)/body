{
  if (values == null) {
    return true;
  }
  for (  final String str : values) {
    if (TextUtils.isEmpty(str)) {
      continue;
    }
    final int length=str.length();
    for (int i=0; i < length; i=str.offsetByCodePoints(i,1)) {
      if (!Character.isWhitespace(str.codePointAt(i))) {
        return false;
      }
    }
  }
  return true;
}
