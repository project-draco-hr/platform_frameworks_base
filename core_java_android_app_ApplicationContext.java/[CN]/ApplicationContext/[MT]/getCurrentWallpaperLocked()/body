{
  try {
    ParcelFileDescriptor fd=getWallpaperService().getWallpaper(mWallpaperCallback);
    if (fd != null) {
      Bitmap bm=BitmapFactory.decodeFileDescriptor(fd.getFileDescriptor());
      if (bm != null) {
        return new BitmapDrawable(bm);
      }
    }
  }
 catch (  RemoteException e) {
  }
  return null;
}
