{
  int heightOverride=mHeightOverride != -1 ? mHeightOverride : getMeasuredHeight();
  int height=mQSCustomizer.isCustomizing() ? mQSCustomizer.getHeight() : (int)(mQsExpansion * (heightOverride - mHeader.getCollapsedHeight())) + mHeader.getCollapsedHeight();
  setBottom(getTop() + height);
  mQSDetail.setBottom(getTop() + height);
}
