{
synchronized (NBestRecognitionResultImpl.class) {
    if (nativeObject == 0)     throw new IllegalStateException("Object has been disposed");
    if ((VoicetagId == null) || (VoicetagId.length() == 0))     throw new IllegalArgumentException("VoicetagId may not be null or empty string.");
    return new VoicetagItemImpl(createVoicetagItemProxy(nativeObject,VoicetagId,listener),false);
  }
}
