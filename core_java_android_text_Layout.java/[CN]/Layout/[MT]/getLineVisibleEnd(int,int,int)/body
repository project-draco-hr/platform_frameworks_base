{
  if (Config.DEBUG) {
    Assert.assertTrue(getLineStart(line) == start && getLineStart(line + 1) == end);
  }
  CharSequence text=mText;
  char ch;
  if (line == getLineCount() - 1) {
    return end;
  }
  for (; end > start; end--) {
    ch=text.charAt(end - 1);
    if (ch == '\n') {
      return end - 1;
    }
    if (ch != ' ' && ch != '\t') {
      break;
    }
  }
  return end;
}
