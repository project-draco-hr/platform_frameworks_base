{
  if (mTask != task) {
    if (DEBUG)     Slog.v(TAG,"Load complete of old task, trying to cancel");
    dispatchOnCancelled(task,data);
  }
 else {
    if (isAbandoned()) {
      onCanceled(data);
    }
 else {
      commitContentChanged();
      mLastLoadCompleteTime=SystemClock.uptimeMillis();
      mTask=null;
      if (DEBUG)       Slog.v(TAG,"Delivering result");
      deliverResult(data);
    }
  }
}
