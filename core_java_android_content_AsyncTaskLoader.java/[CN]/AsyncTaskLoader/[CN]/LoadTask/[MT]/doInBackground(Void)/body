{
  if (DEBUG)   Log.v(TAG,this + " >>> doInBackground");
  try {
    D data=AsyncTaskLoader.this.onLoadInBackground();
    if (DEBUG)     Log.v(TAG,this + "  <<< doInBackground");
    return data;
  }
 catch (  OperationCanceledException ex) {
    if (!isCancelled()) {
      throw ex;
    }
    if (DEBUG)     Log.v(TAG,this + "  <<< doInBackground (was canceled)",ex);
    return null;
  }
}
