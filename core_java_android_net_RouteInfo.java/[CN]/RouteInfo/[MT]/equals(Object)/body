{
  if (this == obj)   return true;
  if (!(obj instanceof RouteInfo))   return false;
  RouteInfo target=(RouteInfo)obj;
  boolean sameDestination=(mDestination == null) ? target.getDestination() == null : mDestination.equals(target.getDestination());
  boolean sameAddress=(mGateway == null) ? target.getGateway() == null : mGateway.equals(target.getGateway());
  return sameDestination && sameAddress && mIsDefault == target.mIsDefault;
}
