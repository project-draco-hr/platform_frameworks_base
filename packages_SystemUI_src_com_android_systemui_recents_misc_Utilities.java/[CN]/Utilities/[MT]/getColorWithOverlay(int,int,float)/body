{
  return Color.rgb((int)(overlayAlpha * Color.red(baseColor) + (1f - overlayAlpha) * Color.red(overlayColor)),(int)(overlayAlpha * Color.green(baseColor) + (1f - overlayAlpha) * Color.green(overlayColor)),(int)(overlayAlpha * Color.blue(baseColor) + (1f - overlayAlpha) * Color.blue(overlayColor)));
}
