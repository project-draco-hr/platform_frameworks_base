{
  for (int i=0; i < files.length; i++) {
    File file=files[i];
    if (!file.exists()) {
      continue;
    }
    try {
      RandomAccessFile raf=new RandomAccessFile(file,"r");
      return getNumberFromFile(raf,file.getAbsolutePath());
    }
 catch (    IOException e) {
      Log.w(TAG,"Exception opening TCP statistics file " + file.getAbsolutePath(),e);
    }
  }
  return 0L;
}
