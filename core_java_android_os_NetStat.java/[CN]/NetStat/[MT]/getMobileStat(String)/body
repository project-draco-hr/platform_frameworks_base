{
  String filename="/sys/class/net/rmnet0/statistics/" + whatStat;
  RandomAccessFile raf=getFile(filename);
  if (raf == null) {
    filename="/sys/class/net/ppp0/statistics/" + whatStat;
    raf=getFile(filename);
  }
  if (raf == null) {
    return 0L;
  }
  return getNumberFromFile(raf,filename);
}
