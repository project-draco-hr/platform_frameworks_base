{
  RssiCurve curve=new RssiCurve(-100,100,new byte[]{-10,10});
  assertEquals(-10,curve.lookupScore(-200));
  assertEquals(-10,curve.lookupScore(-100));
  assertEquals(-10,curve.lookupScore(-50));
  assertEquals(10,curve.lookupScore(0));
  assertEquals(10,curve.lookupScore(50));
  assertEquals(10,curve.lookupScore(100));
  assertEquals(10,curve.lookupScore(200));
}
