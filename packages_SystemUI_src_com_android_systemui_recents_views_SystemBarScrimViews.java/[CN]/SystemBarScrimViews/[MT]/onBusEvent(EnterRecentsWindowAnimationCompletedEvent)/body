{
  if (mHasNavBarScrim) {
    AnimationProps animation=mShouldAnimateNavBarScrim ? new AnimationProps().setDuration(AnimationProps.BOUNDS,mNavBarScrimEnterDuration).setInterpolator(AnimationProps.BOUNDS,Interpolators.DECELERATE_QUINT) : AnimationProps.IMMEDIATE;
    animateNavBarScrimVisibility(true,animation);
  }
}
