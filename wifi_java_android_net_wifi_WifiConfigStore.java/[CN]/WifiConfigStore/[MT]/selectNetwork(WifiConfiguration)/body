{
  if (config != null) {
    NetworkUpdateResult result=addOrUpdateNetworkNative(config);
    int netId=result.getNetworkId();
    if (netId != INVALID_NETWORK_ID) {
      selectNetwork(netId);
    }
 else {
      Log.e(TAG,"Failed to update network " + config);
    }
    return netId;
  }
  return INVALID_NETWORK_ID;
}
