{
  int idx=nativeGetAttributeIndex(mParseState,namespace,name);
  if (idx >= 0) {
    if (DEBUG)     System.out.println("getAttributeName of " + namespace + ":"+ name+ " index = "+ idx);
    if (DEBUG)     System.out.println("Namespace=" + getAttributeNamespace(idx) + "Name="+ getAttributeName(idx)+ ", Value="+ getAttributeValue(idx));
    return getAttributeValue(idx);
  }
  return null;
}
