{
  if (isHorizontalDivision()) {
    if (dismissTarget == mSnapAlgorithm.getDismissStartTarget()) {
      return mStableInsets.top != 0;
    }
 else {
      return mStableInsets.bottom != 0;
    }
  }
 else {
    if (dismissTarget == mSnapAlgorithm.getDismissStartTarget()) {
      return mStableInsets.left != 0;
    }
 else {
      return mStableInsets.right != 0;
    }
  }
}
