{
  inoutInfo.setTouchableInsets(InternalInsetsInfo.TOUCHABLE_INSETS_REGION);
  inoutInfo.touchableRegion.set(mHandle.getLeft(),mHandle.getTop(),mHandle.getRight(),mHandle.getBottom());
  inoutInfo.touchableRegion.op(mBackground.getLeft(),mBackground.getTop(),mBackground.getRight(),mBackground.getBottom(),Op.UNION);
}
