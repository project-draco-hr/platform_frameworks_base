{
  if (mState != STATE_WATING_FOR_REQUEST_ARC_REQUEST_RESPONSE || !HdmiUtils.checkCommandSource(cmd,mAvrAddress,TAG)) {
    return false;
  }
  int opcode=cmd.getOpcode();
switch (opcode) {
case HdmiCec.MESSAGE_FEATURE_ABORT:
    disableArcTransmission();
  finish();
return true;
default :
Slog.w(TAG,"Unsupported opcode:" + cmd.toString());
}
return false;
}
