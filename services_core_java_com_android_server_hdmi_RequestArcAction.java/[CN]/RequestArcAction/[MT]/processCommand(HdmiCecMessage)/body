{
  if (mState != STATE_WATING_FOR_REQUEST_ARC_REQUEST_RESPONSE || !HdmiUtils.checkCommandSource(cmd,mAvrAddress,TAG)) {
    return false;
  }
  int opcode=cmd.getOpcode();
switch (opcode) {
case Constants.MESSAGE_FEATURE_ABORT:
    int originalOpcode=cmd.getParams()[0] & 0xFF;
  if (originalOpcode == Constants.MESSAGE_REQUEST_ARC_INITIATION || originalOpcode == Constants.MESSAGE_REQUEST_ARC_TERMINATION) {
    disableArcTransmission();
    finish();
    return true;
  }
 else {
    return false;
  }
}
return false;
}
