{
  final PositionMetadata posMetadata=getUnflattenedPos(flatListPos);
  if (posMetadata.position.type == ExpandableListPosition.GROUP) {
    return mExpandableListAdapter.getGroup(posMetadata.position.groupPos);
  }
 else   if (posMetadata.position.type == ExpandableListPosition.CHILD) {
    return mExpandableListAdapter.getChild(posMetadata.position.groupPos,posMetadata.position.childPos);
  }
 else {
    throw new RuntimeException("Flat list position is of unknown type");
  }
}
