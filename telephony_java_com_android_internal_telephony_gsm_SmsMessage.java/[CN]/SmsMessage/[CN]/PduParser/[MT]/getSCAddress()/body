{
  int len;
  String ret;
  len=getByte();
  if (len == 0) {
    ret=null;
  }
 else {
    try {
      ret=PhoneNumberUtils.calledPartyBCDToString(pdu,cur,len);
    }
 catch (    RuntimeException tr) {
      Log.d(LOG_TAG,"invalid SC address: ",tr);
      ret=null;
    }
  }
  cur+=len;
  return ret;
}
