{
  mDatabase.beginTransaction();
  try {
    int deleted=0;
    deleted+=mDatabase.delete(TABLE_ROOT_EXTRA,Root.COLUMN_ROOT_ID + " IN (" + SQLiteQueryBuilder.buildQueryString(false,TABLE_DOCUMENTS,new String[]{Document.COLUMN_DOCUMENT_ID},selection,null,null,null,null)+ ")",args);
    deleted+=mDatabase.delete(TABLE_DOCUMENTS,selection,args);
    mDatabase.setTransactionSuccessful();
    return deleted != 0;
  }
  finally {
    mDatabase.endTransaction();
  }
}
