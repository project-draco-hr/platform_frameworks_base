{
  Looper.prepare();
  mHandler=new Handler(){
    public void handleMessage(    Message msg){
switch (msg.what) {
case MSG_FRAME:
        for (int i=0; i < mFrameCallbacks.size(); i++) {
          mFrameCallbacks.get(i).doFrame(SystemClock.uptimeMillis());
        }
      break;
default :
    break;
}
}
}
;
Looper.loop();
}
