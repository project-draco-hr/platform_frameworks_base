{
  if (src == null) {
    mRadius=0;
    mRect=null;
    mPath=null;
  }
 else {
    if (src.mPath != null) {
      if (mPath == null) {
        mPath=new Path();
      }
      mPath.set(src.mPath);
      mRect=null;
    }
    if (src.mRect != null) {
      if (mRect == null) {
        mRect=new Rect();
      }
      mRect.set(src.mRect);
    }
    mRadius=src.mRadius;
  }
}
