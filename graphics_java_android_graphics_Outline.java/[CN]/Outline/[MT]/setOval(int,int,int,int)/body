{
  if (left >= right || top >= bottom) {
    setEmpty();
    return;
  }
  if ((bottom - top) == (right - left)) {
    setRoundRect(left,top,right,bottom,(bottom - top) / 2.0f);
    return;
  }
  if (mPath == null)   mPath=new Path();
  mPath.reset();
  mPath.addOval(left,top,right,bottom,Path.Direction.CW);
  mRect=null;
}
