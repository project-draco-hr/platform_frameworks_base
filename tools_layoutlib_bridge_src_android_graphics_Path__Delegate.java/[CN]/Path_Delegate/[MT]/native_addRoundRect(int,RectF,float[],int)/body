{
  native_addRoundRect(nPath,rect,radii[0],radii[1],dir);
  for (int i=0; i < 3; i++) {
    if (radii[i * 2] != radii[(i + 1) * 2] || radii[i * 2 + 1] != radii[(i + 1) * 2 + 1]) {
      Bridge.getLog().fidelityWarning(LayoutLog.TAG_UNSUPPORTED,"Different corner size is not support in Path.addRoundRect.",null,null);
      break;
    }
  }
}
