{
  LoaderInfo info=mLoaders.get(id);
  if (info != null) {
    if (mInactiveLoaders.get(id) != null) {
      info.destroy();
    }
 else {
      mInactiveLoaders.put(id,info);
    }
  }
  info=createLoader(id,args,(LoaderManager.LoaderCallbacks<Object>)callback);
  return (Loader<D>)info.mLoader;
}
