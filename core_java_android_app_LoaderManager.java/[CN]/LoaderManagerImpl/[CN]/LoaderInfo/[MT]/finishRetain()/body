{
  if (mRetaining) {
    if (DEBUG)     Log.v(TAG,"  Finished Retaining: " + this);
    mRetaining=false;
    if (mStarted != mRetainingStarted) {
      if (!mStarted) {
        stop();
      }
    }
    if (mStarted && mData != null && mCallbacks != null) {
      mCallbacks.onLoadFinished(mLoader,mData);
    }
  }
}
