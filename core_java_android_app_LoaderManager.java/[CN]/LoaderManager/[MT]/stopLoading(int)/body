{
  if (mLoaders != null) {
    int idx=mLoaders.indexOfKey(id);
    if (idx >= 0) {
      LoaderInfo info=mLoaders.valueAt(idx);
      mLoaders.removeAt(idx);
      Loader<Object> loader=info.loader;
      if (loader != null) {
        loader.unregisterListener(info);
        loader.destroy();
      }
    }
  }
}
