{
  if (mDestroyed) {
    return;
  }
  mData=data;
  if (mCallbacks != null) {
    mCallbacks.onLoadFinished(loader,data);
  }
  LoaderInfo info=mInactiveLoaders.get(mId);
  if (info != null) {
    Loader<Object> oldLoader=info.mLoader;
    if (oldLoader != null) {
      oldLoader.unregisterListener(info);
      oldLoader.destroy();
    }
    mInactiveLoaders.remove(mId);
  }
}
