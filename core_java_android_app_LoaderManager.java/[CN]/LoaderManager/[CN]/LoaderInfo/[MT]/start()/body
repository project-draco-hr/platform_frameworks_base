{
  if (mRetaining && mRetainingStarted) {
    mStarted=true;
    return;
  }
  if (mLoader == null && mCallbacks != null) {
    mLoader=mCallbacks.onCreateLoader(mId,mArgs);
  }
  if (mLoader != null) {
    mLoader.registerListener(mId,this);
    mListenerRegistered=true;
    mLoader.startLoading();
    mStarted=true;
  }
}
