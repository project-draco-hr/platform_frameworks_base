{
  assertEquals(mHttpHost,Proxy.getPreferredHttpHost(mContext,null));
  assertEquals(mHttpHost,Proxy.getPreferredHttpHost(mContext,""));
  assertEquals(mHttpHost,Proxy.getPreferredHttpHost(mContext,"bad:"));
  assertEquals(mHttpHost,Proxy.getPreferredHttpHost(mContext,"bad"));
  assertEquals(mHttpHost,Proxy.getPreferredHttpHost(mContext,"bad:\\"));
  assertEquals(mHttpHost,Proxy.getPreferredHttpHost(mContext,"bad://#"));
  assertEquals(mHttpHost,Proxy.getPreferredHttpHost(mContext,"://#"));
}
