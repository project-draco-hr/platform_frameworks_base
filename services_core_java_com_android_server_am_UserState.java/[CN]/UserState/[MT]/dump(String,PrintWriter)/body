{
  pw.print(prefix);
  pw.print("mState=");
switch (mState) {
case STATE_BOOTING:
    pw.print("BOOTING");
  break;
case STATE_RUNNING:
pw.print("RUNNING");
break;
case STATE_STOPPING:
pw.print("STOPPING");
break;
case STATE_SHUTDOWN:
pw.print("SHUTDOWN");
break;
default :
pw.print(mState);
break;
}
if (switching) pw.print(" SWITCHING");
if (initializing) pw.print(" INITIALIZING");
pw.println();
}
