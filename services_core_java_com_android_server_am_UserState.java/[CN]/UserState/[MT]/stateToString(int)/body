{
switch (state) {
case STATE_BOOTING:
    return "BOOTING";
case STATE_RUNNING_LOCKED:
  return "RUNNING_LOCKED";
case STATE_RUNNING_UNLOCKING:
return "RUNNING_UNLOCKING";
case STATE_RUNNING_UNLOCKED:
return "RUNNING_UNLOCKED";
case STATE_STOPPING:
return "STOPPING";
case STATE_SHUTDOWN:
return "SHUTDOWN";
default :
return Integer.toString(state);
}
}
