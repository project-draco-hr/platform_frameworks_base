{
  if (inputLength > 0) {
    mConsumedInputSizeBytes+=inputLength;
    mInputBuffer.write(input,inputOffset,inputLength);
  }
  byte[] bufferedInput=mInputBuffer.toByteArray();
  mInputBuffer.reset();
  return mDelegate.doFinal(bufferedInput,0,Math.min(bufferedInput.length,((mGroupSizeBits + 7) / 8)),signature,additionalEntropy);
}
