{
  final List<WifiConfiguration> configs=mWifiManager.getConfiguredNetworks();
  if (configs != null) {
    for (    WifiConfiguration config : configs) {
      if (mLastInfo != null && networkId == config.networkId && !(config.selfAdded && config.numAssociation == 0)) {
        return config;
      }
    }
  }
  return null;
}
