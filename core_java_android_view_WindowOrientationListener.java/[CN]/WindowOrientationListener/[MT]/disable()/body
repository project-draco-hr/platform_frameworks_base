{
  if (mSensor == null) {
    Log.w(TAG,"Cannot detect sensors. Invalid disable");
    return;
  }
  if (mEnabled == true) {
    if (localLOGV)     Log.d(TAG,"WindowOrientationListener disabled");
    mSensorRotation=-1;
    mSensorManager.unregisterListener(mSensorEventListener);
    mEnabled=false;
  }
}
