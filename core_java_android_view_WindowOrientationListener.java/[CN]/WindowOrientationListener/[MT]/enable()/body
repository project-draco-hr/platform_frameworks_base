{
  if (mSensor == null) {
    Log.w(TAG,"Cannot detect sensors. Not enabled");
    return;
  }
  if (mEnabled == false) {
    if (LOG) {
      Log.d(TAG,"WindowOrientationListener enabled");
    }
    mSensorEventListener.reset();
    mSensorManager.registerListener(mSensorEventListener,mSensor,mRate);
    mEnabled=true;
  }
}
