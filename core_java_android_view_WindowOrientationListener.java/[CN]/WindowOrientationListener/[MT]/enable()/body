{
  if (mSensor == null) {
    Log.w(TAG,"Cannot detect sensors. Not enabled");
    return;
  }
  if (mEnabled == false) {
    if (localLOGV)     Log.d(TAG,"WindowOrientationListener enabled");
    mSensorRotation=-1;
    mSensorManager.registerListener(mSensorEventListener,mSensor,mRate);
    mEnabled=true;
  }
}
