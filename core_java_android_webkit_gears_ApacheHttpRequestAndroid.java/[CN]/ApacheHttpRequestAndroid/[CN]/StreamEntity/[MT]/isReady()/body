{
  mStreamingReadyLock.lock();
  try {
    if (mOutputStream == null) {
      mStreamingReady.await();
    }
  }
 catch (  InterruptedException e) {
    if (LOGV_ENABLED) {
      Log.i(LOG_TAG,"InterruptedException in " + "StreamEntity::isReady() : ",e);
    }
  }
 finally {
    mStreamingReadyLock.unlock();
  }
  if (mOutputStream == null) {
    return false;
  }
  return true;
}
