{
  if (mMethod == null)   return;
  Iterator<String[]> it=mRequestHeaders.values().iterator();
  while (it.hasNext()) {
    String[] entry=it.next();
    if (Config.LOGV) {
      Log.i(LOG_TAG,"apply header " + entry[HEADERS_MAP_INDEX_KEY] + " => "+ entry[HEADERS_MAP_INDEX_VALUE]);
    }
    mMethod.setHeader(entry[HEADERS_MAP_INDEX_KEY],entry[HEADERS_MAP_INDEX_VALUE]);
  }
}
