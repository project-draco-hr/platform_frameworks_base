{
  if (mResponseHeaders != null) {
    String[] value=mResponseHeaders.get(name.toLowerCase());
    if (value != null) {
      return value[HEADERS_MAP_INDEX_VALUE];
    }
 else {
      return null;
    }
  }
 else {
    if (Config.LOGV) {
      Log.i(LOG_TAG,"getResponseHeader() called but " + "response not received");
    }
    return null;
  }
}
