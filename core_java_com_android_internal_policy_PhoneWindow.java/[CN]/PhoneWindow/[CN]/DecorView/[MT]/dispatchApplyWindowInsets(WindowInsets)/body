{
  if (mOutsetBottomPx != 0) {
    WindowInsets newInsets=insets.replaceSystemWindowInsets(insets.getSystemWindowInsetLeft(),insets.getSystemWindowInsetTop(),insets.getSystemWindowInsetRight(),mOutsetBottomPx);
    return super.dispatchApplyWindowInsets(newInsets);
  }
 else {
    return super.dispatchApplyWindowInsets(insets);
  }
}
