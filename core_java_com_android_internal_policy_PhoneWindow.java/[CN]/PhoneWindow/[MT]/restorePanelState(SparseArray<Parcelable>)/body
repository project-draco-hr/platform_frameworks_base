{
  PanelFeatureState st;
  int curFeatureId;
  for (int i=icicles.size() - 1; i >= 0; i--) {
    curFeatureId=icicles.keyAt(i);
    st=getPanelState(curFeatureId,false);
    if (st == null) {
      continue;
    }
    st.onRestoreInstanceState(icicles.get(curFeatureId));
    invalidatePanelMenu(curFeatureId);
  }
}
