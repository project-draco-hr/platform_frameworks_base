{
  mResetInterval=DEFAULT_RESET_INTERVAL_SEC * 1000L;
  mMaxDailyUpdates=DEFAULT_MAX_DAILY_UPDATES;
  mMaxDynamicShortcuts=DEFAULT_MAX_SHORTCUTS_PER_APP;
  final int iconDimensionDp=(injectIsLowRamDevice() ? DEFAULT_MAX_ICON_DIMENSION_LOWRAM_DP : DEFAULT_MAX_ICON_DIMENSION_DP);
  mMaxIconDimension=(int)TypedValue.applyDimension(TypedValue.COMPLEX_UNIT_DIP,iconDimensionDp,mContext.getResources().getDisplayMetrics());
}
