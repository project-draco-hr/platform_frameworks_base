{
  final ArrayList<ShortcutChangeListener> copy;
  final List<ShortcutInfo> shortcuts=new ArrayList<>();
synchronized (mLock) {
    copy=new ArrayList<>(mListeners);
    getPackageShortcutsLocked(packageName,userId).findAll(shortcuts,null,ShortcutInfo.CLONE_REMOVE_NON_KEY_INFO);
  }
  for (int i=copy.size() - 1; i >= 0; i--) {
    copy.get(i).onShortcutChanged(packageName,shortcuts,userId);
  }
}
