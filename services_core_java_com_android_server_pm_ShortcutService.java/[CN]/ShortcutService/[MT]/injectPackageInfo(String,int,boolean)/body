{
  try {
    return mIPackageManager.getPackageInfo(packageName,PackageManager.MATCH_DIRECT_BOOT_AWARE | PackageManager.MATCH_DIRECT_BOOT_UNAWARE | (getSignatures ? PackageManager.GET_SIGNATURES : 0),userId);
  }
 catch (  RemoteException e) {
    Slog.wtf(TAG,"RemoteException",e);
    return null;
  }
}
