{
  Preconditions.checkStringNotEmpty(packageName,"packageName");
  Preconditions.checkNotNull(shortcutIds,"shortcutIds");
synchronized (mLock) {
    getUserShortcutsLocked(userId).ensurePackageInfo(ShortcutService.this,callingPackage,launcherUserId);
    getLauncherShortcuts(callingPackage,userId,launcherUserId).pinShortcuts(ShortcutService.this,packageName,shortcutIds);
  }
  userPackageChanged(packageName,userId);
}
