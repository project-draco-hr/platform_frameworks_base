{
  Preconditions.checkStringNotEmpty(packageName,"packageName");
  Preconditions.checkNotNull(shortcutIds,"shortcutIds");
synchronized (mLock) {
    final ShortcutLauncher launcher=getLauncherShortcuts(callingPackage,userId,launcherUserId);
    launcher.ensureNotShadowAndSave(ShortcutService.this);
    launcher.pinShortcuts(ShortcutService.this,userId,packageName,shortcutIds);
  }
  userPackageChanged(packageName,userId);
}
