{
  pw.print(prefix);
  pw.print("User: ");
  pw.print(mUserId);
  pw.println();
  pw.print(prefix);
  pw.print("  ");
  pw.print("Default launcher: ");
  pw.print(mLauncherComponent);
  pw.println();
  for (int i=0; i < mPackages.size(); i++) {
    mPackages.valueAt(i).dump(s,pw,prefix + "  ");
  }
}
