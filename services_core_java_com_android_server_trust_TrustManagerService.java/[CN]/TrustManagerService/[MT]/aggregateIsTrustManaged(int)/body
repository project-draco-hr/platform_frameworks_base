{
  if (!mUserHasAuthenticatedSinceBoot.get(userId)) {
    return false;
  }
  for (int i=0; i < mActiveAgents.size(); i++) {
    AgentInfo info=mActiveAgents.valueAt(i);
    if (info.userId == userId) {
      if (info.agent.isManagingTrust()) {
        return true;
      }
    }
  }
  return false;
}
