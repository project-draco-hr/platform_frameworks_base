{
  try {
    if (session != null) {
      IInputMethodSessionWrapper wrap=new IInputMethodSessionWrapper(mContext,session,mChannel);
      mCb.sessionCreated(wrap);
    }
 else {
      if (mChannel != null) {
        mChannel.dispose();
      }
      mCb.sessionCreated(null);
    }
  }
 catch (  RemoteException e) {
  }
}
