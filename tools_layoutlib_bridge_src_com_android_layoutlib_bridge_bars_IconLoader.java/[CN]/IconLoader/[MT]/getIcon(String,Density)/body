{
  mCurrentPath.setLength(0);
  if (mDirection == LayoutDirection.RTL) {
    mCurrentPath.append(resourceDir).append(mDirection.getResourceValue()).append('-').append(density.getResourceValue()).append('/').append(mIconName);
    InputStream stream=getClass().getResourceAsStream(mCurrentPath.toString());
    if (stream != null) {
      return stream;
    }
    mCurrentPath.setLength(0);
  }
  mCurrentPath.append(resourceDir).append(density.getResourceValue()).append('/').append(mIconName);
  return getClass().getResourceAsStream(mCurrentPath.toString());
}
