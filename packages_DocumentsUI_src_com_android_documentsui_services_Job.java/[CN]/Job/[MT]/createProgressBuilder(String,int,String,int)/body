{
  Notification.Builder progressBuilder=new Notification.Builder(serviceContext).setContentTitle(title).setContentIntent(PendingIntent.getActivity(appContext,0,buildNavigateIntent(),0)).setCategory(Notification.CATEGORY_PROGRESS).setSmallIcon(icon).setOngoing(true);
  final Intent cancelIntent=createCancelIntent();
  progressBuilder.addAction(actionIcon,actionTitle,PendingIntent.getService(serviceContext,0,cancelIntent,PendingIntent.FLAG_ONE_SHOT | PendingIntent.FLAG_CANCEL_CURRENT));
  return progressBuilder;
}
