{
  if (bitmap == null) {
    throw new NullPointerException("getInternalFormat can't be used with a null Bitmap");
  }
switch (bitmap.getConfig()) {
case ALPHA_8:
    return GL10.GL_ALPHA;
case ARGB_4444:
case ARGB_8888:
  return GL10.GL_RGBA;
case RGB_565:
return GL10.GL_RGB;
}
throw new IllegalArgumentException("Unknown internalformat");
}
