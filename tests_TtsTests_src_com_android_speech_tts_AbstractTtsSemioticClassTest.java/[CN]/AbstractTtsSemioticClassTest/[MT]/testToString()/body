{
  TtsMock t=new TtsMock().setAnimacy(Utterance.ANIMACY_INANIMATE).setCase(Utterance.CASE_INSTRUMENTAL).setGender(Utterance.GENDER_FEMALE).setMultiplicity(Utterance.MULTIPLICITY_PLURAL);
  String str="animacy: \"2\" " + "case: \"8\" " + "gender: \"3\" "+ "multiplicity: \"3\"";
  assertEquals(str,t.toString());
}
