{
  if (DBG)   Log.d(TAG,"getUnreadMessages(" + device + ")");
  if (mService != null && isEnabled() && isValidDevice(device)) {
    try {
      return mService.getUnreadMessages(device);
    }
 catch (    RemoteException e) {
      Log.e(TAG,Log.getStackTraceString(new Throwable()));
      return false;
    }
  }
  return false;
}
