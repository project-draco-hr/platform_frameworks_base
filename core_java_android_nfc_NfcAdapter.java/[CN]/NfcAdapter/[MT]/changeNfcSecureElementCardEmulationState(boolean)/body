{
  int seId=11259375;
  if (state) {
    try {
      sService.selectSecureElement(seId);
    }
 catch (    RemoteException e) {
      Log.e(TAG,"Enable card emulation failed",e);
    }
  }
 else {
    try {
      sService.deselectSecureElement();
    }
 catch (    RemoteException e) {
      Log.e(TAG," card emulation failed",e);
    }
  }
}
