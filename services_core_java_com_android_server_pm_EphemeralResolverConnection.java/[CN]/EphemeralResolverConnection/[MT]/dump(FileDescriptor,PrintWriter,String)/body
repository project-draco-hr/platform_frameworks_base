{
synchronized (mLock) {
    pw.append(prefix).append("bound=").append((mRemoteInstance != null) ? "true" : "false").println();
    pw.flush();
    try {
      getRemoteInstanceLazy().asBinder().dump(fd,new String[]{prefix});
    }
 catch (    TimeoutException te) {
    }
catch (    RemoteException re) {
    }
  }
}
