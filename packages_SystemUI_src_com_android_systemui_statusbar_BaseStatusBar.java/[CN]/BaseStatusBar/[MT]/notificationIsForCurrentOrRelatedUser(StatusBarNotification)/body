{
  final int thisUserId=mCurrentUserId;
  final int notificationUserId=n.getUserId();
  if (DEBUG && MULTIUSER_DEBUG) {
    Log.v(TAG,String.format("%s: current userid: %d, notification userid: %d",n,thisUserId,notificationUserId));
  }
synchronized (mRelatedUsers) {
    return notificationUserId == UserHandle.USER_ALL || thisUserId == notificationUserId || mRelatedUsers.get(notificationUserId) != null;
  }
}
