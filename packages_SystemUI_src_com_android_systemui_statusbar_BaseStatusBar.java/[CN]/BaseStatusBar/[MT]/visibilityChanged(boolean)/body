{
  if (mPanelSlightlyVisible != visible) {
    mPanelSlightlyVisible=visible;
    if (!visible) {
      dismissPopups();
    }
    try {
      if (visible) {
        mBarService.onPanelRevealed();
      }
 else {
        mBarService.onPanelHidden();
      }
    }
 catch (    RemoteException ex) {
    }
  }
}
