{
  if (DEBUG) {
    Log.d(TAG,"createNotificationViews(notification=" + sbn);
  }
  Notification n=sbn.getNotification();
  final StatusBarIconView iconView=new StatusBarIconView(mContext,sbn.getPackageName() + "/0x" + Integer.toHexString(sbn.getId()),n);
  iconView.setScaleType(ImageView.ScaleType.CENTER_INSIDE);
  final StatusBarIcon ic=new StatusBarIcon(sbn.getPackageName(),sbn.getUser(),n.icon,n.iconLevel,n.number,n.tickerText);
  if (!iconView.set(ic)) {
    handleNotificationError(sbn,"Couldn't create icon: " + ic);
    return null;
  }
  NotificationData.Entry entry=new NotificationData.Entry(sbn,iconView);
  if (!inflateViews(entry,mStackScroller)) {
    handleNotificationError(sbn,"Couldn't expand RemoteViews for: " + sbn);
    return null;
  }
  return entry;
}
