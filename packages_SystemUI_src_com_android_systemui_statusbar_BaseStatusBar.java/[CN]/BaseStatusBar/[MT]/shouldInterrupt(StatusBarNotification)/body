{
  boolean interrupt=notification.getNotification().fullScreenIntent == null && notification.getScore() >= INTERRUPTION_THRESHOLD && mPowerManager.isScreenOn() && !mKeyguardManager.isKeyguardLocked();
  try {
    interrupt=interrupt && !mDreamManager.isDreaming();
  }
 catch (  RemoteException e) {
    Log.d(TAG,"failed to query dream manager",e);
  }
  return interrupt;
}
