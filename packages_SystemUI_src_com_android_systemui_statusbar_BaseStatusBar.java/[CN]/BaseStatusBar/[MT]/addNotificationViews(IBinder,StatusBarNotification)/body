{
  if (DEBUG) {
    Slog.d(TAG,"addNotificationViews(key=" + key + ", notification="+ notification);
  }
  final StatusBarIconView iconView=new StatusBarIconView(mContext,notification.pkg + "/0x" + Integer.toHexString(notification.id),notification.notification);
  iconView.setScaleType(ImageView.ScaleType.CENTER_INSIDE);
  final StatusBarIcon ic=new StatusBarIcon(notification.pkg,notification.user,notification.notification.icon,notification.notification.iconLevel,notification.notification.number,notification.notification.tickerText);
  if (!iconView.set(ic)) {
    handleNotificationError(key,notification,"Couldn't create icon: " + ic);
    return null;
  }
  NotificationData.Entry entry=new NotificationData.Entry(key,notification,iconView);
  if (!inflateViews(entry,mPile)) {
    handleNotificationError(key,notification,"Couldn't expand RemoteViews for: " + notification);
    return null;
  }
  int pos=mNotificationData.add(entry);
  if (DEBUG) {
    Slog.d(TAG,"addNotificationViews: added at " + pos);
  }
  updateExpansionStates();
  updateNotificationIcons();
  return iconView;
}
