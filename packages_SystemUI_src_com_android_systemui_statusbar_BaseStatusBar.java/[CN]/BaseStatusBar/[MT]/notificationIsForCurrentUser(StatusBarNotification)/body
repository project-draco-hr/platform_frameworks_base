{
  final int thisUserId=mCurrentUserId;
  final int notificationUserId=n.getUserId();
  if (DEBUG && MULTIUSER_DEBUG) {
    Slog.v(TAG,String.format("%s: current userid: %d, notification userid: %d",n,thisUserId,notificationUserId));
  }
  return notificationUserId == UserHandle.USER_ALL || thisUserId == notificationUserId;
}
