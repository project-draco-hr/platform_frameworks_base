{
  try {
    if (visibleToUser) {
      boolean clearNotificationEffects=!isPanelFullyCollapsed() && (mShowLockscreenNotifications || (mState == StatusBarState.SHADE || mState == StatusBarState.SHADE_LOCKED));
      int notificationLoad=mNotificationData.getActiveNotifications().size();
      if (mHeadsUpManager.hasPinnedHeadsUp() && isPanelFullyCollapsed()) {
        notificationLoad=1;
      }
 else {
        MetricsLogger.histogram(mContext,"note_load",notificationLoad);
      }
      mBarService.onPanelRevealed(clearNotificationEffects,notificationLoad);
    }
 else {
      mBarService.onPanelHidden();
    }
  }
 catch (  RemoteException ex) {
  }
}
