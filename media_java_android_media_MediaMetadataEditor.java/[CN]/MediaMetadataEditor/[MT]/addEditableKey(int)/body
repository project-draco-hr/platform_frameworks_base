{
  if (mApplied) {
    Log.e(TAG,"Can't change editable keys of a previously applied MetadataEditor");
    return;
  }
  if (key == RATING_KEY_BY_USER) {
    mEditableKeys|=(KEY_EDITABLE_MASK & key);
    mMetadataChanged=true;
  }
 else {
    Log.e(TAG,"Metadata key " + key + " cannot be edited");
  }
}
