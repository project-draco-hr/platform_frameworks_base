{
  boolean videoRecordedResult=false;
  try {
    mCamera=Camera.open();
    Camera.Parameters parameters=mCamera.getParameters();
    parameters.setPreviewSize(352,288);
    parameters.set("orientation","portrait");
    mCamera.setParameters(parameters);
    mCamera.unlock();
    mRecorder.setCamera(mCamera);
    Thread.sleep(1000);
    recordVideo(15,352,288,MediaRecorder.VideoEncoder.H263,MediaRecorder.OutputFormat.THREE_GPP,MediaNames.RECORDED_PORTRAIT_H263,true);
    videoRecordedResult=validateVideo(MediaNames.RECORDED_PORTRAIT_H263,352,288);
    mCamera.lock();
    mCamera.release();
  }
 catch (  Exception e) {
    Log.v(TAG,e.toString());
  }
  assertTrue("PortraitH263",videoRecordedResult);
}
