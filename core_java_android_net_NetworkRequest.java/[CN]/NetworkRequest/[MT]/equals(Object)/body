{
  if (obj instanceof NetworkRequest == false)   return false;
  NetworkRequest that=(NetworkRequest)obj;
  return (that.needsBroadcasts == this.needsBroadcasts && that.requestId == this.requestId && ((that.networkCapabilities == null && this.networkCapabilities == null) || (that.networkCapabilities != null && that.networkCapabilities.equals(this.networkCapabilities))));
}
