{
  if (header == null) {
    throw new NullPointerException("header cannot be null");
  }
  if (header.contains(":")) {
    throw new IllegalArgumentException("header may not contain ':'");
  }
  if (value == null) {
    value="";
  }
  mRequestHeaders.add(Pair.create(header,value));
  return this;
}
