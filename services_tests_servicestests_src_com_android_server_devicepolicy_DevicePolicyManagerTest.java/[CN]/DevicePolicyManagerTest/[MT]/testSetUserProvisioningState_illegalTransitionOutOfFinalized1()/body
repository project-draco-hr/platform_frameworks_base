{
  setupProfileOwner();
  mContext.callerPermissions.add(permission.MANAGE_PROFILE_AND_DEVICE_OWNERS);
  try {
    exerciseUserProvisioningTransitions(DpmMockContext.CALLER_USER_HANDLE,DevicePolicyManager.STATE_USER_SETUP_FINALIZED,DevicePolicyManager.STATE_USER_UNMANAGED);
    fail("Expected IllegalStateException");
  }
 catch (  IllegalStateException e) {
    MoreAsserts.assertContainsRegex("Cannot move to user provisioning state",e.getMessage());
  }
}
