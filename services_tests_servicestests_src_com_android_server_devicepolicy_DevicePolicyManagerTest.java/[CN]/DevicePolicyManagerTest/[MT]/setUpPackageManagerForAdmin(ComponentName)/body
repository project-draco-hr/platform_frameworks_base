{
  final Intent resolveIntent=new Intent();
  resolveIntent.setComponent(admin);
  final List<ResolveInfo> realResolveInfo=mRealTestContext.getPackageManager().queryBroadcastReceivers(resolveIntent,PackageManager.GET_META_DATA);
  assertNotNull(realResolveInfo);
  assertEquals(1,realResolveInfo.size());
  realResolveInfo.get(0).activityInfo.applicationInfo.uid=DpmMockContext.CALLER_UID;
  doReturn(realResolveInfo).when(mContext.packageManager).queryBroadcastReceivers(MockUtils.checkIntentComponent(admin),eq(PackageManager.GET_META_DATA | PackageManager.GET_DISABLED_UNTIL_USED_COMPONENTS),eq(DpmMockContext.CALLER_USER_HANDLE));
}
