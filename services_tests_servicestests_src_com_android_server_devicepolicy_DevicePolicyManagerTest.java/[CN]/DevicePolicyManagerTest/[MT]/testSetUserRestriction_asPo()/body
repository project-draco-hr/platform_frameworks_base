{
  setAsProfileOwner(admin1);
  assertFalse(dpms.getProfileOwnerAdminLocked(DpmMockContext.CALLER_USER_HANDLE).ensureUserRestrictions().getBoolean(UserManager.DISALLOW_INSTALL_UNKNOWN_SOURCES));
  assertFalse(dpms.getProfileOwnerAdminLocked(DpmMockContext.CALLER_USER_HANDLE).ensureUserRestrictions().getBoolean(UserManager.DISALLOW_OUTGOING_CALLS));
  dpm.addUserRestriction(admin1,UserManager.DISALLOW_INSTALL_UNKNOWN_SOURCES);
  dpm.addUserRestriction(admin1,UserManager.DISALLOW_OUTGOING_CALLS);
  assertTrue(dpms.getProfileOwnerAdminLocked(DpmMockContext.CALLER_USER_HANDLE).ensureUserRestrictions().getBoolean(UserManager.DISALLOW_INSTALL_UNKNOWN_SOURCES));
  assertTrue(dpms.getProfileOwnerAdminLocked(DpmMockContext.CALLER_USER_HANDLE).ensureUserRestrictions().getBoolean(UserManager.DISALLOW_OUTGOING_CALLS));
  dpm.clearUserRestriction(admin1,UserManager.DISALLOW_INSTALL_UNKNOWN_SOURCES);
  assertFalse(dpms.getProfileOwnerAdminLocked(DpmMockContext.CALLER_USER_HANDLE).ensureUserRestrictions().getBoolean(UserManager.DISALLOW_INSTALL_UNKNOWN_SOURCES));
  assertTrue(dpms.getProfileOwnerAdminLocked(DpmMockContext.CALLER_USER_HANDLE).ensureUserRestrictions().getBoolean(UserManager.DISALLOW_OUTGOING_CALLS));
  dpm.clearUserRestriction(admin1,UserManager.DISALLOW_OUTGOING_CALLS);
  assertFalse(dpms.getProfileOwnerAdminLocked(DpmMockContext.CALLER_USER_HANDLE).ensureUserRestrictions().getBoolean(UserManager.DISALLOW_INSTALL_UNKNOWN_SOURCES));
  assertFalse(dpms.getProfileOwnerAdminLocked(DpmMockContext.CALLER_USER_HANDLE).ensureUserRestrictions().getBoolean(UserManager.DISALLOW_OUTGOING_CALLS));
}
