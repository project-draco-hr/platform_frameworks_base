{
  Display display=mDisplays.get(displayId);
  if (display == null) {
    display=mGlobal.getCompatibleDisplay(displayId,mContext.getCompatibilityInfo(displayId));
    if (display != null) {
      mDisplays.put(displayId,display);
    }
  }
 else   if (!assumeValid && !display.isValid()) {
    display=null;
  }
  return display;
}
