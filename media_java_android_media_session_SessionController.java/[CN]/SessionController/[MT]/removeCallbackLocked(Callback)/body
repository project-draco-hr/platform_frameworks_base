{
  if (cb == null) {
    throw new IllegalArgumentException("Callback cannot be null");
  }
  for (int i=mCallbacks.size() - 1; i >= 0; i--) {
    MessageHandler handler=mCallbacks.get(i);
    if (cb == handler.mCallback) {
      mCallbacks.remove(i);
      return true;
    }
  }
  return false;
}
