{
  final ListView listView=parent instanceof ListView ? (ListView)parent : null;
  if (listView != null) {
    position-=listView.getHeaderViewsCount();
  }
  if (position < 0) {
    return false;
  }
  ResolveInfo ri=mAdapter.resolveInfoForPosition(position,true);
  showTargetDetails(ri);
  return true;
}
