{
  if (TextUtils.isEmpty(getExtendedInfo())) {
    return null;
  }
  if (mBadge == null && mResolveInfo != null && mResolveInfo.activityInfo != null && mResolveInfo.activityInfo.applicationInfo != null) {
    if (mResolveInfo.activityInfo.icon == 0 || mResolveInfo.activityInfo.icon == mResolveInfo.activityInfo.applicationInfo.icon) {
      return null;
    }
    mBadge=mResolveInfo.activityInfo.applicationInfo.loadIcon(mPm);
  }
  return mBadge;
}
