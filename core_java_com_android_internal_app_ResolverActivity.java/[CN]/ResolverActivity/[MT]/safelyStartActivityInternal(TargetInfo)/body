{
  if (mProfileSwitchMessageId != -1) {
    Toast.makeText(this,getString(mProfileSwitchMessageId),Toast.LENGTH_LONG).show();
  }
  if (!mSafeForwardingMode) {
    if (cti.start(this,null)) {
      onActivityStarted(cti);
    }
    return;
  }
  try {
    if (cti.startAsCaller(this,null,UserHandle.USER_NULL)) {
      onActivityStarted(cti);
    }
  }
 catch (  RuntimeException e) {
    String launchedFromPackage;
    try {
      launchedFromPackage=ActivityManagerNative.getDefault().getLaunchedFromPackage(getActivityToken());
    }
 catch (    RemoteException e2) {
      launchedFromPackage="??";
    }
    Slog.wtf(TAG,"Unable to launch as uid " + mLaunchedFromUid + " package "+ launchedFromPackage+ ", while running in "+ ActivityThread.currentProcessName(),e);
  }
}
