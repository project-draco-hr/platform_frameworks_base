{
  super.onStop();
  if (mRegistered) {
    mPackageMonitor.unregister();
    mRegistered=false;
  }
  final Intent intent=getIntent();
  if ((intent.getFlags() & FLAG_ACTIVITY_NEW_TASK) != 0 && !isVoiceInteraction() && !mResolvingHome) {
    if (!isChangingConfigurations()) {
      finish();
    }
  }
}
