{
  if (JniUtil.useChromiumHttpStack()) {
    if (url.indexOf("://") == -1)     url="http://" + url;
    return nativeGetCookie(url);
  }
  WebAddress uri;
  try {
    uri=new WebAddress(url);
  }
 catch (  ParseException ex) {
    Log.e(LOGTAG,"Bad address: " + url);
    return null;
  }
  return getCookie(uri);
}
