{
  if (urlPath.startsWith(path)) {
    int len=path.length();
    if (len == 0) {
      Log.w(LOGTAG,"Empty cookie path");
      return false;
    }
    int urlLen=urlPath.length();
    if (path.charAt(len - 1) != PATH_DELIM && urlLen > len) {
      return urlPath.charAt(len) == PATH_DELIM;
    }
    return true;
  }
  return false;
}
