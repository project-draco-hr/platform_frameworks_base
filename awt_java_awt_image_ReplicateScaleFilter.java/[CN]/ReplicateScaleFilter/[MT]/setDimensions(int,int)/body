{
  srcWidth=w;
  srcHeight=h;
  if (destWidth < 0 && destHeight < 0) {
    destWidth=srcWidth;
    destHeight=srcHeight;
  }
 else   if (destWidth < 0) {
    destWidth=destHeight * srcWidth / srcHeight;
  }
 else   if (destHeight < 0) {
    destHeight=destWidth * srcHeight / srcWidth;
  }
  consumer.setDimensions(destWidth,destHeight);
}
