{
switch (inCallCallState) {
case NEW:
    return STATE_NEW;
case DIALING:
  return STATE_DIALING;
case RINGING:
return STATE_RINGING;
case ACTIVE:
return STATE_ACTIVE;
case ON_HOLD:
return STATE_HOLDING;
case DISCONNECTED:
return STATE_DISCONNECTED;
case ABORTED:
return STATE_DISCONNECTED;
default :
Log.wtf(this,"Unrecognized CallState %s",inCallCallState);
return STATE_NEW;
}
}
