{
  try {
    if (mCloseGuard != null) {
      mCloseGuard.warnIfOpen();
    }
    if (!mProviderReleased && mContentProvider != null) {
      Log.w(TAG,"Cursor finalized without prior close()");
      ContentResolver.this.releaseProvider(mContentProvider);
    }
  }
  finally {
    super.finalize();
  }
}
