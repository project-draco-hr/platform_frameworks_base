{
  for (int i=0, n=SYNC_ERROR_NAMES.length; i < n; i++) {
    if (SYNC_ERROR_NAMES[i].equals(error)) {
      return i + 1;
    }
  }
  if (error != null) {
    try {
      return Integer.parseInt(error);
    }
 catch (    NumberFormatException e) {
      Log.d(TAG,"error parsing sync error: " + error);
    }
  }
  return 0;
}
