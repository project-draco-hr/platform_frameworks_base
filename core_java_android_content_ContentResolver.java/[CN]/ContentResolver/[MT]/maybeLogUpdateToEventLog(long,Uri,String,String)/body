{
  int samplePercent=samplePercentForDuration(durationMillis);
  if (samplePercent < 100) {
synchronized (mRandom) {
      if (mRandom.nextInt(100) >= samplePercent) {
        return;
      }
    }
  }
  String blockingPackage=ActivityThread.currentPackageName();
  EventLog.writeEvent(EventLogTags.CONTENT_UPDATE_SAMPLE,uri.toString(),operation,selection != null ? selection : "",durationMillis,blockingPackage != null ? blockingPackage : "",samplePercent);
}
