{
  Preconditions.checkNotNull(url,"url");
  IContentProvider provider=acquireProvider(url);
  if (provider == null) {
    return null;
  }
  try {
    return provider.canonicalize(mPackageName,url);
  }
 catch (  RemoteException e) {
    return null;
  }
 finally {
    releaseProvider(provider);
  }
}
