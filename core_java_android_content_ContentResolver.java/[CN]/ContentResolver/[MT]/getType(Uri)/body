{
  IContentProvider provider=acquireProvider(url);
  if (provider == null) {
    return null;
  }
  try {
    return provider.getType(url);
  }
 catch (  RemoteException e) {
    return null;
  }
catch (  java.lang.Exception e) {
    return null;
  }
 finally {
    releaseProvider(provider);
  }
}
