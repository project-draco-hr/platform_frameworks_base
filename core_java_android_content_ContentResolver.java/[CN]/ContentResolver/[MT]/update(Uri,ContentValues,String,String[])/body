{
  IContentProvider provider=acquireProvider(uri);
  if (provider == null) {
    throw new IllegalArgumentException("Unknown URI " + uri);
  }
  try {
    return provider.update(uri,values,where,selectionArgs);
  }
 catch (  RemoteException e) {
    return -1;
  }
 finally {
    releaseProvider(provider);
  }
}
