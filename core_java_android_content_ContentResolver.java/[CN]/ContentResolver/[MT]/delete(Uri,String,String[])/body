{
  IContentProvider provider=acquireProvider(url);
  if (provider == null) {
    throw new IllegalArgumentException("Unknown URL " + url);
  }
  try {
    return provider.delete(url,where,selectionArgs);
  }
 catch (  RemoteException e) {
    return -1;
  }
 finally {
    releaseProvider(provider);
  }
}
