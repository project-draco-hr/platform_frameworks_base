{
  post(new Runnable(){
    @Override public void run(){
      for (      SignalCallback signalCluster : mSignalCallbacks) {
        signalCluster.setMobileDataIndicators(statusIcon,qsIcon,darkStatusIcon,statusType,qsType,activityIn,activityOut,typeContentDescription,description,isWide,subId);
      }
    }
  }
);
}
