{
  initTestFiles();
  bot.openRoot(ROOT_0_ID);
  assertDefaultContentOfTestDir0();
  String query=fileName1;
  bot.openSearchView();
  bot.setSearchQuery(query);
  device.pressEnter();
  bot.openRoot(ROOT_1_ID);
  assertDefaultContentOfTestDir1();
  bot.openRoot(ROOT_0_ID);
  assertDefaultContentOfTestDir0();
}
