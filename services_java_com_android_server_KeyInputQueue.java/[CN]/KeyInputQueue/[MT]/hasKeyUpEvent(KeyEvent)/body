{
synchronized (mFirst) {
    final int keyCode=origEvent.getKeyCode();
    QueuedEvent cur=mLast.prev;
    while (cur.prev != null) {
      if (cur.classType == RawInputEvent.CLASS_KEYBOARD) {
        KeyEvent ke=(KeyEvent)cur.event;
        if (ke.getAction() == KeyEvent.ACTION_UP && ke.getKeyCode() == keyCode) {
          return true;
        }
      }
      cur=cur.prev;
    }
  }
  return false;
}
