{
  InputDevice.AbsoluteInfo info=new InputDevice.AbsoluteInfo();
  if (getAbsoluteInfo(id,channel,info) && info.minValue != info.maxValue) {
    Log.i(TAG,"  " + name + ": min="+ info.minValue+ " max="+ info.maxValue+ " flat="+ info.flat+ " fuzz="+ info.fuzz);
    info.range=info.maxValue - info.minValue;
    return info;
  }
  Log.i(TAG,"  " + name + ": unknown values");
  return null;
}
