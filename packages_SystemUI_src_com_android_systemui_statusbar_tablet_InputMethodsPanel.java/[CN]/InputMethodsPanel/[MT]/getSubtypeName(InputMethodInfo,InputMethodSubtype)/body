{
  if (imi == null || subtype == null)   return null;
  if (DEBUG) {
    Log.d(TAG,"Get text from: " + imi.getPackageName() + subtype.getNameResId()+ imi.getServiceInfo().applicationInfo);
  }
  return mPackageManager.getText(imi.getPackageName(),subtype.getNameResId(),imi.getServiceInfo().applicationInfo);
}
