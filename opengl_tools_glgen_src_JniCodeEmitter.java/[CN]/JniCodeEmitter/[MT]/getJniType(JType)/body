{
  if (jType.isVoid()) {
    return "void";
  }
  String baseType=jType.getBaseType();
  if (jType.isPrimitive()) {
    if (baseType.equals("String")) {
      return "jstring";
    }
 else {
      return "j" + baseType;
    }
  }
 else   if (jType.isArray()) {
    return "j" + baseType + "Array";
  }
 else {
    return "jobject";
  }
}
