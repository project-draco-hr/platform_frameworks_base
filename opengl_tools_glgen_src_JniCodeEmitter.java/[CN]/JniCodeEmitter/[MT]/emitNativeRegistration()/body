{
  mCStream.println("static const char *classPathName = \"" + mClassPathName + "\";");
  mCStream.println();
  mCStream.println("static JNINativeMethod methods[] = {");
  mCStream.println("{\"_nativeClassInit\", \"()V\", (void*)nativeClassInit },");
  Iterator<String> i=nativeRegistrations.iterator();
  while (i.hasNext()) {
    mCStream.println(i.next());
  }
  mCStream.println("};");
  mCStream.println();
  mCStream.println("int register_com_google_android_gles_jni_GLImpl(JNIEnv *_env)");
  mCStream.println("{");
  mCStream.println(indent + "int err;");
  mCStream.println(indent + "err = android::AndroidRuntime::registerNativeMethods(_env, classPathName, methods, NELEM(methods));");
  mCStream.println(indent + "return err;");
  mCStream.println("}");
}
