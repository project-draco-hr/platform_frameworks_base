{
  cStream.println("static const char *classPathName = \"" + mClassPathName + "\";");
  cStream.println();
  cStream.println("static JNINativeMethod methods[] = {");
  cStream.println("{\"_nativeClassInit\", \"()V\", (void*)nativeClassInit },");
  Iterator<String> i=nativeRegistrations.iterator();
  while (i.hasNext()) {
    cStream.println(i.next());
  }
  cStream.println("};");
  cStream.println();
  cStream.println("int " + registrationFunctionName + "(JNIEnv *_env)");
  cStream.println("{");
  cStream.println(indent + "int err;");
  cStream.println(indent + "err = android::AndroidRuntime::registerNativeMethods(_env, classPathName, methods, NELEM(methods));");
  cStream.println(indent + "return err;");
  cStream.println("}");
}
