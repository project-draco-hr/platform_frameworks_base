{
  Trace.traceBegin(RenderScript.TRACE_TAG,"copy2DRangeFromUnchecked");
  mRS.validate();
  validate2DRange(xoff,yoff,w,h);
  mRS.nAllocationData2D(getIDSafe(),xoff,yoff,mSelectedLOD,mSelectedFace.mID,w,h,data,data.length * 4,Element.DataType.SIGNED_32);
  Trace.traceEnd(RenderScript.TRACE_TAG);
}
