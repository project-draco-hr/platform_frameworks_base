{
switch (srcLocation) {
case USAGE_SCRIPT:
case USAGE_GRAPHICS_CONSTANTS:
case USAGE_GRAPHICS_TEXTURE:
case USAGE_GRAPHICS_VERTEX:
    break;
default :
  throw new RSIllegalArgumentException("Source must be exactly one usage type.");
}
mRS.validate();
mRS.nAllocationSyncAll(getID(),srcLocation);
}
