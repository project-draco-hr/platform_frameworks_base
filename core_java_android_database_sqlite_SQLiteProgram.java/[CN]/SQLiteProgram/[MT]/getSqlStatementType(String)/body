{
  if (mSql.length() < 6) {
    return OTHER_STMT;
  }
  String prefixSql=mSql.substring(0,6);
  if (prefixSql.equalsIgnoreCase("SELECT")) {
    return SELECT_STMT;
  }
 else   if (prefixSql.equalsIgnoreCase("INSERT") || prefixSql.equalsIgnoreCase("UPDATE") || prefixSql.equalsIgnoreCase("REPLAC")|| prefixSql.equalsIgnoreCase("DELETE")) {
    return UPDATE_STMT;
  }
  return OTHER_STMT;
}
