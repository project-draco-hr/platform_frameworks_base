{
  mDatabase.verifyDbIsOpen();
  SQLiteDatabase db=(getSqlStatementType(mSql) != SELECT_STMT) ? mDatabase : mDatabase.getDbConnection(mSql);
  if (!db.equals(mDatabase)) {
    detachObjectFromDatabase();
    attachObjectToDatabase(db);
  }
  mDatabase.lock();
  try {
    compileSql();
  }
  finally {
    mDatabase.unlock();
  }
}
