{
  if (mCompiledSql == null) {
    return;
  }
  if ((mStatementType & STATEMENT_CACHEABLE) == 0) {
    mCompiledSql.releaseSqlStatement();
  }
 else {
    if (!mDatabase.isInStatementCache(mCompiledSql)) {
      mCompiledSql.releaseSqlStatement();
    }
 else {
      mCompiledSql.release();
    }
  }
  mCompiledSql=null;
  nStatement=0;
}
