{
  super.onCreate(savedInstanceState);
  final Intent intent=getIntent();
  if (intent == null) {
    finish();
  }
  final String packageName=intent.getStringExtra(INTENT_EXTRA_PACKAGE_NAME);
  final String className=intent.getStringExtra(INTENT_EXTRA_CLASS_NAME);
  final int npp=intent.getIntExtra(INTENT_EXTRA_NPP_INSTANCE,-1);
  PluginStub stub=PluginUtil.getPluginStub(this,packageName,className);
  if (stub != null) {
    View pluginView=stub.getFullScreenView(npp,this);
    if (pluginView != null) {
      setContentView(pluginView);
    }
 else {
      finish();
    }
  }
 else {
    finish();
  }
}
